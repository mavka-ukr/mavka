    _11: адреса<ПредметТексту>;
    _12: адреса<ПредметТексту>;
    _13: адреса<ПредметТексту>;
    _14: адреса<ПредметТексту>;
    _15: адреса<ПредметТексту>;
    _16: адреса<ПредметТексту>;
    _17: адреса<ПредметТексту>;
    _18: адреса<ПредметТексту>;
    _19: адреса<ПредметТексту>;
    _20: адреса<ПредметТексту>;
    _21: адреса<ПредметТексту>;
    _22: адреса<ПредметТексту>;
    _23: адреса<ПредметТексту>;
    _24: адреса<ПредметТексту>;
    _25: адреса<ПредметТексту>;
    _26: адреса<ПредметТексту>;
    _27: адреса<ПредметТексту>;
    _28: адреса<ПредметТексту>;
    _29: адреса<ПредметТексту>;
    _30: адреса<ПредметТексту>;
    _31: адреса<ПредметТексту>;
    _32: адреса<ПредметТексту>;
    _33: адреса<ПредметТексту>;
    _34: адреса<ПредметТексту>;
    _35: адреса<ПредметТексту>;
    _36: адреса<ПредметТексту>;
    _37: адреса<ПредметТексту>;
