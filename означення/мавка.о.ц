/* Мавка 0.123.0 */

простір мавка {
  стала версія = 0;
}

простір мавка {
  структура Місцезнаходження;
  структура Притримане;
  структура Притримувач;
  структура КористувацькіДані;
  структура Дійсне;
  структура Послідовність<Т>;
  структура Дійсність;
  структура ПредметДаних;
  структура ПредметДіапазонуДробових;
  структура ПредметДіапазонуЦілих;
  структура ПредметДії;
  структура ПредметДробового;
  структура ПредметЗміннихДаних;
  структура ПредметКоду;
  структура ПредметМодуля;
  структура ПредметПараметра;
  структура ПредметПереборуДіапазонуДробових;
  структура ПредметПереборуДіапазонуЦілих;
  структура ПредметСловника;
  структура ПредметСписку;
  структура ПредметСтруктури;
  структура ПредметТексту;
  структура ПредметЦілого;
  структура СкладенийПредмет;
  структура Машина;

  синонім Значення = адреса<Дійсне>;
  синонім ЗначенняАбоСтанПадіння = Значення;
  синонім СтанПадіння = ніщо;

  стала ВПокластиНедійсне = 0; // []() -> [недійсне]
  стала ВПокластиДійсне = 1; // []() -> [дійсне]
  стала ВПокластиПослідовність = 2; // []() -> [послідовність]
  стала ВПокластиПредмет = 3; // []() -> [Предмет]
  стала ВПокластиЯ = 4; // []() -> [значення_я]
  стала ВПокластиКонстанту = 5; // [](позиція_константи) -> [константа]
  стала ВСтворитиПослідовність = 6; // [...](кількість_елементів) -> [значення_послідовності]
  стала ВСтворитиДію = 7; // [назва, тип_результату, ..., код](кількість_параметрів) -> [предмет_дії]
  стала ВСтворитиСтруктуру = 8; // [назва, предок, ...](кількість_параметрів) -> [предмет_структури]
  стала ВСтворитиМодуль = 9; // [назва, код]() -> [предмет_модуля]
  стала ВСтворитиСписок = 10; // [...](кількість_елементів) -> [предмет_списку]
  стала ВСтворитиСловник = 11; // [...](кількість_елементів) -> [предмет_словника]
  стала ВСтворитиДіапазон = 12; // [від, включно, до]() -> [предмет_діапазону]
  стала ВСтворитиПараметр = 13; // [назва, тип, дія_значення]() -> [параметр]
  стала ВСтворитиТип = 14; // [...](кількість_структур) -> [тип]
  стала ВЗаписатиМетод = 15; // [значення_власна, предмет_структури, назва, предмет_дії]() -> [предмет_дії]
  стала ВВизначити = 16; // [назва, значення]() -> []
  стала ВЗвернутись = 17; // [назва]() -> [предмет]
  стала ВЗдійснити = 18; // [предмет_дії, ..., ...](кількість_аргументів, кількість_іменованих_аргументів) -> [результат]
  стала ВОтриматиВластивість = 19; // [предмет, назва]() -> [предмет_властивості]
  стала ВЗмінитиВластивість = 20; // [предмет, назва, значення]() -> []
  стала ВЗдійснитиВластивість = 21; // [предмет, назва, ..., ...](кількість_аргументів, кількість_іменованих_аргументів) -> [результат]
  стала ВОтриматиЕлемент = 22; // [предмет, ключ]() -> [елемент]
  стала ВЗмінитиЕлемент = 23; // [предмет, ключ, значення]() -> []
  стала ВОтриматиПеребір = 24; // [предмет]() -> [предмет_перебору]
  стала ВДодати = 25; // [а, б]() -> [результат]
  стала ВВідняти = 26; // [а, б]() -> [результат]
  стала ВПомножити = 27; // [а, б]() -> [результат]
  стала ВПоділити = 28; // [а, б]() -> [результат]
  стала ВОтриматиОстачуДілення = 29; // [а, б]() -> [результат]
  стала ВОтриматиНеповнуЧасткуДілення = 30; // [а, б]() -> [результат]
  стала ВПіднестиДоСтепеня = 31; // [а, б]() -> [результат]
  стала ВЗсунутиВліво = 32; // [а, б]() -> [результат]
  стала ВЗсунутиВправо = 33; // [а, б]() -> [результат]
  стала ВЗсунутиВправо2 = 34; // [а, б]() -> [результат]
  стала ВДвійковеІ = 35; // [а, б]() -> [результат]
  стала ВДвійковеАБО = 36; // [а, б]() -> [результат]
  стала ВДвійковеВиключнеАБО = 37; // [а, б]() -> [результат]
  стала ВМенше = 38; // [а, б]() -> [результат]
  стала ВБільше = 39; // [а, б]() -> [результат]
  стала ВНеБільше = 40; // [а, б]() -> [результат]
  стала ВНеМенше = 41; // [а, б]() -> [результат]
  стала ВРівно = 42; // [а, б]() -> [результат]
  стала ВНеРівно = 43; // [а, б]() -> [результат]
  стала ВМістить = 44; // [а, б]() -> [результат]
  стала ВНеМістить = 45; // [а, б]() -> [результат]
  стала ВЄ = 46; // [а, б]() -> [результат]
  стала ВНеЄ = 47; // [а, б]() -> [результат]
  стала ВДвійковеНЕ = 48; // [а]() -> [результат]
  стала ВВідʼємне = 49; // [а]() -> [результат]
  стала ВЗаперечити = 51; // [а]() -> [результат]
  стала ВПочатиСпробу = 52; // [назва](позиція_вказівки_зловити) -> []
  стала ВВпасти = 53; // [предмет_помилки]() -> []
  стала ВЗакінчитиСпробу = 54; // [](позиція_вказівки_виходу) -> []
  стала ВВзяти = 55; // [назва, ...](кількість_назв) -> [предмет_модуля]
  стала ВДати = 56; // [предмет, назва]() -> []
  стала ВПеребратиДалі = 57; // [предмет_перебору]() -> [предмет_перебору, результат_виконання]
  стала ВЗбитиТекст = 58; // [...](кількість_предметів) -> [збитий_текст]
  стала ВСтрибнути = 59; // [](позиція_вказівки) -> []
  стала ВСтрибнутиЯкщоНі = 60; // [предмет](позиція_вказівки) -> []
  стала ВЗабрати = 61; // [предмет]() -> []
  стала ВВернути = 62; // [предмет]() -> []
  стала ВСтрибнутиЯкщоТак = 63; // [предмет](позиція_вказівки) -> [предмет]
  стала ВДублювати = 64; // [предмет]() -> [предмет, предмет]
  стала ВВизначитиНавпаки = 65; // [значення, назва]() -> []

  структура Шлях {
    розмір: природне;
    дані: памʼять<п8>;
  }

  структура Місцезнаходження {
    шлях_або_код: адреса<ПредметДаних> | адреса<ПредметКоду>;
    рядок_або_позиція_вказівки: природне;
  }

  структура Притримане {
    попереднє: адреса<Притримане>;
    наступне: адреса<Притримане>;
    значення: Значення;
  }

  структура Притримувач {
    перше: адреса<Притримане>;
    останнє: адреса<Притримане>;
  }

  синонім ДіяПередЗнищеннямКористувацькихДаних = (М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>) -> ніщо;

  структура КористувацькіДані {
    значення: невідома_адреса;
    притримувач: адреса<Притримувач>;
    дія_перед_знищенням: ДіяПередЗнищеннямКористувацькихДаних;
  }

  структура НакопичувачТексту {
    розмір: природне;
    дані: памʼять<п8>;
    вмісткість: природне;
  }

  структура Дійсне {
    наступне: Значення;
    позначено: логічне;
    тип: Значення;
    користувацькі_дані: адреса<КористувацькіДані>;
  }

  структура Послідовність<Т> {
    наступне: Значення;
    позначено: логічне;
    тип: Значення;
    користувацькі_дані: адреса<КористувацькіДані>;
    розмір: природне;
    дані: памʼять<Т>;
  }

  структура Властивість {
    назва: адреса<ПредметТексту>;
    значення: Значення;
  }

  структура Властивості {
    розмір: природне;
    дані: памʼять<Властивість>;
  }

  структура Дійсність {
    наступне: Значення;
    позначено: логічне;
    тип: Значення;
    користувацькі_дані: адреса<КористувацькіДані>;
    зовні: адреса<Дійсність>;
    властивості: Властивості;
  }

  структура Метод {
    назва: адреса<ПредметТексту>;
    значення: адреса<ПредметДії>;
  }

  структура Методи {
    розмір: природне;
    дані: памʼять<Метод>;
  }

  структура ІменованоЗадіяне {
    назва: адреса<ПредметТексту>;
    значення: Значення;
  }

  структура ІменованіЗадіяні {
    розмір: природне;
    елементи: памʼять<ІменованоЗадіяне>;
  }

  структура Задіяні {
    розмір: природне;
    дані: памʼять<Значення>;
    іменовано_задіяні: адреса<ІменованіЗадіяні>;
  }

  синонім РіднаДія = (
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> Значення;

  структура ЕлементСловника {
    ключ: Значення;
    значення: Значення;
  }

  структура ПредметСтруктури {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    заборонити_наслідування: логічне;
    назва: адреса<ПредметТексту>;
    предок: адреса<ПредметСтруктури>;
    параметри: адреса<Послідовність<адреса<ПредметПараметра>>>;
    методи: Методи;
    властивості: Властивості;
  }

  структура ПредметДії {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    дійсність_створення: адреса<Дійсність>;
    рідна: логічне;
    назва: адреса<ПредметТексту>;
    параметри: адреса<Послідовність<адреса<ПредметПараметра>>>;
    тип_результату: Значення;
    код: адреса<ПредметКоду> | РіднаДія;
    метод_структури: адреса<ПредметСтруктури>;
    шлях_до_файлу_модуля: адреса<ПредметДаних>;
  }

  структура ПредметСписку {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    розмір: природне;
    елементи: памʼять<Значення>;
  }

  структура ПредметСловника {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    розмір: природне;
    елементи: памʼять<ЕлементСловника>;
  }

  структура ПредметМодуля {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    готовий: логічне;
    назва: адреса<ПредметТексту>;
    властивості: Властивості;
  }

  структура ПредметПараметра {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    назва: адреса<ПредметТексту>;
    тип_: невідома_адреса;
    дія_значення: Значення;
  }

  структура ПредметЦілого {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    значення: ц64;
  }

  структура ПредметДробового {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    значення: д64;
  }

  структура ПредметТексту {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    значення: кд;
  }

  структура МісцезнаходженняВказівки {
    початок: п32;
    кінець: п32;
    рядок: п32;
  }

  структура МісцезнаходженняВказівок {
    розмір: природне;
    дані: памʼять<МісцезнаходженняВказівки>;
  }

  структура ПредметКоду {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    константи: адреса<Послідовність<Значення>>;
    вказівки: адреса<ПредметДаних>;
    шлях: адреса<ПредметДаних>;
    місцезнаходження_вказівок: МісцезнаходженняВказівок;
  }

  структура ПредметДаних {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    розмір: природне;
    дані: памʼять<п8>;
  }

  структура ПредметЗміннихДаних {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    розмір: природне;
    дані: памʼять<п8>;
    вмісткість: природне;
  }

  структура ПредметДіапазонуЦілих {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    від: адреса<ПредметЦілого>;
    до: адреса<ПредметЦілого>;
    включно: логічне;
  }

  структура ПредметДіапазонуДробових {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    від: адреса<ПредметДробового>;
    до: адреса<ПредметДробового>;
    включно: логічне;
  }

  структура ПредметПереборуДіапазонуЦілих {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    предмет_діапазону: адреса<ПредметДіапазонуЦілих>;
    поточне_значення: адреса<ПредметЦілого>;
  }

  структура ПредметПереборуДіапазонуДробових {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    предмет_діапазону: адреса<ПредметДіапазонуДробових>;
    поточне_значення: адреса<ПредметДробового>;
  }

  структура ПредметПереборуСписку {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    предмет_списку: адреса<ПредметСписку>;
    почато: логічне;
    позиція: природне;
    поточне_значення: Значення;
  }

  структура СкладенийПредмет {
    наступне: Значення;
    позначено: логічне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    властивості: Властивості;
  }

  структура Налаштування {
    виділити_сиру_памʼять: (М: адреса<Машина>, розмір: природне) -> памʼять<п8>;
    перевиділити_сиру_памʼять: (М: адреса<Машина>, значення: памʼять<п8>, новий_розмір: природне) -> памʼять<п8>;
    звільнити_сиру_памʼять: (М: адреса<Машина>, значення: памʼять<п8>) -> ніщо;
    запанікувати: (М: адреса<Машина>, повідомлення: кд) -> ніщо;
    вивести_кд: (М: адреса<Машина>, значення: кд) -> ніщо;
    перетворити_п64_в_кд: (М: адреса<Машина>, значення: п64, вихід_текстового_значення: адреса<кд>) -> логічне;
    перетворити_ц64_в_кд: (М: адреса<Машина>, значення: ц64, вихід_текстового_значення: адреса<кд>) -> логічне;
    перетворити_д64_в_кд: (М: адреса<Машина>, значення: д64, вихід_текстового_значення: адреса<кд>) -> логічне;
    піднести_до_степеня_п64: (М: адреса<Машина>, значення: п64, степінь: п64) -> п64;
    піднести_до_степеня_ц64: (М: адреса<Машина>, значення: ц64, степінь: ц64) -> ц64;
    піднести_до_степеня_д64: (М: адреса<Машина>, значення: д64, степінь: д64) -> д64;
    взяти_модуль: (М: адреса<Машина>, шлях_до_файлу_поточного_модуля: адреса<ПредметДаних>, відносно: логічне, довжина_шляху: природне, шлях: памʼять<адреса<ПредметТексту>>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
    взяти_пак: (М: адреса<Машина>, довжина_шляху: природне, шлях: памʼять<адреса<ПредметТексту>>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
    взяти_біб: (М: адреса<Машина>, довжина_шляху: природне, шлях: памʼять<адреса<ПредметТексту>>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  }

  структура НакопичувачЗначень {
    розмір: природне;
    дані: памʼять<Значення>;
    вмісткість: природне;
  }

  структура Спроба {
    назва_зловити: адреса<ПредметТексту>;
    позиція_вказівки_зловити: природне;
    розмір_історії_здійснення: природне;
    розмір_накопичувача_значень: природне;
  }

  структура Спроби {
    розмір: природне;
    дані: памʼять<Спроба>;
  }

  структура ЕлементІсторіїЗдійснення {
    спроби: Спроби;
    дійсність: адреса<Дійсність>;
    здійснювач: Значення;
    предмет_дії: адреса<ПредметДії>;
    задіяні: Задіяні;
    місцезнаходження: Місцезнаходження;
  }

  структура ІсторіяЗдійснення {
    розмір: природне;
    дані: памʼять<ЕлементІсторіїЗдійснення>;
    вмісткість: природне;
  }

  структура МісцезнаходженняІнформаціїПадіння {
    шлях: адреса<ПредметДаних>;
    рядок: природне;
  }

  структура ЕлементІсторіїЗдійсненняІнформаціїПадіння {
    місцезнаходження: МісцезнаходженняІнформаціїПадіння;
    назва: адреса<ПредметТексту>;
  }

  структура ІнформаціяПадіння {
    значення: Значення;
    місцезнаходження: МісцезнаходженняІнформаціїПадіння;
    розмір_історії: природне;
    історія: памʼять<ЕлементІсторіїЗдійсненняІнформаціїПадіння>;
  }

  структура КонстантиНазв {
    _значення: адреса<ПредметТексту>;
    _дійсне: адреса<ПредметТексту>;
    _послідовність: адреса<ПредметТексту>;
    _дійсність: адреса<ПредметТексту>;
    _Предмет: адреса<ПредметТексту>;
    _Структура: адреса<ПредметТексту>;
    _Модуль: адреса<ПредметТексту>;
    _Параметр: адреса<ПредметТексту>;
    _Число: адреса<ПредметТексту>;
    _Ціле: адреса<ПредметТексту>;
    _Дробове: адреса<ПредметТексту>;
    _Текст: адреса<ПредметТексту>;
    _Код: адреса<ПредметТексту>;
    _Дія: адреса<ПредметТексту>;
    _Список: адреса<ПредметТексту>;
    _Словник: адреса<ПредметТексту>;
    _Дані: адреса<ПредметТексту>;
    _ЗмінніДані: адреса<ПредметТексту>;
    _ДіапазонЦілих: адреса<ПредметТексту>;
    _ДіапазонДробових: адреса<ПредметТексту>;
    _ПеребірДіапазонуЦілих: адреса<ПредметТексту>;
    _ПеребірДіапазонуДробових: адреса<ПредметТексту>;
    _ПеребірСписку: адреса<ПредметТексту>;
    _далі: адреса<ПредметТексту>;
    _з: адреса<ПредметТексту>;
    _чародія_ціле: адреса<ПредметТексту>;
    _чародія_текст: адреса<ПредметТексту>;
    _довжина: адреса<ПредметТексту>;
    _0: адреса<ПредметТексту>;
    _1: адреса<ПредметТексту>;
    _2: адреса<ПредметТексту>;
    _3: адреса<ПредметТексту>;
    _4: адреса<ПредметТексту>;
    _5: адреса<ПредметТексту>;
    _6: адреса<ПредметТексту>;
    _7: адреса<ПредметТексту>;
    _8: адреса<ПредметТексту>;
    _9: адреса<ПредметТексту>;
    _10: адреса<ПредметТексту>;
    _11: адреса<ПредметТексту>;
    _12: адреса<ПредметТексту>;
    _13: адреса<ПредметТексту>;
    _14: адреса<ПредметТексту>;
    _15: адреса<ПредметТексту>;
    _16: адреса<ПредметТексту>;
    _17: адреса<ПредметТексту>;
    _18: адреса<ПредметТексту>;
    _19: адреса<ПредметТексту>;
    _20: адреса<ПредметТексту>;
    _21: адреса<ПредметТексту>;
    _22: адреса<ПредметТексту>;
    _23: адреса<ПредметТексту>;
    _24: адреса<ПредметТексту>;
    _25: адреса<ПредметТексту>;
    _26: адреса<ПредметТексту>;
    _27: адреса<ПредметТексту>;
    _28: адреса<ПредметТексту>;
    _29: адреса<ПредметТексту>;
    _30: адреса<ПредметТексту>;
    _31: адреса<ПредметТексту>;
    _32: адреса<ПредметТексту>;
    _33: адреса<ПредметТексту>;
    _34: адреса<ПредметТексту>;
    _35: адреса<ПредметТексту>;
    _36: адреса<ПредметТексту>;
    _37: адреса<ПредметТексту>;
    _38: адреса<ПредметТексту>;
    _39: адреса<ПредметТексту>;
    _40: адреса<ПредметТексту>;
    _41: адреса<ПредметТексту>;
    _42: адреса<ПредметТексту>;
    _43: адреса<ПредметТексту>;
    _44: адреса<ПредметТексту>;
    _45: адреса<ПредметТексту>;
    _46: адреса<ПредметТексту>;
    _47: адреса<ПредметТексту>;
    _48: адреса<ПредметТексту>;
    _49: адреса<ПредметТексту>;
    _50: адреса<ПредметТексту>;
    _51: адреса<ПредметТексту>;
    _52: адреса<ПредметТексту>;
    _53: адреса<ПредметТексту>;
    _54: адреса<ПредметТексту>;
    _55: адреса<ПредметТексту>;
    _56: адреса<ПредметТексту>;
    _57: адреса<ПредметТексту>;
    _58: адреса<ПредметТексту>;
    _59: адреса<ПредметТексту>;
    _60: адреса<ПредметТексту>;
    _61: адреса<ПредметТексту>;
    _62: адреса<ПредметТексту>;
    _63: адреса<ПредметТексту>;
    _модуль: адреса<ПредметТексту>;
    _пак: адреса<ПредметТексту>;
    _біб: адреса<ПредметТексту>;
    _математика: адреса<ПредметТексту>;
    _ПІ: адреса<ПредметТексту>;
  }

  структура ВзятеЗначення {
    шлях: адреса<ПредметДаних>;
    значення: Значення;
  }

  структура ВзятіЗначення {
    розмір: природне;
    дані: памʼять<ВзятеЗначення>;
  }

  структура ЕлементЧергиЗдійснення;

  структура ЕлементЧергиЗдійснення {
    наступний: адреса<ЕлементЧергиЗдійснення>;
    значення: Значення;
    задіяні: Задіяні;
  }

  структура ЧергаЗдійснення {
    перший: адреса<ЕлементЧергиЗдійснення>;
    останній: адреса<ЕлементЧергиЗдійснення>;
  }

  структура Машина {
    налаштування: Налаштування;
    перше_дійсне: Значення;
    останнє_дійсне: Значення;
    кількість_дійсних: природне;
    кількість_дійсних_для_очищення: природне;
    потребує_очищення: логічне;
    значення_дійсне: Значення;
    значення_послідовність: Значення;
    значення_дійсність: Значення;
    значення_пустої_послідовності: адреса<Послідовність<Значення>>;
    предмет_структури_Предмет: адреса<ПредметСтруктури>;
    предмет_структури_Структура: адреса<ПредметСтруктури>;
    предмет_структури_Дані: адреса<ПредметСтруктури>;
    предмет_структури_ДіапазонДробових: адреса<ПредметСтруктури>;
    предмет_структури_ДіапазонЦілих: адреса<ПредметСтруктури>;
    предмет_структури_Дія: адреса<ПредметСтруктури>;
    предмет_структури_Дробове: адреса<ПредметСтруктури>;
    предмет_структури_ЗмінніДані: адреса<ПредметСтруктури>;
    предмет_структури_Код: адреса<ПредметСтруктури>;
    предмет_структури_Модуль: адреса<ПредметСтруктури>;
    предмет_структури_Параметр: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірДіапазонуДробових: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірДіапазонуЦілих: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірСписку: адреса<ПредметСтруктури>;
    предмет_структури_Словник: адреса<ПредметСтруктури>;
    предмет_структури_Список: адреса<ПредметСтруктури>;
    предмет_структури_Текст: адреса<ПредметСтруктури>;
    предмет_структури_Ціле: адреса<ПредметСтруктури>;
    предмет_структури_Число: адреса<ПредметСтруктури>;
    глобальна_дійсність: адреса<Дійсність>;
    накопичувач_значень: НакопичувачЗначень;
    історія_здійснення: ІсторіяЗдійснення;
    стан_падіння: логічне;
    значення_падіння: Значення;
    місцезнаходження_падіння: Місцезнаходження;
    константи_назв: КонстантиНазв;
    взяті_значення: ВзятіЗначення;
    користувацькі_дані: адреса<КористувацькіДані>;
    черга_здійснення: ЧергаЗдійснення;
  }

  дія виділити<Т>(М: адреса<Машина>) -> адреса<Т> {
    вернути М.налаштування.виділити_сиру_памʼять(М, Т.розмір) як адреса<Т>;
  }

  дія виділити_памʼять<Т>(М: адреса<Машина>, кількість: природне) -> памʼять<Т> {
    вернути М.налаштування.виділити_сиру_памʼять(М, кількість * Т.розмір) як памʼять<Т>;
  }

  дія перевиділити_памʼять<Т>(М: адреса<Машина>, значення: невідома_памʼять, нова_кількість: природне) -> памʼять<Т> {
    вернути М.налаштування.перевиділити_сиру_памʼять(М, значення як памʼять<п8>, нова_кількість * Т.розмір) як памʼять<Т>;
  }

  дія звільнити(М: адреса<Машина>, значення: невідома_адреса) -> ніщо {
    М.налаштування.звільнити_сиру_памʼять(М, значення як памʼять<п8>);
  }

  дія звільнити_памʼять(М: адреса<Машина>, значення: невідома_памʼять) -> ніщо {
    М.налаштування.звільнити_сиру_памʼять(М, значення як памʼять<п8>);
  }

  дія запанікувати(М: адреса<Машина>, повідомлення: кд) {
    М.налаштування.запанікувати(М, повідомлення);
  }

  дія вивести_кд(М: адреса<Машина>, значення: кд) {
    М.налаштування.вивести_кд(М, значення);
  }

  дія надрукувати_кд(М: адреса<Машина>, значення: кд) {
    вивести_кд(М, значення);
    вивести_кд(М, "\р");
  }

  дія перетворити_п64_в_кд(М: адреса<Машина>, значення: п64, вихід_текстового_значення: адреса<кд>) -> логічне {
    вернути М.налаштування.перетворити_п64_в_кд(М, значення, вихід_текстового_значення);
  }

  дія перетворити_ц64_в_кд(М: адреса<Машина>, значення: ц64, вихід_текстового_значення: адреса<кд>) -> логічне {
    вернути М.налаштування.перетворити_ц64_в_кд(М, значення, вихід_текстового_значення);
  }

  дія перетворити_д64_в_кд(М: адреса<Машина>, значення: д64, вихід_текстового_значення: адреса<кд>) -> логічне {
    вернути М.налаштування.перетворити_д64_в_кд(М, значення, вихід_текстового_значення);
  }

  дія вивести_п64(М: адреса<Машина>, значення: п64) {
    змінна текстове_значення: кд;

    якщо перетворити_п64_в_кд(М, значення, текстове_значення::адреса) {
      вивести_кд(М, текстове_значення);

      звільнити_памʼять(М, текстове_значення.дані);
    }
  }

  дія надрукувати_п64(М: адреса<Машина>, значення: п64) {
    вивести_п64(М, значення);
    вивести_кд(М, "\р");
  }

  дія вивести_ц64(М: адреса<Машина>, значення: ц64) {
    змінна текстове_значення: кд;

    якщо перетворити_ц64_в_кд(М, значення, текстове_значення::адреса) {
      вивести_кд(М, текстове_значення);

      звільнити_памʼять(М, текстове_значення.дані);
    }
  }

  дія надрукувати_ц64(М: адреса<Машина>, значення: ц64) {
    вивести_ц64(М, значення);
    вивести_кд(М, "\р");
  }

  дія вивести_д64(М: адреса<Машина>, значення: д64) {
    змінна текстове_значення: кд;

    якщо перетворити_д64_в_кд(М, значення, текстове_значення::адреса) {
      вивести_кд(М, текстове_значення);

      звільнити_памʼять(М, текстове_значення.дані);
    }
  }

  дія надрукувати_д64(М: адреса<Машина>, значення: д64) {
    вивести_д64(М, значення);
    вивести_кд(М, "\р");
  }

  дія піднести_до_степеня_п64(М: адреса<Машина>, значення: п64, степінь: п64) -> п64 {
    вернути М.налаштування.піднести_до_степеня_п64(М, значення, степінь);
  }

  дія піднести_до_степеня_ц64(М: адреса<Машина>, значення: ц64, степінь: ц64) -> ц64 {
    вернути М.налаштування.піднести_до_степеня_ц64(М, значення, степінь);
  }

  дія піднести_до_степеня_д64(М: адреса<Машина>, значення: д64, степінь: д64) -> д64 {
    вернути М.налаштування.піднести_до_степеня_д64(М, значення, степінь);
  }
}

простір мавка {
  структура Накопичувач<Т> {
    розмір: природне;
    дані: памʼять<Т>;
    вмісткість: природне;
  }

  дія виділити_накопичувач<Т>(М: адреса<Машина>, початкова_вмісткість: природне) -> адреса<Накопичувач<Т>> {
    змінна накопичувач = виділити<Накопичувач<Т>>();

    накопичувач.розмір = 0;
    якщо початкова_вмісткість > 0 {
      накопичувач.дані = виділити_памʼять<Т>(М, початкова_вмісткість);
    } інакше {
      накопичувач.дані = пусто;
    }
    накопичувач.вмісткість = початкова_вмісткість;

    вернути накопичувач;
  }

  дія зробити_накопичувач<Т>(М: адреса<Машина>, початкова_вмісткість: природне) -> Накопичувач<Т> {
    змінна накопичувач: Накопичувач<Т>;

    накопичувач.розмір = 0;
    якщо початкова_вмісткість > 0 {
      накопичувач.дані = виділити_памʼять<Т>(М, початкова_вмісткість);
    } інакше {
      накопичувач.дані = пусто;
    }
    накопичувач.вмісткість = початкова_вмісткість;

    вернути накопичувач;
  }

  дія покласти_в_накопичувач<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, значення: Т) {
    якщо накопичувач.розмір == накопичувач.вмісткість {
      накопичувач.вмісткість += 1;
      накопичувач.дані = перевиділити_памʼять<Т>(М, накопичувач.дані, накопичувач.вмісткість);
    }

    накопичувач.дані[накопичувач.розмір] = значення;
    накопичувач.розмір += 1;
  }

  дія змінити_в_накопичувачі<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, позиція: природне, значення: Т) {
    якщо позиція >= накопичувач.вмісткість {
      накопичувач.вмісткість = позиція + 1;
      накопичувач.дані = перевиділити_памʼять<Т>(М, накопичувач.дані, накопичувач.вмісткість);
    }

    накопичувач.дані[позиція] = значення;
    накопичувач.розмір = позиція + 1;
  }

  дія змінити_в_накопичувачі_без_розширення<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, позиція: природне, значення: Т) {
    накопичувач.дані[позиція] = значення;
  }

  дія обернути_накопичувач<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>) {
    змінна п: природне = 0;

    поки п < накопичувач.розмір / 2 {
      змінна тимчасове = накопичувач.дані[п];

      накопичувач.дані[п] = накопичувач.дані[накопичувач.розмір - п - 1];
      накопичувач.дані[накопичувач.розмір - п - 1] = тимчасове;

      п += 1;
    }
  }

  дія звільнити_значення_накопичувача<Т>(М: адреса<Машина>, накопичувач: Накопичувач<Т>) {
    звільнити_памʼять(М, накопичувач.дані);
  }

  дія звільнити_накопичувач<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>) {
    звільнити_значення_накопичувача<Т>(М, накопичувач::вміст);
    звільнити(М, накопичувач);
  }
}

простір мавка {
  дія виділити_дійсне(М: адреса<Машина>) -> Значення {
    змінна дійсне = виділити<Дійсне>(М);

    дійсне.наступне = пусто;
    дійсне.позначено = ні;
    дійсне.тип = М.значення_дійсне;
    дійсне.користувацькі_дані = пусто;

    М.останнє_дійсне.наступне = дійсне;
    М.останнє_дійсне = дійсне;
    М.кількість_дійсних += 1;
    М.потребує_очищення = М.кількість_дійсних >= М.кількість_дійсних_для_очищення;

    вернути дійсне;
  }

  дія виділити_послідовність(М: адреса<Машина>) -> адреса<Послідовність<Значення>> {
    змінна послідовність = виділити<Послідовність<Значення>>(М);

    послідовність.наступне = пусто;
    послідовність.позначено = ні;
    послідовність.тип = М.значення_послідовність;
    послідовність.користувацькі_дані = пусто;
    послідовність.розмір = 0;
    послідовність.дані = пусто;

    М.останнє_дійсне.наступне = послідовність як Значення;
    М.останнє_дійсне = послідовність як Значення;
    М.кількість_дійсних += 1;
    М.потребує_очищення = М.кількість_дійсних >= М.кількість_дійсних_для_очищення;

    вернути послідовність;
  }

  дія виділити_дійсність(М: адреса<Машина>) -> адреса<Дійсність> {
    змінна дійсність = виділити<Дійсність>(М);

    дійсність.наступне = пусто;
    дійсність.позначено = ні;
    дійсність.тип = М.значення_дійсність;
    дійсність.користувацькі_дані = пусто;
    дійсність.зовні = пусто;
    дійсність.властивості = Властивості { 0, пусто };

    М.останнє_дійсне.наступне = дійсність як Значення;
    М.останнє_дійсне = дійсність як Значення;
    М.кількість_дійсних += 1;
    М.потребує_очищення = М.кількість_дійсних >= М.кількість_дійсних_для_очищення;

    вернути дійсність;
  }

  дія виділити_предмет<Т>(М: адреса<Машина>, тип: адреса<ПредметСтруктури>) -> адреса<Т> {
    змінна предмет = виділити<Т>(М);

    предмет.наступне = пусто;
    предмет.позначено = ні;
    предмет.тип = тип;
    предмет.користувацькі_дані = пусто;

    М.останнє_дійсне.наступне = предмет як Значення;
    М.останнє_дійсне = предмет як Значення;
    М.кількість_дійсних += 1;
    М.потребує_очищення = М.кількість_дійсних >= М.кількість_дійсних_для_очищення;

    вернути предмет;
  }

  // ВзятіЗначення
  місцева дія записати_взяте_значення(М: адреса<Машина>, шлях: адреса<ПредметДаних>, значення: Значення);
  місцева дія знайти_взяте_значення(М: адреса<Машина>, шлях: адреса<ПредметДаних>, вихід_значення: адреса<Значення>) -> логічне;
  місцева дія видалити_взяте_значення(М: адреса<Машина>, шлях: адреса<ПредметДаних>) -> логічне;

  // Властивості
  місцева дія записати_властивість(М: адреса<Машина>, властивості: адреса<Властивості>, назва: адреса<ПредметТексту>, значення: Значення);
  місцева дія знайти_властивість(М: адреса<Машина>, властивості: Властивості, назва: адреса<ПредметТексту>, вихід: адреса<Значення>) -> логічне;

  // Дійсність
  місцева дія створити_дійсність(М: адреса<Машина>, зовні: адреса<Дійсність>) -> адреса<Дійсність>;
  місцева дія записати_в_дійсність(М: адреса<Машина>, дійсність: адреса<Дійсність>, назва: адреса<ПредметТексту>, значення: Значення);
  місцева дія знайти_в_дійсності(М: адреса<Машина>, дійсність: адреса<Дійсність>, назва: адреса<ПредметТексту>, вихід_значення: адреса<Значення>) -> логічне;

  // Допоміжне
  місцева дія перекодувати_ю8_в_кд(М: адреса<Машина>, вхід: ю8, вихід: адреса<кд>, вихід_позиції_помилки: адреса<природне>) -> логічне;
  місцева дія перекодувати_кд_в_ю8(М: адреса<Машина>, вхід: кд, вихід: адреса<ю8>, вихід_позиції_помилки: адреса<природне>) -> логічне;
  місцева дія надрукувати_код(М: адреса<Машина>, розмір: природне, дані: памʼять<п8>, дія_виводу: (М: адреса<Машина>, значення: кд) -> ніщо);

  // Задіяні
  синонім ДіяПереборуПозиційнихЗадіяних = (М: адреса<Машина>, позиція: природне, значення: Значення, аргумент: невідома_адреса) -> ніщо;
  синонім ДіяПереборуІменованихЗадіяних = (М: адреса<Машина>, позиція: природне, назва: адреса<ПредметТексту>, значення: Значення, аргумент: невідома_адреса) -> ніщо;
  місцева дія знайти_задіяне(М: адреса<Машина>, задіяні: Задіяні, позиція: природне, назва: адреса<ПредметТексту>, вихід_значення: адреса<Значення>) -> логічне;
  місцева дія перебрати_позиційні_задіяні(М: адреса<Машина>, задіяні: Задіяні, дія_перебору: ДіяПереборуПозиційнихЗадіяних, аргумент: невідома_адреса);
  місцева дія перебрати_іменовані_задіяні(М: адреса<Машина>, задіяні: Задіяні, дія_перебору: ДіяПереборуІменованихЗадіяних, аргумент: невідома_адреса);
  місцева дія звільнити_задіяні(М: адреса<Машина>, задіяні: Задіяні);

  // Значення
  місцева дія здійснити(М: адреса<Машина>, здійснювач: Значення, значення: Значення,  задіяні: Задіяні, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія отримати_властивість(М: адреса<Машина>, предмет: Значення, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія змінити_властивість(М: адреса<Машина>, предмет: Значення, назва: адреса<ПредметТексту>, значення: Значення, місцезнаходження: Місцезнаходження) -> СтанПадіння;
  місцева дія здійснити_властивість(М: адреса<Машина>, предмет: Значення, назва: адреса<ПредметТексту>, задіяні: Задіяні, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія отримати_елемент(М: адреса<Машина>, предмет: Значення, ключ: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія змінити_елемент(М: адреса<Машина>, предмет: Значення, ключ: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> СтанПадіння;
  місцева дія отримати_текст(М: адреса<Машина>, значення: Значення, місцезнаходження: Місцезнаходження) -> адреса<ПредметТексту>;
  місцева дія отримати_діапазон(М: адреса<Машина>, від: Значення, до: Значення, включно: логічне, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія отримати_перебір(М: адреса<Машина>, предмет: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_додати(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_відняти(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_помножити(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_поділити(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_остача(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_частка(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_степінь(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_вліво(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_вправо(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_двійкове_і(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_двійкове_або(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_двійкове_вабо(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_менше(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_більше(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_містить(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_двійкове_не(М: адреса<Машина>, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_змінити_знак(М: адреса<Машина>, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія перевірити_чи_значення_рівні(М: адреса<Машина>, а: Значення, б: Значення) -> логічне;
  місцева дія перевірити_чи_значення_є(М: адреса<Машина>, а: Значення, б: адреса<ПредметСтруктури>) -> логічне;

  // ІменованіЗадіяні
  місцева дія створити_іменовано_задіяні(М: адреса<Машина>) -> адреса<ІменованіЗадіяні>;
  місцева дія знищити_іменовано_задіяні(М: адреса<Машина>, іменовано_задіяні: адреса<ІменованіЗадіяні>) -> ніщо;
  місцева дія покласти_іменовано_задіяне(М: адреса<Машина>, іменовано_задіяні: адреса<ІменованіЗадіяні>, назва: адреса<ПредметТексту>, значення: Значення);

  // ІнформаціяПадіння
  місцева дія отримати_інформацію_падіння(М: адреса<Машина>, вихід: адреса<ІнформаціяПадіння>) -> логічне;
  місцева дія знищити_інформацію_падіння(М: адреса<Машина>, інформація_падіння: ІнформаціяПадіння) -> ніщо;

  // КористувацькіДані
  місцева дія створити_користувацькі_дані(М: адреса<Машина>, значення: невідома_адреса, дія_перед_знищенням: ДіяПередЗнищеннямКористувацькихДаних) -> адреса<КористувацькіДані>;
  місцева дія притримати_в_користувацьких_даних(М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>, значення: Значення) -> адреса<Притримане>;
  місцева дія відпустити_з_користувацьких_даних(М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>, притримане: адреса<Притримане>);

  // Машина
  місцева дія налаштувати_машину(М: адреса<Машина>, налаштування: Налаштування);
  місцева дія розлаштувати_машину(М: адреса<Машина>);
  місцева дія увімкнути_стан_падіння(М: адреса<Машина>, значення_падіння: Значення, місцезнаходження_падіння: Місцезнаходження);
  місцева дія вимкнути_стан_падіння(М: адреса<Машина>);
  місцева дія покласти_в_історію_здійснення(М: адреса<Машина>, дійсність: адреса<Дійсність>, здійснювач: Значення, предмет_дії: адреса<ПредметДії>, задіяні: Задіяні, місцезнаходження: Місцезнаходження) -> адреса<ЕлементІсторіїЗдійснення>;
  місцева дія видалити_з_історії_здійснення(М: адреса<Машина>, кількість: природне);
  місцева дія покласти_в_чергу_здійснення(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, задіяні: Задіяні);
  місцева дія забрати_з_черги_здійснення(М: адреса<Машина>, вихід: адреса<ЕлементЧергиЗдійснення>) -> логічне;
  місцева дія взяти_модуль(М: адреса<Машина>, шлях_до_файлу_поточного_модуля: адреса<ПредметДаних>, відносно: логічне, довжина_шляху: природне, шлях: памʼять<адреса<ПредметТексту>>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія взяти_пак(М: адреса<Машина>, довжина_шляху: природне, шлях: памʼять<адреса<ПредметТексту>>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія взяти_біб(М: адреса<Машина>, довжина_шляху: природне, шлях: памʼять<адреса<ПредметТексту>>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія виконати_код(М: адреса<Машина>, накопичувач_значень: адреса<НакопичувачЗначень>, спроби: адреса<Спроби>, дійсність: адреса<Дійсність>, здійснювач: Значення, код: адреса<ПредметКоду>, початкова_позиція_вказівки: природне, структура_предка: адреса<ПредметСтруктури>, шлях_до_файлу_поточного_модуля: адреса<ПредметДаних>) -> ЗначенняАбоСтанПадіння;

  // МісцезнаходженняВказівок
  місцева дія покласти_місцезнаходження_вказівки(М: адреса<Машина>, місцезнаходження_вказівок: адреса<МісцезнаходженняВказівок>, рядок: п32, позиція_вказівки: п32);

  // НакопичувачТексту
  місцева дія зробити_накопичувач_тексту(М: адреса<Машина>, вмісткість: природне) -> НакопичувачТексту;
  місцева дія покласти_в_накопичувач_тексту(М: адреса<Машина>, накопичувач: адреса<НакопичувачТексту>, значення: кд);

  // Очищувач
  місцева дія запустити_очищувач(М: адреса<Машина>);

  // Послідовність
  місцева дія створити_послідовність(М: адреса<Машина>, розмір: природне, дані: памʼять<Значення>) -> адреса<Послідовність<Значення>>;
  місцева дія створити_послідовність_без_копіювання(М: адреса<Машина>, розмір: природне, дані: памʼять<Значення>) -> адреса<Послідовність<Значення>>;
  місцева дія створити_сиру_послідовність(М: адреса<Машина>, розмір: природне) -> адреса<Послідовність<Значення>>;

  // Притримувач
  місцева дія створити_притримувач(М: адреса<Машина>) -> адреса<Притримувач>;
  місцева дія притримати(М: адреса<Машина>, притримувач: адреса<Притримувач>, значення: Значення) -> адреса<Притримане>;
  місцева дія відпустити_притримане(М: адреса<Машина>, притримувач: адреса<Притримувач>, притримане: адреса<Притримане>);

  // Спроби
  місцева дія покласти_спробу(М: адреса<Машина>, спроби: адреса<Спроби>, назва_зловити: адреса<ПредметТексту>, позиція_вказівки_зловити: природне) -> адреса<Спроба>;
  місцева дія видалити_останню_спробу(М: адреса<Машина>, спроби: адреса<Спроби>);
}

простір мавка {
  // ПредметДаних
  місцева дія заповнити_структуру_Дані(М: адреса<Машина>);
  місцева дія створити_дані(М: адреса<Машина>, розмір: природне, дані: памʼять<п8>) -> адреса<ПредметДаних>;
  місцева дія створити_дані_без_копіювання(М: адреса<Машина>, розмір: природне, дані: памʼять<п8>) -> адреса<ПредметДаних>;
  місцева дія предмет_даних_отримати_властивість(М: адреса<Машина>, предмет_даних: адреса<ПредметДаних>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметДіапазонуДробових
  місцева дія заповнити_структуру_ДіапазонДробових(М: адреса<Машина>);
  місцева дія створити_діапазон_дробових(М: адреса<Машина>, від: адреса<ПредметДробового>, до: адреса<ПредметДробового>, включно: логічне) -> адреса<ПредметДіапазонуДробових>;
  місцева дія предмет_діапазону_дробових_отримати_властивість(М: адреса<Машина>, предмет_діапазону_дробових: адреса<ПредметДіапазонуДробових>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметДіапазонуЦілих
  місцева дія заповнити_структуру_ДіапазонЦілих(М: адреса<Машина>);
  місцева дія створити_діапазон_цілих(М: адреса<Машина>, від: адреса<ПредметЦілого>, до: адреса<ПредметЦілого>, включно: логічне) -> адреса<ПредметДіапазонуЦілих>;
  місцева дія предмет_діапазону_цілих_отримати_властивість(М: адреса<Машина>, предмет_діапазону_цілих: адреса<ПредметДіапазонуЦілих>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметДії
  місцева дія заповнити_структуру_Дія(М: адреса<Машина>);
  місцева дія створити_дію(М: адреса<Машина>, дійсність_створення: адреса<Дійсність>, назва: адреса<ПредметТексту>, параметри: адреса<Послідовність<адреса<ПредметПараметра>>>, тип_результату: Значення, код: адреса<ПредметКоду>, шлях_до_файлу_модуля: адреса<ПредметДаних>) -> адреса<ПредметДії>;
  місцева дія створити_рідну_дію(М: адреса<Машина>, назва: адреса<ПредметТексту>, параметри: адреса<Послідовність<адреса<ПредметПараметра>>>, тип_результату: Значення, рідна_дія: РіднаДія, шлях_до_файлу_модуля: адреса<ПредметДаних>) -> адреса<ПредметДії>;
  місцева дія предмет_дії_здійснити(М: адреса<Машина>, здійснювач: Значення, предмет_дії: адреса<ПредметДії>, задіяні: Задіяні, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дії_отримати_властивість(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметДробового
  місцева дія заповнити_структуру_Дробове(М: адреса<Машина>);
  місцева дія створити_дробове(М: адреса<Машина>, значення: д64) -> адреса<ПредметДробового>;
  місцева дія предмет_дробового_додати(М: адреса<Машина>, предмет: адреса<ПредметДробового>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_відняти(М: адреса<Машина>, предмет: адреса<ПредметДробового>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_помножити(М: адреса<Машина>, предмет: адреса<ПредметДробового>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_поділити(М: адреса<Машина>, предмет: адреса<ПредметДробового>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_остача(М: адреса<Машина>, предмет: адреса<ПредметДробового>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_частка(М: адреса<Машина>, предмет: адреса<ПредметДробового>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_степінь(М: адреса<Машина>, предмет: адреса<ПредметДробового>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_менше(М: адреса<Машина>, предмет: адреса<ПредметДробового>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_більше(М: адреса<Машина>, предмет: адреса<ПредметДробового>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_змінити_знак(М: адреса<Машина>, предмет: адреса<ПредметДробового>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_дробового_отримати_властивість(М: адреса<Машина>, предмет_дробового: адреса<ПредметДробового>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметЗміннихДаних
  місцева дія заповнити_структуру_ЗмінніДані(М: адреса<Машина>);
  місцева дія створити_змінні_дані(М: адреса<Машина>, розмір: природне, дані: памʼять<п8>) -> адреса<ПредметЗміннихДаних>;
  місцева дія створити_змінні_дані_без_копіювання(М: адреса<Машина>, розмір: природне, дані: памʼять<п8>) -> адреса<ПредметЗміннихДаних>;
  місцева дія предмет_змінних_даних_отримати_властивість(М: адреса<Машина>, предмет_змінних_даних: адреса<ПредметЗміннихДаних>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметКоду
  місцева дія заповнити_структуру_Код(М: адреса<Машина>);
  місцева дія створити_код(М: адреса<Машина>, константи: адреса<Послідовність<Значення>>, вказівки: адреса<ПредметДаних>, шлях: адреса<ПредметДаних>, місцезнаходження_вказівок: МісцезнаходженняВказівок) -> адреса<ПредметКоду>;
  місцева дія предмет_коду_отримати_властивість(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія знайти_рядок_вказівки(місцезнаходження_вказівок: МісцезнаходженняВказівок, позиція_вказівки: п32) -> п32;

  // ПредметМодуля
  місцева дія заповнити_структуру_Модуль(М: адреса<Машина>);
  місцева дія створити_модуль(М: адреса<Машина>, назва: адреса<ПредметТексту>) -> адреса<ПредметМодуля>;
  місцева дія предмет_модуля_отримати_властивість(М: адреса<Машина>, предмет_модуля: адреса<ПредметМодуля>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_модуля_змінити_властивість(М: адреса<Машина>, предмет_модуля: адреса<ПредметМодуля>, назва: адреса<ПредметТексту>, значення: Значення, місцезнаходження: Місцезнаходження);

  // ПредметПараметра
  місцева дія заповнити_структуру_Параметр(М: адреса<Машина>);
  місцева дія створити_параметр(М: адреса<Машина>, назва: адреса<ПредметТексту>, тип: Значення, дія_значення: Значення) -> адреса<ПредметПараметра>;
  місцева дія предмет_параметра_отримати_властивість(М: адреса<Машина>, предмет_параметра: адреса<ПредметПараметра>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметПереборуДіапазонуДробових
  місцева дія заповнити_структуру_ПеребірДіапазонуДробових(М: адреса<Машина>);
  місцева дія створити_перебір_діапазону_дробових(М: адреса<Машина>, предмет_діапазону: адреса<ПредметДіапазонуДробових>) -> адреса<ПредметПереборуДіапазонуДробових>;
  місцева дія предмет_перебору_діапазону_дробових_отримати_властивість(М: адреса<Машина>, предмет_перебору_діапазону_дробових: адреса<ПредметПереборуДіапазонуДробових>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметПереборуДіапазонуЦілих
  місцева дія заповнити_структуру_ПеребірДіапазонуЦілих(М: адреса<Машина>);
  місцева дія створити_перебір_діапазону_цілих(М: адреса<Машина>, предмет_діапазону: адреса<ПредметДіапазонуЦілих>) -> адреса<ПредметПереборуДіапазонуЦілих>;
  місцева дія предмет_перебору_діапазону_цілих_отримати_властивість(М: адреса<Машина>, предмет_перебору_діапазону_цілих: адреса<ПредметПереборуДіапазонуЦілих>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметПереборуСписку
  місцева дія заповнити_структуру_ПеребірСписку(М: адреса<Машина>);
  місцева дія створити_перебір_списку(М: адреса<Машина>, предмет_списку: адреса<ПредметСписку>) -> адреса<ПредметПереборуСписку>;
  місцева дія предмет_перебору_списку_отримати_властивість(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметСловника
  місцева дія заповнити_структуру_Словник(М: адреса<Машина>);
  місцева дія створити_словник(М: адреса<Машина>) -> адреса<ПредметСловника>;
  місцева дія предмет_словника_отримати_властивість(М: адреса<Машина>, предмет_словника: адреса<ПредметСловника>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія записати_в_словник(М: адреса<Машина>, словник: адреса<ПредметСловника>, ключ: Значення, значення: Значення);

  // ПредметСписку
  місцева дія заповнити_структуру_Список(М: адреса<Машина>);
  місцева дія створити_список(М: адреса<Машина>, розмір: природне) -> адреса<ПредметСписку>;
  місцева дія предмет_списку_отримати_властивість(М: адреса<Машина>, предмет_списку: адреса<ПредметСписку>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія записати_в_список(М: адреса<Машина>, значення_списку: адреса<ПредметСписку>, позиція: природне, значення: Значення);

  // ПредметСтруктури
  місцева дія заповнити_структуру_Структура(М: адреса<Машина>);
  місцева дія створити_структуру(М: адреса<Машина>, назва: адреса<ПредметТексту>, предок: адреса<ПредметСтруктури>, параметри: адреса<Послідовність<адреса<ПредметПараметра>>>) -> адреса<ПредметСтруктури>;
  місцева дія предмет_структури_здійснити(М: адреса<Машина>, значення: адреса<ПредметСтруктури>, задіяні: Задіяні, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія знайти_метод_в_структурі(М: адреса<Машина>, значення_структури: адреса<ПредметСтруктури>, назва: адреса<ПредметТексту>) -> адреса<ПредметДії>;
  місцева дія записати_метод_структури(М: адреса<Машина>, значення_структури: адреса<ПредметСтруктури>, назва: адреса<ПредметТексту>, значення_дії: адреса<ПредметДії>);
  місцева дія предмет_структури_змінити_властивість(М: адреса<Машина>, предмет_структури: адреса<ПредметСтруктури>, назва: адреса<ПредметТексту>, значення: Значення, місцезнаходження: Місцезнаходження);
  місцева дія предмет_структури_отримати_властивість(М: адреса<Машина>, предмет_структури: адреса<ПредметСтруктури>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // ПредметТексту
  місцева дія заповнити_структуру_Текст(М: адреса<Машина>);
  місцева дія створити_текст(М: адреса<Машина>, значення: кд) -> адреса<ПредметТексту>;
  місцева дія створити_текст_без_копіювання(М: адреса<Машина>, розмір: природне, дані: памʼять<п8>) -> адреса<ПредметТексту>;
  місцева дія предмет_тексту_отримати_властивість(М: адреса<Машина>, предмет_тексту: адреса<ПредметТексту>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія перевірити_чи_предмети_тексту_рівні(М: адреса<Машина>, а: адреса<ПредметТексту>, б: адреса<ПредметТексту>) -> логічне;

  // ПредметЦілого
  місцева дія заповнити_структуру_Ціле(М: адреса<Машина>);
  місцева дія створити_ціле(М: адреса<Машина>, значення: ц64) -> адреса<ПредметЦілого>;
  місцева дія предмет_цілого_додати(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_відняти(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_помножити(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_поділити(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_остача(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_частка(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_степінь(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_вліво(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_вправо(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_двійкове_і(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_двійкове_або(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_двійкове_вабо(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_менше(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_більше(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_двійкове_не(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_змінити_знак(М: адреса<Машина>, предмет: адреса<ПредметЦілого>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;
  місцева дія предмет_цілого_отримати_властивість(М: адреса<Машина>, предмет_цілого: адреса<ПредметЦілого>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння;

  // СкладенийПредмет
  місцева дія створити_складений_предмет(М: адреса<Машина>, значення_структури: адреса<ПредметСтруктури>) -> адреса<СкладенийПредмет>;
}