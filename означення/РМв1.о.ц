простір РМв1 {
  структура Розширення;
  структура Дані;
  структура Предмет;
  структура Місцезнаходження;
  структура ІменованоЗадіяні;
  структура ЕлементиСписку;
  структура ЕлементСловника;
  структура ЕлементиСловника;
  структура Притримувач;
  структура Притримане;

  синонім Значення = адреса<Предмет>;
  синонім ЗначенняАбоСтанПадіння = Значення;

  структура Дані {
    розмір: природне;
    дані: памʼять<п8>;
  }

  структура Місцезнаходження {
    шлях: Значення;
    рядок: природне;
  }

  структура ЕлементиСписку {
    розмір: природне;
    дані: памʼять<Значення>;
  }

  структура ЕлементСловника {
    ключ: Значення;
    значення: Значення;
  }

  структура ЕлементиСловника {
    розмір: природне;
    дані: памʼять<ЕлементСловника>;
  }

  синонім РіднаДія = (Р: адреса<Розширення>, здійснювач: Значення, предмет_дії: Значення, задіяні: адреса<ПредметПослідовності>, іменовано_задіяні: адреса<ІменованоЗадіяні>) -> ЗначенняАбоСтанПадіння;

  структура Розширення {
    М: невідома_адреса;
    дані: невідома_адреса;
    відклик_перед_знищенням: (Р: адреса<Розширення>) -> ніщо;
    значення_дійсне: Значення;
    вивести_кд: (Р: адреса<Розширення>, значення: адреса<кд>) -> ніщо;
    надрукувати_кд: (Р: адреса<Розширення>, значення: адреса<кд>) -> ніщо;
    перевірити_чи_стан_падіння: (Р: адреса<Розширення>) -> логічне;
    увімкнути_стан_падіння: (Р: адреса<Розширення>, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ніщо;
    створити_рідну_дію: (Р: адреса<Розширення>, назва: Значення, кількість_параметрів: природне, параметри: памʼять<Значення>, тип_результату: Значення, рідна_дія: РіднаДія) -> ЗначенняАбоСтанПадіння;
    створити_ціле: (Р: адреса<Розширення>, значення: ц64) -> ЗначенняАбоСтанПадіння;
    створити_дробове: (Р: адреса<Розширення>, значення: д64) -> ЗначенняАбоСтанПадіння;
    створити_текст: (Р: адреса<Розширення>, значення: адреса<кд>) -> ЗначенняАбоСтанПадіння;
    створити_текст_без_копіювання: (Р: адреса<Розширення>, значення: адреса<кд>) -> ЗначенняАбоСтанПадіння;
    створити_список: (Р: адреса<Розширення>, вмісткість: природне) -> ЗначенняАбоСтанПадіння;
    створити_словник: (Р: адреса<Розширення>) -> ЗначенняАбоСтанПадіння;
    створити_дані: (Р: адреса<Розширення>, розмір: природне, дані: памʼять<п8>) -> ЗначенняАбоСтанПадіння;
    створити_дані_без_копіювання: (Р: адреса<Розширення>, розмір: природне, дані: памʼять<п8>) -> ЗначенняАбоСтанПадіння;
    створити_модуль: (Р: адреса<Розширення>, назва: Значення) -> ЗначенняАбоСтанПадіння;
    створити_параметр: (Р: адреса<Розширення>, назва: Значення, тип: Значення, дія_значення: Значення) -> ЗначенняАбоСтанПадіння;
    предмет_додати: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_відняти: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_помножити: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_поділити: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_остача: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_частка: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_степінь: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_вліво: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_вправо: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_двійкове_і: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_двійкове_або: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_двійкове_вабо: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_менше: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_більше: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_містить: (Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_двійкове_не: (Р: адреса<Розширення>, предмет: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_змінити_знак: (Р: адреса<Розширення>, предмет: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_отримати_число: (Р: адреса<Розширення>, предмет: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_отримати_текст: (Р: адреса<Розширення>, предмет: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_отримати_перебір: (Р: адреса<Розширення>, предмет: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_отримати_діапазон: (Р: адреса<Розширення>, предмет: Значення, включно: логічне, до: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ЗначенняАбоСтанПадіння;
    предмет_змінити_властивість: (Р: адреса<Розширення>, предмет: Значення, назва: Значення, значення: Значення, місцезнаходження: адреса<Місцезнаходження>) -> ніщо;
    перевірити_чи_предмет_є_дією: (Р: адреса<Розширення>, предмет: Значення) -> логічне;
    перевірити_чи_предмет_є_числом: (Р: адреса<Розширення>, предмет: Значення) -> логічне;
    перевірити_чи_предмет_є_текстом: (Р: адреса<Розширення>, предмет: Значення) -> логічне;
    перевірити_чи_предмет_є_списком: (Р: адреса<Розширення>, предмет: Значення) -> логічне;
    перевірити_чи_предмет_є_словником: (Р: адреса<Розширення>, предмет: Значення) -> логічне;
    перевірити_чи_предмет_є_даними: (Р: адреса<Розширення>, предмет: Значення) -> логічне;
    перевірити_чи_предмет_є_модулем: (Р: адреса<Розширення>, предмет: Значення) -> логічне;
    перевірити_чи_предмет_є_параметром: (Р: адреса<Розширення>, предмет: Значення) -> логічне;
    отримати_значення_цілого: (Р: адреса<Розширення>, предмет: Значення) -> ц64;
    отримати_значення_дробового: (Р: адреса<Розширення>, предмет: Значення) -> д64;
    отримати_значення_тексту: (Р: адреса<Розширення>, предмет: Значення, вихід: адреса<кд>) -> ніщо;
    отримати_копію_значення_тексту: (Р: адреса<Розширення>, предмет: Значення, вихід: адреса<кд>) -> логічне;
    отримати_копію_елементів_списку: (Р: адреса<Розширення>, предмет: Значення, вихід: адреса<ЕлементиСписку>) -> логічне;
    отримати_копію_елементів_словника: (Р: адреса<Розширення>, предмет: Значення, вихід: адреса<ЕлементиСловника>) -> логічне;
    отримати_значення_даних: (Р: адреса<Розширення>, предмет: Значення, вихід: адреса<Дані>) -> ніщо;
    отримати_копію_значення_даних: (Р: адреса<Розширення>, предмет: Значення, вихід: адреса<Дані>) -> логічне;
    притримати_глобально: (Р: адреса<Розширення>, предмет: Значення) -> адреса<Притримане>;
    відпустити_глобально: (Р: адреса<Розширення>, притримане: адреса<Притримане>) -> ніщо;
    назва: (Р: адреса<Розширення>, значення: адреса<кд>) -> Значення;
    шлях_до_файлу_розширення: Значення;
  }
}

простір РМв1 {
  дія змінити_дані(Р: адреса<Розширення>, значення: невідома_адреса) {
    Р.дані = значення;
  }

  дія змінити_відклик_перед_знищенням(Р: адреса<Розширення>, відклик_перед_знищенням: (Р: адреса<Розширення>) -> ніщо) {
    Р.відклик_перед_знищенням = відклик_перед_знищенням;
  }

  дія вивести_кд(Р: адреса<Розширення>, значення: кд) {
    Р.вивести_кд(Р, значення::адреса);
  }

  дія надрукувати_кд(Р: адреса<Розширення>, значення: кд) {
    Р.надрукувати_кд(Р, значення::адреса);
  }

  дія перевірити_чи_стан_падіння(Р: адреса<Розширення>) -> логічне {
    вернути Р.перевірити_чи_стан_падіння(Р);
  }

  дія увімкнути_стан_падіння(Р: адреса<Розширення>, значення: Значення, місцезнаходження: Місцезнаходження) {
    Р.увімкнути_стан_падіння(Р, значення, місцезнаходження::адреса);
  }

  дія отримати_значення_дійсне(Р: адреса<Розширення>) -> Значення {
    вернути Р.значення_дійсне;
  }

  дія створити_рідну_дію(Р: адреса<Розширення>, назва: Значення, кількість_параметрів: природне, параметри: памʼять<Значення>, тип_результату: Значення, рідна_дія: РіднаДія) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_рідну_дію(Р, назва, кількість_параметрів, параметри, тип_результату, рідна_дія);
  }

  дія створити_ціле(Р: адреса<Розширення>, значення: ц64) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_ціле(Р, значення);
  }

  дія створити_дробове(Р: адреса<Розширення>, значення: д64) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_дробове(Р, значення);
  }

  дія створити_текст(Р: адреса<Розширення>, значення: кд) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_текст(Р, значення::адреса);
  }

  дія створити_текст_без_копіювання(Р: адреса<Розширення>, значення: кд) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_текст_без_копіювання(Р, значення::адреса);
  }

  дія створити_список(Р: адреса<Розширення>, вмісткість: природне) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_список(Р, вмісткість);
  }

  дія створити_словник(Р: адреса<Розширення>) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_словник(Р);
  }

  дія створити_дані(Р: адреса<Розширення>, розмір: природне, дані: памʼять<п8>) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_дані(Р, розмір, дані);
  }

  дія створити_дані_без_копіювання(Р: адреса<Розширення>, розмір: природне, дані: памʼять<п8>) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_дані_без_копіювання(Р, розмір, дані);
  }

  дія створити_модуль(Р: адреса<Розширення>, назва: Значення) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_модуль(Р, назва);
  }

  дія створити_параметр(Р: адреса<Розширення>, назва: Значення) -> ЗначенняАбоСтанПадіння {
    вернути Р.створити_параметр(Р, назва);
  }

  дія предмет_додати(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    вернути Р.предмет_додати(Р, предмет, значення, місцезнаходження::адреса);
  }

  дія предмет_відняти(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    вернути Р.предмет_відняти(Р, предмет, значення, місцезнаходження::адреса);
  }

  дія предмет_помножити(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    вернути Р.предмет_помножити(Р, предмет, значення, місцезнаходження::адреса);
  }

  дія предмет_поділити(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    вернути Р.предмет_поділити(Р, предмет, значення, місцезнаходження::адреса);
  }

  дія предмет_остача(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    вернути Р.предмет_остача(Р, предмет, значення, місцезнаходження::адреса);
  }

  дія предмет_неповна_частка(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    вернути Р.предмет_неповна_частка(Р, предмет, значення, місцезнаходження::адреса);
  }

  дія предмет_степінь(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    вернути Р.предмет_степінь(Р, предмет, значення, місцезнаходження::адреса);
  }

  дія предмет_зсунути_вліво(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    вернути Р.предмет_зсунути_вліво(Р, предмет, значення, місцезнаходження::адреса);
  }

  дія предмет_зсунути_вправо(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    вернути Р.предмет_зсунути_вправо(Р, предмет, значення, місцезнаходження::адреса);
  }

  дія предмет_зсунути_вправо_2(Р: адреса<Розширення>, предмет: Значення, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
