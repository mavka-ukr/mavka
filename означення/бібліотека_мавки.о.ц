/* Бібліотека Мавки */

взяти означення мавка;

зовнішня дія бібліотека_мавки_читати_кд(вихід_розміру: адреса<природне>, вихід_даних: адреса<памʼять<п8>>) -> логічне;

простір мавка {
  простір бібліотека {
    стала ПІ = 3.14159265358979323846;

    структура КористувацькіДаніБібліотекиМавки {
      взято__математика: адреса<Притримане>; // ПредметМодуля
      взято__читати: адреса<Притримане>; // ПредметДії
    }

    місцева дія ініціалізувати(М: адреса<Машина>);
    місцева дія ініціалізувати__математика(М: адреса<Машина>, предмет_модуля: адреса<ПредметМодуля>);
    місцева дія створити_дію__читати(М: адреса<Машина>) -> адреса<ПредметДії>;
  }
}