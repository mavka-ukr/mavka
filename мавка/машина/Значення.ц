      вернути предмет_цілого_степінь(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_степінь(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }
  
  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати піднесення до степеня можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_вліво(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> Значення {
