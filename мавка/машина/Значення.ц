      М,
      створити_текст(М, "Виконати двійкове зсув вліво можна лише між цілими числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_вправо(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> Значення {
