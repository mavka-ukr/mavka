взяти означення мавка;

простір мавка {
  місцева дія створити_предмет(
    М: адреса<Машина>,
    значення_структури: адреса<ПредметСтруктури>
  ) -> адреса<Предмет> {
    змінна предмет = виділити_предмет<Предмет>(М, значення_структури);
    
    предмет.властивості = Властивості { 0, пусто };
    
    вернути предмет;
  }

  місцева дія записати_властивість_предмета(
    М: адреса<Машина>,
    предмет: адреса<Предмет>,
    назва: адреса<ПредметТексту>,
    значення: Значення
  ) {

    змінна властивості = (предмет.властивості як Властивості)::адреса;
    
    змінна п: природне = 0;
    поки п < властивості.розмір {
      змінна властивість = властивості.дані[п]::адреса;

      якщо перевірити_чи_значення_рівні(М, властивість.назва як Значення, назва як Значення) {
        властивість.значення = значення;

        вернути;
      }

      п += 1;
    }

    властивості.дані = перевиділити_памʼять<Властивість>(М, властивості.дані, властивості.розмір + 1);
    властивості.дані[властивості.розмір] = Властивість { назва, значення };
    властивості.розмір += 1;
  }

  місцева дія прочитати_властивість_предмета(
    М: адреса<Машина>,
    предмет: адреса<Предмет>,
    назва: адреса<ПредметТексту>,
    вихід: адреса<Значення>
  ) -> логічне {
    змінна властивості = предмет.властивості як Властивості;
    
    змінна п: природне = 0;
    поки п < властивості.розмір {
      змінна властивість = властивості.дані[п]::адреса;

      якщо властивість.назва == назва або перевірити_чи_предмети_тексту_рівні(М, властивість.назва, назва) {
        вихід::вміст = властивість.значення;
        
        вернути так;
      }

      п += 1;
    }

    вернути ні;
  }

  місцева дія здійснити(
    М: адреса<Машина>,
    здійснювач: Значення,
    значення: Значення,
    кількість_задіяних: природне,
    задіяні: памʼять<Значення>,
    іменовано_задіяні: адреса<ІменованіЗадіяні>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо значення.тип == М.предмет_структури_Дія {
      вернути предмет_дії_здійснити(
        М,
        здійснювач,
        значення,
        кількість_задіяних,
        задіяні,
        іменовано_задіяні,
        місцезнаходження
      );
    }
    
    якщо значення.тип == М.предмет_структури_Структура {
      вернути предмет_структури_здійснити(
        М,
        значення як адреса<ПредметСтруктури>,
        кількість_задіяних,
        задіяні,
        іменовано_задіяні,
        місцезнаходження
      );
    }          
    
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати дію можна лише над діями.") як Значення,
      місцезнаходження
    );
    вернути пусто;               
  }

  // позначка: прохід по типах предметів
  місцева дія отримати_властивість(
    М: адреса<Машина>,
    предмет: Значення,
    назва: адреса<ПредметТексту>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо предмет == пусто {
      вернути пусто;
    }

    якщо предмет.тип == М.значення_дійсне {
      вернути пусто;
    }
    
    якщо предмет.тип == М.значення_послідовність {
      вернути пусто;
    }
    
    якщо предмет.тип == М.значення_дійсність {
      вернути пусто;
    }
    
    якщо предмет.тип == М.значення_предмет {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Структура {
      вернути предмет_структури_отримати_властивість(
        М,
        предмет як адреса<ПредметСтруктури>,
        назва,
        місцезнаходження
      );
    }
    
    якщо предмет.тип == М.предмет_структури_Модуль {
      вернути предмет_модуля_отримати_властивість(
        М,
        предмет як адреса<ПредметМодуля>,
        назва,
        місцезнаходження
      );
    }
    
    якщо предмет.тип == М.предмет_структури_Параметр {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Число {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_отримати_властивість(
        М,
        предмет як адреса<ПредметЦілого>,
        назва,
        місцезнаходження
      );
    }
    
    якщо предмет.тип == М.предмет_структури_Дробове {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Текст {
      вернути предмет_тексту_отримати_властивість(
        М,
        предмет як адреса<ПредметТексту>,
        назва,
        місцезнаходження
      );
    }
    
    якщо предмет.тип == М.предмет_структури_Код {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Дія {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Список {
      вернути предмет_списку_отримати_властивість(
        М,
        предмет як адреса<ПредметСписку>,
        назва,
        місцезнаходження
      );
    }
    
    якщо предмет.тип == М.предмет_структури_Словник {
      вернути предмет_словника_отримати_властивість(
        М,
        предмет як адреса<ПредметСловника>,
        назва,
        місцезнаходження
      );
    }
    
    якщо предмет.тип == М.предмет_структури_Дані {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ЗмінніДані {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонЦілих {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонДробових {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      вернути предмет_перебору_діапазону_цілих_отримати_властивість(
        М,
        предмет як адреса<ПредметПереборуДіапазонуЦілих>,
        назва,
        місцезнаходження
      );
    }
    
    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      вернути предмет_перебору_діапазону_дробових_отримати_властивість(
        М,
        предмет як адреса<ПредметПереборуДіапазонуДробових>,
        назва,
        місцезнаходження
      );
    }

    змінна значення: Значення = пусто;

    якщо прочитати_властивість_предмета(
      М,
      предмет як адреса<Предмет>,
      назва,
      значення::адреса
    ) {
      вернути значення;
    }

    якщо предмет.тип.тип == М.предмет_структури_Структура {
      вернути знайти_метод_в_структурі(
        М,
        предмет.тип як адреса<ПредметСтруктури>,
        назва
      ) як Значення;
    }

    вернути пусто;
  }

  // позначка: прохід по типах предметів
  місцева дія змінити_властивість(
    М: адреса<Машина>,
    предмет: Значення,
    назва: адреса<ПредметТексту>,
    значення: Значення,
    місцезнаходження: Місцезнаходження
  ) {
    якщо предмет == пусто {
      вернути;
    }

    якщо предмет.тип == М.значення_дійсне {
      вернути;
    }
    
    якщо предмет.тип == М.значення_послідовність {
      вернути;
    }
    
    якщо предмет.тип == М.значення_дійсність {
      вернути;
    }
    
    якщо предмет.тип == М.значення_предмет {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Структура {
      предмет_структури_змінити_властивість(
        М,
        предмет як адреса<ПредметСтруктури>,
        назва,
        значення,
        місцезнаходження
      );
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Модуль {
      змінна властивості = (предмет як адреса<ПредметМодуля>).властивості::адреса;
      
      змінна п: природне = 0;
      поки п < властивості.розмір {
        змінна властивість = властивості.дані[п]::адреса;
  
        якщо перевірити_чи_значення_рівні(М, властивість.назва як Значення, назва як Значення) {
          властивість.значення = значення;
  
          вернути;
        }
  
        п += 1;
      }
  
      властивості.дані = перевиділити_памʼять<Властивість>(М, властивості.дані, властивості.розмір + 1);
      властивості.дані[властивості.розмір] = Властивість { назва, значення };
      властивості.розмір += 1;
  
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Параметр {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Число {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Ціле {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Дробове {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Текст {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Код {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Дія {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Список {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Словник {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Дані {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_ЗмінніДані {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонЦілих {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонДробових {
      вернути;
    }

    записати_властивість_предмета(
      М,
      предмет як адреса<Предмет>,
      назва,
      значення
    );
  }

  місцева дія здійснити_властивість(
    М: адреса<Машина>,
    предмет: Значення,
    назва: адреса<ПредметТексту>,
    кількість_задіяних: природне,
    задіяні: памʼять<Значення>,
    іменовано_задіяні: адреса<ІменованіЗадіяні>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    змінна значення_властивості = отримати_властивість(М, предмет, назва, місцезнаходження);
    якщо М.стан_падіння {
      звільнити_памʼять(М, задіяні);
      якщо іменовано_задіяні != пусто {
        знищити_іменовано_задіяні(М, іменовано_задіяні);
      }
      вернути пусто;
    }

    якщо значення_властивості == пусто {
      увімкнути_стан_падіння(
        М, 
        створити_текст(М, "Не влалось здійснити властивість.") як Значення, 
        місцезнаходження
      );
      звільнити_памʼять(М, задіяні);
      якщо іменовано_задіяні != пусто {
        знищити_іменовано_задіяні(М, іменовано_задіяні);
      }
      вернути пусто;
    }

    вернути здійснити(
      М,
      предмет,
      значення_властивості,
      кількість_задіяних,
      задіяні,
      іменовано_задіяні,
      місцезнаходження
    );
  }

  // позначка: прохід по типах предметів
  місцева дія отримати_елемент(
    М: адреса<Машина>,
    предмет: Значення,
    ключ: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо предмет == пусто {
      вернути пусто;
    }

    якщо предмет.тип == М.значення_дійсне {
      вернути пусто;
    }
    
    якщо предмет.тип == М.значення_послідовність {
      вернути пусто;
    }
    
    якщо предмет.тип == М.значення_дійсність {
      вернути пусто;
    }
    
    якщо предмет.тип == М.значення_предмет {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Структура {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Модуль {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Параметр {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Число {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Ціле {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Дробове {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Текст {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Код {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Дія {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Список {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Словник {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Дані {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ЗмінніДані {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонЦілих {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонДробових {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      вернути пусто;
    }

    вернути пусто;
  }

  // позначка: прохід по типах предметів
  місцева дія змінити_елемент(
    М: адреса<Машина>,
    предмет: Значення,
    ключ: Значення,
    значення: Значення,
    місцезнаходження: Місцезнаходження
  ) {
    якщо предмет == пусто {
      вернути;
    }

    якщо предмет.тип == М.значення_дійсне {
      вернути;
    }
    
    якщо предмет.тип == М.значення_послідовність {
      вернути;
    }
    
    якщо предмет.тип == М.значення_дійсність {
      вернути;
    }
    
    якщо предмет.тип == М.значення_предмет {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Структура {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Модуль {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Параметр {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Число {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Ціле {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Дробове {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Текст {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Код {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Дія {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Список {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Словник {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_Дані {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_ЗмінніДані {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонЦілих {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонДробових {
      вернути;
    }
    
    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      вернути;
    }

    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      вернути;
    }

    вернути;
  }

  // позначка: прохід по типах предметів
  місцева дія отримати_текст(М: адреса<Машина>, значення: Значення, місцезнаходження: Місцезнаходження) -> Значення {
    якщо значення == пусто {
      вернути створити_текст(М, "недійсне") як Значення;
    }

    якщо значення.тип == М.значення_дійсне {
      вернути створити_текст(М, "дійсне") як Значення;
    }
    
    якщо значення.тип == М.значення_послідовність {
      вернути створити_текст(М, "послідовність") як Значення;
    }
    
    якщо значення.тип == М.значення_дійсність {
      вернути створити_текст(М, "дійсність") як Значення;
    }
    
    якщо значення.тип == М.значення_предмет {
      вернути створити_текст(М, "предмет") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_Структура {
      вернути створити_текст(М, "<Структура>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_Модуль {
      вернути створити_текст(М, "<Модуль>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_Параметр {
      вернути створити_текст(М, "<Параметр>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_Число {
      вернути створити_текст(М, "<Число>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_Ціле {
      змінна значення = (значення як адреса<ПредметЦілого>).значення;
      змінна значення_кд: кд;
      
      якщо перетворити_ц64_в_кд(М, значення, значення_кд::адреса) {
        вернути створити_текст_без_копіювання(М, значення_кд.розмір, значення_кд.дані) як Значення;
      }

      увімкнути_стан_падіння(
        М,
        створити_текст(М, "Неможливо перетворити ціле число в текст.") як Значення,
        місцезнаходження
      );
      вернути пусто;
    }
    
    якщо значення.тип == М.предмет_структури_Дробове {
      змінна значення = (значення як адреса<ПредметДробового>).значення;
      змінна значення_кд: кд;
      
      якщо перетворити_д64_в_кд(М, значення, значення_кд::адреса) {
        вернути створити_текст_без_копіювання(М, значення_кд.розмір, значення_кд.дані) як Значення;
      }

      увімкнути_стан_падіння(
        М,
        створити_текст(М, "Неможливо перетворити дробове число в текст.") як Значення,
        місцезнаходження
      );
      вернути пусто;
    }
    
    якщо значення.тип == М.предмет_структури_Текст {
      вернути значення;
    }
    
    якщо значення.тип == М.предмет_структури_Код {
      вернути створити_текст(М, "<Код>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_Дія {
      вернути створити_текст(М, "<Дія>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_Список {
      вернути створити_текст(М, "<Список>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_Словник {
      вернути створити_текст(М, "<Словник>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_Дані {
      вернути створити_текст(М, "<Дані>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_ЗмінніДані {
      вернути створити_текст(М, "<ЗмінніДані>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_ДіапазонЦілих {
      вернути створити_текст(М, "<ДіапазонЦілих>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_ДіапазонДробових {
      вернути створити_текст(М, "<ДіапазонДробових>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      вернути створити_текст(М, "<ПеребірДіапазонуЦілих>") як Значення;
    }
    
    якщо значення.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      вернути створити_текст(М, "<ПеребірДіапазонуДробових>") як Значення;
    }
    
    вернути пусто;
  }

  // позначка: прохід по типах предметів
  місцева дія отримати_діапазон(
    М: адреса<Машина>,
    від: Значення,
    до: Значення,
    включно: логічне,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо від == пусто {
      вернути пусто;
    }

    якщо від.тип == М.значення_дійсне {
      вернути пусто;
    }
    
    якщо від.тип == М.значення_послідовність {
      вернути пусто;
    }
    
    якщо від.тип == М.значення_дійсність {
      вернути пусто;
    }
    
    якщо від.тип == М.значення_предмет {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Структура {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Модуль {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Параметр {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Число {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Ціле {
      якщо до.тип == М.предмет_структури_Ціле {
        вернути створити_діапазон_цілих(М, від як адреса<ПредметЦілого>, до як адреса<ПредметЦілого>, включно) як Значення;
      } інакше {
        увімкнути_стан_падіння(
          М,
          створити_текст(М, "Неможливо створити діапазон цілих чисел з різнорідних типів.") як Значення,
          місцезнаходження
        );

        вернути пусто;
      }
    }
    
    якщо від.тип == М.предмет_структури_Дробове {
      якщо до.тип == М.предмет_структури_Дробове {
        вернути створити_діапазон_дробових(М, від як адреса<ПредметДробового>, до як адреса<ПредметДробового>, включно) як Значення;
      } інакше {
        увімкнути_стан_падіння(
          М,
          створити_текст(М, "Неможливо створити діапазон дробових чисел з різнорідних типів.") як Значення,
          місцезнаходження
        );

        вернути пусто;
      }
    }
    
    якщо від.тип == М.предмет_структури_Текст {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Код {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Дія {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Список {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Словник {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_Дані {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_ЗмінніДані {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_ДіапазонЦілих {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_ДіапазонДробових {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      вернути пусто;
    }
    
    якщо від.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      вернути пусто;
    }

    змінна кількість_задіяних: природне = 2;
    змінна задіяні = виділити_памʼять<Значення>(М, кількість_задіяних);
    задіяні[0] = до;
    якщо включно {
      задіяні[1] = М.значення_дійсне;
    } інакше {
      задіяні[1] = пусто як Значення;
    }

    вернути здійснити_властивість(
      М,
      від,
      створити_текст(М, "чародія_діапазон"),
      кількість_задіяних,
      задіяні,
      пусто,
      місцезнаходження
    );
  }

  // позначка: прохід по типах предметів
  місцева дія отримати_перебір(
    М: адреса<Машина>,
    предмет: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо предмет == пусто {
      вернути пусто;
    }

    якщо предмет.тип == М.значення_дійсне {
      вернути пусто;
    }
    
    якщо предмет.тип == М.значення_послідовність {
      вернути пусто;
    }
    
    якщо предмет.тип == М.значення_дійсність {
      вернути пусто;
    }
    
    якщо предмет.тип == М.значення_предмет {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Структура {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Модуль {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Параметр {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Число {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Ціле {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Дробове {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Текст {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Код {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Дія {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Список {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Словник {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_Дані {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ЗмінніДані {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонЦілих {
      вернути створити_перебір_діапазону_цілих(М, предмет як адреса<ПредметДіапазонуЦілих>) як Значення;
    }
    
    якщо предмет.тип == М.предмет_структури_ДіапазонДробових {
      вернути створити_перебір_діапазону_дробових(М, предмет як адреса<ПредметДіапазонуДробових>) як Значення;
    }

    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      вернути пусто;
    }
    
    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      вернути пусто;
    }

    вернути здійснити_властивість(
      М,
      предмет,
      створити_текст(М, "чародія_перебір"),
      0,
      пусто,
      пусто,
      місцезнаходження
    );
  }

  місцева дія виконати_додати(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }
    
    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_додати(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_додати(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }
  
  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати додавання можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_відняти(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }
    
    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_відняти(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_відняти(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }
  
  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати віднімання можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_помножити(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }
    
    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_помножити(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_помножити(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }
  
  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати множення можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_поділити(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }
    
    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_поділити(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_поділити(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }
  
  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати ділення можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_остача(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }
    
    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_остача(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_остача(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }
  
  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати отримання остачі ділення можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_частка(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }
    
    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_частка(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_частка(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }
  
  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати отримання неповної частки ділення можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_степінь(М: адреса<Машина>, ліво: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> Значення {
    якщо ліво == пусто {
