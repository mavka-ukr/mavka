взяти означення мавка;

простір мавка {
  місцева дія створити_діапазон_цілих(М: адреса<Машина>, від: адреса<ПредметЦілого>, до: адреса<ПредметЦілого>, включно: логічне) -> адреса<ПредметДіапазонуЦілих> {
    змінна предмет = виділити_предмет<ПредметДіапазонуЦілих>(М, М.предмет_структури_ДіапазонЦілих);

    предмет.від = від;
    предмет.до = до;
    предмет.включно = включно;

    вернути предмет;
  }
}