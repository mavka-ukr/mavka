  зовнішня дія складений_предмет_отримати_елемент(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути складений_предмет_виконати_властивість(М, складений_предмет, назва(М, "чародія_отримати_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  зовнішня дія складений_предмет_змінити_елемент(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, ключ: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути складений_предмет_виконати_властивість(М, складений_предмет, назва(М, "чародія_змінити_елемент"), 2, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  зовнішня дія складений_предмет_видалити_елемент(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути складений_предмет_виконати_властивість(М, складений_предмет, назва(М, "чародія_видалити_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  зовнішня дія складений_предмет_отримати_перебір(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути складений_предмет_виконати_властивість(М, складений_предмет, назва(М, "чародія_перебір"), 0, пусто, пусто, місцезнаходження);
  }

  зовнішня дія складений_предмет_отримати_діапазон(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, включно: логічне, до: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна аргументи = виділити_памʼять<адреса<Предмет>>(М, 2);
    якщо включно {
      аргументи[0] = М.предмет_логічного_так як адреса<Предмет>;
    } інакше {
      аргументи[0] = М.предмет_логічного_ні як адреса<Предмет>;
    }
    аргументи[1] = до;
    змінна результат = складений_предмет_виконати_властивість(М, складений_предмет, назва(М, "чародія_діапазон"), 2, аргументи, пусто, місцезнаходження);
    звільнити_памʼять(М, аргументи);
    вернути результат;
  }
}