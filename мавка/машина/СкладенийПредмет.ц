взяти визначення мавка;

секція мавка {
  зовнішня дія створити_складений_предмет(М: адреса<Машина>, тип: адреса<ПредметСтруктури>): адреса<СкладенийПредмет> {
    змінна складений_предмет = створити_предмет<СкладенийПредмет>(М, тип);
    складений_предмет.властивості = зробити_реєстр_з_даними<адреса<ПредметТексту>, адреса<Предмет>, Машина>(М, М, перевірити_чи_предмети_назви_рівні);
    вернути складений_предмет;
  }

  зовнішня дія складений_предмет_виконати(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути складений_предмет_виконати_властивість(М, складений_предмет, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія складений_предмет_отримати_властивість(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна елемент_властивості = ЕлементРеєстру<адреса<ПредметТексту>, адреса<Предмет>> { пусто, пусто };
    якщо знайти_в_реєстрі<адреса<ПредметТексту>, адреса<Предмет>>(М, складений_предмет.властивості::адреса, назва, елемент_властивості::адреса) {
      вернути елемент_властивості.значення;
    }
    якщо складений_предмет.тип != пусто {
      змінна метод: адреса<ПредметДії> = пусто;
      якщо знайти_метод_в_структурі(М, складений_предмет.тип, назва, метод::адреса) {
        вернути метод як адреса<Предмет>;
      }
    }
    вернути пусто;
  }

  зовнішня дія складений_предмет_змінити_властивість(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінити_в_реєстрі<адреса<ПредметТексту>, адреса<Предмет>>(М, складений_предмет.властивості::адреса, назва, значення);
    вернути пусто;
  }

  зовнішня дія складений_предмет_видалити_властивість(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо видалити_з_реєстру<адреса<ПредметТексту>, адреса<Предмет>>(М, складений_предмет.властивості::адреса, назва) {
      вернути М.предмет_логічного_так як адреса<Предмет>;
    } інакше {
      вернути М.предмет_логічного_ні як адреса<Предмет>;
    }
  }

  зовнішня дія складений_предмет_виконати_властивість(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна предмет_властивості = складений_предмет_отримати_властивість(М, складений_предмет, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, складений_предмет як адреса<Предмет>, кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

