взяти визначення мавка;

секція мавка {
  дія предмет_діапазону_чисел_виконати_метод_чародія_перебір(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути створити_предмет_перебору_діапазону_чисел(М, предмет_діапазону_чисел) як адреса<Предмет>;
  }
}

секція мавка {
  дія записати_параметр_структури_ДіапазонЧисел_від(М: адреса<Машина>) {
    змінна параметр_від = створити_предмет_параметра(М, створити_назву(М, "від"), пусто);
    М.предмет_структури_ДіапазонЧисел.параметри.дані[0] = параметр_від;
  }

  дія записати_параметр_структури_ДіапазонЧисел_включно(М: адреса<Машина>) {
    змінна параметр_включно = створити_предмет_параметра(М, створити_назву(М, "включно"), пусто);
    М.предмет_структури_ДіапазонЧисел.параметри.дані[1] = параметр_включно;
  }

  дія записати_параметр_структури_ДіапазонЧисел_до(М: адреса<Машина>) {
    змінна параметр_до = створити_предмет_параметра(М, створити_назву(М, "до"), пусто);
    М.предмет_структури_ДіапазонЧисел.параметри.дані[2] = параметр_до;
  }

  дія рідна_дія_метода_чародія_перебір(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: натуральне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_я.с != М.предмет_структури_ДіапазонЧисел {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметБайтів>, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_перебір\" не для діапазону чисел") як адреса<Предмет>);
    }
    змінна предмет_діапазону_чисел = предмет_я як адреса<ПредметДіапазонуЧисел>;
    вернути предмет_діапазону_чисел_виконати_метод_чародія_перебір(М, предмет_діапазону_чисел, Місцезнаходження { пусто як адреса<ПредметБайтів>, 0 });
  }

  дія записати_метод_структури_ДіапазонЧисел_чародія_перебір(М: адреса<Машина>) {
    змінна предмет_дії_метода_чародія_перебір = створити_предмет_рідної_дії(М, створити_назву(М, "чародія_перебір"), 0, пусто, рідна_дія_метода_чародія_перебір, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ДіапазонЧисел, створити_назву(М, "чародія_перебір"), предмет_дії_метода_чародія_перебір);
  }

  місцева дія заповнити_предмет_структури_ДіапазонЧисел(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ДіапазонЧисел_від(М);
    записати_параметр_структури_ДіапазонЧисел_включно(М);
    записати_параметр_структури_ДіапазонЧисел_до(М);

    // методи
    записати_метод_структури_ДіапазонЧисел_чародія_перебір(М);
  }

  місцева дія створити_предмет_діапазону_чисел(М: адреса<Машина>, від: адреса<ПредметЧисла>, включно: логічне, до: адреса<ПредметЧисла>): адреса<ПредметДіапазонуЧисел> {
    змінна предмет_діапазону_чисел = створити_предмет<ПредметДіапазонуЧисел>(М, М.предмет_структури_ДіапазонЧисел);
    предмет_діапазону_чисел.від = від;
    предмет_діапазону_чисел.включно = натуральне(включно);
    предмет_діапазону_чисел.до = до;
    вернути предмет_діапазону_чисел;
  }

  місцева дія предмет_діапазону_чисел_виконати(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, предмет_я: адреса<Предмет>, кількість_аргументів: натуральне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_виконати"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_отримати_властивість(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "від")) {
      вернути предмет_діапазону_чисел.від як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "включно")) {
      якщо предмет_діапазону_чисел.включно == натуральне(так) {
        вернути М.предмет_логічного_так як адреса<Предмет>;
      }
      вернути М.предмет_логічного_ні як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "до")) {
      вернути предмет_діапазону_чисел.до як адреса<Предмет>;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_діапазону_чисел.с, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  місцева дія предмет_діапазону_чисел_змінити_властивість(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути пусто;
  }

  місцева дія предмет_діапазону_чисел_видалити_властивість(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  місцева дія предмет_діапазону_чисел_виконати_властивість(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, назва: адреса<ПредметТексту>, кількість_аргументів: натуральне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна предмет_властивості = предмет_діапазону_чисел_отримати_властивість(М, предмет_діапазону_чисел, назва, місцезнаходження);
    якщо М.стан_падіння {
      вернути пусто;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<н8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета діапазону чисел \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_діапазону_чисел як адреса<Предмет>, кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_додати(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_відняти(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_помножити(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_поділити(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_остача(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_остача"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_неповна_частка(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_неповна_частка"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_степінь(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_степінь"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_зсунути_вліво(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_вліво"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_зсунути_вправо(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_вправо"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_зсунути_вправо_2(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_вправо_2"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_двійкове_і(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_двійкове_і"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_двійкове_або(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_двійкове_або"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_двійкове_виключне_або(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_двійкове_виключне_або"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_двійкове_заперечення(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_двійкове_заперечення"), 0, пусто, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_менше(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_менше"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

