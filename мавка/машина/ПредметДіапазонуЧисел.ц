  місцева дія предмет_діапазону_чисел_отримати_елемент(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_отримати_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_змінити_елемент(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, ключ: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_змінити_елемент"), 2, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_видалити_елемент(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_видалити_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_отримати_перебір(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_діапазону_чисел_виконати_метод_чародія_перебір(М, предмет_діапазону_чисел, місцезнаходження);
  }

  місцева дія предмет_діапазону_чисел_отримати_діапазон(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, включно: логічне, до: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна аргументи = виділити_памʼять<адреса<Предмет>>(М, 2);
    якщо включно {
      аргументи[0] = М.предмет_логічного_так як адреса<Предмет>;
    } інакше {
      аргументи[0] = М.предмет_логічного_ні як адреса<Предмет>;
    }
    аргументи[1] = до;
    змінна результат = предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, створити_назву(М, "чародія_діапазон"), 2, аргументи, пусто, місцезнаходження);
    звільнити_памʼять(М, аргументи);
    вернути результат;
  }
}