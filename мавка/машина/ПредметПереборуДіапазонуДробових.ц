взяти означення мавка;

простір мавка {
  місцева дія створити_перебір_діапазону_дробових(М: адреса<Машина>, значення_діапазону: адреса<ПредметДіапазонуДробових>) -> адреса<ПредметПереборуДіапазонуДробових> {
    змінна предмет = виділити_предмет<ПредметПереборуДіапазонуДробових>(М, М.предмет_структури_ПеребірДіапазонуДробових);

    предмет.значення_діапазону = значення_діапазону;
    предмет.поточне_значення = пусто;

    вернути предмет;
  }
}