      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметБайтів>, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"фільтрувати\" не для списку") як адреса<Предмет>);
    }
    змінна предмет_списку = предмет_я як адреса<ПредметСписку>;
    вернути предмет_списку_виконати_метод_фільтрувати(М, предмет_списку, аргумент_перевіряч, Місцезнаходження { пусто як адреса<ПредметБайтів>, 0 });
