взяти визначення мавка;

секція мавка {
  дія предмет_перебору_діапазону_чисел_виконати_метод_далі(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_перебору_діапазону_чисел.поточне_значення == пусто {
      предмет_перебору_діапазону_чисел.поточне_значення = предмет_перебору_діапазону_чисел.предмет_діапазону.від;
    } інакше {
      предмет_перебору_діапазону_чисел.поточне_значення = створити_предмет_числа(М, предмет_перебору_діапазону_чисел.поточне_значення.значення + 1.0);
    }
    якщо предмет_перебору_діапазону_чисел.предмет_діапазону.включно == позитивне(так) {
      якщо предмет_перебору_діапазону_чисел.поточне_значення.значення <= предмет_перебору_діапазону_чисел.предмет_діапазону.до.значення {
        вернути М.предмет_логічного_так як адреса<Предмет>;
      }
    } інакше {
      якщо предмет_перебору_діапазону_чисел.поточне_значення.значення < предмет_перебору_діапазону_чисел.предмет_діапазону.до.значення {
        вернути М.предмет_логічного_так як адреса<Предмет>;
      }
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }
}

секція мавка {
  дія записати_параметр_структури_ПеребірДіапазонуЧисел_діапазон_чисел(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_діапазон_чисел = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_діапазон_чисел[0] = М.предмет_структури_ДіапазонЧисел;
    змінна тип_параметра_діапазон_чисел = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_діапазон_чисел });
    змінна параметр_діапазон_чисел = створити_предмет_параметра(М, назва(М, "діапазон_чисел"), тип_параметра_діапазон_чисел, пусто);
    М.предмет_структури_ПеребірДіапазонуЧисел.параметри.дані[0] = параметр_діапазон_чисел;
  }

  дія записати_параметр_структури_ПеребірДіапазонуЧисел_значення(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_значення = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_значення[0] = М.предмет_структури_число;
    змінна тип_параметра_значення = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_значення });
    змінна параметр_значення = створити_предмет_параметра(М, назва(М, "значення"), тип_параметра_значення, пусто);
    М.предмет_структури_ПеребірДіапазонуЧисел.параметри.дані[1] = параметр_значення;
  }

  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірДіапазонуЧисел {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору діапазону чисел") як адреса<Предмет>);
    }
    змінна предмет_перебору_діапазону_чисел = предмет_я як адреса<ПредметПереборуДіапазонуЧисел>;
    вернути предмет_перебору_діапазону_чисел_виконати_метод_далі(М, предмет_перебору_діапазону_чисел, місцезнаходження);
  }

  дія записати_метод_структури_ПеребірДіапазонуЧисел_далі(М: адреса<Машина>) {
    змінна предмет_дії_метода_далі = створити_предмет_рідної_дії(М, назва(М, "далі"), 0, пусто, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірДіапазонуЧисел, назва(М, "далі"), предмет_дії_метода_далі);
  }

  місцева дія заповнити_предмет_структури_ПеребірДіапазонуЧисел(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ПеребірДіапазонуЧисел_діапазон_чисел(М);
    записати_параметр_структури_ПеребірДіапазонуЧисел_значення(М);

    // методи
    записати_метод_структури_ПеребірДіапазонуЧисел_далі(М);
  }

  місцева дія створити_предмет_перебору_діапазону_чисел(М: адреса<Машина>, предмет_діапазону: адреса<ПредметДіапазонуЧисел>): адреса<ПредметПереборуДіапазонуЧисел> {
    змінна предмет_перебору_діапазону_чисел = створити_предмет<ПредметПереборуДіапазонуЧисел>(М, М.предмет_структури_ПеребірДіапазонуЧисел);
    предмет_перебору_діапазону_чисел.предмет_діапазону = предмет_діапазону;
    предмет_перебору_діапазону_чисел.поточне_значення = пусто;
    вернути предмет_перебору_діапазону_чисел;
  }

  місцева дія предмет_перебору_діапазону_чисел_виконати(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_отримати_властивість(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "діапазон_чисел")) {
      вернути предмет_перебору_діапазону_чисел.предмет_діапазону як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "значення")) {
      вернути предмет_перебору_діапазону_чисел.поточне_значення як адреса<Предмет>;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_діапазону_чисел.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  місцева дія предмет_перебору_діапазону_чисел_змінити_властивість(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  місцева дія предмет_перебору_діапазону_чисел_видалити_властивість(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  місцева дія предмет_перебору_діапазону_чисел_виконати_властивість(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна предмет_властивості = предмет_перебору_діапазону_чисел_отримати_властивість(М, предмет_перебору_діапазону_чисел, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета перебору діапазону чисел \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_перебору_діапазону_чисел як адреса<Предмет>, кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_додати(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_відняти(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_помножити(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_поділити(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_остача(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_остача"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_неповна_частка(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_неповна_частка"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_степінь(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_степінь"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_зсунути_вліво(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_вліво"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_зсунути_вправо(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_вправо"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_діапазону_чисел_зсунути_вправо_2(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_діапазону_чисел_виконати_властивість(М, предмет_перебору_діапазону_чисел, назва(М, "чародія_вправо_2"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
