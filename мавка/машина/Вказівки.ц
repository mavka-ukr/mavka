взяти визначення мавка;

секція мавка {
  секція В1 {
    дія покласти_рядок_вказівки(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: н32, позиція_вказівки: н32) {
      якщо будівельник_вказівок.місцезнаходження.розмір != 0 {
        змінна м = будівельник_вказівок.місцезнаходження.дані[будівельник_вказівок.місцезнаходження.розмір - 1]::адреса;

        якщо м.рядок == рядок {
          якщо м.початок > позиція_вказівки {
            м.початок = позиція_вказівки;
          }
          якщо м.кінець < позиція_вказівки {
            м.кінець = позиція_вказівки;
          }
          вернути;
        }
      }

      покласти_в_накопичувач<МісцезнаходженняВказівок>(
        М,
        будівельник_вказівок.місцезнаходження::адреса,
        МісцезнаходженняВказівок {
          початок = позиція_вказівки,
          кінець = позиція_вказівки,
          рядок = рядок
        }
      );
    }

    дія покласти_вказівку(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: н32, вказівка: н8) -> н32 {
      покласти_рядок_вказівки(М, будівельник_вказівок, рядок, н32(будівельник_вказівок.вказівки.розмір));
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, вказівка);
      вернути н32(будівельник_вказівок.вказівки.розмір) - 1;
    }

    дія покласти_вказівку_з_аргументом(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: н32, вказівка: н8, аргумент: н32) -> н32 {
      покласти_рядок_вказівки(М, будівельник_вказівок, рядок, н32(будівельник_вказівок.вказівки.розмір));
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, вказівка);
      змінна аргумент_як_памʼять_н8 = аргумент::адреса як памʼять<н8>;
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_н8[0]);
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_н8[1]);
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_н8[2]);
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_н8[3]);
      вернути н32(будівельник_вказівок.вказівки.розмір) - 1;
    }

    дія покласти_вказівку_з_двома_аргументами(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: н32, вказівка: п8, аргумент: н32, другий_аргумент: н32) -> н32 {
      покласти_рядок_вказівки(М, будівельник_вказівок, рядок, н32(будівельник_вказівок.вказівки.розмір));
      покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, вказівка);
      змінна аргумент_як_памʼять_п8 = аргумент::адреса як памʼять<п8>;
      покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[0]);
      покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[1]);
      покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[2]);
      покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[3]);
      змінна другий_аргумент_як_памʼять_п8 = другий_аргумент::адреса як памʼять<п8>;
      покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, другий_аргумент_як_памʼять_п8[0]);
      покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, другий_аргумент_як_памʼять_п8[1]);
      покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, другий_аргумент_як_памʼять_п8[2]);
      покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, другий_аргумент_як_памʼять_п8[3]);
      вернути н32(будівельник_вказівок.вказівки.розмір) - 1;
    }

    місцева дія перезаписати_аргумент_вказівки(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, позиція: н32, значення: н32) {
      змінна значення_як_памʼять_н8 = значення::адреса як памʼять<н8>;
      будівельник_вказівок.вказівки.дані[позиція] = значення_як_памʼять_н8[0];
      будівельник_вказівок.вказівки.дані[позиція + 1] = значення_як_памʼять_н8[1];
      будівельник_вказівок.вказівки.дані[позиція + 2] = значення_як_памʼять_н8[2];
      будівельник_вказівок.вказівки.дані[позиція + 3] = значення_як_памʼять_н8[3];
    }

    місцева дія покласти_вказівку_ПокластиПусто(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: н32) -> н32 {
      вернути покласти_вказівку(М, будівельник_вказівок, рядок, ВПокластиПусто);
    }

    місцева дія покласти_вказівку_ПокластиТак(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: н32) -> н32 {
