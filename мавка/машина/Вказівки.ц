      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, рядок_як_памʼять_н8[4]);
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, рядок_як_памʼять_н8[5]);
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, рядок_як_памʼять_н8[6]);
