    місцева дія перезаписати_аргумент_вказівки(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, позиція: натуральне, значення: натуральне) {
    змінна значення_як_памʼять_н8 = значення::адреса як памʼять<н8>;
    будівельник_вказівок.вказівки.дані[позиція] = значення_як_памʼять_н8[0];
    будівельник_вказівок.вказівки.дані[позиція + 1] = значення_як_памʼять_н8[1];
    будівельник_вказівок.вказівки.дані[позиція + 2] = значення_як_памʼять_н8[2];
    будівельник_вказівок.вказівки.дані[позиція + 3] = значення_як_памʼять_н8[3];
    будівельник_вказівок.вказівки.дані[позиція + 4] = значення_як_памʼять_н8[4];
    будівельник_вказівок.вказівки.дані[позиція + 5] = значення_як_памʼять_н8[5];
    будівельник_вказівок.вказівки.дані[позиція + 6] = значення_як_памʼять_н8[6];
    будівельник_вказівок.вказівки.дані[позиція + 7] = значення_як_памʼять_н8[7];
  }

