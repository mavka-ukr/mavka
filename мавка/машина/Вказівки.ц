    дія покласти_вказівку(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: натуральне, вказівка: н8) -> натуральне {
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, вказівка);
      змінна рядок_як_памʼять_н8 = рядок::адреса як памʼять<н8>;
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, рядок_як_памʼять_н8[0]);
      покласти_в_накопичувач<н8>(М, будівельник_вказівок.вказівки::адреса, рядок_як_памʼять_н8[1]);
