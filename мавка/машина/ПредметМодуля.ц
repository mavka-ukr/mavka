взяти означення мавка;

простір мавка {
  місцева дія створити_модуль(М: адреса<Машина>, назва: адреса<ПредметТексту>) -> Значення {
    змінна предмет = виділити_предмет<ПредметМодуля>(М, М.предмет_структури_Модуль);
    
    предмет.готовий = ні;
    предмет.назва = назва;
    предмет.властивості = Властивості { 0, пусто };
    
    вернути предмет як Значення;
  }
}