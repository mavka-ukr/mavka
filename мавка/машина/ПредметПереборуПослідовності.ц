взяти визначення мавка;

секція мавка {
  дія предмет_перебору_послідовності_виконати_метод_далі(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_перебору_послідовності.почато == натуральне(так) {
      предмет_перебору_послідовності.позиція += 1;
    } інакше {
      предмет_перебору_послідовності.позиція = 0;
      предмет_перебору_послідовності.почато = натуральне(так);
    }
    якщо предмет_перебору_послідовності.позиція < предмет_перебору_послідовності.предмет_послідовності.елементи.розмір {
      предмет_перебору_послідовності.поточне_значення = предмет_перебору_послідовності.предмет_послідовності.елементи.дані[предмет_перебору_послідовності.позиція];
      вернути М.предмет_логічного_так як адреса<Предмет>;
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }
}

секція мавка {
  дія записати_параметр_структури_ПеребірПослідовності_предмет_послідовності(М: адреса<Машина>) {
    змінна параметр_предмет_послідовності = створити_предмет_параметра(М, створити_назву(М, "предмет_послідовності"), пусто);
    М.предмет_структури_ПеребірПослідовності.параметри.дані[0] = параметр_предмет_послідовності;
  }

  дія записати_параметр_структури_ПеребірПослідовності_позиція(М: адреса<Машина>) {
    змінна параметр_позиція = створити_предмет_параметра(М, створити_назву(М, "позиція"), пусто);
    М.предмет_структури_ПеребірПослідовності.параметри.дані[1] = параметр_позиція;
  }

  дія записати_параметр_структури_ПеребірПослідовності_значення(М: адреса<Машина>) {
    змінна параметр_значення = створити_предмет_параметра(М, створити_назву(М, "значення"), пусто);
    М.предмет_структури_ПеребірПослідовності.параметри.дані[2] = параметр_значення;
  }

  дія записати_параметр_структури_ПеребірПослідовності_почато(М: адреса<Машина>) {
    змінна параметр_почато = створити_предмет_параметра(М, створити_назву(М, "почато"), пусто);
    М.предмет_структури_ПеребірПослідовності.параметри.дані[3] = параметр_почато;
  }

  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: натуральне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_я.с != М.предмет_структури_ПеребірПослідовності {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметБайтів>, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору послідовності") як адреса<Предмет>);
    }
    змінна предмет_перебору_послідовності = предмет_я як адреса<ПредметПереборуПослідовності>;
    вернути предмет_перебору_послідовності_виконати_метод_далі(М, предмет_перебору_послідовності, Місцезнаходження { пусто як адреса<ПредметБайтів>, 0 });
  }

  дія записати_метод_структури_ПеребірПослідовності_далі(М: адреса<Машина>) {
    змінна предмет_дії_метода_далі = створити_предмет_рідної_дії(М, створити_назву(М, "далі"), 0, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірПослідовності, створити_назву(М, "далі"), предмет_дії_метода_далі);
  }

  місцева дія заповнити_предмет_структури_ПеребірПослідовності(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ПеребірПослідовності_предмет_послідовності(М);
    записати_параметр_структури_ПеребірПослідовності_позиція(М);
    записати_параметр_структури_ПеребірПослідовності_значення(М);
    записати_параметр_структури_ПеребірПослідовності_почато(М);

    // методи
    записати_метод_структури_ПеребірПослідовності_далі(М);
  }

  місцева дія створити_предмет_перебору_послідовності(М: адреса<Машина>, предмет_послідовності: адреса<ПредметПослідовності>): адреса<ПредметПереборуПослідовності> {
    змінна предмет_перебору_послідовності = створити_предмет<ПредметПереборуПослідовності>(М, М.предмет_структури_ПеребірПослідовності);
    предмет_перебору_послідовності.предмет_послідовності = предмет_послідовності;
    предмет_перебору_послідовності.почато = натуральне(ні);
    предмет_перебору_послідовності.позиція = 0;
    предмет_перебору_послідовності.поточне_значення = пусто;
    вернути предмет_перебору_послідовності;
  }

  місцева дія предмет_перебору_послідовності_виконати(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, предмет_я: адреса<Предмет>, кількість_аргументів: натуральне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_виконати"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_отримати_властивість(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "предмет_послідовності")) {
      вернути предмет_перебору_послідовності.предмет_послідовності як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "почато")) {
      якщо предмет_перебору_послідовності.почато == натуральне(так) {
        вернути М.предмет_логічного_так як адреса<Предмет>;
      }
      вернути М.предмет_логічного_ні як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "позиція")) {
      вернути створити_предмет_числа(М, р64(предмет_перебору_послідовності.позиція)) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "значення")) {
      вернути предмет_перебору_послідовності.поточне_значення;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_послідовності.с, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  місцева дія предмет_перебору_послідовності_змінити_властивість(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути пусто;
  }

  місцева дія предмет_перебору_послідовності_видалити_властивість(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  місцева дія предмет_перебору_послідовності_виконати_властивість(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, назва: адреса<ПредметТексту>, кількість_аргументів: натуральне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна предмет_властивості = предмет_перебору_послідовності_отримати_властивість(М, предмет_перебору_послідовності, назва, місцезнаходження);
    якщо М.стан_падіння {
      вернути пусто;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<н8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета перебору послідовності \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_перебору_послідовності як адреса<Предмет>, кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_додати(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_відняти(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_помножити(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_поділити(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_остача(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_остача"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_неповна_частка(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_неповна_частка"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_степінь(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_степінь"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_зсунути_вліво(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_вліво"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_зсунути_вправо(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_вправо"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_послідовності_зсунути_вправо_2(М: адреса<Машина>, предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_послідовності_виконати_властивість(М, предмет_перебору_послідовності, створити_назву(М, "чародія_вправо_2"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

