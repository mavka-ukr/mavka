взяти означення мавка;

простір мавка {
  місцева дія перевірити_чи_кд_рівні(а: кд, б: кд): логічне {
    якщо а.розмір != б.розмір {
      вернути ні;
    }
    змінна п: природне = 0;
    поки п < а.розмір {
      якщо а.дані[п] != б.дані[п] {
        вернути ні;
      }
      п += 1;
    }
    вернути так;
  }

  місцева дія виділити_копію_кд(М: адреса<Машина>, значення: кд, вихід: адреса<кд>): логічне {
    змінна копія = кд {
      значення.розмір,
      виділити_памʼять<п8>(М, значення.розмір)
    };
    змінна п: природне = 0;
    поки п < значення.розмір {
      копія.дані[п] = значення.дані[п];
      п += 1;
    }
    вихід::вміст = копія;
    вернути так;
  }

  місцева дія виділити_копію_памʼяті(М: адреса<Машина>, розмір: природне, значення: памʼять<п8>, вихід: адреса<памʼять<п8>>): логічне {
    змінна копія = виділити_памʼять<п8>(М, розмір);
    змінна п: природне = 0;
    поки п < розмір {
      копія[п] = значення[п];
      п += 1;
    }
    вихід::вміст = копія;
    вернути так;
  }
}
