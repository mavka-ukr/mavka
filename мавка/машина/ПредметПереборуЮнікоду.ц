взяти визначення мавка;

секція мавка {
  дія предмет_перебору_юнікоду_виконати_метод_далі(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_перебору_юнікоду.поточне_значення == пусто {
      предмет_перебору_юнікоду.позиція = 0;
    } інакше {
      предмет_перебору_юнікоду.позиція += 1;
    }
    якщо предмет_перебору_юнікоду.позиція < предмет_перебору_юнікоду.предмет_юнікоду.значення.розмір {
      предмет_перебору_юнікоду.поточне_значення = створити_предмет_числа(М, д64(предмет_перебору_юнікоду.предмет_юнікоду.значення.дані[предмет_перебору_юнікоду.позиція]));
      вернути М.предмет_логічного_так як адреса<Предмет>;
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }
}

секція мавка {
  дія записати_параметр_структури_ПеребірЮнікоду_предмет_юнікоду(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_предмет_юнікоду = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_предмет_юнікоду[0] = М.предмет_структури_юнікод;
    змінна тип_параметра_предмет_юнікоду = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_предмет_юнікоду });
    змінна параметр_предмет_юнікоду = створити_предмет_параметра(М, назва(М, "предмет_юнікоду"), тип_параметра_предмет_юнікоду, пусто);
    М.предмет_структури_ПеребірЮнікоду.параметри.дані[0] = параметр_предмет_юнікоду;
  }

  дія записати_параметр_структури_ПеребірЮнікоду_позиція(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_позиція = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_позиція[0] = М.предмет_структури_число;
    змінна тип_параметра_позиція = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_позиція });
    змінна параметр_позиція = створити_предмет_параметра(М, назва(М, "позиція"), тип_параметра_позиція, пусто);
    М.предмет_структури_ПеребірЮнікоду.параметри.дані[1] = параметр_позиція;
  }

  дія записати_параметр_структури_ПеребірЮнікоду_значення(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_значення = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_значення[0] = М.предмет_структури_число;
    змінна тип_параметра_значення = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_значення });
    змінна параметр_значення = створити_предмет_параметра(М, назва(М, "значення"), тип_параметра_значення, пусто);
    М.предмет_структури_ПеребірЮнікоду.параметри.дані[2] = параметр_значення;
  }

  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірЮнікоду {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору юнікоду") як адреса<Предмет>);
    }
    змінна предмет_перебору_юнікоду = предмет_я як адреса<ПредметПереборуЮнікоду>;
    вернути предмет_перебору_юнікоду_виконати_метод_далі(М, предмет_перебору_юнікоду, місцезнаходження);
  }

  дія записати_метод_структури_ПеребірЮнікоду_далі(М: адреса<Машина>) {
    змінна предмет_дії_метода_далі = створити_предмет_рідної_дії(М, назва(М, "далі"), 0, пусто, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірЮнікоду, назва(М, "далі"), предмет_дії_метода_далі);
  }

  місцева дія заповнити_предмет_структури_ПеребірЮнікоду(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ПеребірЮнікоду_предмет_юнікоду(М);
    записати_параметр_структури_ПеребірЮнікоду_позиція(М);
    записати_параметр_структури_ПеребірЮнікоду_значення(М);

    // методи
    записати_метод_структури_ПеребірЮнікоду_далі(М);
  }

  місцева дія створити_предмет_перебору_юнікоду(М: адреса<Машина>, предмет_юнікоду: адреса<ПредметЮнікоду>): адреса<ПредметПереборуЮнікоду> {
    змінна предмет_перебору_юнікоду = створити_предмет<ПредметПереборуЮнікоду>(М, М.предмет_структури_ПеребірЮнікоду);
    предмет_перебору_юнікоду.предмет_юнікоду = предмет_юнікоду;
    предмет_перебору_юнікоду.позиція = 0;
    предмет_перебору_юнікоду.поточне_значення = пусто;
    вернути предмет_перебору_юнікоду;
  }

  місцева дія предмет_перебору_юнікоду_виконати(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_отримати_властивість(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "предмет_юнікоду")) {
      вернути предмет_перебору_юнікоду.предмет_юнікоду як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "позиція")) {
      вернути створити_предмет_числа(М, д64(предмет_перебору_юнікоду.позиція)) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "значення")) {
      вернути предмет_перебору_юнікоду.поточне_значення як адреса<Предмет>;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_юнікоду.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  місцева дія предмет_перебору_юнікоду_змінити_властивість(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  місцева дія предмет_перебору_юнікоду_видалити_властивість(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  місцева дія предмет_перебору_юнікоду_виконати_властивість(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна предмет_властивості = предмет_перебору_юнікоду_отримати_властивість(М, предмет_перебору_юнікоду, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета перебору юнікоду \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_перебору_юнікоду як адреса<Предмет>, кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_додати(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_відняти(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_помножити(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_поділити(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_остача(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_остача"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_неповна_частка(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_неповна_частка"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_степінь(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_степінь"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_зсунути_вліво(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_вліво"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_зсунути_вправо(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_вправо"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_юнікоду_зсунути_вправо_2(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_юнікоду_виконати_властивість(М, предмет_перебору_юнікоду, назва(М, "чародія_вправо_2"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

