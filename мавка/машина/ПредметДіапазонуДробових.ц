взяти означення мавка;

простір мавка {
  місцева дія створити_діапазон_дробових(М: адреса<Машина>, від: адреса<ПредметДробового>, до: адреса<ПредметДробового>, включно: логічне) -> Значення {
    змінна предмет = виділити_предмет<ПредметДіапазонуДробових>(М, М.предмет_структури_ДіапазонДробових);

    предмет.від = від;
    предмет.до = до;
    предмет.включно = включно;

    вернути предмет як Значення;
  }
}