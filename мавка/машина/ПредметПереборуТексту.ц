взяти визначення мавка;

секція мавка {
  дія предмет_перебору_тексту_виконати_метод_далі(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_перебору_тексту.поточне_значення == пусто {
      предмет_перебору_тексту.позиція = 0;
    } інакше {
      предмет_перебору_тексту.позиція += 1;
    }
    якщо предмет_перебору_тексту.позиція < предмет_перебору_тексту.предмет_тексту.значення.розмір {
      предмет_перебору_тексту.поточне_значення = створити_предмет_числа(М, д64(предмет_перебору_тексту.предмет_тексту.значення.дані[предмет_перебору_тексту.позиція]));
      вернути М.предмет_логічного_так як адреса<Предмет>;
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }
}

секція мавка {
  дія записати_параметр_структури_ПеребірТексту_предмет_тексту(М: адреса<Машина>) {
    змінна параметр_предмет_тексту = створити_предмет_параметра(М, створити_назву(М, "предмет_тексту"), пусто);
    М.предмет_структури_ПеребірТексту.параметри.дані[0] = параметр_предмет_тексту;
  }

  дія записати_параметр_структури_ПеребірТексту_позиція(М: адреса<Машина>) {
    змінна параметр_позиція = створити_предмет_параметра(М, створити_назву(М, "позиція"), пусто);
    М.предмет_структури_ПеребірТексту.параметри.дані[1] = параметр_позиція;
  }

  дія записати_параметр_структури_ПеребірТексту_значення(М: адреса<Машина>) {
    змінна параметр_значення = створити_предмет_параметра(М, створити_назву(М, "значення"), пусто);
    М.предмет_структури_ПеребірТексту.параметри.дані[2] = параметр_значення;
  }

  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_я.тип != М.предмет_структури_ПеребірТексту {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору тексту") як адреса<Предмет>);
    }
    змінна предмет_перебору_тексту = предмет_я як адреса<ПредметПереборуТексту>;
    вернути предмет_перебору_тексту_виконати_метод_далі(М, предмет_перебору_тексту, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 });
  }

  дія записати_метод_структури_ПеребірТексту_далі(М: адреса<Машина>) {
    змінна предмет_дії_метода_далі = створити_предмет_рідної_дії(М, створити_назву(М, "далі"), 0, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірТексту, створити_назву(М, "далі"), предмет_дії_метода_далі);
  }

  місцева дія заповнити_предмет_структури_ПеребірТексту(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ПеребірТексту_предмет_тексту(М);
    записати_параметр_структури_ПеребірТексту_позиція(М);
    записати_параметр_структури_ПеребірТексту_значення(М);

    // методи
    записати_метод_структури_ПеребірТексту_далі(М);
  }

  місцева дія створити_предмет_перебору_тексту(М: адреса<Машина>, предмет_тексту: адреса<ПредметТексту>): адреса<ПредметПереборуТексту> {
    змінна предмет_перебору_тексту = створити_предмет<ПредметПереборуТексту>(М, М.предмет_структури_ПеребірТексту);
    предмет_перебору_тексту.предмет_тексту = предмет_тексту;
    предмет_перебору_тексту.позиція = 0;
    предмет_перебору_тексту.поточне_значення = пусто;
    вернути предмет_перебору_тексту;
  }

  місцева дія предмет_перебору_тексту_виконати(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_виконати"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_отримати_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "предмет_тексту")) {
      вернути предмет_перебору_тексту.предмет_тексту як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "позиція")) {
      вернути створити_предмет_числа(М, д64(предмет_перебору_тексту.позиція)) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "значення")) {
      вернути предмет_перебору_тексту.поточне_значення як адреса<Предмет>;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_тексту.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  місцева дія предмет_перебору_тексту_змінити_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути пусто;
  }

  місцева дія предмет_перебору_тексту_видалити_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  місцева дія предмет_перебору_тексту_виконати_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна предмет_властивості = предмет_перебору_тексту_отримати_властивість(М, предмет_перебору_тексту, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета перебору тексту \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_перебору_тексту як адреса<Предмет>, кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_додати(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_відняти(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_помножити(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_поділити(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_остача(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_остача"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_неповна_частка(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_неповна_частка"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_степінь(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_степінь"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_зсунути_вліво(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_вліво"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_зсунути_вправо(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_вправо"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_зсунути_вправо_2(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_вправо_2"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_двійкове_і(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_двійкове_і"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_двійкове_або(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_двійкове_або"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_тексту_двійкове_виключне_або(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, створити_назву(М, "чародія_двійкове_виключне_або"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

