взяти визначення мавка;

секція мавка {
  дія предмет_перебору_буфера_виконати_метод_далі(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_перебору_буфера.поточне_значення == пусто {
      предмет_перебору_буфера.позиція = 0;
    } інакше {
      предмет_перебору_буфера.позиція += 1;
    }
    якщо предмет_перебору_буфера.позиція < предмет_перебору_буфера.предмет_буфера.значення.розмір {
      предмет_перебору_буфера.поточне_значення = створити_предмет_числа(М, д64(предмет_перебору_буфера.предмет_буфера.значення.дані[предмет_перебору_буфера.позиція]));
      вернути М.предмет_логічного_так як адреса<Предмет>;
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }
}

секція мавка {
  дія записати_параметр_структури_ПеребірБуфера_предмет_буфера(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_предмет_буфера = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_предмет_буфера[0] = М.предмет_структури_Буфер;
    змінна тип_параметра_предмет_буфера = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_предмет_буфера });
    змінна параметр_предмет_буфера = створити_предмет_параметра(М, створити_назву(М, "предмет_буфера"), тип_параметра_предмет_буфера, пусто);
    М.предмет_структури_ПеребірБуфера.параметри.дані[0] = параметр_предмет_буфера;
  }

  дія записати_параметр_структури_ПеребірБуфера_позиція(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_позиція = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_позиція[0] = М.предмет_структури_число;
    змінна тип_параметра_позиція = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_позиція });
    змінна параметр_позиція = створити_предмет_параметра(М, створити_назву(М, "позиція"), тип_параметра_позиція, пусто);
    М.предмет_структури_ПеребірБуфера.параметри.дані[1] = параметр_позиція;
  }

  дія записати_параметр_структури_ПеребірБуфера_значення(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_значення = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_значення[0] = М.предмет_структури_число;
    змінна тип_параметра_значення = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_значення });
    змінна параметр_значення = створити_предмет_параметра(М, створити_назву(М, "значення"), тип_параметра_значення, пусто);
    М.предмет_структури_ПеребірБуфера.параметри.дані[2] = параметр_значення;
  }

  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірБуфера {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору буфера") як адреса<Предмет>);
    }
    змінна предмет_перебору_буфера = предмет_я як адреса<ПредметПереборуБуфера>;
    вернути предмет_перебору_буфера_виконати_метод_далі(М, предмет_перебору_буфера, Місцезнаходження { пусто, 0 });
  }

  дія записати_метод_структури_ПеребірБуфера_далі(М: адреса<Машина>) {
    змінна предмет_дії_метода_далі = створити_предмет_рідної_дії(М, створити_назву(М, "далі"), 0, пусто, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірБуфера, створити_назву(М, "далі"), предмет_дії_метода_далі);
  }

  місцева дія заповнити_предмет_структури_ПеребірБуфера(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ПеребірБуфера_предмет_буфера(М);
    записати_параметр_структури_ПеребірБуфера_позиція(М);
    записати_параметр_структури_ПеребірБуфера_значення(М);

    // методи
    записати_метод_структури_ПеребірБуфера_далі(М);
  }

  місцева дія створити_предмет_перебору_буфера(М: адреса<Машина>, предмет_буфера: адреса<ПредметБуфера>): адреса<ПредметПереборуБуфера> {
    змінна предмет_перебору_буфера = створити_предмет<ПредметПереборуБуфера>(М, М.предмет_структури_ПеребірБуфера);
    предмет_перебору_буфера.предмет_буфера = предмет_буфера;
    предмет_перебору_буфера.позиція = 0;
    предмет_перебору_буфера.поточне_значення = пусто;
    вернути предмет_перебору_буфера;
  }

  місцева дія предмет_перебору_буфера_виконати(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_виконати"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_отримати_властивість(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "предмет_буфера")) {
      вернути предмет_перебору_буфера.предмет_буфера як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "позиція")) {
      вернути створити_предмет_числа(М, д64(предмет_перебору_буфера.позиція)) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "значення")) {
      вернути предмет_перебору_буфера.поточне_значення як адреса<Предмет>;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_буфера.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  місцева дія предмет_перебору_буфера_змінити_властивість(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  місцева дія предмет_перебору_буфера_видалити_властивість(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  місцева дія предмет_перебору_буфера_виконати_властивість(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна предмет_властивості = предмет_перебору_буфера_отримати_властивість(М, предмет_перебору_буфера, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета перебору буфера \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_перебору_буфера як адреса<Предмет>, кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_додати(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_відняти(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_помножити(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_поділити(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_остача(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_остача"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_неповна_частка(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_неповна_частка"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_степінь(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_степінь"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_зсунути_вліво(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_вліво"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_зсунути_вправо(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_вправо"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_перебору_буфера_зсунути_вправо_2(М: адреса<Машина>, предмет_перебору_буфера: адреса<ПредметПереборуБуфера>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_буфера_виконати_властивість(М, предмет_перебору_буфера, створити_назву(М, "чародія_вправо_2"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
