    вернути предмет_числа_виконати_метод_чародія_зображення(М, предмет_числа, Місцезнаходження { пусто як адреса<ПредметБайтів>, 0 });
  }

  дія записати_метод_структури_число_чародія_зображення(М: адреса<Машина>) {
