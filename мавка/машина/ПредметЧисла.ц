  дія рідна_дія_метода_чародія_поділити(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_я.тип != М.предмет_структури_число {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметБайтів>, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_поділити\" не для числа") як адреса<Предмет>);
    }
    змінна аргумент_значення: адреса<Предмет> = пусто;
    змінна знайдено_аргумент_значення = ні;
    якщо іменовані_аргументи != пусто {
      знайдено_аргумент_значення = знайти_іменований_аргумент(М, іменовані_аргументи, створити_назву(М, "значення"), аргумент_значення::адреса);
    }
    якщо кількість_аргументів > 0 {
      якщо не знайдено_аргумент_значення {
        аргумент_значення = аргументи[0];
        знайдено_аргумент_значення = так;
      }
    }
    змінна предмет_числа = предмет_я як адреса<ПредметЧисла>;
    вернути предмет_числа_виконати_метод_чародія_поділити(М, предмет_числа, аргумент_значення як адреса<ПредметЧисла>, Місцезнаходження { пусто як адреса<ПредметБайтів>, 0 });
  }

