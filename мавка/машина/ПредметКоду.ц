взяти означення мавка;

простір мавка {
  місцева дія створити_код(М: адреса<Машина>, константи: адреса<Послідовність>, вказівки: адреса<ПредметДаних>) -> адреса<ПредметКоду> {
    змінна предмет = виділити_предмет<ПредметКоду>(М, М.предмет_структури_Код);
    
    предмет.константи = константи;
    предмет.вказівки = вказівки;
    
    вернути предмет;
  }
}