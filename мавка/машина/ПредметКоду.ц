  }

  місцева дія предмет_коду_зменшити(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_зменшити"), 0, пусто, пусто, місцезнаходження);
  }

  місцева дія предмет_коду_зображення(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_зображення"), 0, пусто, пусто, місцезнаходження);
  }

  місцева дія предмет_коду_текст(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_текст"), 0, пусто, пусто, місцезнаходження);
  }

  місцева дія предмет_коду_отримати_байти(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_байти"), 0, пусто, пусто, місцезнаходження);
  }

  місцева дія предмет_коду_отримати_елемент(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_отримати_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_коду_змінити_елемент(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, ключ: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_змінити_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_коду_видалити_елемент(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_видалити_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_коду_отримати_перебір(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_перебір"), 0, пусто, пусто, місцезнаходження);
  }

  місцева дія предмет_коду_отримати_діапазон(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, включно: логічне, до: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна аргументи = виділити_памʼять<адреса<Предмет>>(М, 2);
    якщо включно {
      аргументи[0] = М.предмет_логічного_так як адреса<Предмет>;
    } інакше {
      аргументи[0] = М.предмет_логічного_ні як адреса<Предмет>;
    }
    аргументи[1] = до;
    змінна результат = предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_діапазон"), 2, аргументи, пусто, місцезнаходження);
    звільнити_памʼять(М, аргументи);
    вернути результат;
  }
}