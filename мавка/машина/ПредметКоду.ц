взяти визначення мавка;

секція мавка {
}

секція мавка {
  дія записати_параметр_структури_Код_версія(М: адреса<Машина>) {
    змінна параметр_версія = створити_предмет_параметра(М, створити_назву(М, "версія"), пусто);
    М.предмет_структури_Код.параметри.дані[0] = параметр_версія;
  }

  дія записати_параметр_структури_Код_шлях(М: адреса<Машина>) {
    змінна параметр_шлях = створити_предмет_параметра(М, створити_назву(М, "шлях"), пусто);
    М.предмет_структури_Код.параметри.дані[1] = параметр_шлях;
  }

  дія записати_параметр_структури_Код_константи(М: адреса<Машина>) {
    змінна параметр_константи = створити_предмет_параметра(М, створити_назву(М, "константи"), пусто);
    М.предмет_структури_Код.параметри.дані[2] = параметр_константи;
  }

  дія записати_параметр_структури_Код_вказівки(М: адреса<Машина>) {
    змінна параметр_вказівки = створити_предмет_параметра(М, створити_назву(М, "вказівки"), пусто);
    М.предмет_структури_Код.параметри.дані[3] = параметр_вказівки;
  }

  місцева дія заповнити_предмет_структури_Код(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_Код_версія(М);
    записати_параметр_структури_Код_шлях(М);
    записати_параметр_структури_Код_константи(М);
    записати_параметр_структури_Код_вказівки(М);

    // методи
  }

  місцева дія створити_предмет_коду(М: адреса<Машина>, шлях: адреса<ПредметБайтів>, константи: Послідовність<адреса<Предмет>>, місцезнаходження_вказівок: Послідовність<МісцезнаходженняВказівок>, вказівки: адреса<ПредметБайтів>): адреса<ПредметКоду> {
    змінна предмет_коду = створити_предмет<ПредметКоду>(М, М.предмет_структури_Код);
    предмет_коду.шлях = шлях;
    предмет_коду.константи = константи;
    предмет_коду.місцезнаходження_вказівок = місцезнаходження_вказівок;
    предмет_коду.байти = вказівки;
    вернути предмет_коду;
  }

  місцева дія предмет_коду_виконати(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_виконати"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_коду_отримати_властивість(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "версія")) {
      вернути створити_предмет_числа(М, 0) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "шлях")) {
      вернути предмет_коду.шлях як адреса<ПредметТексту> як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "константи")) {
      вернути створити_предмет_списку_1(М, предмет_коду.константи) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "вказівки")) {
      вернути предмет_коду.байти як адреса<Предмет>;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_коду.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  місцева дія предмет_коду_змінити_властивість(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути пусто;
  }

  місцева дія предмет_коду_видалити_властивість(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  місцева дія предмет_коду_виконати_властивість(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна предмет_властивості = предмет_коду_отримати_властивість(М, предмет_коду, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути пусто;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета коду \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_коду як адреса<Предмет>, кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_коду_додати(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, створити_назву(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_коду_відняти(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
