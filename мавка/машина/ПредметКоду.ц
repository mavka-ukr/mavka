взяти означення мавка;

простір мавка {
  місцева дія створити_код(М: адреса<Машина>, константи: адреса<Послідовність>, вказівки: Значення) -> Значення {
    змінна предмет = виділити_предмет<ПредметКоду>(М, М.предмет_структури_Код);
    
    предмет.константи = константи;
    предмет.вказівки = вказівки як адреса<ПредметДаних>;
    
    вернути предмет як Значення;
  }
}