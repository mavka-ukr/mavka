взяти означення мавка;

простір мавка {
  місцева дія створити_дробове(М: адреса<Машина>, значення: д64) -> Значення {
    змінна предмет = виділити_предмет<ПредметДробового>(М, М.предмет_структури_Дробове);

    предмет.значення = значення;
    
    вернути предмет як Значення;
  }

  місцева дія предмет_дробового_додати(М: адреса<Машина>, значення: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    якщо право == пусто {
      стрибнути крок_помилка;
    }

    змінна властивості_ліво = значення як адреса<ПредметДробового>;

    якщо право.тип == М.предмет_структури_Дробове {
      змінна властивості_право = право як адреса<ПредметДробового>;
    
      вернути створити_дробове(М, властивості_ліво.значення + властивості_право.значення);
    }

    якщо право.тип == М.предмет_структури_Ціле {
      змінна властивості_право = право як адреса<ПредметЦілого>;
    
      вернути створити_дробове(М, властивості_ліво.значення + д64(властивості_право.значення));
    }
    
  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Додати до дробового числа можна лише або ціле або дробове число.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }
  
  місцева дія предмет_дробового_відняти(М: адреса<Машина>, значення: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    якщо право == пусто {
      стрибнути крок_помилка;
    }

    змінна властивості_ліво = значення як адреса<ПредметДробового>;

    якщо право.тип == М.предмет_структури_Дробове {
      змінна властивості_право = право як адреса<ПредметДробового>;
    
      вернути створити_дробове(М, властивості_ліво.значення - властивості_право.значення);
    }

    якщо право.тип == М.предмет_структури_Ціле {
      змінна властивості_право = право як адреса<ПредметЦілого>;
    
      вернути створити_дробове(М, властивості_ліво.значення - д64(властивості_право.значення));
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Відняти від дробового числа можна лише або ціле або дробове число.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія предмет_дробового_помножити(М: адреса<Машина>, значення: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    якщо право == пусто {
      стрибнути крок_помилка;
    }

    змінна властивості_ліво = значення як адреса<ПредметДробового>;

    якщо право.тип == М.предмет_структури_Дробове {
      змінна властивості_право = право як адреса<ПредметДробового>;
    
      вернути створити_дробове(М, властивості_ліво.значення * властивості_право.значення);
    }

    якщо право.тип == М.предмет_структури_Ціле {
      змінна властивості_право = право як адреса<ПредметЦілого>;
    
      вернути створити_дробове(М, властивості_ліво.значення * д64(властивості_право.значення));
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Помножити дробове число можна лише на або ціле або дробове число.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія предмет_дробового_поділити(М: адреса<Машина>, значення: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    якщо право == пусто {
      стрибнути крок_помилка;
    }

    змінна властивості_ліво = значення як адреса<ПредметДробового>;

    якщо право.тип == М.предмет_структури_Дробове {
      змінна властивості_право = право як адреса<ПредметДробового>;
    
      вернути створити_дробове(М, властивості_ліво.значення / властивості_право.значення);
    }

    якщо право.тип == М.предмет_структури_Ціле {
      змінна властивості_право = право як адреса<ПредметЦілого>;
    
      вернути створити_дробове(М, властивості_ліво.значення / д64(властивості_право.значення));
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Поділити дробове число можна лише на або ціле або дробове число.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія предмет_дробового_остача(М: адреса<Машина>, значення: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    якщо право == пусто {
      стрибнути крок_помилка;
    }

    змінна властивості_ліво = значення як адреса<ПредметДробового>;

    якщо право.тип == М.предмет_структури_Дробове {
      змінна властивості_право = право як адреса<ПредметДробового>;
    
      вернути створити_дробове(М, властивості_ліво.значення % властивості_право.значення);
    }

    якщо право.тип == М.предмет_структури_Ціле {
      змінна властивості_право = право як адреса<ПредметЦілого>;

      вернути створити_дробове(М, властивості_ліво.значення % д64(властивості_право.значення));
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Отримати остачу ділення дробового числа можна лише з або цілим або дробовим числом.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія предмет_дробового_частка(М: адреса<Машина>, значення: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    якщо право == пусто {
      стрибнути крок_помилка;
    }

    змінна властивості_ліво = значення як адреса<ПредметДробового>;

    якщо право.тип == М.предмет_структури_Дробове {
      змінна властивості_право = право як адреса<ПредметДробового>;

      вернути створити_дробове(М, властивості_ліво.значення / властивості_право.значення);
    }

    якщо право.тип == М.предмет_структури_Ціле {
      змінна властивості_право = право як адреса<ПредметЦілого>;

      вернути створити_дробове(М, властивості_ліво.значення / д64(властивості_право.значення));
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Отримати неповну частку ділення дробового числа можна лише з або цілим або дробовим числом.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія предмет_дробового_степінь(М: адреса<Машина>, значення: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    якщо право == пусто {
      стрибнути крок_помилка;
    }

    змінна властивості_ліво = значення як адреса<ПредметДробового>;

    якщо право.тип == М.предмет_структури_Дробове {
      змінна властивості_право = право як адреса<ПредметДробового>;

      вернути створити_дробове(М, піднести_до_степеня_д64(М, властивості_ліво.значення, властивості_право.значення));
    }

    якщо право.тип == М.предмет_структури_Ціле {
      змінна властивості_право = право як адреса<ПредметЦілого>;

      вернути створити_дробове(М, піднести_до_степеня_д64(М, властивості_ліво.значення, д64(властивості_право.значення)));
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Отримати степінь дробового числа можна лише з або цілим або дробовим числом.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія предмет_дробового_менше(М: адреса<Машина>, значення: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    якщо право == пусто {
      стрибнути крок_помилка;
    }

    змінна властивості_ліво = значення як адреса<ПредметДробового>;

    якщо право.тип == М.предмет_структури_Дробове {
      змінна властивості_право = право як адреса<ПредметДробового>;

      якщо властивості_ліво.значення < властивості_право.значення {
        вернути М.значення_дійсне;
      } інакше {
        вернути пусто;
      }
    }

    якщо право.тип == М.предмет_структури_Ціле {
      змінна властивості_право = право як адреса<ПредметЦілого>;

      якщо властивості_ліво.значення < д64(властивості_право.значення) {
        вернути М.значення_дійсне;
      } інакше {
        вернути пусто;
      }
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконути перевірку чи дробове число менше можна з цілим або дробовим числом.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія предмет_дробового_більше(М: адреса<Машина>, значення: Значення, право: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    якщо право == пусто {
      стрибнути крок_помилка;
    }

    змінна властивості_ліво = значення як адреса<ПредметДробового>;

    якщо право.тип == М.предмет_структури_Дробове {
      змінна властивості_право = право як адреса<ПредметДробового>;

      якщо властивості_ліво.значення > властивості_право.значення {
        вернути М.значення_дійсне;
      } інакше {
        вернути пусто;
      }
    }

    якщо право.тип == М.предмет_структури_Ціле {
      змінна властивості_право = право як адреса<ПредметЦілого>;

      якщо властивості_ліво.значення > д64(властивості_право.значення) {
        вернути М.значення_дійсне;
      } інакше {
        вернути пусто;
      }
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконути перевірку чи дробове число менше можна з цілим або дробовим числом.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія предмет_дробового_змінити_знак(М: адреса<Машина>, значення: Значення, місцезнаходження: Місцезнаходження) -> ЗначенняАбоСтанПадіння {
    змінна властивості_предмета = значення як адреса<ПредметДробового>;

    вернути створити_дробове(М, -властивості_предмета.значення);
  }
}