  місцева дія предмет_байтів_отримати_байти(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_байтів як адреса<Предмет>;
  }

  місцева дія предмет_байтів_отримати_елемент(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    якщо ключ == пусто {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Ключ отримання елемента з байтів має бути числом") як адреса<Предмет>);
    }
    якщо ключ.тип != М.предмет_структури_число {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Ключ отримання елемента з байтів має бути числом") як адреса<Предмет>);
    }
    змінна предмет_числа = ключ як адреса<ПредметЧисла>;
    вернути предмет_байтів_виконати_метод_чародія_отримати_елемент(М, предмет_байтів, предмет_числа, місцезнаходження);
  }

  місцева дія предмет_байтів_змінити_елемент(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, ключ: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_байтів_виконати_властивість(М, предмет_байтів, створити_назву(М, "чародія_змінити_елемент"), 2, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_байтів_видалити_елемент(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_байтів_виконати_властивість(М, предмет_байтів, створити_назву(М, "чародія_видалити_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, пусто, місцезнаходження);
  }

  місцева дія предмет_байтів_отримати_перебір(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_байтів_виконати_метод_чародія_перебір(М, предмет_байтів, місцезнаходження);
  }

  місцева дія предмет_байтів_отримати_діапазон(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, включно: логічне, до: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна аргументи = виділити_памʼять<адреса<Предмет>>(М, 2);
    якщо включно {
      аргументи[0] = М.предмет_логічного_так як адреса<Предмет>;
    } інакше {
      аргументи[0] = М.предмет_логічного_ні як адреса<Предмет>;
    }
    аргументи[1] = до;
    змінна результат = предмет_байтів_виконати_властивість(М, предмет_байтів, створити_назву(М, "чародія_діапазон"), 2, аргументи, пусто, місцезнаходження);
    звільнити_памʼять(М, аргументи);
    вернути результат;
  }
}