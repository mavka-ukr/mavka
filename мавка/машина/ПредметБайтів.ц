взяти визначення мавка;

секція мавка {
  дія предмет_байтів_виконати_метод_чародія_отримати_елемент(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, ключ: адреса<ПредметЧисла>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна позиція = позитивне(ключ.значення);
    якщо предмет_байтів.розмір > позиція {
      вернути створити_предмет_числа(М, д64(предмет_байтів.дані[позиція])) як адреса<Предмет>;
    }
    вернути пусто;
  }

  дія предмет_байтів_виконати_метод_чародія_перебір(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути створити_предмет_перебору_байтів(М, предмет_байтів) як адреса<Предмет>;
  }

  дія предмет_байтів_виконати_метод_чародія_текст(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути створити_предмет_тексту(М, т8 { предмет_байтів.розмір, предмет_байтів.дані }) як адреса<Предмет>;
  }

  дія предмет_байтів_виконати_метод_чародія_байти(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_байтів як адреса<Предмет>;
  }
}

секція мавка {
  дія записати_параметр_структури_байти_розмір(М: адреса<Машина>) {
    змінна параметр_розмір = створити_предмет_параметра(М, створити_назву(М, "розмір"), пусто);
    М.предмет_структури_байти.параметри.дані[0] = параметр_розмір;
  }

  дія рідна_дія_метода_чародія_отримати_елемент(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_я.тип != М.предмет_структури_байти {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_отримати_елемент\" не для байтів") як адреса<Предмет>);
    }
    змінна аргумент_ключ: адреса<Предмет> = пусто;
    змінна знайдено_аргумент_ключ = ні;
    якщо іменовані_аргументи != пусто {
      знайдено_аргумент_ключ = знайти_іменований_аргумент(М, іменовані_аргументи, створити_назву(М, "ключ"), аргумент_ключ::адреса);
    }
    якщо кількість_аргументів > 0 {
      якщо знайдено_аргумент_ключ == ні {
        аргумент_ключ = аргументи[0];
        знайдено_аргумент_ключ = так;
      }
    }
    змінна предмет_байтів = предмет_я як адреса<ПредметБайтів>;
    вернути предмет_байтів_виконати_метод_чародія_отримати_елемент(М, предмет_байтів, аргумент_ключ як адреса<ПредметЧисла>, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 });
  }

  дія записати_метод_структури_байти_чародія_отримати_елемент(М: адреса<Машина>) {
    змінна параметр_ключ = створити_предмет_параметра(М, створити_назву(М, "ключ"), пусто);
    змінна дані_параметрів_метода_чародія_отримати_елемент = виділити_памʼять<адреса<ПредметПараметра>>(М, 1);
    дані_параметрів_метода_чародія_отримати_елемент[0] = параметр_ключ;
    змінна предмет_дії_метода_чародія_отримати_елемент = створити_предмет_рідної_дії(М, створити_назву(М, "чародія_отримати_елемент"), 1, дані_параметрів_метода_чародія_отримати_елемент, рідна_дія_метода_чародія_отримати_елемент, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_байти, створити_назву(М, "чародія_отримати_елемент"), предмет_дії_метода_чародія_отримати_елемент);
  }

  дія рідна_дія_метода_чародія_перебір(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_я.тип != М.предмет_структури_байти {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_перебір\" не для байтів") як адреса<Предмет>);
    }
    змінна предмет_байтів = предмет_я як адреса<ПредметБайтів>;
    вернути предмет_байтів_виконати_метод_чародія_перебір(М, предмет_байтів, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 });
  }

  дія записати_метод_структури_байти_чародія_перебір(М: адреса<Машина>) {
    змінна предмет_дії_метода_чародія_перебір = створити_предмет_рідної_дії(М, створити_назву(М, "чародія_перебір"), 0, пусто, рідна_дія_метода_чародія_перебір, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_байти, створити_назву(М, "чародія_перебір"), предмет_дії_метода_чародія_перебір);
  }

  дія рідна_дія_метода_чародія_текст(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_я.тип != М.предмет_структури_байти {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_текст\" не для байтів") як адреса<Предмет>);
    }
    змінна предмет_байтів = предмет_я як адреса<ПредметБайтів>;
    вернути предмет_байтів_виконати_метод_чародія_текст(М, предмет_байтів, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 });
  }

  дія записати_метод_структури_байти_чародія_текст(М: адреса<Машина>) {
    змінна предмет_дії_метода_чародія_текст = створити_предмет_рідної_дії(М, створити_назву(М, "чародія_текст"), 0, пусто, рідна_дія_метода_чародія_текст, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_байти, створити_назву(М, "чародія_текст"), предмет_дії_метода_чародія_текст);
  }

  дія рідна_дія_метода_чародія_байти(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння<Предмет> {
    якщо предмет_я.тип != М.предмет_структури_байти {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 }, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_байти\" не для байтів") як адреса<Предмет>);
    }
    змінна предмет_байтів = предмет_я як адреса<ПредметБайтів>;
    вернути предмет_байтів_виконати_метод_чародія_байти(М, предмет_байтів, Місцезнаходження { пусто як адреса<ПредметШляху>, 0 });
  }

  дія записати_метод_структури_байти_чародія_байти(М: адреса<Машина>) {
    змінна предмет_дії_метода_чародія_байти = створити_предмет_рідної_дії(М, створити_назву(М, "чародія_байти"), 0, пусто, рідна_дія_метода_чародія_байти, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_байти, створити_назву(М, "чародія_байти"), предмет_дії_метода_чародія_байти);
  }

  місцева дія заповнити_предмет_структури_байти(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_байти_розмір(М);

    // методи
    записати_метод_структури_байти_чародія_отримати_елемент(М);
    записати_метод_структури_байти_чародія_перебір(М);
    записати_метод_структури_байти_чародія_текст(М);
    записати_метод_структури_байти_чародія_байти(М);
  }

  місцева дія виконати_предмет_структури_байти(М: адреса<Машина>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна аргумент_значення: адреса<Предмет> = пусто;
    змінна знайдено_аргумент_значення = ні;
    якщо іменовані_аргументи != пусто {
      знайдено_аргумент_значення = знайти_іменований_аргумент(М, іменовані_аргументи, створити_назву(М, "значення"), аргумент_значення::адреса);
    }
    якщо кількість_аргументів > 0 {
      якщо не знайдено_аргумент_значення {
        аргумент_значення = аргументи[0];
        знайдено_аргумент_значення = так;
      }
    }
    якщо не знайдено_аргумент_значення {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Пропущено аргумент \"значення\"") як адреса<Предмет>);
    }
    якщо аргумент_значення == пусто {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо отримати байти з пусто") як адреса<Предмет>);
    }
    вернути предмет_виконати_властивість(М, аргумент_значення, створити_назву(М, "чародія_байти"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія створити_предмет_байтів(М: адреса<Машина>, розмір: позитивне, дані: памʼять<п8>): адреса<ПредметБайтів> {
    змінна предмет_байтів = створити_предмет<ПредметБайтів>(М, М.предмет_структури_байти);
    предмет_байтів.розмір = розмір;
    якщо виділити_копію_памʼяті(М, розмір, дані, предмет_байтів.дані::адреса) == ні {
      ПОМЕРТИ(М, "Не вдалось виділити копію памʼяті!");
      вернути пусто;
    }
    вернути предмет_байтів;
  }

  місцева дія створити_предмет_байтів_без_копіювання(М: адреса<Машина>, розмір: позитивне, дані: памʼять<п8>): адреса<ПредметБайтів> {
    змінна предмет_байтів = створити_предмет<ПредметБайтів>(М, М.предмет_структури_байти);
    предмет_байтів.розмір = розмір;
    предмет_байтів.дані = дані;
    вернути предмет_байтів;
  }

  місцева дія предмет_байтів_виконати(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути предмет_байтів_виконати_властивість(М, предмет_байтів, створити_назву(М, "чародія_виконати"), кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_байтів_отримати_властивість(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, створити_назву(М, "розмір")) {
      вернути створити_предмет_числа(М, д64(предмет_байтів.розмір)) як адреса<Предмет>;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_байтів.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  місцева дія предмет_байтів_змінити_властивість(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути пусто;
  }

  місцева дія предмет_байтів_видалити_властивість(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  місцева дія предмет_байтів_виконати_властивість(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
    змінна предмет_властивості = предмет_байтів_отримати_властивість(М, предмет_байтів, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути пусто;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета байтів \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_байтів як адреса<Предмет>, кількість_аргументів, аргументи, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_байтів_додати(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження) -> ПредметАбоСтанПадіння<Предмет> {
