взяти означення мавка;

простір мавка {
  місцева дія створити_діапазон_дробових(М: адреса<Машина>, від: Значення, до: Значення, включно: логічне) -> Значення {
    змінна предмет = виділити_предмет(М, М.значення_структура_ДіапазонДробових як Значення);

    предмет.властивості = ВластивостіДіапазонуДробових {
      від,
      до,
      включно
    };

    вернути предмет як Значення;
  }
}