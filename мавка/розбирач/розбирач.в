    аргументи: памʼять<адреса<АСДАргумент>>;
  }

  структура АСДДаніДія {
    тривала: позитивне;
    модифікатор: позитивне;
    структура_: адреса<АСДЗначення>;
    ідентифікатор: адреса<АСДІдентифікатор>;
    кількість_параметрів: позитивне;
    параметри: памʼять<адреса<АСДПараметр>>;
    кількість_типів_результату: позитивне;
    типи_результату: памʼять<адреса<АСДЗначення>>;
    тіло: адреса<СписокАСДЗначень>;
  }

  структура АСДДаніСтруктура {
