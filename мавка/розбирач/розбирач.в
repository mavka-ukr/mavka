  структура Місцезнаходження {
    текст_коду: адреса<ТекстКоду>;
    рядок: позитивне;
    стовпець: позитивне;
  }

  структура АСДЗначення {
    вид: позитивне;
    дані: адреса;
    місцезнаходження: адреса<Місцезнаходження>;
  }

  структура СписокАСДЗначень {
    довжина: позитивне;
    елементи: памʼять<адреса<АСДЗначення>>;
  }

  структура АСДІдентифікатор {
    значення: памʼять<п8>;
    місцезнаходження: адреса<Місцезнаходження>;
  }

  структура АСДПараметр {
