  структура АСДДаніСамоОперація {
    операція: позитивне;
    обʼєкт: адреса<АСДЗначення>;
  }

  структура АСДДаніВзяти {
    тип: адреса<АСДІдентифікатор>;
    довжина_шляху: позитивне;
    шлях: памʼять<адреса<АСДІдентифікатор>>;
    ідентифікатор_як: адреса<АСДІдентифікатор>;
    кількість_елементів: позитивне;
    елементи: памʼять<адреса<АСДЕлементВзяти>>;
