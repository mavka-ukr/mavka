    дія додати_вказівку_до_коду(М: адреса<Машина>, код: адреса<Код>, рядок: позитивне, вказівка: п8): позитивне {
      покласти_в_накопичувач<п8>(М, код.накопичувач_вказівок::адреса, вказівка);
      змінна рядок_як_памʼять_п8 = рядок::адреса як памʼять<п8>;
      покласти_в_накопичувач<п8>(М, код.накопичувач_вказівок::адреса, рядок_як_памʼять_п8[0]);
      покласти_в_накопичувач<п8>(М, код.накопичувач_вказівок::адреса, рядок_як_памʼять_п8[1]);
      покласти_в_накопичувач<п8>(М, код.накопичувач_вказівок::адреса, рядок_як_памʼять_п8[2]);
      покласти_в_накопичувач<п8>(М, код.накопичувач_вказівок::адреса, рядок_як_памʼять_п8[3]);
      покласти_в_накопичувач<п8>(М, код.накопичувач_вказівок::адреса, рядок_як_памʼять_п8[4]);
      покласти_в_накопичувач<п8>(М, код.накопичувач_вказівок::адреса, рядок_як_памʼять_п8[5]);
      покласти_в_накопичувач<п8>(М, код.накопичувач_вказівок::адреса, рядок_як_памʼять_п8[6]);
      покласти_в_накопичувач<п8>(М, код.накопичувач_вказівок::адреса, рядок_як_памʼять_п8[7]);
      вернути код.накопичувач_вказівок.розмір - 1;
    }

    дія записати_позитивне_в_код_за_позицією(М: адреса<Машина>, код: адреса<Код>, позиція: позитивне, значення: позитивне) {
      змінна значення_як_памʼять_п8 = значення::адреса як памʼять<п8>;
      код.накопичувач_вказівок.дані[позиція] = значення_як_памʼять_п8[0];
      код.накопичувач_вказівок.дані[позиція + 1] = значення_як_памʼять_п8[1];
      код.накопичувач_вказівок.дані[позиція + 2] = значення_як_памʼять_п8[2];
      код.накопичувач_вказівок.дані[позиція + 3] = значення_як_памʼять_п8[3];
      код.накопичувач_вказівок.дані[позиція + 4] = значення_як_памʼять_п8[4];
      код.накопичувач_вказівок.дані[позиція + 5] = значення_як_памʼять_п8[5];
      код.накопичувач_вказівок.дані[позиція + 6] = значення_як_памʼять_п8[6];
      код.накопичувач_вказівок.дані[позиція + 7] = значення_як_памʼять_п8[7];
    }

