    синонім ВНеЄ = 79;
    синонім ВВідʼємне = 80;
    синонім ВЧислове = 81;
    синонім ВЗаперечити = 82;
    синонім ВЗбільшити = 83;
    синонім ВЗбільшитиПопереднє = 84;
