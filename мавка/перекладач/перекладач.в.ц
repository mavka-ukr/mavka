взяти визначення мавка;
взяти визначення мавка/розбирач;

секція мавка {
  секція перекладач {
    синонім ВидКонстантиНазва = 1;
    синонім ВидКонстантиЧисло = 2;
    синонім ВидКонстантиТекст = 3;
    синонім ВидКонстантиЮнікод8 = 4;
    синонім ВидКонстантиЮнікод32 = 5;
    синонім ВидКонстантиКод = 6;

    структура Код {
      накопичувач_констант: Накопичувач<адреса<Предмет>>;
      кількість_констант: позитивне;
      накопичувач_вказівок: Накопичувач<п8>;
    }

    структура ПомилкаПерекладуВМаМа {
      повідомлення: т8;
      позиція_слова: позитивне;
    }

    структура КонтекстПерекладу {
      значення_ю8: ю8;
      слова: розбирач::Слова;
    }

    дія виділити_контекст_компіляції(М: адреса<Машина>, значення_ю8: ю8, слова: розбирач::Слова): адреса<КонтекстПерекладу> {
      змінна контекст = виділити<КонтекстПерекладу>(М);
      контекст.значення_ю8 = значення_ю8;
      контекст.слова = слова;
      вернути контекст;
    }

    місцева дія створити_код(М: адреса<Машина>): адреса<Код>;
    місцева дія знищити_код(М: адреса<Машина>, код: адреса<Код>);
    місцева дія створити_контекст(М: адреса<Машина>, значення_ю8: ю8, слова: розбирач::Слова): адреса<КонтекстПерекладу>;
    місцева дія знищити_контекст(М: адреса<Машина>, контекст: адреса<КонтекстПерекладу>);
    місцева дія перекласти_в_МаМа(М: адреса<Машина>, контекст: адреса<КонтекстПерекладу>, гілки: розбирач::Гілки, вернути_в_кінці: логічне, код: адреса<Код>, вихід_помилки: адреса<ПомилкаПерекладуВМаМа>): логічне;
  }
}