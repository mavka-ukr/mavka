взяти визначення мавка;
взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    місцева дія обробник_взяття_біб(М: адреса<Машина>, розмір_шляху: позитивне, шлях: памʼять<адреса<Предмет>>): предмет_або_стан_падіння {
      якщо розмір_шляху == 1 {

        якщо перевірити_чи_предмети_рівні(М, шлях[0], назва(М, "математика") як адреса<Предмет>) {
          вернути взяти_модуль_біб_математика(М);
        }
        якщо перевірити_чи_предмети_рівні(М, шлях[0], назва(М, "мавка") як адреса<Предмет>) {
          вернути взяти_модуль_біб_мавка(М);
        }
        якщо перевірити_чи_предмети_рівні(М, шлях[0], назва(М, "читати") як адреса<Предмет>) {
          вернути взяти_модуль_біб_читати(М);
        }
        якщо перевірити_чи_предмети_рівні(М, шлях[0], назва(М, "читати_юнікод") як адреса<Предмет>) {
          вернути взяти_модуль_біб_читати_юнікод(М);
        }
        якщо перевірити_чи_предмети_рівні(М, шлях[0], назва(М, "інтернет") як адреса<Предмет>) {
          вернути взяти_модуль_біб_інтернет(М);
        }
        якщо перевірити_чи_предмети_рівні(М, шлях[0], назва(М, "планувальник") як адреса<Предмет>) {
          вернути взяти_модуль_біб_планувальник(М);
        }
      }
      вернути падіння(М, "Неможливо взяти модуль біб");
    }
  }
}