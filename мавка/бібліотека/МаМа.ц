взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    дія нативна_дія_мавки_отримати_код(предмет_нативної_дії: адреса<Предмет>, обчислювач: адреса<Обчислювач>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, дані: адреса): Результат {
      вернути результат_успіх(обчислювач, пусто);
    }

    місцева дія заповнити_модуль_МаМа(обчислювач: адреса<Обчислювач>, предмет_модуля: адреса<Предмет>) {
      предмет_змінити_властивість(предмет_модуля, обчислювач, назва(обчислювач, т8"отримати_код"), створити_нативну_дію(обчислювач, назва(обчислювач, т8"отримати_код"), нативна_дія_мавки_отримати_код, пусто, пусто));
    }
  }
}