взяти визначення мавка;
взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    зовнішня дія створити_рідну_дію_бібліотеки_мавка_завантажити_РМв1(обчислювач: адреса<Обчислювач>, предмет_модуля: адреса<Предмет>): предмет_або_стан_падіння;

    дія створити_модуль_мавка(обчислювач: адреса<Обчислювач>): предмет_або_стан_падіння {
      змінна М = обчислювач;

      змінна результат = МаМа::створити_предмет_модуля(обчислювач, МаМа::назва(М, "мавка")) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }

      змінна предмет_модуля_мавка = результат;

      змінна предмет_тексту_версія = МаМа::створити_предмет_тексту_без_копіювання(обчислювач, мавка__отримати_версію_мавки_як_т8(обчислювач)) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_тексту_версія;
      }
      результат = МаМа::предмет_змінити_властивість(М, предмет_модуля_мавка, МаМа::назва(М, "версія"), предмет_тексту_версія, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }

      результат = створити_рідну_дію_бібліотеки_мавка_завантажити_РМв1(обчислювач, предмет_модуля_мавка);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }
      результат = МаМа::предмет_змінити_властивість(М, предмет_модуля_мавка, МаМа::назва(М, "завантажити_РМв1"), результат, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }

      вернути предмет_модуля_мавка;
    }

    зовнішня дія взяти_модуль_біб_мавка(обчислювач: адреса<Обчислювач>): предмет_або_стан_падіння {
      змінна М = обчислювач;
      змінна результат = МаМа::створити_предмет_тексту(обчислювач, "мавка:бібліотека/мавка/мавка.ц") як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }
      змінна предмет_шляху = результат;
      змінна взятий_предмет: адреса<МаМа::Предмет> = пусто;
      якщо МаМа::отримати_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, взятий_предмет::адреса) {
        вернути взятий_предмет як адреса<Предмет>;
      }
      результат = створити_модуль_мавка(обчислювач);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }
      МаМа::покласти_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, результат як адреса<МаМа::Предмет>);
      вернути результат;
    }
  }
}