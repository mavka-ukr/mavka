взяти визначення МаМа;
взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    місцева дія створити_рідну_дію_бібліотеки_мавка_завантажити_РМв1(обчислювач: адреса<Обчислювач>, предмет_модуля: адреса<Предмет>): предмет_або_стан_падіння;

    дія створити_модуль_мавка(обчислювач: адреса<Обчислювач>): предмет_або_стан_падіння {
      змінна результат = створити_модуль(обчислювач, назва(обчислювач, "мавка"), 0, пусто);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }

      змінна предмет_модуля_мавка = результат;

      змінна предмет_тексту_версія = створити_текст_без_копіювання(обчислювач, отримати_версію_мавки_як_т8(обчислювач));
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_тексту_версія;
      }
      результат = предмет_змінити_властивість(обчислювач, предмет_модуля_мавка, назва(обчислювач, "версія"), предмет_тексту_версія, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }

      результат = створити_рідну_дію_бібліотеки_мавка_завантажити_РМв1(обчислювач, предмет_модуля_мавка);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }
      результат = предмет_змінити_властивість(обчислювач, предмет_модуля_мавка, назва(обчислювач, "завантажити_РМв1"), результат, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }

      вернути предмет_модуля_мавка;
    }

    місцева дія взяти_модуль_біб_мавка(обчислювач: адреса<Обчислювач>): предмет_або_стан_падіння {
      змінна М = обчислювач;
      змінна результат = створити_текст(обчислювач, "мавка:бібліотека/мавка/мавка.ц");
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }
      змінна предмет_шляху = результат;
      змінна взятий_предмет: адреса<МаМа::Предмет> = пусто;
      якщо МаМа::отримати_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, взятий_предмет::адреса) {
        вернути взятий_предмет як адреса<Предмет>;
      }
      результат = створити_модуль_мавка(обчислювач);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }
      МаМа::покласти_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, результат як адреса<МаМа::Предмет>);
      вернути результат;
    }
  }
}