взяти визначення МаМа;
взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    зовнішня дія створити_рідну_дію_бібліотеки_мавка_завантажити_РМв1(обчислювач: адреса<Обчислювач>, предмет_модуля: адреса<Предмет>): Результат;

    дія створити_модуль_мавка(обчислювач: адреса<Обчислювач>): Результат {
      змінна ціль результат = створити_модуль(обчислювач, назва(обчислювач, "мавка"), 0, пусто);
      якщо результат.стан_падіння == позитивне(так) {
        вернути результат;
      }
      ціль предмет_модуля_мавка = результат.значення;
      результат = створити_рідну_дію_бібліотеки_мавка_завантажити_РМв1(обчислювач, предмет_модуля_мавка);
      якщо результат.стан_падіння == позитивне(так) {
        вернути результат;
      }
      результат = змінити_властивість(обчислювач, предмет_модуля_мавка, назва(обчислювач, "завантажити_РМв1"), результат.значення, Місцезнаходження { пусто, 0 });
      якщо результат.стан_падіння == позитивне(так) {
        вернути результат;
      }
      вернути успіх(обчислювач, предмет_модуля_мавка);
    }

    зовнішня дія взяти_модуль_біб_мавка(обчислювач: адреса<Обчислювач>): Результат {
      ціль М = обчислювач;
      змінна ціль результат = створити_текст(обчислювач, "мавка:бібліотека/мавка/мавка.ц");
      якщо результат.стан_падіння == позитивне(так) {
        вернути результат;
      }
      ціль предмет_шляху = результат.значення;
      змінна ціль взятий_предмет: адреса<МаМа::Предмет> = пусто;
      якщо МаМа::отримати_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, взятий_предмет::адреса) {
        вернути успіх(обчислювач, взятий_предмет як адреса<Предмет>);
      }
      результат = створити_модуль_мавка(обчислювач);
      якщо результат.стан_падіння == позитивне(так) {
        вернути результат;
      }
      МаМа::покласти_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, результат.значення як адреса<МаМа::Предмет>);
      вернути результат;
    }
  }
}