    дія рідна_дія_модуля_математики_степінь(обчислювач: адреса<Обчислювач>, предмет_дії: адреса<МаМа::ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: МаМа::Місцезнаходження): предмет_або_стан_падіння {
      змінна знайдено_аргумент_а = ні;
      змінна аргумент_а: адреса<Предмет> = пусто;
      змінна знайдено_аргумент_б = ні;
      змінна аргумент_б: адреса<Предмет> = пусто;
      змінна М = обчислювач;
      якщо іменовані_аргументи != пусто {
        знайдено_аргумент_а = МаМа::знайти_іменований_аргумент(обчислювач, іменовані_аргументи, МаМа::назва(М, т8"а"), аргумент_а::адреса);
        знайдено_аргумент_б = МаМа::знайти_іменований_аргумент(обчислювач, іменовані_аргументи, МаМа::назва(М, т8"б"), аргумент_б::адреса);
      }
      якщо аргументи != пусто {
        якщо знайдено_аргумент_а == ні {
          якщо кількість_аргументів > 0 {
            знайдено_аргумент_а = так;
            аргумент_а = аргументи[0];
          }
        }
        якщо знайдено_аргумент_б == ні {
          якщо кількість_аргументів > 1 {
            знайдено_аргумент_б = так;
            аргумент_б = аргументи[1];
          }
        }
      }
      якщо знайдено_аргумент_а == ні {
        вернути падіння(обчислювач, "Недостатньо аргументів");
      }
      якщо знайдено_аргумент_б == ні {
        вернути падіння(обчислювач, "Недостатньо аргументів");
      }
      якщо МаМа::перевірити_чи_предмет_є_числом(обчислювач, аргумент_а) == ні {
        вернути падіння(обчислювач, "Очікується числове значення");
      }
      якщо МаМа::перевірити_чи_предмет_є_числом(обчислювач, аргумент_б) == ні {
        вернути падіння(обчислювач, "Очікується числове значення");
      }
      змінна значення_числа_а = отримати_значення_числа(обчислювач, аргумент_а як адреса<МаМа::ПредметЧисла>);
      змінна значення_числа_б = отримати_значення_числа(обчислювач, аргумент_б як адреса<МаМа::ПредметЧисла>);
      вернути МаМа::створити_предмет_числа(обчислювач, мавка_система_степінь_д64(значення_числа_а, значення_числа_б)) як адреса<Предмет>;
    }

    дія рідна_дія_модуля_математики_стеля(обчислювач: адреса<Обчислювач>, предмет_дії: адреса<МаМа::ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: МаМа::Місцезнаходження): предмет_або_стан_падіння {
