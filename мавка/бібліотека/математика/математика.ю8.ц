        вернути предмет_дії_степінь;
      }
      мавка__притримати(обчислювач, притримувач, предмет_дії_степінь);
      результат_зміни_властивості = МаМа::предмет_змінити_властивість(М, предмет_модуля, МаМа::назва(М, "степінь"), предмет_дії_степінь, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_дії_стеля = МаМа::створити_предмет_рідної_дії(обчислювач, МаМа::назва(М, т8"стеля"), 0, пусто, пусто, рідна_дія_модуля_математики_стеля, пусто, пусто, пусто) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_дії_стеля;
      }
      мавка__притримати(обчислювач, притримувач, предмет_дії_стеля);
      результат_зміни_властивості = МаМа::предмет_змінити_властивість(М, предмет_модуля, МаМа::назва(М, "стеля"), предмет_дії_стеля, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_дії_підлога = МаМа::створити_предмет_рідної_дії(обчислювач, МаМа::назва(М, т8"підлога"), 0, пусто, пусто, рідна_дія_модуля_математики_підлога, пусто, пусто, пусто) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_дії_підлога;
      }
      мавка__притримати(обчислювач, притримувач, предмет_дії_підлога);
      результат_зміни_властивості = МаМа::предмет_змінити_властивість(М, предмет_модуля, МаМа::назва(М, "підлога"), предмет_дії_підлога, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_дії_округлити = МаМа::створити_предмет_рідної_дії(обчислювач, МаМа::назва(М, т8"округлити"), 0, пусто, пусто, рідна_дія_модуля_математики_округлити, пусто, пусто, пусто) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_дії_округлити;
      }
      мавка__притримати(обчислювач, притримувач, предмет_дії_округлити);
      результат_зміни_властивості = МаМа::предмет_змінити_властивість(М, предмет_модуля, МаМа::назва(М, "округлити"), предмет_дії_округлити, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_дії_найбільше = МаМа::створити_предмет_рідної_дії(обчислювач, МаМа::назва(М, т8"найбільше"), 0, пусто, пусто, рідна_дія_модуля_математики_найбільше, пусто, пусто, пусто) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_дії_найбільше;
      }
      мавка__притримати(обчислювач, притримувач, предмет_дії_найбільше);
      результат_зміни_властивості = МаМа::предмет_змінити_властивість(М, предмет_модуля, МаМа::назва(М, "найбільше"), предмет_дії_найбільше, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_дії_найменше = МаМа::створити_предмет_рідної_дії(обчислювач, МаМа::назва(М, т8"найменше"), 0, пусто, пусто, рідна_дія_модуля_математики_найменше, пусто, пусто, пусто) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_дії_найменше;
      }
      мавка__притримати(обчислювач, притримувач, предмет_дії_найменше);
      результат_зміни_властивості = МаМа::предмет_змінити_властивість(М, предмет_модуля, МаМа::назва(М, "найменше"), предмет_дії_найменше, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }

      мавка__забрати_притримувач(обчислювач);
      вернути предмет_модуля;
    }

    зовнішня дія взяти_модуль_біб_математика(обчислювач: адреса<Обчислювач>): предмет_або_стан_падіння {
      змінна М = обчислювач;
      змінна результат = МаМа::створити_предмет_тексту(обчислювач, "мавка:бібліотека/математика/математика.ц") як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат як адреса<Предмет>;
      }
      змінна предмет_шляху = результат;
      змінна взятий_предмет: адреса<МаМа::Предмет> = пусто;
      якщо МаМа::отримати_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, взятий_предмет::адреса) {
        вернути взятий_предмет як адреса<Предмет>;
      }
      результат = створити_модуль_математика(обчислювач);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }
      МаМа::покласти_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, результат як адреса<МаМа::Предмет>);
      вернути результат;
    }
  }
}