      вернути МаМа::створити_предмет_числа(обчислювач, мавка_система_округлити_д64(значення_числа)) як адреса<Предмет>;
    }

    дія рідна_дія_модуля_математики_найбільше(обчислювач: адреса<Обчислювач>, предмет_дії: адреса<МаМа::ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: МаМа::Місцезнаходження): предмет_або_стан_падіння {
      змінна знайдено_аргумент_значення = ні;
      змінна аргумент_значення: адреса<Предмет> = пусто;
      змінна М = обчислювач;
      якщо іменовані_аргументи != пусто {
        знайдено_аргумент_значення = МаМа::знайти_іменований_аргумент(обчислювач, іменовані_аргументи, МаМа::назва(М, "значення"), аргумент_значення::адреса);
      }
      якщо аргументи != пусто {
        якщо знайдено_аргумент_значення == ні {
          якщо кількість_аргументів > 0 {
            знайдено_аргумент_значення = так;
            аргумент_значення = аргументи[0];
          }
        }
      }
      якщо знайдено_аргумент_значення == ні {
        вернути падіння(обчислювач, "Недостатньо аргументів");
      }
      якщо МаМа::перевірити_чи_предмет_є_списком(обчислювач, аргумент_значення) == ні {
        вернути падіння(обчислювач, "Очікується спискове значення");
      }
      змінна предмет_списку = аргумент_значення як адреса<МаМа::ПредметСписку>;
      змінна результат: адреса<Предмет> = пусто;
      якщо предмет_списку.елементи.розмір > 0 {
        результат = предмет_списку.елементи.дані[0] як адреса<Предмет>;
      }
      змінна п: позитивне = 1;
      поки п < предмет_списку.елементи.розмір {
        змінна значення = предмет_списку.елементи.дані[п] як адреса<Предмет>;
        змінна результат_більше = МаМа::предмет_більше(обчислювач, значення, результат, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
        якщо перевірити_чи_стан_падіння(обчислювач) {
          вернути результат_більше;
        }
        якщо МаМа::перевірити_чи_предмет_виражається_логічним_так(обчислювач, результат_більше) == так {
          результат = значення;
        }
        п += 1;
      }
      вернути результат;
    }

    дія рідна_дія_модуля_математики_найменше(обчислювач: адреса<Обчислювач>, предмет_дії: адреса<МаМа::ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>, місцезнаходження: МаМа::Місцезнаходження): предмет_або_стан_падіння {
      змінна знайдено_аргумент_значення = ні;
      змінна аргумент_значення: адреса<Предмет> = пусто;
      змінна М = обчислювач;
      якщо іменовані_аргументи != пусто {
        знайдено_аргумент_значення = МаМа::знайти_іменований_аргумент(обчислювач, іменовані_аргументи, МаМа::назва(М, "значення"), аргумент_значення::адреса);
      }
      якщо аргументи != пусто {
        якщо знайдено_аргумент_значення == ні {
          якщо кількість_аргументів > 0 {
            знайдено_аргумент_значення = так;
            аргумент_значення = аргументи[0];
          }
        }
      }
      якщо знайдено_аргумент_значення == ні {
        вернути падіння(обчислювач, "Недостатньо аргументів");
      }
      якщо МаМа::перевірити_чи_предмет_є_списком(обчислювач, аргумент_значення) == ні {
        вернути падіння(обчислювач, "Очікується спискове значення");
      }
      змінна предмет_списку = аргумент_значення як адреса<МаМа::ПредметСписку>;
      змінна результат: адреса<Предмет> = пусто;
      якщо предмет_списку.елементи.розмір > 0 {
        результат = предмет_списку.елементи.дані[0] як адреса<Предмет>;
      }
      змінна п: позитивне = 1;
      поки п < предмет_списку.елементи.розмір {
        змінна значення = предмет_списку.елементи.дані[п] як адреса<Предмет>;
        змінна результат_менше = МаМа::предмет_менше(обчислювач, значення, результат, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
        якщо перевірити_чи_стан_падіння(обчислювач) {
          вернути результат_менше;
        }
        якщо МаМа::перевірити_чи_предмет_виражається_логічним_так(обчислювач, результат_менше) == так {
          результат = значення;
        }
        п += 1;
      }
      вернути результат;
    }

    дія створити_модуль_математика(обчислювач: адреса<Обчислювач>): предмет_або_стан_падіння {
      змінна притримувач = мавка__покласти_притримувач(обчислювач);

      змінна М = обчислювач;

      змінна предмет_модуля = МаМа::створити_предмет_модуля(обчислювач, МаМа::назва(М, "математика")) як адреса<Предмет>;
      мавка__притримати(обчислювач, притримувач, предмет_модуля);

      змінна предмет_числа_ЧИСЛО_Е = МаМа::створити_предмет_числа(обчислювач, ЧИСЛО_Е) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_Е;
      }
      мавка__притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_Е);
      змінна результат_зміни_властивості = МаМа::предмет_змінити_властивість(М, предмет_модуля, МаМа::назва(М, "Е"), предмет_числа_ЧИСЛО_Е, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_числа_ЧИСЛО_ЛОГ2Е = МаМа::створити_предмет_числа(обчислювач, ЧИСЛО_ЛОГ2Е) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ЛОГ2Е;
      }
      мавка__притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ЛОГ2Е);
      результат_зміни_властивості = МаМа::предмет_змінити_властивість(М, предмет_модуля, МаМа::назва(М, "ЛОГ2Е"), предмет_числа_ЧИСЛО_ЛОГ2Е, МаМа::Місцезнаходження { пусто як адреса<МаМа::ПредметТексту>, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_числа_ЧИСЛО_ЛОГ10Е = МаМа::створити_предмет_числа(обчислювач, ЧИСЛО_ЛОГ10Е) як адреса<Предмет>;
      якщо перевірити_чи_стан_падіння(обчислювач) {
