взяти визначення МаМа;
взяти визначення мавка/бібліотека;

місцева дія мавка_система_синус_д64(значення: д64): д64;
місцева дія мавка_система_косинус_д64(значення: д64): д64;
місцева дія мавка_система_тангенс_д64(значення: д64): д64;
місцева дія мавка_система_арксинус_д64(значення: д64): д64;
місцева дія мавка_система_арккосинус_д64(значення: д64): д64;
місцева дія мавка_система_арктангенс_д64(значення: д64): д64;
місцева дія мавка_система_арктангенс2_д64(а: д64, б: д64): д64;
місцева дія мавка_система_абсолютне_д64(значення: д64): д64;
місцева дія мавка_система_експонента_д64(значення: д64): д64;
місцева дія мавка_система_корінь_д64(значення: д64): д64;
місцева дія мавка_система_степінь_д64(а: д64, б: д64): д64;
місцева дія мавка_система_стеля_д64(значення: д64): д64;
місцева дія мавка_система_підлога_д64(значення: д64): д64;
місцева дія мавка_система_округлити_д64(значення: д64): д64;
місцева дія мавка_система_остача_від_ділення_д64(а: д64, б: д64): д64;

секція мавка {
  секція бібліотека {
    дія рідна_дія_модуля_математики_синус(обчислювач: адреса<Обчислювач>, предмет_дії: адреса<Предмет>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>): предмет_або_стан_падіння {
      змінна знайдено_аргумент_значення = ні;
      змінна аргумент_значення: адреса<Предмет> = пусто;
      якщо іменовані_аргументи != пусто {
        знайдено_аргумент_значення = знайти_іменований_аргумент(обчислювач, іменовані_аргументи, назва(обчислювач, "значення"), аргумент_значення::адреса);
      }
      якщо аргументи != пусто {
        якщо знайдено_аргумент_значення == ні {
          якщо кількість_аргументів > 0 {
            знайдено_аргумент_значення = так;
            аргумент_значення = аргументи[0];
          }
        }
      }
      якщо знайдено_аргумент_значення == ні {
        вернути падіння(обчислювач, "Недостатньо аргументів");
      }
      якщо перевірити_чи_предмет_є_числом(обчислювач, аргумент_значення) == ні {
        вернути падіння(обчислювач, "Очікується числове значення");
      }
      змінна значення_числа = отримати_значення_числа(обчислювач, аргумент_значення);
      вернути створити_число(обчислювач, мавка_система_синус_д64(значення_числа));
    }

    дія рідна_дія_модуля_математики_косинус(обчислювач: адреса<Обчислювач>, предмет_дії: адреса<Предмет>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>): предмет_або_стан_падіння {
      змінна знайдено_аргумент_значення = ні;
      змінна аргумент_значення: адреса<Предмет> = пусто;
      якщо іменовані_аргументи != пусто {
        знайдено_аргумент_значення = знайти_іменований_аргумент(обчислювач, іменовані_аргументи, назва(обчислювач, "значення"), аргумент_значення::адреса);
      }
      якщо аргументи != пусто {
        якщо знайдено_аргумент_значення == ні {
          якщо кількість_аргументів > 0 {
            знайдено_аргумент_значення = так;
            аргумент_значення = аргументи[0];
          }
        }
      }
      якщо знайдено_аргумент_значення == ні {
        вернути падіння(обчислювач, "Недостатньо аргументів");
      }
      якщо перевірити_чи_предмет_є_числом(обчислювач, аргумент_значення) == ні {
        вернути падіння(обчислювач, "Очікується числове значення");
      }
      змінна значення_числа = отримати_значення_числа(обчислювач, аргумент_значення);
      вернути створити_число(обчислювач, мавка_система_косинус_д64(значення_числа));
    }

    дія рідна_дія_модуля_математики_тангенс(обчислювач: адреса<Обчислювач>, предмет_дії: адреса<Предмет>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>): предмет_або_стан_падіння {
