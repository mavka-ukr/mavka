взяти визначення МаМа;
взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    дія створити_модуль_М(обчислювач: адреса<Обчислювач>): Результат {
      вернути падіння(обчислювач, "Модуль біб математика не втілено");
    }

    зовнішня дія взяти_модуль_біб_математика(обчислювач: адреса<Обчислювач>): Результат {
      ціль М = обчислювач;
      змінна ціль результат = створити_текст(обчислювач, "мавка:бібліотека/математика/математика.ц");
      якщо результат.стан_падіння == позитивне(так) {
        вернути результат;
      }
      ціль предмет_шляху = результат.значення;
      змінна ціль взятий_предмет: адреса<МаМа::Предмет> = пусто;
      якщо МаМа::отримати_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, взятий_предмет::адреса) {
        вернути успіх(обчислювач, взятий_предмет як адреса<Предмет>);
      }
      результат = створити_модуль_М(обчислювач);
      якщо результат.стан_падіння == позитивне(так) {
        вернути результат;
      }
      МаМа::покласти_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, результат.значення як адреса<МаМа::Предмет>);
      вернути результат;
    }
  }
}