взяти визначення МаМа;
взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    дія створити_модуль_М(обчислювач: адреса<Обчислювач>): предмет_або_стан_падіння {
      ціль притримувач = покласти_притримувач(обчислювач);

      ціль предмет_модуля = створити_модуль(обчислювач, назва(обчислювач, "математика"), 0, пусто);
      притримати(обчислювач, притримувач, предмет_модуля);

      ціль предмет_числа_ЧИСЛО_Е = створити_число(обчислювач, ЧИСЛО_Е);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_Е;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_Е);
      змінна ціль результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "Е"), предмет_числа_ЧИСЛО_Е, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      ціль предмет_числа_ЧИСЛО_ЛОГ2Е = створити_число(обчислювач, ЧИСЛО_ЛОГ2Е);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ЛОГ2Е;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ЛОГ2Е);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "ЛОГ2Е"), предмет_числа_ЧИСЛО_ЛОГ2Е, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      ціль предмет_числа_ЧИСЛО_ЛОГ10Е = створити_число(обчислювач, ЧИСЛО_ЛОГ10Е);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ЛОГ10Е;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ЛОГ10Е);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "ЛОГ10Е"), предмет_числа_ЧИСЛО_ЛОГ10Е, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      ціль предмет_числа_ЧИСЛО_ЛОГ2 = створити_число(обчислювач, ЧИСЛО_ЛОГ2);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ЛОГ2;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ЛОГ2);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "ЛОГ2"), предмет_числа_ЧИСЛО_ЛОГ2, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      ціль предмет_числа_ЧИСЛО_ЛОГ10 = створити_число(обчислювач, ЧИСЛО_ЛОГ10);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ЛОГ10;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ЛОГ10);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "ЛОГ10"), предмет_числа_ЧИСЛО_ЛОГ10, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      ціль предмет_числа_ЧИСЛО_ПІ = створити_число(обчислювач, ЧИСЛО_ПІ);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ПІ;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ПІ);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "ПІ"), предмет_числа_ЧИСЛО_ПІ, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      ціль предмет_числа_ЧИСЛО_КОРІНЬ2 = створити_число(обчислювач, ЧИСЛО_КОРІНЬ2);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_КОРІНЬ2;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_КОРІНЬ2);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "КОРІНЬ2"), предмет_числа_ЧИСЛО_КОРІНЬ2, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      ціль предмет_числа_ЧИСЛО_КОРІНЬ1_2 = створити_число(обчислювач, ЧИСЛО_КОРІНЬ1_2);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_КОРІНЬ1_2;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_КОРІНЬ1_2);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "КОРІНЬ1_2"), предмет_числа_ЧИСЛО_КОРІНЬ1_2, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }

      забрати_притримувач(обчислювач);
      вернути предмет_модуля;
    }

    зовнішня дія взяти_модуль_біб_математика(обчислювач: адреса<Обчислювач>): предмет_або_стан_падіння {
      ціль М = обчислювач;
      змінна ціль результат = створити_текст(обчислювач, "мавка:бібліотека/математика/математика.ц");
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }
      ціль предмет_шляху = результат;
      змінна ціль взятий_предмет: адреса<МаМа::Предмет> = пусто;
      якщо МаМа::отримати_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, взятий_предмет::адреса) {
        вернути взятий_предмет як адреса<Предмет>;
      }
      результат = створити_модуль_М(обчислювач);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат;
      }
      МаМа::покласти_взятий_предмет(М, предмет_шляху як адреса<МаМа::Предмет>, результат як адреса<МаМа::Предмет>);
      вернути результат;
    }
  }
}