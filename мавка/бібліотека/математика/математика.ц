    дія рідна_дія_модуля_математики_найбільше(обчислювач: адреса<Обчислювач>, предмет_дії: адреса<Предмет>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>): предмет_або_стан_падіння {
      змінна знайдено_аргумент_значення = ні;
      змінна аргумент_значення: адреса<Предмет> = пусто;
      якщо іменовані_аргументи != пусто {
        знайдено_аргумент_значення = знайти_іменований_аргумент(обчислювач, іменовані_аргументи, назва(обчислювач, "значення"), аргумент_значення::адреса);
      }
      якщо аргументи != пусто {
        якщо знайдено_аргумент_значення == ні {
          якщо кількість_аргументів > 0 {
            знайдено_аргумент_значення = так;
            аргумент_значення = аргументи[0];
          }
        }
      }
      якщо знайдено_аргумент_значення == ні {
        вернути падіння(обчислювач, "Недостатньо аргументів");
      }
      якщо перевірити_чи_предмет_є_списком(обчислювач, аргумент_значення) == ні {
        вернути падіння(обчислювач, "Очікується спискове значення");
      }
      змінна предмет_списку = аргумент_значення як адреса<МаМа::ПредметСписку>;
      змінна результат: адреса<Предмет> = пусто;
      якщо предмет_списку.елементи.розмір > 0 {
        результат = предмет_списку.елементи.дані[0] як адреса<Предмет>;
      }
      змінна п: позитивне = 1;
      поки п < предмет_списку.елементи.розмір {
        змінна значення = предмет_списку.елементи.дані[п] як адреса<Предмет>;
        змінна результат_більше = предмет_більше(обчислювач, значення, результат, Місцезнаходження { пусто, 0 });
        якщо перевірити_чи_стан_падіння(обчислювач) {
          вернути результат_більше;
        }
        якщо перевірити_чи_предмет_виражається_логічним_так(обчислювач, результат_більше) == так {
          результат = значення;
        }
        п += 1;
      }
      вернути результат;
    }

    дія рідна_дія_модуля_математики_найменше(обчислювач: адреса<Обчислювач>, предмет_дії: адреса<Предмет>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>): предмет_або_стан_падіння {
      змінна знайдено_аргумент_значення = ні;
      змінна аргумент_значення: адреса<Предмет> = пусто;
      якщо іменовані_аргументи != пусто {
        знайдено_аргумент_значення = знайти_іменований_аргумент(обчислювач, іменовані_аргументи, назва(обчислювач, "значення"), аргумент_значення::адреса);
      }
      якщо аргументи != пусто {
        якщо знайдено_аргумент_значення == ні {
          якщо кількість_аргументів > 0 {
            знайдено_аргумент_значення = так;
            аргумент_значення = аргументи[0];
          }
        }
      }
      якщо знайдено_аргумент_значення == ні {
        вернути падіння(обчислювач, "Недостатньо аргументів");
      }
      якщо перевірити_чи_предмет_є_списком(обчислювач, аргумент_значення) == ні {
        вернути падіння(обчислювач, "Очікується спискове значення");
      }
      змінна предмет_списку = аргумент_значення як адреса<МаМа::ПредметСписку>;
      змінна результат: адреса<Предмет> = пусто;
      якщо предмет_списку.елементи.розмір > 0 {
        результат = предмет_списку.елементи.дані[0] як адреса<Предмет>;
      }
      змінна п: позитивне = 1;
      поки п < предмет_списку.елементи.розмір {
        змінна значення = предмет_списку.елементи.дані[п] як адреса<Предмет>;
        змінна результат_менше = предмет_менше(обчислювач, значення, результат, Місцезнаходження { пусто, 0 });
        якщо перевірити_чи_стан_падіння(обчислювач) {
          вернути результат_менше;
        }
        якщо перевірити_чи_предмет_виражається_логічним_так(обчислювач, результат_менше) == так {
          результат = значення;
        }
        п += 1;
      }
      вернути результат;
    }

    дія створити_модуль_математика(обчислювач: адреса<Обчислювач>): предмет_або_стан_падіння {
      змінна притримувач = покласти_притримувач(обчислювач);

      змінна предмет_модуля = створити_модуль(обчислювач, назва(обчислювач, "математика"), 0, пусто);
      притримати(обчислювач, притримувач, предмет_модуля);

      змінна предмет_числа_ЧИСЛО_Е = створити_число(обчислювач, ЧИСЛО_Е);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_Е;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_Е);
      змінна результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "Е"), предмет_числа_ЧИСЛО_Е, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_числа_ЧИСЛО_ЛОГ2Е = створити_число(обчислювач, ЧИСЛО_ЛОГ2Е);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ЛОГ2Е;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ЛОГ2Е);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "ЛОГ2Е"), предмет_числа_ЧИСЛО_ЛОГ2Е, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_числа_ЧИСЛО_ЛОГ10Е = створити_число(обчислювач, ЧИСЛО_ЛОГ10Е);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ЛОГ10Е;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ЛОГ10Е);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "ЛОГ10Е"), предмет_числа_ЧИСЛО_ЛОГ10Е, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_числа_ЧИСЛО_ЛОГ2 = створити_число(обчислювач, ЧИСЛО_ЛОГ2);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ЛОГ2;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ЛОГ2);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "ЛОГ2"), предмет_числа_ЧИСЛО_ЛОГ2, Місцезнаходження { пусто, 0 });
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути результат_зміни_властивості;
      }
      змінна предмет_числа_ЧИСЛО_ЛОГ10 = створити_число(обчислювач, ЧИСЛО_ЛОГ10);
      якщо перевірити_чи_стан_падіння(обчислювач) {
        вернути предмет_числа_ЧИСЛО_ЛОГ10;
      }
      притримати(обчислювач, притримувач, предмет_числа_ЧИСЛО_ЛОГ10);
      результат_зміни_властивості = предмет_змінити_властивість(обчислювач, предмет_модуля, назва(обчислювач, "ЛОГ10"), предмет_числа_ЧИСЛО_ЛОГ10, Місцезнаходження { пусто, 0 });
