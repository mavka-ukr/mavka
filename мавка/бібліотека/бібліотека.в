взяти визначення мавка;

секція мавка {
  секція бібліотека {
    зовнішня дія взяти_модуль_біб_математика(обчислювач: адреса<Обчислювач>): Результат;
    зовнішня дія взяти_модуль_біб_мавка(обчислювач: адреса<Обчислювач>): Результат;

    зовнішня дія обробник_взяття_біб(обчислювач: адреса<Обчислювач>, розмір_шляху: позитивне, шлях: памʼять<адреса<Предмет>>): Результат;
  }
}