взяти визначення мавка/мавка_втілення;

секція мавка {
  секція бібліотека {
    синонім ЧИСЛО_Е = 2.71828182845904523536; // е
    синонім ЧИСЛО_ЛОГ2Е = 1.44269504088896340736; // лог2(е)
    синонім ЧИСЛО_ЛОГ10Е = 0.434294481903251827651; // лог10(е)
    синонім ЧИСЛО_ЛОГ2 = 0.693147180559945309417; // ln(2)
    синонім ЧИСЛО_ЛОГ10 = 2.30258509299404568402; // ln(10)
    синонім ЧИСЛО_ПІ = 3.14159265358979323846; // π
    синонім ЧИСЛО_КОРІНЬ2 = 1.41421356237309504880; // корінь(2)
    синонім ЧИСЛО_КОРІНЬ1_2 = 0.707106781186547524401; // корінь(1/2)

    місцева дія заповнити_модуль_М(обчислювач: адреса<Обчислювач>, предмет_модуля: адреса<Предмет>);
    місцева дія заповнити_модуль_мавка(обчислювач: адреса<Обчислювач>, предмет_модуля: адреса<Предмет>);
    місцева дія заповнити_модуль_МаМа(обчислювач: адреса<Обчислювач>, предмет_модуля: адреса<Предмет>);
    місцева дія заповнити_модуль_читати_юнікод(обчислювач: адреса<Обчислювач>, предмет_модуля: адреса<Предмет>);
  }
}