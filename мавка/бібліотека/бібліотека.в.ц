взяти визначення мавка;

секція мавка {
  секція бібліотека {
    місцева дія взяти_модуль_біб_математика(М: адреса<Машина>) -> ПредметАбоСтанПадіння<Предмет>;
    місцева дія взяти_модуль_біб_мавка(М: адреса<Машина>) -> ПредметАбоСтанПадіння<Предмет>;
    місцева дія взяти_модуль_біб_читати(М: адреса<Машина>) -> ПредметАбоСтанПадіння<Предмет>;
    місцева дія взяти_модуль_біб_інтернет(М: адреса<Машина>) -> ПредметАбоСтанПадіння<Предмет>;
    місцева дія взяти_модуль_біб_планувальник(М: адреса<Машина>) -> ПредметАбоСтанПадіння<Предмет>;

    місцева дія обробник_взяття_біб(М: адреса<Машина>, розмір_шляху: позитивне, шлях: памʼять<адреса<Предмет>>) -> ПредметАбоСтанПадіння<Предмет>;
  }
}