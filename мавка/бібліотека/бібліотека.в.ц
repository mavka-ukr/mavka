взяти визначення мавка;

секція мавка {
  секція бібліотека {
    місцева дія взяти_модуль_біб_математика(М: адреса<Машина>): предмет_або_стан_падіння;
    місцева дія взяти_модуль_біб_мавка(М: адреса<Машина>): предмет_або_стан_падіння;
    місцева дія взяти_модуль_біб_читати(М: адреса<Машина>): предмет_або_стан_падіння;
    місцева дія взяти_модуль_біб_інтернет(М: адреса<Машина>): предмет_або_стан_падіння;
    місцева дія взяти_модуль_біб_планувальник(М: адреса<Машина>): предмет_або_стан_падіння;

    місцева дія обробник_взяття_біб(М: адреса<Машина>, розмір_шляху: позитивне, шлях: памʼять<адреса<Предмет>>): предмет_або_стан_падіння;
  }
}