взяти визначення мавка;
взяти визначення мавка/бібліотека;

секція мавка {
  секція бібліотека {
    зовнішня дія обробник_взяття_біб(обчислювач: адреса<Обчислювач>, розмір_шляху: позитивне, шлях: памʼять<адреса<Предмет>>): предмет_або_стан_падіння {
      якщо розмір_шляху == 1 {
        змінна М = обчислювач;
        якщо МаМа::перевірити_чи_предмети_рівні(обчислювач, шлях[0], МаМа::назва(М, "математика") як адреса<Предмет>) {
          вернути взяти_модуль_біб_математика(обчислювач);
        }
        якщо МаМа::перевірити_чи_предмети_рівні(обчислювач, шлях[0], МаМа::назва(М, "мавка") як адреса<Предмет>) {
          вернути взяти_модуль_біб_мавка(обчислювач);
        }
        якщо МаМа::перевірити_чи_предмети_рівні(обчислювач, шлях[0], МаМа::назва(М, "читати") як адреса<Предмет>) {
          вернути взяти_модуль_біб_читати(обчислювач);
        }
        якщо МаМа::перевірити_чи_предмети_рівні(обчислювач, шлях[0], МаМа::назва(М, "читати_юнікод") як адреса<Предмет>) {
          вернути взяти_модуль_біб_читати_юнікод(обчислювач);
        }
        якщо МаМа::перевірити_чи_предмети_рівні(обчислювач, шлях[0], МаМа::назва(М, "інтернет") як адреса<Предмет>) {
          вернути взяти_модуль_біб_інтернет(обчислювач);
        }
        якщо МаМа::перевірити_чи_предмети_рівні(обчислювач, шлях[0], МаМа::назва(М, "планувальник") як адреса<Предмет>) {
          вернути взяти_модуль_біб_планувальник(обчислювач);
        }
      }
      вернути падіння(обчислювач, "Неможливо взяти модуль біб");
    }
  }
}