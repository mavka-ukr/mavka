простір ДЧ {
  структура Виділяч;
  структура Ціле;

  структура Виділяч {
    виділити_сиру_памʼять: (виділяч: адреса<Виділяч>, розмір: природне) -> памʼять<п8>;
    перевиділити_сиру_памʼять: (виділяч: адреса<Виділяч>, значення: памʼять<п8>, новий_розмір: природне) -> памʼять<п8>;
    звільнити_сиру_памʼять: (виділяч: адреса<Виділяч>, значення: памʼять<п8>) -> ніщо;
    користувацькі_дані: невідома_адреса;
  }

  структура Ціле {
    розмір: ц32;
    вмісткість: ц32;
    цифри: памʼять<п32>;
  }

  структура Дробове {
    мантиса: Ціле;
    експонента: ц32;
  }

  дія виділити<Т>(виділяч: адреса<Виділяч>) -> адреса<Т> {
    вернути виділяч.виділити_сиру_памʼять(виділяч, Т.розмір) як адреса<Т>;
  }

  дія перевиділити<Т>(виділяч: адреса<Виділяч>, значення: адреса<Т>, кількість: природне) -> памʼять<Т> {
    вернути виділяч.перевиділити_сиру_памʼять(виділяч, значення як невідома_адреса, кількість * Т.розмір) як памʼять<Т>;
  }

  дія звільнити(виділяч: адреса<Виділяч>, значення: невідома_адреса) {
    виділяч.звільнити_сиру_памʼять(виділяч, значення як памʼять<п8>);
  }

  дія виділити_памʼять<Т>(виділяч: адреса<Виділяч>, кількість: природне) -> памʼять<Т> {
    вернути виділяч.виділити_сиру_памʼять(виділяч, кількість * Т.розмір) як памʼять<Т>;
  }

  дія перевиділити_памʼять<Т>(виділяч: адреса<Виділяч>, значення: памʼять<Т>, кількість: природне) -> памʼять<Т> {
    вернути виділяч.перевиділити_сиру_памʼять(виділяч, значення як памʼять<п8>, кількість * Т.розмір) як памʼять<Т>;
  }

  дія звільнити_памʼять(виділяч: адреса<Виділяч>, значення: невідома_памʼять) {
    виділяч.звільнити_сиру_памʼять(виділяч, значення як памʼять<п8>);
  }

  місцева дія ініціалізувати_ціле_з_ц64(виділяч: адреса<Виділяч>, значення: ц64, вихід: адреса<Ціле>) -> логічне;
  місцева дія ініціалізувати_ціле_з_п64(виділяч: адреса<Виділяч>, значення: п64, вихід: адреса<Ціле>) -> логічне;
  місцева дія ініціалізувати_ціле_з_кд(виділяч: адреса<Виділяч>, значення: кд, вихід: адреса<Ціле>) -> логічне;
  місцева дія ініціалізувати_ціле_нуль(виділяч: адреса<Виділяч>, вихід: адреса<Ціле>) -> логічне;
  місцева дія ініціалізувати_ціле_один(виділяч: адреса<Виділяч>, вихід: адреса<Ціле>) -> логічне;
  місцева дія звільнити_ціле(виділяч: адреса<Виділяч>, значення: Ціле);
  місцева дія копіювати_ціле(виділяч: адреса<Виділяч>, значення: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія перетворити_ціле_в_ц64(виділяч: адреса<Виділяч>, значення: Ціле, вихід: адреса<ц64>) -> логічне;
  місцева дія перетворити_ціле_в_кд(виділяч: адреса<Виділяч>, значення: Ціле, вихід: адреса<кд>) -> логічне;
  місцева дія порівняти_цілі(а: Ціле, б: Ціле) -> ц8;
  місцева дія перевірити_чи_ціле_нуль(значення: Ціле) -> логічне;
  місцева дія перевірити_чи_ціле_відʼємне(значення: Ціле) -> логічне;
  місцева дія негація_цілого(виділяч: адреса<Виділяч>, значення: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія додати_цілі(виділяч: адреса<Виділяч>, а: Ціле, б: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія відняти_цілі(виділяч: адреса<Виділяч>, а: Ціле, б: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія помножити_цілі(виділяч: адреса<Виділяч>, а: Ціле, б: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія поділити_цілі(виділяч: адреса<Виділяч>, а: Ціле, б: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія залишок_ділення_цілих(виділяч: адреса<Виділяч>, а: Ціле, б: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія абсолютне_ціле(виділяч: адреса<Виділяч>, значення: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія степінь_цілого(виділяч: адреса<Виділяч>, основа: Ціле, показник: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія найбільший_спільний_дільник_цілих(виділяч: адреса<Виділяч>, а: Ціле, б: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія зсунути_ціле_вправо(виділяч: адреса<Виділяч>, значення: Ціле, кількість: ц32, вихід: адреса<Ціле>) -> логічне;
  місцева дія зсунути_ціле_вліво(виділяч: адреса<Виділяч>, значення: Ціле, кількість: ц32, вихід: адреса<Ціле>) -> логічне;
  місцева дія двійкове_заперечення_цілого(виділяч: адреса<Виділяч>, значення: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія двійкове_і_цілих(виділяч: адреса<Виділяч>, а: Ціле, б: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія двійкове_або_цілих(виділяч: адреса<Виділяч>, а: Ціле, б: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія двійкове_вабо_цілих(виділяч: адреса<Виділяч>, а: Ціле, б: Ціле, вихід: адреса<Ціле>) -> логічне;
  місцева дія змінити_знак_цілого(значення: адреса<Ціле>);

  місцева дія ініціалізувати_дробове_з_д64(виділяч: адреса<Виділяч>, значення: д64, вихід: адреса<Дробове>) -> логічне;
  місцева дія ініціалізувати_дробове_з_ц64(виділяч: адреса<Виділяч>, значення: ц64, вихід: адреса<Дробове>) -> логічне;
  місцева дія ініціалізувати_дробове_з_кд(виділяч: адреса<Виділяч>, значення: кд, вихід: адреса<Дробове>) -> логічне;
  місцева дія ініціалізувати_дробове_нуль(виділяч: адреса<Виділяч>, вихід: адреса<Дробове>) -> логічне;
  місцева дія ініціалізувати_дробове_один(виділяч: адреса<Виділяч>, вихід: адреса<Дробове>) -> логічне;
  місцева дія звільнити_дробове(виділяч: адреса<Виділяч>, значення: Дробове);
  місцева дія копіювати_дробове(виділяч: адреса<Виділяч>, значення: Дробове, вихід: адреса<Дробове>) -> логічне;
  місцева дія перетворити_дробове_в_д64(виділяч: адреса<Виділяч>, значення: Дробове, вихід: адреса<д64>) -> логічне;
  місцева дія перетворити_дробове_в_кд(виділяч: адреса<Виділяч>, значення: Дробове, вихід: адреса<кд>) -> логічне;
  місцева дія порівняти_дробові(виділяч: адреса<Виділяч>, а: Дробове, б: Дробове) -> ц8;
  місцева дія перевірити_чи_дробове_нуль(значення: Дробове) -> логічне;
  місцева дія перевірити_чи_дробове_відʼємне(значення: Дробове) -> логічне;
  місцева дія додати_дробові(виділяч: адреса<Виділяч>, а: Дробове, б: Дробове, вихід: адреса<Дробове>) -> логічне;
  місцева дія відняти_дробові(виділяч: адреса<Виділяч>, а: Дробове, б: Дробове, вихід: адреса<Дробове>) -> логічне;
  місцева дія помножити_дробові(виділяч: адреса<Виділяч>, а: Дробове, б: Дробове, точність: ц32, вихід: адреса<Дробове>) -> логічне;
  місцева дія поділити_дробові(виділяч: адреса<Виділяч>, а: Дробове, б: Дробове, точність: ц32, вихід: адреса<Дробове>) -> логічне;
  місцева дія залишок_ділення_дробових(виділяч: адреса<Виділяч>, а: Дробове, б: Дробове, точність: ц32, вихід: адреса<Дробове>) -> логічне;
  місцева дія абсолютне_дробове(виділяч: адреса<Виділяч>, значення: Дробове, вихід: адреса<Дробове>) -> логічне;
  місцева дія степінь_дробового(виділяч: адреса<Виділяч>, основа: Дробове, показник: Дробове, точність: ц32, вихід: адреса<Дробове>) -> логічне;
  місцева дія піднести_дробове_до_цілого_степеня(виділяч: адреса<Виділяч>, основа: Дробове, степінь: ц64, точність: ц32, вихід: адреса<Дробове>) -> логічне;
  місцева дія змінити_знак_дробового(значення: адреса<Дробове>);
  місцева дія негація_дробового(виділяч: адреса<Виділяч>, значення: Дробове, вихід: адреса<Дробове>) -> логічне;
  місцева дія натуральний_логарифм_дробового(виділяч: адреса<Виділяч>, х: Дробове, точність: ц32, вихід: адреса<Дробове>) -> логічне;
  місцева дія експонента_дробового(виділяч: адреса<Виділяч>, х: Дробове, точність: ц32, вихід: адреса<Дробове>) -> логічне;
  місцева дія квадратний_корінь_дробового(виділяч: адреса<Виділяч>, а: Дробове, точність: ц32, вихід: адреса<Дробове>) -> логічне;
}