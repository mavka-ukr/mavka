    позначити_предмет_тексту(М, М.константи_назв._46);
    позначити_предмет_тексту(М, М.константи_назв._47);
    позначити_предмет_тексту(М, М.константи_назв._48);
    позначити_предмет_тексту(М, М.константи_назв._49);
