    позначити_предмет_тексту(М, М.константи_назв._57);
    позначити_предмет_тексту(М, М.константи_назв._58);
    позначити_предмет_тексту(М, М.константи_назв._59);
    позначити_предмет_тексту(М, М.константи_назв._60);
    позначити_предмет_тексту(М, М.константи_назв._61);
    позначити_предмет_тексту(М, М.константи_назв._62);
    позначити_предмет_тексту(М, М.константи_назв._63);
    позначити_предмет_тексту(М, М.константи_назв._модуль);
    позначити_предмет_тексту(М, М.константи_назв._пак);
    позначити_предмет_тексту(М, М.константи_назв._біб);
    позначити_предмет_тексту(М, М.константи_назв._математика);
    позначити_предмет_тексту(М, М.константи_назв._ПІ);
  }

  дія позначити_взяті_значення(М: адреса<Машина>, взяті_значення: ВзятіЗначення) {
    змінна п: природне = 0;

    поки п < взяті_значення.розмір {
      змінна взяте_значення = взяті_значення.дані[п]::адреса;

      якщо взяте_значення.шлях != пусто {
        позначити_предмет_даних(М, взяте_значення.шлях);
      }

      якщо взяте_значення.значення != пусто {
        позначити_дійсне(М, взяте_значення.значення);
      }

      п += 1;
    }
  }

  дія позначити_користувацькі_дані(М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>) {
    якщо користувацькі_дані.притримувач != пусто {
      позначити_притримувач(М, користувацькі_дані.притримувач);
    }
  }

  дія позначити_методи(М: адреса<Машина>, методи: Методи) {
    змінна п: природне = 0;

    поки п < методи.розмір {
      змінна метод = методи.дані[п]::адреса;

      позначити_предмет_тексту(М, метод.назва);

      якщо метод.значення != пусто {
        позначити_предмет_дії(М, метод.значення);
      }

      п += 1;
    }
  }

  дія позначити_властивості(М: адреса<Машина>, властивості: Властивості) {
    змінна п: природне = 0;

    поки п < властивості.розмір {
      змінна властивість = властивості.дані[п]::адреса;

      позначити_предмет_тексту(М, властивість.назва);

      якщо властивість.значення != пусто {
        позначити_дійсне(М, властивість.значення);
      }

      п += 1;
    }
  }

  дія позначити_притримувач(М: адреса<Машина>, притримувач: адреса<Притримувач>) {
    змінна притримане = притримувач.перше;

    поки притримане != пусто {
      позначити_дійсне(М, притримане.значення);

      притримане = притримане.наступне;
    }
  }

  дія позначити(
    М: адреса<Машина>
  ) {
    позначити_дійсне(М, М.значення_дійсне);
    позначити_дійсне(М, М.значення_послідовність);
    позначити_дійсне(М, М.значення_дійсність);
    позначити_дійсне(М, М.значення_предмет);

    позначити_предмет_структури(М, М.предмет_структури_Структура);
    позначити_предмет_структури(М, М.предмет_структури_Модуль);
    позначити_предмет_структури(М, М.предмет_структури_Параметр);
    позначити_предмет_структури(М, М.предмет_структури_Число);
    позначити_предмет_структури(М, М.предмет_структури_Ціле);
    позначити_предмет_структури(М, М.предмет_структури_Дробове);
    позначити_предмет_структури(М, М.предмет_структури_Текст);
    позначити_предмет_структури(М, М.предмет_структури_Код);
    позначити_предмет_структури(М, М.предмет_структури_Дія);
    позначити_предмет_структури(М, М.предмет_структури_Список);
    позначити_предмет_структури(М, М.предмет_структури_Словник);
    позначити_предмет_структури(М, М.предмет_структури_Дані);
    позначити_предмет_структури(М, М.предмет_структури_ЗмінніДані);
    позначити_предмет_структури(М, М.предмет_структури_ДіапазонЦілих);
    позначити_предмет_структури(М, М.предмет_структури_ДіапазонДробових);
    позначити_предмет_структури(М, М.предмет_структури_ПеребірДіапазонуЦілих);
    позначити_предмет_структури(М, М.предмет_структури_ПеребірДіапазонуДробових);
    позначити_предмет_структури(М, М.предмет_структури_ПеребірСписку);

    позначити_дійсність(М, М.глобальна_дійсність);
    позначити_накопичувач_значень(М, М.накопичувач_значень);
    позначити_історію_здійснення(М, М.історія_здійснення);
    якщо М.значення_падіння != пусто {
      позначити_дійсне(М, М.значення_падіння);
    }
    позначити_місцезнаходження(М, М.місцезнаходження_падіння);
    позначити_константи_назв(М, М.константи_назв);
    позначити_взяті_значення(М, М.взяті_значення);
    позначити_користувацькі_дані(М, М.користувацькі_дані);
  }

  місцева дія запустити_очищувач(
    М: адреса<Машина>
  ) {
    позначити(М);

    змінна попереднє_значення: адреса<Дійсне> = пусто;
    змінна значення = М.перше_дійсне;

    поки значення != пусто {
      якщо значення.позначено {
        значення.позначено = ні;

        попереднє_значення = значення;

        значення = значення.наступне;
      } інакше {
        змінна значення_до_знищення = значення;

        значення = значення.наступне;

        попереднє_значення.наступне = значення;

        звільнити_дійсне(М, значення_до_знищення);

        М.кількість_дійсних -= 1;
      }
    }

    М.останнє_дійсне = попереднє_значення;

    М.кількість_дійсних_для_очищення = (М.кількість_дійсних * 5) + 1;
  }
}