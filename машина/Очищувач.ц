взяти означення мавка;

простір мавка {
  місцева дія перевірити_чи_треба_запустити_очищувач(
    М: адреса<Машина>
  ) -> логічне {
    вернути ні;
  }

  дія звільнити_дійсне(М: адреса<Машина>, значення: адреса<Дійсне>);
  дія звільнити_дійсність(М: адреса<Машина>, дійсність: адреса<Дійсність>);
  дія звільнити_послідовність(М: адреса<Машина>, послідовність: адреса<Послідовність>);
  дія звільнити_предмет_структури(М: адреса<Машина>, предмет_структури: адреса<ПредметСтруктури>);
  дія звільнити_предмет_модуля(М: адреса<Машина>, предмет_модуля: адреса<ПредметМодуля>);
  дія звільнити_предмет_параметра(М: адреса<Машина>, предмет_параметра: адреса<ПредметПараметра>);
  дія звільнити_предмет_цілого(М: адреса<Машина>, предмет_цілого: адреса<ПредметЦілого>);
  дія звільнити_предмет_дробового(М: адреса<Машина>, предмет_дробового: адреса<ПредметДробового>);
  дія звільнити_предмет_тексту(М: адреса<Машина>, предмет_тексту: адреса<ПредметТексту>);
  дія звільнити_предмет_коду(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>);
  дія звільнити_предмет_дії(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>);
  дія звільнити_предмет_списку(М: адреса<Машина>, предмет_списку: адреса<ПредметСписку>);
  дія звільнити_предмет_словника(М: адреса<Машина>, предмет_словника: адреса<ПредметСловника>);
  дія звільнити_предмет_даних(М: адреса<Машина>, предмет_даних: адреса<ПредметДаних>);
  дія звільнити_предмет_змінних_даних(М: адреса<Машина>, предмет_змінних_даних: адреса<ПредметЗміннихДаних>);
  дія звільнити_предмет_діапазону_цілих(М: адреса<Машина>, предмет_діапазону_цілих: адреса<ПредметДіапазонуЦілих>);
  дія звільнити_предмет_діапазону_дробових(М: адреса<Машина>, предмет_діапазону_дробових: адреса<ПредметДіапазонуДробових>);
  дія звільнити_предмет_перебору_діапазону_цілих(М: адреса<Машина>, предмет_перебору_діапазону_цілих: адреса<ПредметПереборуДіапазонуЦілих>);
  дія звільнити_предмет_перебору_діапазону_дробових(М: адреса<Машина>, предмет_перебору_діапазону_дробових: адреса<ПредметПереборуДіапазонуДробових>);
  дія звільнити_предмет_перебору_списку(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>);
  дія звільнити_складений_предмет(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>);
  дія звільнити_користувацькі_дані(М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>);
  дія звільнити_методи(М: адреса<Машина>, методи: Методи);
  дія звільнити_властивості(М: адреса<Машина>, властивості: Властивості);
  дія звільнити_притримувач(М: адреса<Машина>, притримувач: адреса<Притримувач>);

  дія звільнити_дійсне(
    М: адреса<Машина>,
    значення: адреса<Дійсне>
  ) {
    якщо значення == М.значення_дійсне {
      // надрукувати_кд(М, "звільнення: М.значення_дійсне");
      якщо значення.користувацькі_дані != пусто {
        звільнити_користувацькі_дані(М, значення.користувацькі_дані);
      }
      звільнити(М, значення);
      вернути;
    }

    якщо значення == М.значення_послідовність {
      // надрукувати_кд(М, "звільнення: М.значення_послідовність");
      якщо значення.користувацькі_дані != пусто {
        звільнити_користувацькі_дані(М, значення.користувацькі_дані);
      }
      звільнити(М, значення);
      вернути;
    }

    якщо значення == М.значення_дійсність {
      // надрукувати_кд(М, "звільнення: М.значення_дійсність");
      якщо значення.користувацькі_дані != пусто {
        звільнити_користувацькі_дані(М, значення.користувацькі_дані);
      }
      звільнити(М, значення);
      вернути;
    }

    якщо значення == М.значення_предмет {
      // надрукувати_кд(М, "звільнення: М.значення_предмет");
      якщо значення.користувацькі_дані != пусто {
        звільнити_користувацькі_дані(М, значення.користувацькі_дані);
      }
      звільнити(М, значення);
      вернути;
    }

    якщо значення.тип == М.значення_дійсне {
      запанікувати(М, "[Помилка Машини][звільнити_дійсне]: значення.тип == М.значення_дійсне");
      вернути;
    }

    якщо значення.тип == М.значення_послідовність {
      звільнити_послідовність(М, значення як адреса<Послідовність>);
      вернути;
    }

    якщо значення.тип == М.значення_дійсність {
      звільнити_дійсність(М, значення як адреса<Дійсність>);
      вернути;
    }

    якщо значення.тип == М.значення_предмет {
      запанікувати(М, "[Помилка Машини][звільнити_дійсне]: значення.тип == М.значення_предмет");
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Структура {
      звільнити_предмет_структури(М, значення як адреса<ПредметСтруктури>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Модуль {
      звільнити_предмет_модуля(М, значення як адреса<ПредметМодуля>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Параметр {
      звільнити_предмет_параметра(М, значення як адреса<ПредметПараметра>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Число {
      запанікувати(М, "[Помилка Машини][звільнити_дійсне]: значення.тип == М.предмет_структури_Число");
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Ціле {
      звільнити_предмет_цілого(М, значення як адреса<ПредметЦілого>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Дробове {
      звільнити_предмет_дробового(М, значення як адреса<ПредметДробового>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Текст {
      звільнити_предмет_тексту(М, значення як адреса<ПредметТексту>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Код {
      звільнити_предмет_коду(М, значення як адреса<ПредметКоду>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Дія {
      звільнити_предмет_дії(М, значення як адреса<ПредметДії>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Список {
      звільнити_предмет_списку(М, значення як адреса<ПредметСписку>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Словник {
      звільнити_предмет_словника(М, значення як адреса<ПредметСловника>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Дані {
      звільнити_предмет_даних(М, значення як адреса<ПредметДаних>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ЗмінніДані {
      звільнити_предмет_змінних_даних(М, значення як адреса<ПредметЗміннихДаних>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ДіапазонЦілих {
      звільнити_предмет_діапазону_цілих(М, значення як адреса<ПредметДіапазонуЦілих>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ДіапазонДробових {
      звільнити_предмет_діапазону_дробових(М, значення як адреса<ПредметДіапазонуДробових>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      звільнити_предмет_перебору_діапазону_цілих(М, значення як адреса<ПредметПереборуДіапазонуЦілих>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      звільнити_предмет_перебору_діапазону_дробових(М, значення як адреса<ПредметПереборуДіапазонуДробових>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ПеребірСписку {
      звільнити_предмет_перебору_списку(М, значення як адреса<ПредметПереборуСписку>);
      вернути;
    }

    звільнити_складений_предмет(М, значення як адреса<СкладенийПредмет>);
  }

  дія звільнити_дійсність(
    М: адреса<Машина>,
    дійсність: адреса<Дійсність>
  ) {
    якщо дійсність.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, дійсність.користувацькі_дані);
    }
    звільнити_памʼять(М, дійсність.діячі);
    звільнити(М, дійсність);
  }

  дія звільнити_послідовність(
    М: адреса<Машина>,
    послідовність: адреса<Послідовність>
  ) {
    // надрукувати_кд(М, "звільнити_послідовність");
    якщо послідовність.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, послідовність.користувацькі_дані);
    }
    звільнити_памʼять(М, послідовність.дані);
    звільнити(М, послідовність);
  }

  дія звільнити_предмет_структури(М: адреса<Машина>, предмет_структури: адреса<ПредметСтруктури>) {
    // надрукувати_кд(М, "звільнити_предмет_структури");
    якщо предмет_структури.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_структури.користувацькі_дані);
    }
    звільнити_методи(М, предмет_структури.методи);
    звільнити_властивості(М, предмет_структури.властивості);
    звільнити(М, предмет_структури);
  }

  дія звільнити_предмет_модуля(М: адреса<Машина>, предмет_модуля: адреса<ПредметМодуля>) {
    // надрукувати_кд(М, "звільнити_предмет_модуля");
    якщо предмет_модуля.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_модуля.користувацькі_дані);
    }
    звільнити_властивості(М, предмет_модуля.властивості);
    звільнити(М, предмет_модуля);
  }

  дія звільнити_предмет_параметра(М: адреса<Машина>, предмет_параметра: адреса<ПредметПараметра>) {
    // надрукувати_кд(М, "звільнити_предмет_параметра");
    якщо предмет_параметра.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_параметра.користувацькі_дані);
    }
    звільнити(М, предмет_параметра);
  }

  дія звільнити_предмет_цілого(М: адреса<Машина>, предмет_цілого: адреса<ПредметЦілого>) {
    // вивести_кд(М, "звільнити_предмет_цілого ");
    // надрукувати_ц64(М, предмет_цілого.значення);
    якщо предмет_цілого.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_цілого.користувацькі_дані);
    }
    звільнити(М, предмет_цілого);
  }

  дія звільнити_предмет_дробового(М: адреса<Машина>, предмет_дробового: адреса<ПредметДробового>) {
    // надрукувати_кд(М, "звільнити_предмет_дробового");
    якщо предмет_дробового.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_дробового.користувацькі_дані);
    }
    звільнити(М, предмет_дробового);
  }

  дія звільнити_предмет_тексту(М: адреса<Машина>, предмет_тексту: адреса<ПредметТексту>) {
    // надрукувати_кд(М, "звільнити_предмет_тексту");
    якщо предмет_тексту.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_тексту.користувацькі_дані);
    }
    звільнити_памʼять(М, предмет_тексту.значення.дані);
    звільнити(М, предмет_тексту);
  }

  дія звільнити_предмет_коду(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>) {
    // надрукувати_кд(М, "звільнити_предмет_коду");
    якщо предмет_коду.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_коду.користувацькі_дані);
    }
    звільнити_памʼять(М, предмет_коду.місцезнаходження_вказівок.дані);
    звільнити(М, предмет_коду);
  }

  дія звільнити_предмет_дії(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>) {
    // надрукувати_кд(М, "звільнити_предмет_дії");
    якщо предмет_дії.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_дії.користувацькі_дані);
    }
    звільнити(М, предмет_дії);
  }

  дія звільнити_предмет_списку(М: адреса<Машина>, предмет_списку: адреса<ПредметСписку>) {
    // надрукувати_кд(М, "звільнити_предмет_списку");
    якщо предмет_списку.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_списку.користувацькі_дані);
    }
    звільнити_памʼять(М, предмет_списку.елементи);
    звільнити(М, предмет_списку);
  }

  дія звільнити_предмет_словника(М: адреса<Машина>, предмет_словника: адреса<ПредметСловника>) {
    // надрукувати_кд(М, "звільнити_предмет_словника");
    якщо предмет_словника.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_словника.користувацькі_дані);
    }
    звільнити_памʼять(М, предмет_словника.елементи);
    звільнити(М, предмет_словника);
  }

  дія звільнити_предмет_даних(М: адреса<Машина>, предмет_даних: адреса<ПредметДаних>) {
    // надрукувати_кд(М, "звільнити_предмет_даних");
    якщо предмет_даних.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_даних.користувацькі_дані);
    }
    звільнити_памʼять(М, предмет_даних.дані);
    звільнити(М, предмет_даних);
  }

  дія звільнити_предмет_змінних_даних(М: адреса<Машина>, предмет_змінних_даних: адреса<ПредметЗміннихДаних>) {
    // надрукувати_кд(М, "звільнити_предмет_змінних_даних");
    якщо предмет_змінних_даних.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_змінних_даних.користувацькі_дані);
    }
    звільнити_памʼять(М, предмет_змінних_даних.дані);
    звільнити(М, предмет_змінних_даних);
  }

  дія звільнити_предмет_діапазону_цілих(М: адреса<Машина>, предмет_діапазону_цілих: адреса<ПредметДіапазонуЦілих>) {
    // надрукувати_кд(М, "звільнити_предмет_діапазону_цілих");
    якщо предмет_діапазону_цілих.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_діапазону_цілих.користувацькі_дані);
    }
    звільнити(М, предмет_діапазону_цілих);
  }

  дія звільнити_предмет_діапазону_дробових(М: адреса<Машина>, предмет_діапазону_дробових: адреса<ПредметДіапазонуДробових>) {
    // надрукувати_кд(М, "звільнити_предмет_діапазону_дробових");
    якщо предмет_діапазону_дробових.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_діапазону_дробових.користувацькі_дані);
    }
    звільнити(М, предмет_діапазону_дробових);
  }

  дія звільнити_предмет_перебору_діапазону_цілих(М: адреса<Машина>, предмет_перебору_діапазону_цілих: адреса<ПредметПереборуДіапазонуЦілих>) {
    // надрукувати_кд(М, "звільнити_предмет_перебору_діапазону_цілих");
    якщо предмет_перебору_діапазону_цілих.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_перебору_діапазону_цілих.користувацькі_дані);
    }
    звільнити(М, предмет_перебору_діапазону_цілих);
  }

  дія звільнити_предмет_перебору_діапазону_дробових(М: адреса<Машина>, предмет_перебору_діапазону_дробових: адреса<ПредметПереборуДіапазонуДробових>) {
    // надрукувати_кд(М, "звільнити_предмет_перебору_діапазону_дробових");
    якщо предмет_перебору_діапазону_дробових.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_перебору_діапазону_дробових.користувацькі_дані);
    }
    звільнити(М, предмет_перебору_діапазону_дробових);
  }

  дія звільнити_предмет_перебору_списку(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>) {
    // надрукувати_кд(М, "звільнити_предмет_перебору_списку");
    якщо предмет_перебору_списку.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, предмет_перебору_списку.користувацькі_дані);
    }
    звільнити(М, предмет_перебору_списку);
  }

  дія звільнити_складений_предмет(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>) {
    // надрукувати_кд(М, "звільнити_складений_предмет");
    якщо складений_предмет.користувацькі_дані != пусто {
      звільнити_користувацькі_дані(М, складений_предмет.користувацькі_дані);
    }
    звільнити_властивості(М, складений_предмет.властивості);
    звільнити(М, складений_предмет);
  }

  дія звільнити_користувацькі_дані(М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>) {
    // надрукувати_кд(М, "звільнити_користувацькі_дані");
    якщо користувацькі_дані.дія_перед_знищенням != пусто {
      користувацькі_дані.дія_перед_знищенням(М, користувацькі_дані);
    }

    якщо користувацькі_дані.притримувач != пусто {
      звільнити_притримувач(М, користувацькі_дані.притримувач);
    }

    звільнити(М, користувацькі_дані);
  }

  дія звільнити_методи(М: адреса<Машина>, методи: Методи) {
    // надрукувати_кд(М, "звільнити_методи");
    звільнити_памʼять(М, методи.дані);
  }

  дія звільнити_властивості(М: адреса<Машина>, властивості: Властивості) {
    // надрукувати_кд(М, "звільнити_властивості");
    звільнити_памʼять(М, властивості.дані);
  }

  дія звільнити_притримувач(М: адреса<Машина>, притримувач: адреса<Притримувач>) {
    // надрукувати_кд(М, "звільнити_притримувач");

    змінна притримане = притримувач.перше;

    поки притримане != пусто {
      змінна наступне_притримане = притримане.наступне;

      звільнити(М, притримане);

      притримане = наступне_притримане;
    }

    звільнити(М, притримувач);
  }

  дія позначити_дійсне(М: адреса<Машина>, значення: адреса<Дійсне>);
  дія позначити_дійсність(М: адреса<Машина>, дійсність: адреса<Дійсність>);
  дія позначити_послідовність(М: адреса<Машина>, послідовність: адреса<Послідовність>);
  дія позначити_предмет_структури(М: адреса<Машина>, предмет_структури: адреса<ПредметСтруктури>);
  дія позначити_предмет_модуля(М: адреса<Машина>, предмет_модуля: адреса<ПредметМодуля>);
  дія позначити_предмет_параметра(М: адреса<Машина>, предмет_параметра: адреса<ПредметПараметра>);
  дія позначити_предмет_цілого(М: адреса<Машина>, предмет_цілого: адреса<ПредметЦілого>);
  дія позначити_предмет_дробового(М: адреса<Машина>, предмет_дробового: адреса<ПредметДробового>);
  дія позначити_предмет_тексту(М: адреса<Машина>, предмет_тексту: адреса<ПредметТексту>);
  дія позначити_предмет_коду(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>);
  дія позначити_предмет_дії(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>);
  дія позначити_предмет_списку(М: адреса<Машина>, предмет_списку: адреса<ПредметСписку>);
  дія позначити_предмет_словника(М: адреса<Машина>, предмет_словника: адреса<ПредметСловника>);
  дія позначити_предмет_даних(М: адреса<Машина>, предмет_даних: адреса<ПредметДаних>);
  дія позначити_предмет_змінних_даних(М: адреса<Машина>, предмет_змінних_даних: адреса<ПредметЗміннихДаних>);
  дія позначити_предмет_діапазону_цілих(М: адреса<Машина>, предмет_діапазону_цілих: адреса<ПредметДіапазонуЦілих>);
  дія позначити_предмет_діапазону_дробових(М: адреса<Машина>, предмет_діапазону_дробових: адреса<ПредметДіапазонуДробових>);
  дія позначити_предмет_перебору_діапазону_цілих(М: адреса<Машина>, предмет_перебору_діапазону_цілих: адреса<ПредметПереборуДіапазонуЦілих>);
  дія позначити_предмет_перебору_діапазону_дробових(М: адреса<Машина>, предмет_перебору_діапазону_дробових: адреса<ПредметПереборуДіапазонуДробових>);
  дія позначити_предмет_перебору_списку(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>);
  дія позначити_складений_предмет(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>);
  дія позначити_накопичувач_значень(М: адреса<Машина>, накопичувач_значень: НакопичувачЗначень);
  дія позначити_історію_здійснення(М: адреса<Машина>, історія_здійснення: ІсторіяЗдійснення);
  дія позначити_місцезнаходження(М: адреса<Машина>, місцезнаходження: Місцезнаходження);
  дія позначити_константи_назв(М: адреса<Машина>, константи_назв: КонстантиНазв);
  дія позначити_взяті_значення(М: адреса<Машина>, взяті_значення: ВзятіЗначення);
  дія позначити_користувацькі_дані(М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>);
  дія позначити_методи(М: адреса<Машина>, методи: Методи);
  дія позначити_властивості(М: адреса<Машина>, властивості: Властивості);
  дія позначити_притримувач(М: адреса<Машина>, притримувач: адреса<Притримувач>);

  // позначка: прохід по типах предметів
  дія позначити_дійсне(
    М: адреса<Машина>,
    значення: адреса<Дійсне>
  ) {
    якщо значення.позначено {
      вернути;
    }

    якщо значення == М.значення_дійсне {
      значення.позначено = так;
      якщо значення.користувацькі_дані != пусто {
        позначити_користувацькі_дані(М, значення.користувацькі_дані);
      }
      вернути;
    }

    якщо значення == М.значення_послідовність {
      значення.позначено = так;
      якщо значення.користувацькі_дані != пусто {
        позначити_користувацькі_дані(М, значення.користувацькі_дані);
      }
      вернути;
    }

    якщо значення == М.значення_дійсність {
      значення.позначено = так;
      якщо значення.користувацькі_дані != пусто {
        позначити_користувацькі_дані(М, значення.користувацькі_дані);
      }
      вернути;
    }

    якщо значення == М.значення_предмет {
      значення.позначено = так;
      якщо значення.користувацькі_дані != пусто {
        позначити_користувацькі_дані(М, значення.користувацькі_дані);
      }
      вернути;
    }

    якщо значення.тип == М.значення_дійсне {
      запанікувати(М, "[Помилка Машини][позначити_дійсне]: значення.тип == М.значення_дійсне");
      вернути;
    }

    якщо значення.тип == М.значення_послідовність {
      позначити_послідовність(М, значення як адреса<Послідовність>);
      вернути;
    }

    якщо значення.тип == М.значення_дійсність {
      позначити_дійсність(М, значення як адреса<Дійсність>);
      вернути;
    }

    якщо значення.тип == М.значення_предмет {
      запанікувати(М, "[Помилка Машини][позначити_дійсне]: значення.тип == М.значення_предмет");
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Структура {
      позначити_предмет_структури(М, значення як адреса<ПредметСтруктури>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Модуль {
      позначити_предмет_модуля(М, значення як адреса<ПредметМодуля>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Параметр {
      позначити_предмет_параметра(М, значення як адреса<ПредметПараметра>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Число {
      запанікувати(М, "[Помилка Машини][позначити_дійсне]: значення.тип == М.предмет_структури_Число");
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Ціле {
      позначити_предмет_цілого(М, значення як адреса<ПредметЦілого>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Дробове {
      позначити_предмет_дробового(М, значення як адреса<ПредметДробового>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Текст {
      позначити_предмет_тексту(М, значення як адреса<ПредметТексту>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Код {
      позначити_предмет_коду(М, значення як адреса<ПредметКоду>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Дія {
      позначити_предмет_дії(М, значення як адреса<ПредметДії>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Список {
      позначити_предмет_списку(М, значення як адреса<ПредметСписку>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Словник {
      позначити_предмет_словника(М, значення як адреса<ПредметСловника>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_Дані {
      позначити_предмет_даних(М, значення як адреса<ПредметДаних>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ЗмінніДані {
      позначити_предмет_змінних_даних(М, значення як адреса<ПредметЗміннихДаних>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ДіапазонЦілих {
      позначити_предмет_діапазону_цілих(М, значення як адреса<ПредметДіапазонуЦілих>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ДіапазонДробових {
      позначити_предмет_діапазону_дробових(М, значення як адреса<ПредметДіапазонуДробових>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      позначити_предмет_перебору_діапазону_цілих(М, значення як адреса<ПредметПереборуДіапазонуЦілих>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      позначити_предмет_перебору_діапазону_дробових(М, значення як адреса<ПредметПереборуДіапазонуДробових>);
      вернути;
    }

    якщо значення.тип == М.предмет_структури_ПеребірСписку {
      позначити_предмет_перебору_списку(М, значення як адреса<ПредметПереборуСписку>);
      вернути;
    }

    позначити_складений_предмет(М, значення як адреса<СкладенийПредмет>);
  }

  дія позначити_дійсність(М: адреса<Машина>, дійсність: адреса<Дійсність>) {
    якщо дійсність.позначено {
      вернути;
    }

    дійсність.позначено = так;

    якщо дійсність.користувацькі_дані != пусто {
      позначити_користувацькі_дані(М, дійсність.користувацькі_дані);
    }

    змінна п: природне = 0;

    поки п < дійсність.кількість_діячів {
      позначити_предмет_тексту(М, дійсність.діячі[п].назва);

      якщо дійсність.діячі[п].значення != пусто {
        позначити_дійсне(М, дійсність.діячі[п].значення);
      }

      п += 1;
    }
  }

  дія позначити_послідовність(М: адреса<Машина>, послідовність: адреса<Послідовність>) {
    якщо послідовність.позначено {
      вернути;
    }

    послідовність.позначено = так;

    якщо послідовність.користувацькі_дані != пусто {
      позначити_користувацькі_дані(М, послідовність.користувацькі_дані);
    }

    змінна п: природне = 0;

    поки п < послідовність.розмір {
      якщо послідовність.дані[п] != пусто {
        позначити_дійсне(М, послідовність.дані[п]);
      }

      п += 1;
    }
  }

  дія позначити_предмет_структури(М: адреса<Машина>, предмет_структури: адреса<ПредметСтруктури>) {
    якщо предмет_структури.позначено {
      вернути;
    }

    предмет_структури.позначено = так;

    якщо предмет_структури.користувацькі_дані != пусто {
      позначити_користувацькі_дані(М, предмет_структури.користувацькі_дані);
    }

    якщо предмет_структури.назва != пусто {
      позначити_предмет_тексту(М, предмет_структури.назва);
    }

    якщо предмет_структури.предок != пусто {
      позначити_предмет_структури(М, предмет_структури.предок);
    }

    якщо предмет_структури.параметри != пусто {
      позначити_послідовність(М, предмет_структури.параметри);
    }

    позначити_методи(М, предмет_структури.методи);
    позначити_властивості(М, предмет_структури.властивості);
  }

  дія позначити_предмет_модуля(М: адреса<Машина>, предмет_модуля: адреса<ПредметМодуля>) {
    якщо предмет_модуля.позначено {
      вернути;
    }

    предмет_модуля.позначено = так;

    якщо предмет_модуля.користувацькі_дані != пусто {
      позначити_користувацькі_дані(М, предмет_модуля.користувацькі_дані);
    }

    якщо предмет_модуля.назва != пусто {
      позначити_предмет_тексту(М, предмет_модуля.назва);
    }

    позначити_властивості(М, предмет_модуля.властивості);
  }

  дія позначити_предмет_параметра(М: адреса<Машина>, предмет_параметра: адреса<ПредметПараметра>) {
    якщо предмет_параметра.позначено {
      вернути;
    }

    предмет_параметра.позначено = так;

    якщо предмет_параметра.користувацькі_дані != пусто {
      позначити_користувацькі_дані(М, предмет_параметра.користувацькі_дані);
    }

    якщо предмет_параметра.назва != пусто {
      позначити_предмет_тексту(М, предмет_параметра.назва);
    }

    якщо предмет_параметра.тип_ != пусто {
      // ...
    }

    якщо предмет_параметра.дія_значення != пусто {
      позначити_дійсне(М, предмет_параметра.дія_значення);
    }
  }

  дія позначити_предмет_цілого(М: адреса<Машина>, предмет_цілого: адреса<ПредметЦілого>) {
    якщо предмет_цілого.позначено {
      вернути;
    }

    предмет_цілого.позначено = так;

    якщо предмет_цілого.користувацькі_дані != пусто {
      позначити_користувацькі_дані(М, предмет_цілого.користувацькі_дані);
    }
  }

  дія позначити_предмет_дробового(М: адреса<Машина>, предмет_дробового: адреса<ПредметДробового>) {
    якщо предмет_дробового.позначено {
      вернути;
    }

    предмет_дробового.позначено = так;

