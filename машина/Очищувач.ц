    }

    якщо предмет_перебору_діапазону_цілих.поточне_значення != пусто {
      позначити_предмет_цілого(М, предмет_перебору_діапазону_цілих.поточне_значення);
    }
  }

  дія позначити_предмет_перебору_діапазону_дробових(М: адреса<Машина>, предмет_перебору_діапазону_дробових: адреса<ПредметПереборуДіапазонуДробових>) {
