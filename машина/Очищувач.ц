    }

    якщо предмет_перебору_діапазону_дробових.предмет_діапазону != пусто {
      позначити_предмет_діапазону_дробових(М, предмет_перебору_діапазону_дробових.предмет_діапазону);
    }

    якщо предмет_перебору_діапазону_дробових.поточне_значення != пусто {
      позначити_предмет_дробового(М, предмет_перебору_діапазону_дробових.поточне_значення);
    }
  }

  дія позначити_предмет_перебору_списку(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>) {
    якщо предмет_перебору_списку.позначено {
      вернути;
    }

    предмет_перебору_списку.позначено = так;

    якщо предмет_перебору_списку.користувацькі_дані != пусто {
      позначити_користувацькі_дані(М, предмет_перебору_списку.користувацькі_дані);
    }

    якщо предмет_перебору_списку.предмет_списку != пусто {
      позначити_предмет_списку(М, предмет_перебору_списку.предмет_списку);
    }

    якщо предмет_перебору_списку.поточне_значення != пусто {
      позначити_дійсне(М, предмет_перебору_списку.поточне_значення);
    }
  }

  дія позначити_складений_предмет(М: адреса<Машина>, складений_предмет: адреса<СкладенийПредмет>) {
    якщо складений_предмет.позначено {
      вернути;
    }

    складений_предмет.позначено = так;

    якщо складений_предмет.користувацькі_дані != пусто {
      позначити_користувацькі_дані(М, складений_предмет.користувацькі_дані);
    }

    позначити_властивості(М, складений_предмет.властивості);
  }

  дія позначити_накопичувач_значень(М: адреса<Машина>, накопичувач_значень: НакопичувачЗначень) {
    змінна п: природне = 0;

    поки п < накопичувач_значень.розмір {
      якщо накопичувач_значень.дані[п] != пусто {
        позначити_дійсне(М, накопичувач_значень.дані[п]);
      }

      п += 1;
    }
  }

  дія позначити_історію_здійснення(М: адреса<Машина>, історія_здійснення: ІсторіяЗдійснення) {
    змінна п: природне = 0;

    поки п < історія_здійснення.розмір {
      змінна здійснюване = історія_здійснення.дані[п];

      якщо здійснюване.здійснювач != пусто {
        позначити_дійсне(М, здійснюване.здійснювач);
      }

      якщо здійснюване.дійсність != пусто {
        позначити_дійсність(М, здійснюване.дійсність);
      }

      якщо здійснюване.предмет_дії != пусто {
        позначити_предмет_дії(М, здійснюване.предмет_дії);
      }

      позначити_місцезнаходження(М, здійснюване.місцезнаходження);

      п += 1;
    }
  }

  дія позначити_місцезнаходження(М: адреса<Машина>, місцезнаходження: Місцезнаходження) {
    змінна шлях = місцезнаходження.шлях_або_код як адреса<ПредметДаних> як адреса<Дійсне>;

    якщо шлях != пусто {
      позначити_дійсне(М, шлях);
    }
  }

  дія позначити_константи_назв(М: адреса<Машина>, константи_назв: КонстантиНазв) {
    позначити_предмет_тексту(М, М.константи_назв._значення);
    позначити_предмет_тексту(М, М.константи_назв._дійсне);
    позначити_предмет_тексту(М, М.константи_назв._послідовність);
    позначити_предмет_тексту(М, М.константи_назв._дійсність);
    позначити_предмет_тексту(М, М.константи_назв._предмет);
    позначити_предмет_тексту(М, М.константи_назв._Структура);
    позначити_предмет_тексту(М, М.константи_назв._Модуль);
    позначити_предмет_тексту(М, М.константи_назв._Параметр);
    позначити_предмет_тексту(М, М.константи_назв._Число);
    позначити_предмет_тексту(М, М.константи_назв._Ціле);
    позначити_предмет_тексту(М, М.константи_назв._Дробове);
    позначити_предмет_тексту(М, М.константи_назв._Текст);
    позначити_предмет_тексту(М, М.константи_назв._Код);
    позначити_предмет_тексту(М, М.константи_назв._Дія);
    позначити_предмет_тексту(М, М.константи_назв._Список);
    позначити_предмет_тексту(М, М.константи_назв._Словник);
    позначити_предмет_тексту(М, М.константи_назв._Дані);
    позначити_предмет_тексту(М, М.константи_назв._ЗмінніДані);
    позначити_предмет_тексту(М, М.константи_назв._ДіапазонЦілих);
    позначити_предмет_тексту(М, М.константи_назв._ДіапазонДробових);
    позначити_предмет_тексту(М, М.константи_назв._ПеребірДіапазонуЦілих);
    позначити_предмет_тексту(М, М.константи_назв._ПеребірДіапазонуДробових);
    позначити_предмет_тексту(М, М.константи_назв._ПеребірСписку);
    позначити_предмет_тексту(М, М.константи_назв._далі);
    позначити_предмет_тексту(М, М.константи_назв._з);
    позначити_предмет_тексту(М, М.константи_назв._чародія_ціле);
    позначити_предмет_тексту(М, М.константи_назв._чародія_текст);
    позначити_предмет_тексту(М, М.константи_назв._довжина);
    позначити_предмет_тексту(М, М.константи_назв._0);
    позначити_предмет_тексту(М, М.константи_назв._1);
    позначити_предмет_тексту(М, М.константи_назв._2);
    позначити_предмет_тексту(М, М.константи_назв._3);
    позначити_предмет_тексту(М, М.константи_назв._4);
    позначити_предмет_тексту(М, М.константи_назв._5);
    позначити_предмет_тексту(М, М.константи_назв._6);
    позначити_предмет_тексту(М, М.константи_назв._7);
    позначити_предмет_тексту(М, М.константи_назв._8);
    позначити_предмет_тексту(М, М.константи_назв._9);
    позначити_предмет_тексту(М, М.константи_назв._10);
    позначити_предмет_тексту(М, М.константи_назв._11);
    позначити_предмет_тексту(М, М.константи_назв._12);
    позначити_предмет_тексту(М, М.константи_назв._13);
    позначити_предмет_тексту(М, М.константи_назв._14);
    позначити_предмет_тексту(М, М.константи_назв._15);
    позначити_предмет_тексту(М, М.константи_назв._16);
    позначити_предмет_тексту(М, М.константи_назв._17);
    позначити_предмет_тексту(М, М.константи_назв._18);
    позначити_предмет_тексту(М, М.константи_назв._19);
    позначити_предмет_тексту(М, М.константи_назв._20);
    позначити_предмет_тексту(М, М.константи_назв._21);
    позначити_предмет_тексту(М, М.константи_назв._22);
    позначити_предмет_тексту(М, М.константи_назв._23);
    позначити_предмет_тексту(М, М.константи_назв._24);
    позначити_предмет_тексту(М, М.константи_назв._25);
    позначити_предмет_тексту(М, М.константи_назв._26);
    позначити_предмет_тексту(М, М.константи_назв._27);
