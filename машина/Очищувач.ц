    позначити_предмет_тексту(М, М.константи_назв._29);
    позначити_предмет_тексту(М, М.константи_назв._30);
    позначити_предмет_тексту(М, М.константи_назв._31);
    позначити_предмет_тексту(М, М.константи_назв._32);
    позначити_предмет_тексту(М, М.константи_назв._33);
