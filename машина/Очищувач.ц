    }

    якщо предмет_діапазону_цілих.від != пусто {
      позначити_предмет_цілого(М, предмет_діапазону_цілих.від);
    }

    якщо предмет_діапазону_цілих.до != пусто {
      позначити_предмет_цілого(М, предмет_діапазону_цілих.до);
    }
  }

  дія позначити_предмет_діапазону_дробових(М: адреса<Машина>, предмет_діапазону_дробових: адреса<ПредметДіапазонуДробових>) {
