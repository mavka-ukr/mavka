взяти означення ДЧ;

// ЦЕ ТРЕБА ПЕРЕПИСАТИ

простір ДЧ {
  місцева дія порівняти_дробове_і_ціле(
    виділяч: адреса<Виділяч>,
    а: Дробове,
    б: Ціле
  ) -> ц8 {
    змінна б_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, б, б_дробове::адреса) {
      вернути 0;
    }

    змінна результат = порівняти_дробові(виділяч, а, б_дробове);

    звільнити_дробове(виділяч, б_дробове);

    вернути результат;
  }

  місцева дія додати_дробове_і_ціле(
    виділяч: адреса<Виділяч>,
    а: Дробове,
    б: Ціле,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна б_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, б, б_дробове::адреса) {
      вернути ні;
    }

    вернути додати_дробові(виділяч, а, б_дробове, точність, вихід);
  }

  місцева дія відняти_дробове_і_ціле(
    виділяч: адреса<Виділяч>,
    а: Дробове,
    б: Ціле,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна б_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, б, б_дробове::адреса) {
      вернути ні;
    }

    вернути відняти_дробові(виділяч, а, б_дробове, точність, вихід);
  }

  місцева дія помножити_дробове_і_ціле(
    виділяч: адреса<Виділяч>,
    а: Дробове,
    б: Ціле,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна б_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, б, б_дробове::адреса) {
      вернути ні;
    }

    вернути помножити_дробові(виділяч, а, б_дробове, точність, вихід);
  }

  місцева дія поділити_дробове_і_ціле(
    виділяч: адреса<Виділяч>,
    а: Дробове,
    б: Ціле,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна б_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, б, б_дробове::адреса) {
      вернути ні;
    }

    вернути поділити_дробові(виділяч, а, б_дробове, точність, вихід);
  }

  місцева дія залишок_ділення_дробового_і_цілого(
    виділяч: адреса<Виділяч>,
    а: Дробове,
    б: Ціле,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна б_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, б, б_дробове::адреса) {
      вернути ні;
    }

    вернути залишок_ділення_дробових(виділяч, а, б_дробове, точність, вихід);
  }

  місцева дія степінь_дробового_і_цілого(
    виділяч: адреса<Виділяч>,
    основа: Дробове,
    степінь: Ціле,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна степінь_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, степінь, степінь_дробове::адреса) {
      вернути ні;
    }

    вернути степінь_дробового(виділяч, основа, степінь_дробове, точність, вихід);
  }
}

простір ДЧ {
  місцева дія порівняти_ціле_і_дробове(
    виділяч: адреса<Виділяч>,
    а: Ціле,
    б: Дробове
  ) -> ц8 {
    змінна а_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, а, а_дробове::адреса) {
      вернути 0;
    }

    змінна результат = порівняти_дробові(виділяч, а_дробове, б);

    звільнити_дробове(виділяч, а_дробове);

    вернути результат;
  }

  місцева дія додати_ціле_і_дробове(
    виділяч: адреса<Виділяч>,
    а: Ціле,
    б: Дробове,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна а_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, а, а_дробове::адреса) {
      вернути ні;
    }

    вернути додати_дробові(виділяч, а_дробове, б, точність, вихід);
  }

  місцева дія відняти_ціле_і_дробове(
    виділяч: адреса<Виділяч>,
    а: Ціле,
    б: Дробове,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна а_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, а, а_дробове::адреса) {
      вернути ні;
    }

    вернути відняти_дробові(виділяч, а_дробове, б, точність, вихід);
  }

  місцева дія помножити_ціле_і_дробове(
    виділяч: адреса<Виділяч>,
    а: Ціле,
    б: Дробове,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна а_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, а, а_дробове::адреса) {
      вернути ні;
    }

    вернути помножити_дробові(виділяч, а_дробове, б, точність, вихід);
  }

  місцева дія поділити_ціле_і_дробове(
    виділяч: адреса<Виділяч>,
    а: Ціле,
    б: Дробове,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна а_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, а, а_дробове::адреса) {
      вернути ні;
    }

    вернути поділити_дробові(виділяч, а_дробове, б, точність, вихід);
  }

  місцева дія залишок_ділення_цілого_і_дробового(
    виділяч: адреса<Виділяч>,
    а: Ціле,
    б: Дробове,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна а_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, а, а_дробове::адреса) {
      вернути ні;
    }

    вернути залишок_ділення_дробових(виділяч, а_дробове, б, точність, вихід);
  }

  місцева дія степінь_цілого_і_дробового(
    виділяч: адреса<Виділяч>,
    основа: Ціле,
    степінь: Дробове,
    точність: ц32,
    вихід: адреса<Дробове>
  ) -> логічне {
    змінна основа_дробове: Дробове;

    якщо не ініціалізувати_дробове_з_цілого(виділяч, основа, основа_дробове::адреса) {
      вернути ні;
    }

    вернути степінь_дробового(виділяч, основа_дробове, степінь, точність, вихід);
  }
}