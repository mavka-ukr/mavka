взяти означення мавка;

простір мавка {
  місцева дія покласти_в_чергу_здійснення(
    М: адреса<Машина>,
    черга_здійснення: адреса<ЧергаЗдійснення>,
    предмет_дії: адреса<ПредметДії>,
    кількість_задіяних: природне,
    задіяні: памʼять<Значення>,
    іменовано_задіяні: адреса<ІменованіЗадіяні>
  ) {
    // ...
  }

  місцева дія забрати_з_черги_здійснення(
    М: адреса<Машина>,
    черга_здійснення: адреса<ЧергаЗдійснення>,
    вихід: адреса<ЕлементЧергиЗдійснення>
  ) -> логічне {
    якщо черга_здійснення.перший != пусто {
      змінна елемент = черга_здійснення.перший;

      черга_здійснення.перший = елемент.наступний;

      вихід::вміст = елемент::вміст;

      звільнити(М, елемент);

      вернути так;
    } інакше {
      черга_здійснення.останній = пусто;

      вернути ні;
    }
  }
}