  }

  // позначка: прохід по типах предметів
  місцева дія отримати_текст(
    М: адреса<Машина>,
    значення: Значення,
    місцезнаходження: Місцезнаходження
  ) -> адреса<ПредметТексту> {
    якщо значення == пусто {
      вернути створити_текст(М, "недійсне");
    }

    якщо значення.тип == М.значення_дійсне {
      вернути створити_текст(М, "дійсне");
    }

    якщо значення.тип == М.значення_послідовність {
      вернути створити_текст(М, "послідовність");
    }

    якщо значення.тип == М.значення_дійсність {
      вернути створити_текст(М, "дійсність");
    }

    якщо значення.тип == М.предмет_структури_Предмет {
      вернути створити_текст(М, "<Предмет>");
    }

    якщо значення.тип == М.предмет_структури_Структура {
      вернути створити_текст(М, "<Структура>");
    }

    якщо значення.тип == М.предмет_структури_Модуль {
      вернути створити_текст(М, "<Модуль>");
    }

    якщо значення.тип == М.предмет_структури_Параметр {
      вернути створити_текст(М, "<Параметр>");
    }

    якщо значення.тип == М.предмет_структури_Число {
      вернути створити_текст(М, "<Число>");
    }

    якщо значення.тип == М.предмет_структури_Ціле {
      змінна значення = (значення як адреса<ПредметЦілого>).значення;
      змінна значення_кд: кд;

      якщо перетворити_ц64_в_кд(М, значення, значення_кд::адреса) {
        вернути створити_текст_без_копіювання(М, значення_кд.розмір, значення_кд.дані);
      }

      увімкнути_стан_падіння(
        М,
        створити_текст(М, "Неможливо перетворити ціле число в текст.") як Значення,
        місцезнаходження
      );
      вернути пусто;
    }

    якщо значення.тип == М.предмет_структури_Дробове {
      змінна значення = (значення як адреса<ПредметДробового>).значення;
      змінна значення_кд: кд;

      якщо перетворити_д64_в_кд(М, значення, значення_кд::адреса) {
        вернути створити_текст_без_копіювання(М, значення_кд.розмір, значення_кд.дані);
      }

      увімкнути_стан_падіння(
        М,
        створити_текст(М, "Неможливо перетворити дробове число в текст.") як Значення,
        місцезнаходження
      );
      вернути пусто;
    }

    якщо значення.тип == М.предмет_структури_Текст {
      вернути значення як адреса<ПредметТексту>;
    }

    якщо значення.тип == М.предмет_структури_Код {
      вернути створити_текст(М, "<Код>");
    }

    якщо значення.тип == М.предмет_структури_Дія {
      вернути створити_текст(М, "<Дія>");
    }

    якщо значення.тип == М.предмет_структури_Список {
      вернути створити_текст(М, "<Список>");
    }

    якщо значення.тип == М.предмет_структури_Словник {
      вернути створити_текст(М, "<Словник>");
    }

    якщо значення.тип == М.предмет_структури_Дані {
      вернути створити_текст(М, "<Дані>");
    }

    якщо значення.тип == М.предмет_структури_ЗмінніДані {
      вернути створити_текст(М, "<ЗмінніДані>");
    }

    якщо значення.тип == М.предмет_структури_ДіапазонЦілих {
      вернути створити_текст(М, "<ДіапазонЦілих>");
    }

    якщо значення.тип == М.предмет_структури_ДіапазонДробових {
      вернути створити_текст(М, "<ДіапазонДробових>");
    }

    якщо значення.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      вернути створити_текст(М, "<ПеребірДіапазонуЦілих>");
    }

    якщо значення.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      вернути створити_текст(М, "<ПеребірДіапазонуДробових>");
    }

    якщо значення.тип == М.предмет_структури_ПеребірСписку {
      вернути створити_текст(М, "<ПеребірСписку>");
    }

    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Не вдалось перетворити на текст.") як Значення,
      Місцезнаходження { пусто, 0 }
    );

    вернути пусто;
  }

  // позначка: прохід по типах предметів
  місцева дія отримати_діапазон(
    М: адреса<Машина>,
    від: Значення,
    до: Значення,
    включно: логічне,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо від == пусто {
      вернути пусто;
    }

    якщо від.тип == М.значення_дійсне {
      вернути пусто;
    }

    якщо від.тип == М.значення_послідовність {
      вернути пусто;
    }

    якщо від.тип == М.значення_дійсність {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Предмет {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Структура {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Модуль {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Параметр {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Число {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Ціле {
      якщо до.тип == М.предмет_структури_Ціле {
        вернути створити_діапазон_цілих(М, від як адреса<ПредметЦілого>, до як адреса<ПредметЦілого>, включно) як Значення;
      } інакше {
        увімкнути_стан_падіння(
          М,
          створити_текст(М, "Неможливо створити діапазон цілих чисел з різнорідних типів.") як Значення,
          місцезнаходження
        );

        вернути пусто;
      }
    }

    якщо від.тип == М.предмет_структури_Дробове {
      якщо до.тип == М.предмет_структури_Дробове {
        вернути створити_діапазон_дробових(М, від як адреса<ПредметДробового>, до як адреса<ПредметДробового>, включно) як Значення;
      } інакше {
        увімкнути_стан_падіння(
          М,
          створити_текст(М, "Неможливо створити діапазон дробових чисел з різнорідних типів.") як Значення,
          місцезнаходження
        );

        вернути пусто;
      }
    }

    якщо від.тип == М.предмет_структури_Текст {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Код {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Дія {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Список {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Словник {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_Дані {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_ЗмінніДані {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_ДіапазонЦілих {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_ДіапазонДробових {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      вернути пусто;
    }

    якщо від.тип == М.предмет_структури_ПеребірСписку {
      вернути пусто;
    }

    змінна кількість_задіяних: природне = 2;
    змінна дані_задіяних = виділити_памʼять<Значення>(М, кількість_задіяних);
    дані_задіяних[0] = до;
    якщо включно {
      дані_задіяних[1] = М.значення_дійсне;
    } інакше {
      дані_задіяних[1] = пусто як Значення;
    }

    вернути здійснити_властивість(
      М,
      від,
      створити_текст(М, "чародія_діапазон"),
      Задіяні { кількість_задіяних, дані_задіяних, пусто },
      місцезнаходження
    );
  }

  // позначка: прохід по типах предметів
  місцева дія отримати_перебір(
    М: адреса<Машина>,
    предмет: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо предмет == пусто {
      вернути пусто;
    }

    якщо предмет.тип == М.значення_дійсне {
      вернути пусто;
    }

    якщо предмет.тип == М.значення_послідовність {
      вернути пусто;
    }

    якщо предмет.тип == М.значення_дійсність {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Предмет {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Структура {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Модуль {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Параметр {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Число {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Ціле {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Дробове {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Текст {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Код {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Дія {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Список {
      вернути створити_перебір_списку(М, предмет як адреса<ПредметСписку>) як Значення;
    }

    якщо предмет.тип == М.предмет_структури_Словник {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_Дані {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_ЗмінніДані {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_ДіапазонЦілих {
      вернути створити_перебір_діапазону_цілих(М, предмет як адреса<ПредметДіапазонуЦілих>) як Значення;
    }

    якщо предмет.тип == М.предмет_структури_ДіапазонДробових {
      вернути створити_перебір_діапазону_дробових(М, предмет як адреса<ПредметДіапазонуДробових>) як Значення;
    }

    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуЦілих {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_ПеребірДіапазонуДробових {
      вернути пусто;
    }

    якщо предмет.тип == М.предмет_структури_ПеребірСписку {
      вернути пусто;
    }

    вернути здійснити_властивість(
      М,
      предмет,
      створити_текст(М, "чародія_перебір"),
      Задіяні { 0, пусто, пусто },
      місцезнаходження
    );
  }

  місцева дія виконати_додати(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }

    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_додати(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_додати(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати додавання можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_відняти(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }

    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_відняти(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_відняти(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати віднімання можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_помножити(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }

    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_помножити(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_помножити(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати множення можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_поділити(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }

    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_поділити(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_поділити(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати ділення можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_остача(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }

    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_остача(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_остача(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати отримання остачі ділення можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_частка(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }

    якщо ліво.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_частка(М, ліво як адреса<ПредметЦілого>, право, місцезнаходження);
    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_частка(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати отримання неповної частки ділення можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_степінь(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо ліво == пусто {
      стрибнути крок_помилка;
    }

    якщо ліво.тип == М.предмет_структури_Ціле {
