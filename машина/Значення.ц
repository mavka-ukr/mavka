    }

    якщо ліво.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_більше(М, ліво як адреса<ПредметДробового>, право, місцезнаходження);
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати перевірку чи більше можна лише між числами.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_містить(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    вернути пусто;
  }

  місцева дія виконати_двійкове_не(
    М: адреса<Машина>,
    значення: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо значення == пусто {
      стрибнути крок_помилка;
    }

    якщо значення.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_двійкове_не(М, значення як адреса<ПредметЦілого>, місцезнаходження);
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати двійкове НЕ можна лише для цілого числа.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія виконати_змінити_знак(
    М: адреса<Машина>,
    значення: Значення,
    місцезнаходження: Місцезнаходження
  ) -> Значення {
    якщо значення == пусто {
      стрибнути крок_помилка;
    }

    якщо значення.тип == М.предмет_структури_Ціле {
      вернути предмет_цілого_змінити_знак(М, значення як адреса<ПредметЦілого>, місцезнаходження);
    }

    якщо значення.тип == М.предмет_структури_Дробове {
      вернути предмет_дробового_змінити_знак(М, значення як адреса<ПредметДробового>, місцезнаходження);
    }

  крок_помилка:
    увімкнути_стан_падіння(
      М,
      створити_текст(М, "Виконати зміну знаку можна лише для чисел.") як Значення,
      місцезнаходження
    );
    вернути пусто;
  }

  місцева дія перевірити_чи_значення_рівні(
    М: адреса<Машина>,
    а: Значення,
    б: Значення
  ) -> логічне {
    якщо а == б {
      вернути так;
    }

    якщо а == пусто або б == пусто {
      вернути ні;
    }

    якщо а.тип == б.тип {
      якщо а.тип == М.предмет_структури_Ціле {
        змінна властивості_цілого_а = а як адреса<ПредметЦілого>;
        змінна властивості_цілого_б = б як адреса<ПредметЦілого>;

        якщо властивості_цілого_а.значення == властивості_цілого_б.значення {
          вернути так;
        }

        вернути ні;
      }

      якщо а.тип == М.предмет_структури_Дробове {
        змінна властивості_дробового_а = а як адреса<ПредметДробового>;
        змінна властивості_дробового_б = б як адреса<ПредметДробового>;

        якщо властивості_дробового_а.значення == властивості_дробового_б.значення {
          вернути так;
        }

        вернути ні;
      }

      якщо а.тип == М.предмет_структури_Текст {
        вернути перевірити_чи_предмети_тексту_рівні(
          М,
          а як адреса<ПредметТексту>,
          б як адреса<ПредметТексту>
        );
      }
    }

    якщо а.тип == М.предмет_структури_Ціле і б.тип == М.предмет_структури_Дробове {
      змінна властивості_цілого_а = а як адреса<ПредметЦілого>;
      змінна властивості_дробового_б = б як адреса<ПредметДробового>;

      якщо д64(властивості_цілого_а.значення) == властивості_дробового_б.значення {
        вернути так;
      }

      вернути ні;
    }

    якщо а.тип == М.предмет_структури_Дробове і б.тип == М.предмет_структури_Ціле {
      змінна властивості_дробового_а = а як адреса<ПредметДробового>;
      змінна властивості_цілого_б = б як адреса<ПредметЦілого>;

      якщо властивості_дробового_а.значення == д64(властивості_цілого_б.значення) {
        вернути так;
      }

      вернути ні;
    }

    вернути ні;
  }

  місцева дія перевірити_чи_значення_є(
    М: адреса<Машина>,
    а: Значення,
    б: адреса<ПредметСтруктури>
  ) -> логічне {
    якщо а == пусто {
      якщо б == пусто {
        вернути так;
      }

      вернути ні;
    }

    якщо б == пусто {
      вернути ні;
    }

    змінна тип_а = а.тип як адреса<ПредметСтруктури>;

  крок_перевірки_типу_а:
    якщо тип_а == б {
      вернути так;
    }

    якщо тип_а == М.значення_дійсне {
      вернути ні;
    }

    тип_а = тип_а.предок;

    стрибнути крок_перевірки_типу_а;
  }
}