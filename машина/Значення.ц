  }

  місцева дія виконати_вправо(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо ліво.вид == ЗЧисло {
      вернути предмет_числа_вправо(М, ліво.дані як д64, право, місцезнаходження);
    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати двійкове зсув вправо можна лише між числами."), місцезнаходження);
  }

  місцева дія виконати_двійкове_і(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо ліво.вид == ЗЧисло {
      вернути предмет_числа_двійкове_і(М, ліво.дані як д64, право, місцезнаходження);
    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати двійкове І можна лише між числами."), місцезнаходження);
  }

  місцева дія виконати_двійкове_або(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо ліво.вид == ЗЧисло {
      вернути предмет_числа_двійкове_або(М, ліво.дані як д64, право, місцезнаходження);
    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати двійкове АБО можна лише між числами."), місцезнаходження);
  }

  місцева дія виконати_двійкове_вабо(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо ліво.вид == ЗЧисло {
      вернути предмет_числа_двійкове_вабо(М, ліво.дані як д64, право, місцезнаходження);
    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати двійкове ВАБО можна лише між числами."), місцезнаходження);
  }

  місцева дія виконати_менше(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо ліво.вид == ЗЧисло {
      вернути предмет_числа_менше(М, ліво.дані як д64, право, місцезнаходження);
    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати перевірку чи менше можна лише між числами."), місцезнаходження);
  }

  місцева дія виконати_більше(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо ліво.вид == ЗЧисло {
      вернути предмет_числа_більше(М, ліво.дані як д64, право, місцезнаходження);
    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати перевірку чи більше можна лише між числами."), місцезнаходження);
  }

  місцева дія виконати_двійкове_не(
    М: адреса<Машина>,
    значення: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо значення.вид == ЗЧисло {
      вернути предмет_числа_двійкове_не(М, значення.дані як д64, місцезнаходження);
    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати двійкове НЕ можна лише для чисел."), місцезнаходження);
  }

  місцева дія виконати_змінити_знак(
    М: адреса<Машина>,
    значення: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо значення.вид == ЗЧисло {
      вернути предмет_числа_змінити_знак(М, значення.дані як д64, місцезнаходження);
    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати зміну знаку можна лише для чисел."), місцезнаходження);
  }

  дія є_невизначеність(значення: д64) -> логічне {
    вернути значення != значення;
  }

  місцева дія перевірити_чи_значення_рівні(
    М: адреса<Машина>,
    а: Значення,
    б: Значення
  ) -> логічне {
    якщо а.вид != б.вид {
      вернути ні;
    }

    якщо а.вид == ЗНедійсне {
      вернути так;
    }

    якщо а.вид == ЗДійсне {
      вернути так;
    }

    якщо а.вид == ЗЧисло {
      якщо є_невизначеність(а.дані як д64) і є_невизначеність(б.дані як д64) {
        вернути ні;
      }

      вернути (а.дані як д64) == (б.дані як д64);
    }

    якщо а.вид != ЗПредмет {
      ЗАПАНІКУВАТИ(М, "Невідомий вид значення у перевірці на рівність.");
    }

    змінна а_предмет = а.дані як адреса<Предмет>;
    змінна б_предмет = б.дані як адреса<Предмет>;

    якщо а_предмет.тип == б_предмет.тип {
      якщо а_предмет.тип == М.предмет_структури_Текст {
        вернути перевірити_чи_предмети_тексту_рівні(
          М,
          а_предмет як адреса<ПредметТексту>,
          б_предмет як адреса<ПредметТексту>
        );
      }

      вернути ні;
    }

    вернути ні;
  }

  місцева дія перевірити_чи_предмет_є(
    М: адреса<Машина>,
    а: адреса<Предмет>,
    б: адреса<ПредметСтруктури>
  ) -> логічне {
    вернути а.тип == б;
  }
}