    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати ділення можна лише між числами."), місцезнаходження);
  }

  місцева дія виконати_остача(
    М: адреса<Машина>,
    ліво: Значення,
    право: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо ліво.вид == ЗЧисло {
      вернути предмет_числа_остача(М, ліво.дані як д64, право, місцезнаходження);
    }

    вернути МІСЦЕПАДІННЯ(М, створити_текст(М, "Виконати остачу можна лише між числами."), місцезнаходження);
  }

  місцева дія виконати_степінь(
    М: адреса<Машина>,
    ліво: Значення,
