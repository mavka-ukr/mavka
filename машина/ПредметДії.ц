взяти означення мавка;

простір мавка {
  місцева дія створити_дію(
    М: адреса<Машина>,
    дійсність_створення: адреса<Дійсність>,
    назва: адреса<ПредметТексту>,
    параметри: адреса<Послідовність>,
    тип_результату: Значення,
    код: адреса<ПредметКоду>,
    шлях_до_файлу_модуля: адреса<ПредметДаних>
  ) -> адреса<ПредметДії> {
    змінна предмет = виділити_предмет<ПредметДії>(М, М.предмет_структури_Дія);

    предмет.дійсність_створення = дійсність_створення;
    предмет.рідна = ні;
    предмет.назва = назва;
    предмет.параметри = параметри;
    предмет.код = код;
    предмет.тип_результату = тип_результату;
    предмет.шлях_до_файлу_модуля = шлях_до_файлу_модуля;

    вернути предмет;
  }

  місцева дія створити_рідну_дію(
    М: адреса<Машина>,
    назва: адреса<ПредметТексту>,
    параметри: адреса<Послідовність>,
    тип_результату: Значення,
    рідна_дія: РіднаДія,
    шлях_до_файлу_модуля: адреса<ПредметДаних>
  ) -> адреса<ПредметДії> {
    змінна предмет = виділити_предмет<ПредметДії>(М, М.предмет_структури_Дія);

    предмет.дійсність_створення = пусто;
    предмет.рідна = так;
    предмет.назва = назва;
    предмет.параметри = параметри;
    предмет.тип_результату = тип_результату;
    предмет.код = рідна_дія;
    предмет.шлях_до_файлу_модуля = шлях_до_файлу_модуля;

    вернути предмет;
  }

  місцева дія предмет_дії_здійснити(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    кількість_задіяних: природне,
    задіяні: памʼять<Значення>,
    іменовано_задіяні: адреса<ІменованіЗадіяні>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо предмет_дії.рідна {
      змінна рідна_дія = предмет_дії.код як РіднаДія;

      покласти_в_історію_здійснення(
        М,
        здійснювач,
        пусто,
        предмет_дії,
        місцезнаходження
      );

      змінна результат = рідна_дія(
        М,
        здійснювач,
        предмет_дії,
        кількість_задіяних,
        задіяні,
        іменовано_задіяні
      );

      якщо не М.стан_падіння {
        видалити_з_історії_здійснення(М, 1);
      }

      звільнити_памʼять(М, задіяні);
      якщо іменовано_задіяні != пусто {
        знищити_іменовано_задіяні(М, іменовано_задіяні);
      }

      вернути результат;
    } інакше {
      змінна дійсність = створити_дійсність(
        М,
        предмет_дії.дійсність_створення
      );

      змінна здійснюване = покласти_в_історію_здійснення(
        М,
        здійснювач,
        дійсність,
        предмет_дії,
        місцезнаходження
      );

      якщо предмет_дії.параметри != пусто {
        змінна п: природне = 0;

        поки п < предмет_дії.параметри.розмір {
          змінна параметр = предмет_дії.параметри.дані[п];
          змінна властивості_параметра = параметр як адреса<ПредметПараметра>;

          якщо кількість_задіяних > п {
            записати_в_дійсність(
              М,
              дійсність,
              властивості_параметра.назва,
              задіяні[п]
            );
          }

          п += 1;
        }

        якщо іменовано_задіяні != пусто {
          п = 0;

          поки п < іменовано_задіяні.розмір {
            змінна іменовано_задіяне = іменовано_задіяні.елементи[п];

            записати_в_дійсність(
              М,
              дійсність,
              іменовано_задіяне.назва,
              іменовано_задіяне.значення
            );

            п += 1;
          }
        }
      }

      змінна структура_предка: адреса<ПредметСтруктури> = пусто;
      якщо предмет_дії.метод_структури != пусто {
        структура_предка = предмет_дії.метод_структури.предок;
      }

      змінна результат = виконати_код(
        М,
        здійснюване.спроби::адреса,
        дійсність,
        здійснювач,
        предмет_дії.код як адреса<ПредметКоду>,
        структура_предка,
        предмет_дії.шлях_до_файлу_модуля
      );

      якщо не М.стан_падіння {
        видалити_з_історії_здійснення(М, 1);
      }

      звільнити_памʼять(М, задіяні);
      якщо іменовано_задіяні != пусто {
        знищити_іменовано_задіяні(М, іменовано_задіяні);
      }

      вернути результат;
    }
  }

  місцева дія предмет_дії_отримати_властивість(
    М: адреса<Машина>,
    предмет_дії: адреса<ПредметДії>,
    назва: адреса<ПредметТексту>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    вернути пусто;
  }
}