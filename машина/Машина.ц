            стрибнути крок_виконання_вказівки;
          }

          стрибнути крок_вихід;
        }

        якщо результат.вид == ЗНедійсне {
          покласти_в_накопичувач_значень(М, накопичувач_значень, значення_дійсне());
        } інакше {
