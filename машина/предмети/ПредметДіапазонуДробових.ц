взяти означення мавка;

простір мавка {
  місцева дія заповнити_структуру_ДіапазонДробових(
    М: адреса<Машина>
  ) {
    М.предмет_структури_ДіапазонДробових.заборонити_наслідування = так;
  }

  місцева дія створити_діапазон_дробових(
    М: адреса<Машина>,
    від: адреса<ПредметДробового>,
    до: адреса<ПредметДробового>,
    включно: логічне
  ) -> адреса<ПредметДіапазонуДробових> {
    змінна предмет = виділити_предмет<ПредметДіапазонуДробових>(М, М.предмет_структури_ДіапазонДробових);

    предмет.від = від;
    предмет.до = до;
    предмет.включно = включно;

    вернути предмет;
  }

  місцева дія предмет_діапазону_дробових_отримати_властивість(
    М: адреса<Машина>,
    предмет_діапазону_дробових: адреса<ПредметДіапазонуДробових>,
    назва: адреса<ПредметТексту>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    вернути пусто;
  }
}