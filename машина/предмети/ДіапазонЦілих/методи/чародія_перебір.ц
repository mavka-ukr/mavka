взяти означення мавка/машина;

простір мавка {
  дія рідна_дія_методу_структури_ДіапазонЦілих__чародія_перебір(
    М: адреса<Машина>,
    здійснювач: адреса<Предмет>,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ПредметАбоСтанПадіння {
    якщо здійснювач == пусто {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію діапазону цілих \"чародія_перебір\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути пусто;
    }

    змінна предмет_діапазону_цілих = здійснювач як адреса<ПредметДіапазонуЦілих>;

    якщо предмет_діапазону_цілих.тип != М.предмет_структури_ДіапазонЦілих {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію діапазону цілих \"чародія_перебір\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути пусто;
    }

    вернути значення_предмет(створити_перебір_діапазону_цілих(
      М,
      предмет_діапазону_цілих
    ));
  }

  місцева дія записати_метод_структури_ДіапазонЦілих__чародія_перебір(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._чародія_перебір,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_ДіапазонЦілих__чародія_перебір,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_ДіапазонЦілих,
      М.константи_назв._чародія_перебір,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"чародія_перебір\" для структури \"ДіапазонЦілих\".");
    }
  }
}