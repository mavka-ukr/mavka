взяти означення мавка/машина;

простір мавка {
  дія рідна_дія_метода_структури_ПеребірПослідовності__далі(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію перебору послідовності \"далі\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_перебору_послідовності = здійснювач.дані як адреса<Предмет> як адреса<ПредметПереборуПослідовності>;

    якщо предмет_перебору_послідовності.тип != М.предмет_структури_ПеребірПослідовності {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію перебору послідовності \"далі\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    якщо предмет_перебору_послідовності.почато {
      предмет_перебору_послідовності.позиція += 1;
    } інакше {
      предмет_перебору_послідовності.позиція = 0;
      предмет_перебору_послідовності.почато = так;
    }

    якщо предмет_перебору_послідовності.позиція < предмет_перебору_послідовності.предмет_послідовності.розмір {
      предмет_перебору_послідовності.поточне_значення = предмет_перебору_послідовності.предмет_послідовності.елементи[предмет_перебору_послідовності.позиція];

      вернути значення_дійсне();
    }

    вернути значення_недійсне();
  }

  дія записати_метод_структури_ПеребірПослідовності__далі(М: адреса<Машина>) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._далі,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_метода_структури_ПеребірПослідовності__далі,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_ПеребірПослідовності,
      М.константи_назв._далі,
      предмет_дії
    );
  }
}

простір мавка {
  місцева дія заповнити_структуру_ПеребірПослідовності(
    М: адреса<Машина>
  ) {
    М.предмет_структури_ПеребірПослідовності.заборонити_наслідування = так;

    записати_метод_структури_ПеребірПослідовності__далі(М);
  }

  місцева дія створити_перебір_послідовності(
    М: адреса<Машина>,
    предмет_послідовності: адреса<ПредметПослідовності>
  ) -> адреса<ПредметПереборуПослідовності> {
    змінна предмет = виділити_предмет<ПредметПереборуПослідовності>(М, М.предмет_структури_ПеребірПослідовності);

    предмет.предмет_послідовності = предмет_послідовності;
    предмет.почато = ні;
    предмет.позиція = 0;
    предмет.поточне_значення = значення_недійсне();

    вернути предмет;
  }

  місцева дія предмет_перебору_послідовності_отримати_властивість(
    М: адреса<Машина>,
    предмет_перебору_послідовності: адреса<ПредметПереборуПослідовності>,
    назва: адреса<ПредметТексту>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо назва == М.константи_назв._значення або перевірити_чи_предмети_тексту_рівні(М, назва, М.константи_назв._значення) {
      вернути предмет_перебору_послідовності.поточне_значення як Значення;
    }

    якщо назва == М.константи_назв._послідовність або перевірити_чи_предмети_тексту_рівні(М, назва, М.константи_назв._послідовність) {
      вернути значення_предмет(предмет_перебору_послідовності.предмет_послідовності);
    }

    змінна предмет_методу = знайти_метод_в_структурі(
      М,
      М.предмет_структури_ПеребірПослідовності,
      назва
    );

    якщо предмет_методу == пусто {
      вернути значення_недійсне();
    }

    вернути значення_предмет(предмет_методу);
  }
}