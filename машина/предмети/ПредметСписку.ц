взяти означення мавка/машина;

простір мавка {
  дія рідна_дія_методу_структури_Список__чародія_перебір(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"чародія_перебір\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_списку = здійснювач.дані як адреса<Предмет> як адреса<ПредметСписку>;

    якщо предмет_списку.тип != М.предмет_структури_Список {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"чародія_перебір\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    вернути значення_предмет(створити_перебір_списку(
      М,
      предмет_списку
    ));
  }

  дія записати_метод_структури_Список__чародія_перебір(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._чародія_перебір,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Список__чародія_перебір,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Список,
      М.константи_назв._чародія_перебір,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"чародія_перебір\" для структури \"Список\".");
    }
  }
}

простір мавка {
  дія рідна_дія_методу_структури_Список__чародія_текст(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"чародія_текст\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_списку = здійснювач.дані як адреса<Предмет> як адреса<ПредметСписку>;

    якщо предмет_списку.тип != М.предмет_структури_Список {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"чародія_текст\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    вернути значення_предмет(створити_текст(М, "<Список>"));
  }

  дія записати_метод_структури_Список__чародія_текст(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._чародія_текст,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Список__чародія_текст,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Список,
      М.константи_назв._чародія_текст,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"чародія_текст\" для структури \"Список\".");
    }
  }
}

простір мавка {
  дія рідна_дія_методу_структури_Список__додати(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"додати\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_списку = здійснювач.дані як адреса<Предмет> як адреса<ПредметСписку>;

    якщо предмет_списку.тип != М.предмет_структури_Список {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"додати\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна задіяне_значення = значення_недійсне();

    знайти_задіяне(М, задіяні, 0, М.константи_назв._значення, задіяне_значення::адреса);

    записати_в_список(
      М,
      предмет_списку,
      предмет_списку.розмір,
      задіяне_значення
    );

    вернути значення_ціле(ц64(предмет_списку.розмір));
  }

  дія записати_метод_структури_Список__додати(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._додати,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Список__додати,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Список,
      М.константи_назв._додати,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"додати\" для структури \"Список\".");
    }
  }
}

простір мавка {
  дія рідна_дія_методу_структури_Список__забрати(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"забрати\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_списку = здійснювач.дані як адреса<Предмет> як адреса<ПредметСписку>;

    якщо предмет_списку.тип != М.предмет_структури_Список {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"забрати\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    якщо предмет_списку.розмір == 0 {
      вернути значення_недійсне();
    }

    предмет_списку.розмір -= 1;

    вернути предмет_списку.елементи[предмет_списку.розмір];
  }

  дія записати_метод_структури_Список__забрати(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._забрати,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Список__забрати,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Список,
      М.константи_назв._забрати,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"забрати\" для структури \"Список\".");
    }
  }
}

простір мавка {
  дія рідна_дія_методу_структури_Список__знайти(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"знайти\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_списку = здійснювач.дані як адреса<Предмет> як адреса<ПредметСписку>;

    якщо предмет_списку.тип != М.предмет_структури_Список {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію списку \"знайти\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна задіяне_дія_перевірки = значення_недійсне();

    знайти_задіяне(М, задіяні, 0, М.константи_назв._дія_перевірки, задіяне_дія_перевірки::адреса);

    змінна п: природне = 0;

    поки п < предмет_списку.розмір {
      змінна поточне_значення = предмет_списку.елементи[п];

      змінна задіяні = Задіяні { 3, виділити_памʼять<Значення>(М, 3), пусто };
      задіяні.дані[0] = поточне_значення;
      задіяні.дані[1] = значення_ціле(ц64(п));
      задіяні.дані[2] = значення_предмет(предмет_списку);

      змінна результат_здійснення = здійснити(
        М,
        значення_недійсне(),
        задіяне_дія_перевірки,
        задіяні,
        Місцезнаходження { пусто, 0 }
      );

      якщо М.стан_падіння {
        вернути значення_недійсне();
      }

      якщо результат_здійснення.вид != ЗНедійсне {
        вернути поточне_значення;
      }

      п += 1;
    }

    вернути значення_недійсне();
  }

  дія записати_метод_структури_Список__знайти(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._знайти,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Список__знайти,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Список,
      М.константи_назв._знайти,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"знайти\" для структури \"Список\".");
    }
  }
}

простір мавка {
  місцева дія заповнити_структуру_Список(
    М: адреса<Машина>
  ) {
    М.предмет_структури_Список.заборонити_наслідування = так;

    записати_метод_структури_Список__чародія_перебір(М);
    записати_метод_структури_Список__чародія_текст(М);
    записати_метод_структури_Список__додати(М);
    записати_метод_структури_Список__забрати(М);
    записати_метод_структури_Список__знайти(М);
  }

  місцева дія створити_список(
    М: адреса<Машина>,
    розмір: природне
  ) -> адреса<ПредметСписку> {
    змінна предмет = виділити_предмет<ПредметСписку>(М, М.предмет_структури_Список);

    змінна дані: памʼять<Значення> = пусто;

    якщо розмір > 0 {
      дані = виділити_памʼять<Значення>(М, розмір);
      змінна п: природне = 0;

      поки п < розмір {
        дані[п] = значення_недійсне();

        п += 1;
      }
    }

    предмет.розмір = розмір;
    предмет.елементи = дані;

    вернути предмет;
  }

  місцева дія записати_в_список(
    М: адреса<Машина>,
    предмет_списку: адреса<ПредметСписку>,
    позиція: природне,
    значення: Значення
  ) {
    якщо позиція >= предмет_списку.розмір {
      предмет_списку.елементи = перевиділити_памʼять<Значення>(М, предмет_списку.елементи, позиція + 1);

      змінна п = предмет_списку.розмір;

      поки п < позиція + 1 {
        предмет_списку.елементи[п] = значення_недійсне();

        п += 1;
      }

      предмет_списку.розмір = позиція + 1;
    }

    предмет_списку.елементи[позиція] = значення;
  }

  місцева дія предмет_списку_отримати_властивість(
    М: адреса<Машина>,
    предмет_списку: адреса<ПредметСписку>,
    назва: адреса<ПредметТексту>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо назва == М.константи_назв._розмір або перевірити_чи_предмети_тексту_рівні(М, назва, М.константи_назв._розмір) {
      вернути значення_ціле(ц64(предмет_списку.розмір));
    }

    змінна предмет_методу = знайти_метод_в_структурі(
      М,
      М.предмет_структури_Список,
      назва
    );

    якщо предмет_методу == пусто {
      вернути значення_недійсне();
    }

    вернути значення_предмет(предмет_методу);
  }

  місцева дія предмет_списку_отримати_елемент(
    М: адреса<Машина>,
    предмет_списку: адреса<ПредметСписку>,
    ключ: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо ключ.вид == ЗЦіле {
      змінна позиція = природне(ключ.дані як ц64);

      якщо позиція >= 0 і позиція < предмет_списку.розмір {
        вернути предмет_списку.елементи[позиція];
      }
    }

    вернути значення_недійсне();
  }

  місцева дія предмет_списку_змінити_елемент(
    М: адреса<Машина>,
    предмет_списку: адреса<ПредметСписку>,
    ключ: Значення,
    значення_елемента: Значення,
    місцезнаходження: Місцезнаходження
  ) -> СтанПадіння {
    якщо ключ.вид == ЗЦіле {
      змінна позиція = ключ.дані як ц64;

      якщо позиція >= 0 {
        записати_в_список(М, предмет_списку, природне(позиція), значення_елемента);
      }
    }
  }
}