взяти означення мавка/машина;

простір мавка {
  місцева дія записати_метод_структури_ПеребірДаних__далі(М: адреса<Машина>);

  місцева дія заповнити_структуру_ПеребірДаних(
    М: адреса<Машина>
  ) {
    М.предмет_структури_ПеребірДаних.заборонити_наслідування = так;

    записати_метод_структури_ПеребірДаних__далі(М);
  }

  місцева дія створити_перебір_даних(
    М: адреса<Машина>,
    предмет_даних: адреса<ПредметДаних>
  ) -> адреса<ПредметПереборуДаних> {
    змінна предмет = виділити_предмет<ПредметПереборуДаних>(М, М.предмет_структури_ПеребірДаних);

    предмет.предмет_даних = предмет_даних;
    предмет.почато = ні;
    предмет.позиція = 0;
    предмет.поточне_значення = значення_недійсне();

    вернути предмет;
  }

  місцева дія предмет_перебору_даних_отримати_властивість(
    М: адреса<Машина>,
    предмет_перебору_даних: адреса<ПредметПереборуДаних>,
    назва: адреса<ПредметТексту>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    якщо назва == М.константи_назв._значення або перевірити_чи_предмети_тексту_рівні(М, назва, М.константи_назв._значення) {
      вернути предмет_перебору_даних.поточне_значення як Значення;
    }

    якщо назва == М.константи_назв._дані або перевірити_чи_предмети_тексту_рівні(М, назва, М.константи_назв._дані) {
      вернути значення_предмет(предмет_перебору_даних.предмет_даних);
    }

    змінна предмет_методу = знайти_метод_в_структурі(
      М,
      М.предмет_структури_ПеребірДаних,
      назва
    );

    якщо предмет_методу == пусто {
      вернути значення_недійсне();
    }

    вернути значення_предмет(предмет_методу);
  }
}