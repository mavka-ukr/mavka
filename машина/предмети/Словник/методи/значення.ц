взяти означення мавка/машина;

простір мавка {
  дія рідна_дія_методу_структури_Словник__значення(
    М: адреса<Машина>,
    здійснювач: адреса<Предмет>,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ПредметАбоСтанПадіння {
    якщо здійснювач == пусто {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"значення\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути пусто;
    }

    змінна предмет_словника = здійснювач як адреса<ПредметСловника>;

    якщо предмет_словника.тип != М.предмет_структури_Словник {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"значення\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути пусто;
    }

    змінна п: природне = 0;

    поки п < предмет_словника.розмір {
      п += 1;
    }

    вернути пусто;
  }

  місцева дія записати_метод_структури_Словник__значення(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._значення,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Словник__значення,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Словник,
      М.константи_назв._значення,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"значення\" для структури \"Словник\".");
    }
  }
}