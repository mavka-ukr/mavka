взяти означення мавка/машина;

простір мавка {
  дія рідна_дія_методу_структури_Дані__чародія_перебір(
    М: адреса<Машина>,
    діяльність: адреса<Діяльність>
  ) -> ЗначенняАбоСтанПадіння {
    якщо діяльність.здійснювач.вид != ЗПредмет {
      вернути ПАДІННЯ(М, створити_текст(М, "Неможливо здійснити дію даних \"чародія_перебір\"."));
    }

    змінна предмет_даних = діяльність.здійснювач.дані як адреса<Предмет> як адреса<ПредметДаних>;

    якщо предмет_даних.тип != М.предмет_структури_Дані {
      вернути ПАДІННЯ(М, створити_текст(М, "Неможливо здійснити дію даних \"чародія_перебір\"."));
    }

    вернути ПРЕДМЕТ(створити_перебір_даних(
      М,
      предмет_даних
    ));
  }

  місцева дія записати_метод_структури_Дані__чародія_перебір(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._чародія_перебір,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Дані__чародія_перебір,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Дані,
      М.константи_назв._чародія_перебір,
      предмет_дії
    );

    якщо М.стан_падіння {
      ЗАПАНІКУВАТИ(М, "Помилка при записі дії \"чародія_перебір\" для структури \"Дані\".");
    }
  }
}