взяти означення мавка/машина;

простір мавка {
  дія рідна_дія_методу_структури_ЗмінніДані__чародія_перебір(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію змінних_даних \"чародія_перебір\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_змінних_даних = здійснювач.дані як адреса<Предмет> як адреса<ПредметЗміннихДаних>;

    якщо предмет_змінних_даних.тип != М.предмет_структури_ЗмінніДані {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію змінних_даних \"чародія_перебір\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    // вернути значення_предмет(створити_перебір_змінних_даних(
    //   М,
    //   предмет_змінних_даних
    // ));

    вернути значення_недійсне();
  }

  дія записати_метод_структури_ЗмінніДані__чародія_перебір(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._чародія_перебір,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_ЗмінніДані__чародія_перебір,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_ЗмінніДані,
      М.константи_назв._чародія_перебір,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"чародія_перебір\" для структури \"ЗмінніДані\".");
    }
  }
}

простір мавка {
  місцева дія заповнити_структуру_ЗмінніДані(
    М: адреса<Машина>
  ) {
    М.предмет_структури_ЗмінніДані.заборонити_наслідування = так;

    записати_метод_структури_ЗмінніДані__чародія_перебір(М);
  }

  місцева дія створити_змінні_дані(
    М: адреса<Машина>,
    розмір: природне,
    дані: памʼять<п8>
  ) -> адреса<ПредметЗміннихДаних> {
    вернути пусто;
  }

  місцева дія створити_змінні_дані_без_копіювання(
    М: адреса<Машина>,
    розмір: природне,
    дані: памʼять<п8>
  ) -> адреса<ПредметЗміннихДаних> {
    вернути пусто;
  }

  місцева дія предмет_змінних_даних_отримати_властивість(
    М: адреса<Машина>,
    предмет_змінних_даних: адреса<ПредметЗміннихДаних>,
    назва: адреса<ПредметТексту>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    змінна предмет_методу = знайти_метод_в_структурі(
      М,
      М.предмет_структури_ЗмінніДані,
      назва
    );

    якщо предмет_методу == пусто {
      вернути значення_недійсне();
    }

    вернути значення_предмет(предмет_методу);
  }

  місцева дія предмет_змінних_даних_отримати_елемент(
    М: адреса<Машина>,
    предмет_змінних_даних: адреса<ПредметЗміннихДаних>,
    ключ: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    вернути значення_недійсне();
  }

  місцева дія предмет_змінних_даних_змінити_елемент(
    М: адреса<Машина>,
    предмет_змінних_даних: адреса<ПредметЗміннихДаних>,
    ключ: Значення,
    значення_елемента: Значення,
    місцезнаходження: Місцезнаходження
  ) -> СтанПадіння {
  }
}