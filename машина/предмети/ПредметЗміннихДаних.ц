взяти означення мавка/машина;

простір мавка {
  місцева дія заповнити_структуру_ЗмінніДані(
    М: адреса<Машина>
  ) {
    М.предмет_структури_ЗмінніДані.заборонити_наслідування = так;
  }

  місцева дія створити_змінні_дані(
    М: адреса<Машина>,
    розмір: природне,
    дані: памʼять<п8>
  ) -> адреса<ПредметЗміннихДаних> {
    вернути пусто;
  }

  місцева дія створити_змінні_дані_без_копіювання(
    М: адреса<Машина>,
    розмір: природне,
    дані: памʼять<п8>
  ) -> адреса<ПредметЗміннихДаних> {
    вернути пусто;
  }

  місцева дія предмет_змінних_даних_отримати_властивість(
    М: адреса<Машина>,
    предмет_змінних_даних: адреса<ПредметЗміннихДаних>,
    назва: адреса<ПредметТексту>,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    змінна предмет_методу = знайти_метод_в_структурі(
      М,
      М.предмет_структури_ЗмінніДані,
      назва
    );

    якщо предмет_методу == пусто {
      вернути значення_недійсне();
    }

    вернути значення_предмет(предмет_методу);
  }

  місцева дія предмет_змінних_даних_отримати_елемент(
    М: адреса<Машина>,
    предмет_змінних_даних: адреса<ПредметЗміннихДаних>,
    ключ: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    вернути значення_недійсне();
  }

  місцева дія предмет_змінних_даних_змінити_елемент(
    М: адреса<Машина>,
    предмет_змінних_даних: адреса<ПредметЗміннихДаних>,
    ключ: Значення,
    значення_елемента: Значення,
    місцезнаходження: Місцезнаходження
  ) -> СтанПадіння {
  }

  місцева дія предмет_змінних_даних_містить(
    М: адреса<Машина>,
    предмет_змінних_даних: адреса<ПредметЗміннихДаних>,
    значення: Значення,
    місцезнаходження: Місцезнаходження
  ) -> ЗначенняАбоСтанПадіння {
    вернути значення_недійсне();
  }
}