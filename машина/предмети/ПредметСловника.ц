взяти означення мавка/машина;

простір мавка {
  дія рідна_дія_методу_структури_Словник__чародія_текст(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"чародія_текст\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_словника = здійснювач.дані як адреса<Предмет> як адреса<ПредметСловника>;

    якщо предмет_словника.тип != М.предмет_структури_Словник {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"чародія_текст\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    вернути значення_предмет(створити_текст(М, "<Словник>"));
  }

  дія записати_метод_структури_Словник__чародія_текст(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._чародія_текст,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Словник__чародія_текст,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Словник,
      М.константи_назв._чародія_текст,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"чародія_текст\" для структури \"Словник\".");
    }
  }
}

простір мавка {
  дія рідна_дія_методу_структури_Словник__чародія_перебір(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"чародія_перебір\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_словника = здійснювач.дані як адреса<Предмет> як адреса<ПредметСловника>;

    якщо предмет_словника.тип != М.предмет_структури_Словник {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"чародія_перебір\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    вернути значення_предмет(створити_перебір_словника(
      М,
      предмет_словника
    ));
  }

  дія записати_метод_структури_Словник__чародія_перебір(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._чародія_перебір,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Словник__чародія_перебір,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Словник,
      М.константи_назв._чародія_перебір,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"чародія_перебір\" для структури \"Словник\".");
    }
  }
}

простір мавка {
  дія рідна_дія_методу_структури_Словник__ключі(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"ключі\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_словника = здійснювач.дані як адреса<Предмет> як адреса<ПредметСловника>;

    якщо предмет_словника.тип != М.предмет_структури_Словник {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"ключі\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна п: природне = 0;

    поки п < предмет_словника.розмір {
      п += 1;
    }

    вернути значення_недійсне();
  }

  дія записати_метод_структури_Словник__ключі(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._ключі,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Словник__ключі,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Словник,
      М.константи_назв._ключі,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"ключі\" для структури \"Словник\".");
    }
  }
}

простір мавка {
  дія рідна_дія_методу_структури_Словник__значення(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"значення\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_словника = здійснювач.дані як адреса<Предмет> як адреса<ПредметСловника>;

    якщо предмет_словника.тип != М.предмет_структури_Словник {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію словника \"значення\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна п: природне = 0;

    поки п < предмет_словника.розмір {
      п += 1;
    }

    вернути значення_недійсне();
  }

  дія записати_метод_структури_Словник__значення(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
