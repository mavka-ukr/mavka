взяти означення мавка;

простір мавка {
  місцева дія створити_складений_предмет(
    М: адреса<Машина>,
    значення_структури: адреса<ПредметСтруктури>
  ) -> адреса<СкладенийПредмет> {
    змінна предмет = виділити_предмет<СкладенийПредмет>(М, значення_структури);

    предмет.властивості = Властивості { 0, пусто };

    вернути предмет;
  }
}