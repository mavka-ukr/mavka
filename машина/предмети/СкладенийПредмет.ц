взяти означення мавка/машина;

простір мавка {
  місцева дія створити_складений_предмет(
    М: адреса<Машина>,
    значення_структури: адреса<ПредметСтруктури>
  ) -> адреса<СкладенийПредмет> {
    змінна предмет = виділити_предмет<СкладенийПредмет>(М, значення_структури);

    предмет.властивості = Властивості { 0, пусто, 0 };

    вернути предмет;
  }

  місцева дія складений_предмет_отримати_властивість(
    М: адреса<Машина>,
    складений_предмет: адреса<СкладенийПредмет>,
    назва: адреса<ПредметТексту>,
    місцезнаходження: Місцезнаходження
  ) -> ПредметАбоСтанПадіння {
    змінна результат: адреса<Предмет>;

    якщо знайти_властивість(
      М,
      складений_предмет.властивості,
      назва,
      результат::адреса
    ) {
      вернути результат;
    }

    змінна предмет_методу = знайти_метод_в_структурі(
      М,
      складений_предмет.тип,
      назва
    );

    якщо предмет_методу == пусто {
      вернути пусто;
    }

    вернути значення_предмет(предмет_методу);
  }

  місцева дія складений_предмет_змінити_властивість(
    М: адреса<Машина>,
    складений_предмет: адреса<СкладенийПредмет>,
    назва: адреса<ПредметТексту>,
    значення: адреса<Предмет>,
    місцезнаходження: Місцезнаходження
  ) {
    записати_властивість(
      М,
      складений_предмет.властивості::адреса,
      назва,
      значення
    );
  }
}