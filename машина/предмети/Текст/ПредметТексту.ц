взяти означення мавка/машина;

простір мавка {
  дія рідна_дія_методу_структури_Текст__чародія_ціле(
    М: адреса<Машина>,
    здійснювач: Значення,
    предмет_дії: адреса<ПредметДії>,
    задіяні: Задіяні
  ) -> ЗначенняАбоСтанПадіння {
    якщо здійснювач.вид != ЗПредмет {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію тексту \"чародія_ціле\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    змінна предмет_тексту = здійснювач.дані як адреса<Предмет> як адреса<ПредметТексту>;

    якщо предмет_тексту.тип != М.предмет_структури_Текст {
      увімкнути_стан_падіння(
        М,
        значення_предмет(створити_текст(М, "Неможливо здійснити дію тексту \"чародія_ціле\".")),
        Місцезнаходження { пусто, 0 }
      );
      вернути значення_недійсне();
    }

    вернути значення_недійсне();
  }

  дія записати_метод_структури_Текст__чародія_ціле(
    М: адреса<Машина>
  ) {
    змінна предмет_дії = створити_рідну_дію(
      М,
      М.константи_назв._чародія_ціле,
      М.предмет_пустої_послідовності,
      пусто,
      рідна_дія_методу_структури_Текст__чародія_ціле,
      пусто
    );

    записати_метод_структури(
      М,
      М.предмет_структури_Текст,
      М.константи_назв._чародія_ціле,
      предмет_дії
    );

    якщо М.стан_падіння {
      запанікувати(М, "Помилка при записі дії \"чародія_ціле\" для структури \"Текст\".");
    }
  }
}

