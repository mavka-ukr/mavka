с = [2, 1, 10, 4, 3, 1]
друк(с)
с.додати(99)
друк(с)
с.забрати()
друк(с)
с2 = с.фільтрувати(дія(значення): значення > 5)
;;с2 = []
друк(с2)
друк(с2 містить 5)
друк(с2 не містить 5)
друк(с2 містить 10)
друк(с.знайти(дія(значення): значення більше 2))
друк(с2[2])
с2[2] = 319
друк(с2[2])
;;друк(с2.чародія_перебір())
сл = [
  "гроші"="кров",
  "влада"="мозок",
  "бізнес"="серце",
  "культура"="душа",
  2=100
]
друк(сл["влада"])
друк(сл[2])
сл[100] = 2
друк(сл)
друк(сл[100])
;;друк(сл.визначення())
;;друк(сл.значення())
;;сл.видалити("культура")
друк(сл)
друк(сл містить "гроші")
друк(сл містить "культура")
друк(сл містить 2)
друк(сл містить 2131)
друк("старт перебір")
;*
перебрати сл як в
  друк(в)
  друк(сл[в])
кінець
*;
друк("стоп перебір")
;;сл.очистити()
друк(сл)

