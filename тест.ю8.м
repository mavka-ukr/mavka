взяти біб інтернет

дія обробити_підключення_звʼязку(звʼязок)
  друк("підключено")
кінець

дія обробити_дані_звʼязку(звʼязок, дані)
    вивести("дані", юнікод(дані))
кінець

дія обробити_відключення_звʼязку(звʼязок)
  друк("відключено")
кінець

дія обробити_помилку_звʼязку(звʼязок, помилка)
  друк("помилка!", помилка)
кінець

звʼязок = інтернет.створити_звʼязок("127.0.0.1", 8080)

звʼязок.обробник_підключення = обробити_підключення_звʼязку
звʼязок.обробник_даних = обробити_дані_звʼязку
звʼязок.обробник_відключення = обробити_відключення_звʼязку
звʼязок.обробник_помилки = обробити_помилку_звʼязку

звʼязок.підключити()