секція РМв1 {
  структура Місцезнаходження;
  структура ІнформаціяПадіння;
  структура Предмет;
  структура Розширення;
  структура ІменованіАргументи;
  структура ЕлементСловника;
  структура Спроба;
  структура Байти;
  структура ЕлементиСписку;
  структура ЕлементиСловника;
  структура ПритримувачПредметів;
  структура ГлобальноПритриманийПредмет;

  синонім ПредметАбоСтанПадіння = адреса<Предмет>;

  структура Місцезнаходження {
    шлях: адреса<Предмет>;
    рядок: натуральне;
  }

  структура ЕлементІсторіїВиконання {
    місцезнаходження: Місцезнаходження;
    назва: т8;
  }

  структура ІнформаціяПадіння {
    місцезнаходження: Місцезнаходження;
    розмір_історії: натуральне;
    історія: памʼять<ЕлементІсторіїВиконання>;
  }

  структура ЕлементСловника {
    ключ: адреса<Предмет>;
    значення: адреса<Предмет>;
  }

  структура ЕлементМодуля {
    назва: адреса<Предмет>;
    значення: адреса<Предмет>;
  }

  структура Байти {
    розмір: натуральне;
    дані: памʼять<н8>;
  }

  структура ЕлементиСписку {
    розмір: натуральне;
    дані: памʼять<адреса<Предмет>>;
  }

  структура ЕлементиСловника {
    розмір: натуральне;
    дані: памʼять<ЕлементСловника>;
  }

  синонім РіднаДія = (Р: адреса<Розширення>, предмет_дії: адреса<Предмет>, предмет_я: адреса<Предмет>, кількість_аргументів: натуральне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння;
  синонім ЗробитиКопіюДанихДії = (Р: адреса<Розширення>, предмет_дії: адреса<Предмет>) -> невідома_адреса;
  синонім ВідкликПередЗнищеннямДії = (Р: адреса<Розширення>, предмет_дії: адреса<Предмет>) -> ніщо;

  структура Розширення {
    М: невідома_адреса;
    основний_притримувач: адреса<ПритримувачПредметів>;
    дані: невідома_адреса;
    відклик_перед_знищенням: (Р: адреса<Розширення>) -> ніщо;
    предмет_модуля: адреса<Предмет>;
    предмет_логічного_так: адреса<Предмет>;
    предмет_логічного_ні: адреса<Предмет>;
    вивести_ю8: (Р: адреса<Розширення>, значення: адреса<ю8>) -> ніщо;
    надрукувати_ю8: (Р: адреса<Розширення>, значення: адреса<ю8>) -> ніщо;
    вивести: (Р: адреса<Розширення>, значення: адреса<т8>) -> ніщо;
    надрукувати: (Р: адреса<Розширення>, значення: адреса<т8>) -> ніщо;
    почати_спробу: (Р: адреса<Розширення>) -> адреса<Спроба>;
    знищити_спробу: (Р: адреса<Розширення>, спроба: адреса<Спроба>) -> ніщо;
    перевірити_чи_стан_падіння: (Р: адреса<Розширення>) -> логічне;
    увімкнути_стан_падіння: (Р: адреса<Розширення>, місцезнаходження: адреса<Місцезнаходження>) -> ніщо;
    вимкнути_стан_падіння: (Р: адреса<Розширення>, спроба: адреса<Спроба>) -> ніщо;
    отримати_інформацію_падіння: (Р: адреса<Розширення>, вихід: адреса<адреса<ІнформаціяПадіння>>) -> логічне;
    знищити_інформацію_падіння: (Р: адреса<Розширення>, інформація_падіння: адреса<ІнформаціяПадіння>) -> ніщо;
    створити_рідну_дію: (Р: адреса<Розширення>, назва: адреса<Предмет>, кількість_параметрів: натуральне, параметри: памʼять<адреса<Предмет>>, рідна_дія: РіднаДія) -> ПредметАбоСтанПадіння;
    отримати_дані_рідної_дії: (Р: адреса<Розширення>, предмет_дії: адреса<Предмет>) -> невідома_адреса;
    створити_число: (Р: адреса<Розширення>, значення: р64) -> ПредметАбоСтанПадіння;
    створити_текст: (Р: адреса<Розширення>, значення: адреса<т8>) -> ПредметАбоСтанПадіння;
    створити_текст_без_копіювання: (Р: адреса<Розширення>, значення: адреса<т8>) -> ПредметАбоСтанПадіння;
    створити_список: (Р: адреса<Розширення>, кількість_елементів: натуральне, елементи: памʼять<адреса<Предмет>>) -> ПредметАбоСтанПадіння;
    створити_словник: (Р: адреса<Розширення>, кількість_елементів: натуральне, елементи: памʼять<ЕлементСловника>) -> ПредметАбоСтанПадіння;
    створити_байти: (Р: адреса<Розширення>, розмір: натуральне, дані: памʼять<н8>) -> ПредметАбоСтанПадіння;
    створити_байти_без_копіювання: (Р: адреса<Розширення>, розмір: натуральне, дані: памʼять<н8>) -> ПредметАбоСтанПадіння;
    створити_модуль: (Р: адреса<Розширення>, назва: адреса<Предмет>, кількість_елементів: натуральне, елементи: памʼять<ЕлементМодуля>) -> ПредметАбоСтанПадіння;
    створити_параметр: (Р: адреса<Розширення>, назва: адреса<Предмет>) -> ПредметАбоСтанПадіння;
    предмет_додати: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_відняти: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_помножити: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_поділити: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_остача: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_неповна_частка: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_степінь: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_зсунути_вліво: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_зсунути_вправо: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_зсунути_вправо_2: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_двійкове_і: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_двійкове_або: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_двійкове_виключне_або: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_двійкове_заперечення: (Р: адреса<Розширення>, предмет: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_менше: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_більше: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_містить: (Р: адреса<Розширення>, предмет: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_числова_негація: (Р: адреса<Розширення>, предмет: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_отримати_число: (Р: адреса<Розширення>, предмет: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_отримати_текст: (Р: адреса<Розширення>, предмет: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_отримати_перебір: (Р: адреса<Розширення>, предмет: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_отримати_діапазон: (Р: адреса<Розширення>, предмет: адреса<Предмет>, включно: логічне, до: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ПредметАбоСтанПадіння;
    предмет_змінити_властивість: (Р: адреса<Розширення>, предмет: адреса<Предмет>, назва: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: адреса<Місцезнаходження>) -> ніщо;
    перевірити_чи_предмет_є_дією: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> логічне;
    перевірити_чи_предмет_є_числом: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> логічне;
    перевірити_чи_предмет_є_текстом: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> логічне;
    перевірити_чи_предмет_є_списком: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> логічне;
    перевірити_чи_предмет_є_словником: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> логічне;
    перевірити_чи_предмет_є_байтами: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> логічне;
    перевірити_чи_предмет_є_модулем: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> логічне;
    перевірити_чи_предмет_є_параметром: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> логічне;
    отримати_значення_числа: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> р64;
    отримати_значення_тексту: (Р: адреса<Розширення>, предмет: адреса<Предмет>, вихід: адреса<т8>) -> ніщо;
    отримати_копію_значення_тексту: (Р: адреса<Розширення>, предмет: адреса<Предмет>, вихід: адреса<т8>) -> логічне;
    отримати_копію_елементів_списку: (Р: адреса<Розширення>, предмет: адреса<Предмет>, вихід: адреса<ЕлементиСписку>) -> логічне;
    отримати_копію_елементів_словника: (Р: адреса<Розширення>, предмет: адреса<Предмет>, вихід: адреса<ЕлементиСловника>) -> логічне;
    отримати_значення_байтів: (Р: адреса<Розширення>, предмет: адреса<Предмет>, вихід: адреса<Байти>) -> ніщо;
    отримати_копію_значення_байтів: (Р: адреса<Розширення>, предмет: адреса<Предмет>, вихід: адреса<Байти>) -> логічне;
    покласти_притримувач: (Р: адреса<Розширення>) -> адреса<ПритримувачПредметів>;
    притримати: (Р: адреса<Розширення>, притримувач: адреса<ПритримувачПредметів>, предмет: адреса<Предмет>) -> ніщо;
    відпустити: (Р: адреса<Розширення>, притримувач: адреса<ПритримувачПредметів>) -> ніщо;
    притримати_глобально: (Р: адреса<Розширення>, предмет: адреса<Предмет>) -> адреса<ГлобальноПритриманийПредмет>;
    відпустити_глобально: (Р: адреса<Розширення>, глобально_притриманий_предмет: адреса<ГлобальноПритриманийПредмет>) -> ніщо;
    забрати_притримувач: (Р: адреса<Розширення>) -> ніщо;
    назва: (Р: адреса<Розширення>, значення: адреса<т8>) -> адреса<Предмет>;
    шлях_до_файлу_розширення: адреса<Предмет>;
  }
}

секція РМв1 {
  дія отримати_основний_притримувач(Р: адреса<Розширення>) -> адреса<ПритримувачПредметів> {
    вернути Р.основний_притримувач;
  }

  дія змінити_дані(Р: адреса<Розширення>, значення: невідома_адреса) {
    Р.дані = значення;
  }

  дія змінити_відклик_перед_знищенням(Р: адреса<Розширення>, відклик_перед_знищенням: (Р: адреса<Розширення>) -> ніщо) {
    Р.відклик_перед_знищенням = відклик_перед_знищенням;
  }

  дія вивести_ю8(Р: адреса<Розширення>, значення: ю8) {
    Р.вивести_ю8(Р, значення::адреса);
  }

  дія надрукувати_ю8(Р: адреса<Розширення>, значення: ю8) {
    Р.надрукувати_ю8(Р, значення::адреса);
  }

  дія вивести(Р: адреса<Розширення>, значення: т8) {
    Р.вивести(Р, значення::адреса);
  }

  дія надрукувати(Р: адреса<Розширення>, значення: т8) {
    Р.надрукувати(Р, значення::адреса);
  }

  дія почати_спробу(Р: адреса<Розширення>) -> адреса<Спроба> {
    вернути Р.почати_спробу(Р);
  }

  дія знищити_спробу(Р: адреса<Розширення>, спроба: адреса<Спроба>) {
    Р.знищити_спробу(Р, спроба);
  }

  дія перевірити_чи_стан_падіння(Р: адреса<Розширення>) -> логічне {
    вернути Р.перевірити_чи_стан_падіння(Р);
  }

  дія увімкнути_стан_падіння(Р: адреса<Розширення>, місцезнаходження: Місцезнаходження) {
    Р.увімкнути_стан_падіння(Р, місцезнаходження::адреса);
  }

  дія вимкнути_стан_падіння(Р: адреса<Розширення>, спроба: адреса<Спроба>) {
    Р.вимкнути_стан_падіння(Р, спроба);
  }

  дія падіння(Р: адреса<Розширення>, значення: адреса<Предмет>) -> ПредметАбоСтанПадіння {
    увімкнути_стан_падіння(Р, Місцезнаходження { пусто, 0 });
    вернути значення;
  }

  дія отримати_інформацію_падіння(Р: адреса<Розширення>, вихід: адреса<адреса<ІнформаціяПадіння>>) -> логічне {
    вернути Р.отримати_інформацію_падіння(Р, вихід);
  }

  дія знищити_інформацію_падіння(Р: адреса<Розширення>, інформація_падіння: адреса<ІнформаціяПадіння>) {
    Р.знищити_інформацію_падіння(Р, інформація_падіння);
  }

  дія отримати_предмет_логічного_так(Р: адреса<Розширення>) -> адреса<Предмет> {
    вернути Р.предмет_логічного_так;
  }

