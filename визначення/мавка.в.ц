  структура Система {
    виділити_сиру_памʼять: (М: адреса<Машина>, розмір: натуральне) -> невідома_адреса;
    перевиділити_сиру_памʼять: (М: адреса<Машина>, значення: невідома_адреса, розмір: натуральне) -> невідома_адреса;
    звільнити_сиру_памʼять: (М: адреса<Машина>, значення: невідома_адреса) -> ніщо;
    вийти: (М: адреса<Машина>, код: ц32) -> ніщо;
    прочитати_файл: (М: адреса<Машина>, шлях: адреса<ПредметБайтів>) -> ПредметАбоСтанПадіння<ПредметБайтів>;
    отримати_абсолютний_шлях: (М: адреса<Машина>, шлях: адреса<ПредметБайтів>) -> ПредметАбоСтанПадіння<ПредметБайтів>;
    отримати_абсолютний_зовнішній_шлях: (М: адреса<Машина>, шлях: адреса<ПредметБайтів>) -> ПредметАбоСтанПадіння<ПредметБайтів>;
    отримати_назву_файлу_без_розширення: (М: адреса<Машина>, шлях: адреса<ПредметБайтів>) -> ПредметАбоСтанПадіння<ПредметТексту>;
    перевірити_чи_шлях_існує_і_є_файлом: (М: адреса<Машина>, шлях: адреса<ПредметБайтів>) -> логічне;
    вивести: (М: адреса<Машина>, значення: т8) -> ніщо;
    вивести_ю8: (М: адреса<Машина>, значення: ю8) -> ніщо;
    вивести_байти: (М: адреса<Машина>, розмір: натуральне, дані: памʼять<н8>) -> ніщо;
    спустити_виведене: (М: адреса<Машина>) -> ніщо;
    перетворити_ц64_на_т8: (М: адреса<Машина>, значення: ц64, вихід: адреса<т8>) -> логічне;
    перетворити_н64_на_т8: (М: адреса<Машина>, значення: н64, вихід: адреса<т8>) -> логічне;
    перетворити_р64_на_т8: (М: адреса<Машина>, значення: р64, вихід: адреса<т8>) -> логічне;
    перетворити_т8_на_ц64: (М: адреса<Машина>, значення: т8, вихід: адреса<ц64>, вихід_позиції_помилки: адреса<натуральне>) -> логічне;
    перетворити_т8_на_н64: (М: адреса<Машина>, значення: т8, вихід: адреса<н64>, вихід_позиції_помилки: адреса<натуральне>) -> логічне;
    перетворити_т8_на_р64: (М: адреса<Машина>, значення: т8, вихід: адреса<р64>, вихід_позиції_помилки: адреса<натуральне>) -> логічне;
    перекодувати_ю8_в_т8: (М: адреса<Машина>, опції: ОпціїПерекодуванняЮ8вТ8, вхід: ю8, вихід: адреса<т8>, вихід_місцезнаходжень: адреса<МісцезнаходженняПерекодування>, вихід_позиції_помилки: адреса<натуральне>) -> логічне;
    перекодувати_т8_в_ю8: (М: адреса<Машина>, опції: ОпціїПерекодуванняТ8вЮ8, вхід: т8, вихід: адреса<ю8>, вихід_місцезнаходжень: адреса<МісцезнаходженняПерекодування>, вихід_позиції_помилки: адреса<натуральне>) -> логічне;
    кодування_шляху: н8;
    кодування_файлів: н8;
    завантажити_розширення: (М: адреса<Машина>, предмет_модуля: адреса<ПредметМодуля>, шлях: адреса<ПредметБайтів>) -> ніщо;
  }

  структура Машина {
    дані: невідома_адреса;

    система: Система;

    останній_предмет: адреса<Предмет>;
    кількість_предметів: натуральне;
    кількість_предметів_для_очищення: натуральне;
    потребує_очищення: натуральне;

    взяті_предмети: Реєстр<адреса<ПредметБайтів>, адреса<Предмет>>;
    обробник_взяття: (М: адреса<Машина>, назва_паку: адреса<ПредметТексту>, назва_репозиторія: адреса<ПредметТексту>, кількість_назв_шляху: натуральне, назви_шляху: памʼять<адреса<ПредметТексту>>) -> ПредметАбоСтанПадіння<Предмет>;

    предмет_глобального_середовища: адреса<ПредметСередовища>;
    // позначка: прохід по типах предмета
    предмет_структури_предмет: адреса<ПредметСтруктури>;
    предмет_структури_Структура: адреса<ПредметСтруктури>;
    предмет_структури_Код: адреса<ПредметСтруктури>;
    предмет_структури_Середовище: адреса<ПредметСтруктури>;
    предмет_структури_Дія: адреса<ПредметСтруктури>;
    предмет_структури_логічне: адреса<ПредметСтруктури>;
    предмет_структури_число: адреса<ПредметСтруктури>;
    предмет_структури_текст: адреса<ПредметСтруктури>;
    предмет_структури_список: адреса<ПредметСтруктури>;
    предмет_структури_послідовність: адреса<ПредметСтруктури>;
    предмет_структури_словник: адреса<ПредметСтруктури>;
    предмет_структури_байти: адреса<ПредметСтруктури>;
    предмет_структури_Буфер: адреса<ПредметСтруктури>;
    предмет_структури_Модуль: адреса<ПредметСтруктури>;
    предмет_структури_Параметр: адреса<ПредметСтруктури>;
    предмет_структури_ДіапазонЧисел: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірДіапазонуЧисел: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірСписку: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірПослідовності: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірТексту: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірБайтів: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірБуфера: адреса<ПредметСтруктури>;
    предмет_логічного_так: адреса<ПредметЛогічного>;
    предмет_логічного_ні: адреса<ПредметЛогічного>;

    історія: Накопичувач<ЕлементІсторії>;
    накопичувач_предметів: Накопичувач<адреса<Предмет>>;
    притримувачі_предметів: Накопичувач<адреса<Накопичувач<адреса<Предмет>>>>;
    стан_падіння: натуральне;
    місцезнаходження_падіння: Місцезнаходження;
    предмет_падіння: адреса<Предмет>;

    черга_запланованих_виконань: Черга<ЗапланованеВиконання>;

    глобально_притримані_предмети: ДвосторонньоПовʼязанийСписок<адреса<Предмет>>;
  }

  структура ЗбереженийСтанРозмірівМашини {
    розмір_історії: натуральне;
    розмір_накопичувача_предметів: натуральне;
    розмір_притримувачів_предметів: натуральне;
  }

  дія виділити<Т>(М: адреса<Машина>): адреса<Т> {
    вернути М.система.виділити_сиру_памʼять(М, Т.розмір) як адреса<Т>;
  }

  дія перевиділити<Т>(М: адреса<Машина>, значення: адреса<Т>, кількість: натуральне): памʼять<Т> {
    вернути М.система.перевиділити_сиру_памʼять(М, значення як памʼять<н8>, кількість * Т.розмір) як памʼять<Т>;
  }

  дія звільнити(М: адреса<Машина>, значення: невідома_адреса) {
    М.система.звільнити_сиру_памʼять(М, значення);
  }

  дія виділити_памʼять<Т>(М: адреса<Машина>, кількість: натуральне): памʼять<Т> {
    вернути М.система.виділити_сиру_памʼять(М, кількість * Т.розмір) як памʼять<Т>;
  }

  дія перевиділити_памʼять<Т>(М: адреса<Машина>, значення: памʼять<Т>, кількість: натуральне): памʼять<Т> {
    вернути М.система.перевиділити_сиру_памʼять(М, значення як невідома_адреса, кількість * Т.розмір) як памʼять<Т>;
  }

  дія звільнити_памʼять(М: адреса<Машина>, значення: невідома_памʼять) {
    М.система.звільнити_сиру_памʼять(М, значення як невідома_адреса);
  }

  дія ПОМЕРТИ(М: адреса<Машина>, повідомлення: т8) {
    М.система.вийти(М, 1);
  }

  дія увімкнути_стан_падіння(М: адреса<Машина>, місцезнаходження_падіння: Місцезнаходження, предмет_падіння: адреса<Предмет>) {
    М.стан_падіння = натуральне(так);
    М.місцезнаходження_падіння = місцезнаходження_падіння;
    М.предмет_падіння = предмет_падіння;
  }

  // не забувати очистити історію!!!
  дія вимкнути_стан_падіння(М: адреса<Машина>) {
    М.стан_падіння = натуральне(ні);
    М.місцезнаходження_падіння = Місцезнаходження { пусто як адреса<ПредметТексту>, 0 };
    М.предмет_падіння = пусто;
  }

  дія результат_стан_падіння(М: адреса<Машина>, місцезнаходження_падіння: Місцезнаходження, предмет_падіння: адреса<Предмет>): адреса<Предмет> {
    увімкнути_стан_падіння(М, місцезнаходження_падіння, предмет_падіння);
    вернути пусто;
  }
}

// Допоміжне
секція мавка {
  місцева дія перевірити_чи_т8_рівні(а: т8, б: т8): логічне;

  місцева дія виділити_копію_т8(М: адреса<Машина>, значення: т8, вихід: адреса<т8>): логічне;
  місцева дія виділити_копію_ю32(М: адреса<Машина>, значення: ю32, вихід: адреса<ю32>): логічне;

  місцева дія виділити_копію_памʼяті(М: адреса<Машина>, розмір: натуральне, значення: памʼять<н8>, вихід: адреса<памʼять<н8>>): логічне;

  дія перекодувати_ю8_в_т8(М: адреса<Машина>, опції: ОпціїПерекодуванняЮ8вТ8, вхід: ю8, вихід: адреса<т8>, вихід_місцезнаходжень: адреса<МісцезнаходженняПерекодування>, вихід_позиції_помилки: адреса<натуральне>) -> логічне {
    вернути М.система.перекодувати_ю8_в_т8(М, опції, вхід, вихід, вихід_місцезнаходжень, вихід_позиції_помилки);
  }

  дія перекодувати_т8_в_ю8(М: адреса<Машина>, опції: ОпціїПерекодуванняТ8вЮ8, вхід: т8, вихід: адреса<ю8>, вихід_місцезнаходжень: адреса<МісцезнаходженняПерекодування>, вихід_позиції_помилки: адреса<натуральне>) -> логічне {
    вернути М.система.перекодувати_т8_в_ю8(М, опції, вхід, вихід, вихід_місцезнаходжень, вихід_позиції_помилки);
  }
}

// Машина
секція мавка {
  місцева дія налаштувати_машину(М: адреса<Машина>, система: Система);
  місцева дія розлаштувати_машину(М: адреса<Машина>);
  місцева дія виконати_код(М: адреса<Машина>, код: адреса<ПредметКоду>, середовище: адреса<ПредметСередовища>) -> ПредметАбоСтанПадіння<Предмет>;
  місцева дія виконати_взяти(М: адреса<Машина>, середовище: адреса<ПредметСередовища>, репозиторій: адреса<ПредметТексту>, назви_шляху: Послідовність<адреса<ПредметТексту>>) -> ПредметАбоСтанПадіння<Предмет>;
  місцева дія отримати_взятий_предмет(М: адреса<Машина>, ключ: адреса<ПредметБайтів>, вихід: адреса<адреса<Предмет>>): логічне;
  місцева дія покласти_взятий_предмет(М: адреса<Машина>, ключ: адреса<ПредметБайтів>, значення: адреса<Предмет>);
  місцева дія видалити_взятий_предмет(М: адреса<Машина>, ключ: адреса<ПредметБайтів>);
  місцева дія знайти_рядок_вказівки(місцезнаходження_вказівок: Послідовність<МісцезнаходженняВказівок>, позиція_вказівки: н32) -> н32;
}

// Очищувач
секція мавка {
  місцева дія звільнити_предмет(М: адреса<Машина>, предмет: адреса<Предмет>);
  місцева дія очистити(М: адреса<Машина>);
}

// Предмет
секція мавка {
  місцева дія перевірити_чи_предмети_назви_рівні(М: адреса<Машина>, а: адреса<ПредметТексту>, б: адреса<ПредметТексту>) -> логічне;
  місцева дія перевірити_чи_предмети_рівні(М: адреса<Машина>, а: адреса<Предмет>, б: адреса<Предмет>) -> логічне;
  місцева дія перевірити_чи_значення_предметів_байтів_рівні(М: адреса<Машина>, а: адреса<ПредметБайтів>, б: адреса<ПредметБайтів>) -> логічне;

  дія створити_предмет<Т>(М: адреса<Машина>, тип: адреса<ПредметСтруктури>): адреса<Т> {
    змінна предмет = М.система.виділити_сиру_памʼять(М, Т.розмір) як адреса<Т>;
    предмет.попередній_предмет = М.останній_предмет;
    предмет.позначено = натуральне(ні);
    предмет.тип = тип;
    предмет.користувацькі_дані = пусто;
    М.останній_предмет = предмет як адреса<Предмет>;
    М.кількість_предметів += 1;
    М.потребує_очищення = натуральне(М.кількість_предметів >= М.кількість_предметів_для_очищення);
    вернути предмет;
  }

  місцева дія заповнити_предмет_структури_предмет(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_Структура(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_Код(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_Середовище(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_Дія(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_логічне(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_число(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_текст(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_список(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_послідовність(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_словник(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_байти(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_Буфер(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_Модуль(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_Параметр(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_ДіапазонЧисел(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_ПеребірДіапазонуЧисел(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_ПеребірСписку(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_ПеребірПослідовності(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_ПеребірТексту(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_ПеребірБайтів(М: адреса<Машина>);
  місцева дія заповнити_предмет_структури_ПеребірБуфера(М: адреса<Машина>);

  місцева дія створити_предмет_структури(М: адреса<Машина>, назва: адреса<ПредметТексту>, предок: адреса<ПредметСтруктури>, кількість_параметрів: натуральне, дані_параметрів: памʼять<адреса<ПредметПараметра>>): адреса<ПредметСтруктури>;
  місцева дія створити_предмет_коду(М: адреса<Машина>, шлях: адреса<ПредметБайтів>, константи: Послідовність<адреса<Предмет>>, місцезнаходження_вказівок: Послідовність<МісцезнаходженняВказівок>, вказівки: адреса<ПредметБайтів>): адреса<ПредметКоду>;
  місцева дія створити_предмет_середовища(М: адреса<Машина>, зовнішнє: адреса<ПредметСередовища>, предмет_модуля: адреса<ПредметМодуля>, предмет_я: адреса<Предмет>): адреса<ПредметСередовища>;
  місцева дія створити_предмет_дії(М: адреса<Машина>, назва: адреса<ПредметТексту>, кількість_параметрів: натуральне, дані_параметрів: памʼять<адреса<ПредметПараметра>>, код: адреса<ПредметКоду>): адреса<ПредметДії>;
  місцева дія створити_предмет_рідної_дії(М: адреса<Машина>, назва: адреса<ПредметТексту>, кількість_параметрів: натуральне, дані_параметрів: памʼять<адреса<ПредметПараметра>>, рідна: РіднаДія, дані: невідома_адреса, зробити_копію_даних: ЗробитиКопіюДанихПредметаДії, відклик_перед_звільненням: ВідкликПередЗвільненнямПредметаДії): адреса<ПредметДії>;
  місцева дія створити_предмет_логічного(М: адреса<Машина>, значення: логічне): адреса<ПредметЛогічного>;
  місцева дія створити_предмет_числа(М: адреса<Машина>, значення: р64): адреса<ПредметЧисла>;
  місцева дія створити_предмет_тексту(М: адреса<Машина>, значення: т8): адреса<ПредметТексту>;
  місцева дія створити_предмет_тексту_без_копіювання(М: адреса<Машина>, значення: т8): адреса<ПредметТексту>;
  місцева дія створити_предмет_списку(М: адреса<Машина>, вмісткість: натуральне): адреса<ПредметСписку>;
  місцева дія створити_предмет_списку_1(М: адреса<Машина>, елементи: Послідовність<адреса<Предмет>>): адреса<ПредметСписку>;
  місцева дія створити_предмет_послідовності(М: адреса<Машина>, елементи: Послідовність<адреса<Предмет>>): адреса<ПредметПослідовності>;
  місцева дія створити_предмет_послідовності_без_копіювання(М: адреса<Машина>, елементи: Послідовність<адреса<Предмет>>): адреса<ПредметПослідовності>;
  місцева дія створити_предмет_словника(М: адреса<Машина>, вмісткість: натуральне): адреса<ПредметСловника>;
  місцева дія створити_предмет_байтів(М: адреса<Машина>, розмір: натуральне, дані: памʼять<н8>): адреса<ПредметБайтів>;
  місцева дія створити_предмет_байтів_без_копіювання(М: адреса<Машина>, розмір: натуральне, дані: памʼять<н8>): адреса<ПредметБайтів>;
  місцева дія створити_предмет_буфера(М: адреса<Машина>, розмір: натуральне, дані: памʼять<н8>): адреса<ПредметБуфера>;
  місцева дія створити_предмет_буфера_без_копіювання(М: адреса<Машина>, розмір: натуральне, дані: памʼять<н8>): адреса<ПредметБуфера>;
  місцева дія створити_предмет_буфера_0(М: адреса<Машина>, розмір: натуральне): адреса<ПредметБуфера>;
  місцева дія створити_предмет_модуля(М: адреса<Машина>, назва: адреса<ПредметТексту>): адреса<ПредметМодуля>;
  місцева дія створити_предмет_параметра(М: адреса<Машина>, назва: адреса<ПредметТексту>, код_значення: адреса<ПредметКоду>): адреса<ПредметПараметра>;
  місцева дія створити_предмет_діапазону_чисел(М: адреса<Машина>, від: адреса<ПредметЧисла>, включно: логічне, до: адреса<ПредметЧисла>): адреса<ПредметДіапазонуЧисел>;
  місцева дія створити_предмет_перебору_діапазону_чисел(М: адреса<Машина>, предмет_діапазону: адреса<ПредметДіапазонуЧисел>): адреса<ПредметПереборуДіапазонуЧисел>;
  місцева дія створити_предмет_перебору_списку(М: адреса<Машина>, предмет_списку: адреса<ПредметСписку>): адреса<ПредметПереборуСписку>;
  місцева дія створити_предмет_перебору_послідовності(М: адреса<Машина>, предмет_послідовності: адреса<ПредметПослідовності>): адреса<ПредметПереборуПослідовності>;
  місцева дія створити_предмет_перебору_тексту(М: адреса<Машина>, предмет_тексту: адреса<ПредметТексту>): адреса<ПредметПереборуТексту>;
  місцева дія створити_предмет_перебору_байтів(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>): адреса<ПредметПереборуБайтів>;
  місцева дія створити_предмет_перебору_буфера(М: адреса<Машина>, предмет_буфера: адреса<ПредметБуфера>): адреса<ПредметПереборуБуфера>;
  місцева дія створити_складений_предмет(М: адреса<Машина>, тип: адреса<ПредметСтруктури>): адреса<СкладенийПредмет>;

  дія створити_назву(М: адреса<Машина>, значення: т8): адреса<ПредметТексту> {
    вернути створити_предмет_тексту(М, значення);
  }

  місцева дія предмет_отримати_користувацькі_дані(М: адреса<Машина>, предмет: адреса<Предмет>) -> адреса<КористувацькіДані>;
  місцева дія предмет_отримати_дані_користувацьких_даних(М: адреса<Машина>, предмет: адреса<Предмет>) -> невідома_адреса;
  місцева дія предмет_змінити_користувацькі_дані(М: адреса<Машина>, предмет: адреса<Предмет>, користувацькі_дані: адреса<КористувацькіДані>);
  місцева дія предмет_видалити_користувацькі_дані(М: адреса<Машина>, предмет: адреса<Предмет>);
  місцева дія створити_користувацькі_дані(М: адреса<Машина>, дані: невідома_адреса, дія_перед_знищенням: ДіяПередЗнищеннямКористувацькихДаних, дія_клонування: ДіяКлонуванняКористувацькихДаних) -> адреса<КористувацькіДані>;
  місцева дія знищити_користувацькі_дані(М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>);

