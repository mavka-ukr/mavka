секція мавка {
  структура Предмет;
  структура ПредметКоду;
  структура ПредметСередовища;
  структура ПредметДії;
  структура ПредметСтруктури;
  структура ПредметЛогічного;
  структура ПредметЧисла;
  структура ПредметТексту;
  структура ПредметСписку;
  структура ПредметПослідовності;
  структура ПредметСловника;
  структура ПредметБайтів;
  структура ПредметБуфера;
  структура ПредметМодуля;
  структура ПредметПараметра;
  структура ПредметДіапазонуЧисел;
  структура ПредметПереборуДіапазонуЧисел;
  структура ПредметПереборуСписку;
  структура ПредметПереборуПослідовності;
  структура ПредметПереборуТексту;
  структура ПредметПереборуБайтів;
  структура ПредметПереборуБуфера;
  структура СкладенийПредмет;
  структура Виконувач;
  структура Машина;
}

секція мавка {
  структура Послідовність<Т> {
    розмір: натуральне;
    дані: памʼять<Т>;
  }

  структура Накопичувач<Т> {
    розмір: натуральне;
    дані: памʼять<Т>;
    вмісткість: натуральне;
  }
  дія зробити_накопичувач<Т>(М: адреса<Машина>): Накопичувач<Т>;
  дія зробити_накопичувач_розміром<Т>(М: адреса<Машина>, розмір: натуральне): Накопичувач<Т>;
  дія виділити_накопичувач<Т>(М: адреса<Машина>): адреса<Накопичувач<Т>>;
  дія звільнити_накопичувач<Т>(М: адреса<Машина>, накопичувач: Накопичувач<Т>);
  дія покласти_в_накопичувач<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, значення: Т);
  дія забрати_з_накопичувача<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, вихід: адреса<Т>): логічне;
  дія розширити_накопичувач<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, розмір: натуральне, пусте_значення: Т);
  дія видалити_з_накопичувача<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, позиція: натуральне): логічне;

  структура ЕлементРеєстру<К, З> {
    ключ: К;
    значення: З;
  }
  структура Реєстр<К, З> {
    елементи: Накопичувач<ЕлементРеєстру<К, З>>;
    дані_дії_перевірки_ключа: невідома_адреса;
    дія_перевірки_ключа: (дані: невідома_адреса, а: К, б: К) -> логічне;
  }
  дія зробити_реєстр<К, З>(М: адреса<Машина>, дія_перевірки_ключа: (дані: невідома_адреса, а: К, б: К) -> логічне): Реєстр<К, З>;
  дія зробити_реєстр_з_даними<К, З, Д>(М: адреса<Машина>, дані_дії_перевірки_ключа: адреса<Д>, дія_перевірки_ключа: (дані: адреса<Д>, а: К, б: К) -> логічне): Реєстр<К, З>;
  дія звільнити_реєстр<К, З>(М: адреса<Машина>, реєстр: Реєстр<К, З>);
  дія змінити_в_реєстрі<К, З>(М: адреса<Машина>, реєстр: адреса<Реєстр<К, З>>, ключ: К, значення: З);
  дія видалити_з_реєстру<К, З>(М: адреса<Машина>, реєстр: адреса<Реєстр<К, З>>, ключ: К): логічне;
  дія знайти_в_реєстрі<К, З>(М: адреса<Машина>, реєстр: адреса<Реєстр<К, З>>, ключ: К, вихід: адреса<ЕлементРеєстру>): логічне;

  структура ЕлементЧерги<Т>;
  структура Черга<Т>;
  структура ЕлементЧерги<Т> {
    наступний_елемент: адреса<ЕлементЧерги<Т>>;
    значення: Т;
  }
  структура Черга<Т> {
    розмір: натуральне;
    перший_елемент: адреса<ЕлементЧерги<Т>>;
    останній_елемент: адреса<ЕлементЧерги<Т>>;
  }
  дія зробити_чергу<Т>(М: адреса<Машина>): Черга<Т>;
  дія покласти_в_чергу_першим<Т>(М: адреса<Машина>, черга: адреса<Черга<Т>>, значення: Т);
  дія покласти_в_чергу<Т>(М: адреса<Машина>, черга: адреса<Черга<Т>>, значення: Т);
  дія забрати_з_черги<Т>(М: адреса<Машина>, черга: адреса<Черга<Т>>, вихід: адреса<Т>): логічне;

  структура ЕлементДвосторонньоПовʼязаногоСписку<Т>;
  структура ДвосторонньоПовʼязанийСписок<Т>;
  структура ЕлементДвосторонньоПовʼязаногоСписку<Т> {
    попередній_елемент: адреса<ЕлементДвосторонньоПовʼязаногоСписку<Т>>;
    наступний_елемент: адреса<ЕлементДвосторонньоПовʼязаногоСписку<Т>>;
    значення: Т;
  }
  структура ДвосторонньоПовʼязанийСписок<Т> {
    розмір: натуральне;
    перший_елемент: адреса<ЕлементДвосторонньоПовʼязаногоСписку<Т>>;
    останній_елемент: адреса<ЕлементДвосторонньоПовʼязаногоСписку<Т>>;
  }
  дія зробити_двосторонньо_повʼязаний_список<Т>(М: адреса<Машина>): ДвосторонньоПовʼязанийСписок<Т>;
  дія покласти_в_двосторонньо_повʼязаний_список<Т>(М: адреса<Машина>, список: адреса<ДвосторонньоПовʼязанийСписок<Т>>, значення: Т): адреса<ЕлементДвосторонньоПовʼязаногоСписку<Т>>;
}

секція мавка {
  стала ВПокластиПусто = 1; // []() -> [пусто]
  стала ВПокластиТак = 2; // []() -> [так]
  стала ВПокластиНі = 3; // []() -> [ні]
  стала ВПокластиКонстанту = 4; // [](позиція_константи) -> [константа]
  стала ВСтворитиДію = 5; // [назва, ..., тип_результату, код](кількість_параметрів) -> [предмет_дії]
  стала ВСтворитиСтруктуру = 6; // [назва, предок, ...](кількість_параметрів) -> [предмет_структури]
  стала ВСтворитиМодуль = 7; // [назва, код]() -> [предмет_модуля]
  стала ВСтворитиСписок = 8; // [](довжина) -> [предмет_списку]
  стала ВПокластиВСписок = 9; // [предмет_списку, елемент]() -> [предмет_списку]
  стала ВСтворитиСловник = 10; // [](розмір) -> [предмет_словника]
  стала ВПокластиВСловник = 11; // [предмет_словника, ключ, значення]() -> [предмет_словника]
