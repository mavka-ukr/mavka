  }
}

секція мавка {
  структура Місцезнаходження {
    шлях_або_код: адреса<ПредметБайтів> | адреса<ПредметКоду>;
    рядок_або_позиція_вказівки: натуральне;
  }

  структура ІменованийАргумент {
    назва: адреса<ПредметТексту>;
    предмет: адреса<Предмет>;
  }

  структура ІменованіАргументи {
    кількість: натуральне;
    дані: памʼять<ІменованийАргумент>;
  }

  синонім ПредметАбоСтанПадіння<Т> = адреса<Т>;
  синонім ніщо_або_стан_падіння = ніщо;
  синонім РіднаДія = (М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: натуральне, аргументи: памʼять<адреса<Предмет>>, іменовані_аргументи: адреса<ІменованіАргументи>) -> ПредметАбоСтанПадіння<Предмет>;
  синонім ЗробитиКопіюДанихПредметаДії = (М: адреса<Машина>, предмет_дії: адреса<ПредметДії>) -> невідома_адреса;
  синонім ВідкликПередЗвільненнямПредметаДії = (М: адреса<Машина>, предмет_дії: адреса<ПредметДії>) -> ніщо;

  синонім ГлобальноПритриманийПредмет = ЕлементДвосторонньоПовʼязаногоСписку<адреса<Предмет>>;

  структура КористувацькіДані;

  синонім ДіяПередЗнищеннямКористувацькихДаних = (М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>) -> ніщо;
  синонім ДіяКлонуванняКористувацькихДаних = (М: адреса<Машина>, користувацькі_дані: адреса<КористувацькіДані>) -> адреса<КористувацькіДані>; 

  структура КористувацькіДані {
    дані: невідома_адреса;
    дія_перед_знищенням: ДіяПередЗнищеннямКористувацькихДаних;
    дія_клонування: ДіяКлонуванняКористувацькихДаних;
  }

  структура Предмет {
    попередній_предмет: адреса<Предмет>;
    позначено: натуральне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
  }

  структура ПредметСтруктури {
    попередній_предмет: адреса<Предмет>;
    позначено: натуральне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    властивості: Реєстр<адреса<ПредметТексту>, адреса<Предмет>>;
    середовище_створення: адреса<ПредметСередовища>;
    назва: адреса<ПредметТексту>;
    предок: адреса<ПредметСтруктури>;
    параметри: Послідовність<адреса<ПредметПараметра>>;
    методи: Реєстр<адреса<ПредметТексту>, адреса<ПредметДії>>;
    власні_дії: Реєстр<адреса<ПредметТексту>, адреса<ПредметДії>>;
    заборонити_наслідування: логічне;
  }

  структура ПредметКоду {
    попередній_предмет: адреса<Предмет>;
    позначено: натуральне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    шлях: адреса<ПредметБайтів>;
    константи: Послідовність<адреса<Предмет>>;
    місцезнаходження_вказівок: Послідовність<МісцезнаходженняВказівок>;
    байти: адреса<ПредметБайтів>;
  }

  структура ПредметСередовища {
    попередній_предмет: адреса<Предмет>;
    позначено: натуральне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    зовнішнє: адреса<ПредметСередовища>;
    підмети: Реєстр<адреса<ПредметТексту>, адреса<Предмет>>;
    предмет_модуля: адреса<ПредметМодуля>;
    предмет_я: адреса<Предмет>;
  }

  структура ПредметДії {
    попередній_предмет: адреса<Предмет>;
    позначено: натуральне;
    тип: адреса<ПредметСтруктури>;
    користувацькі_дані: адреса<КористувацькіДані>;
    середовище_створення: адреса<ПредметСередовища>;
    предмет_я: адреса<Предмет>;
    назва: адреса<ПредметТексту>;
    параметри: Послідовність<адреса<ПредметПараметра>>;
    код: адреса<ПредметКоду>;
    рідна: РіднаДія;
    дані: невідома_адреса;
    зробити_копію_даних: ЗробитиКопіюДанихПредметаДії;
    відклик_перед_звільненням: ВідкликПередЗвільненнямПредметаДії;
  }

