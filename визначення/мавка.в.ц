  стала ВВиконатиВластивість = 26; // [предмет, назва, ..., ...](кількість_аргументів, кількість_іменованих_аргументів) -> [результат]
  стала ВОтриматиЕлемент = 28; // [предмет, ключ]() -> [елемент]
  стала ВЗмінитиЕлемент = 29; // [предмет, ключ, значення]() -> []
  стала ВОтриматиПеребір = 30; // [предмет]() -> [предмет_перебору]
  стала ВВизначитиЗзовні = 31; // [предмет](позиція_константи_назви) -> []
  стала ВВиконатиЗПредка = 33; //  [предмет, назва, ..., ...](кількість_аргументів, кількість_іменованих_аргументів) -> [результат]
  стала ВВидалити = 34; // [назва]() -> [результат]
  стала ВВидалитиВластивість = 35; // [предмет, назва]() -> [результат]
