  стала ВВиконати = 23; // [предмет_дії, ..., ...](кількість_аргументів, кількість_іменованих_аргументів) -> [результат]
  стала ВОтриматиВластивість = 24; // [предмет](позиція_константи_назви) -> [предмет_властивості]
