;; тест 0: просте І
а = 12
б = 10
результат = а ∧ б
очікуваний_результат = 8
якщо результат != очікуваний_результат
  впасти "тест 0: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 1: І з 0
а = 42
б = 0
результат = а ∧ б
очікуваний_результат = 0
якщо результат != очікуваний_результат
  впасти "тест 1: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 2: І з -1 (всі біти)
а = 42
б = -1
результат = а ∧ б
очікуваний_результат = 42
якщо результат != очікуваний_результат
  впасти "тест 2: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 3: І тих самих чисел
а = 15
б = 15
результат = а ∧ б
очікуваний_результат = 15
якщо результат != очікуваний_результат
  впасти "тест 3: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 4: маска молодших бітів
а = 255
б = 15
результат = а ∧ б
очікуваний_результат = 15
якщо результат != очікуваний_результат
  впасти "тест 4: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець