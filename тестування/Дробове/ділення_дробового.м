;; тест 0: просте ділення
а = 12.5
б = 5.0
результат = а / б
очікуваний_результат = 2.5
якщо результат != очікуваний_результат
  впасти "тест 0: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 1: ділення на одиницю
а = 42.7
б = 1.0
результат = а / б
очікуваний_результат = 42.7
якщо результат != очікуваний_результат
  впасти "тест 1: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 2: ділення від'ємних чисел
а = -20.0
б = -4.0
результат = а / б
очікуваний_результат = 5.0
якщо результат != очікуваний_результат
  впасти "тест 2: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 3: ділення додатного на від'ємне
а = 30.0
б = -6.0
результат = а / б
очікуваний_результат = -5.0
якщо результат != очікуваний_результат
  впасти "тест 3: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 4: ділення нуля
а = 0.0
б = 7.5
результат = а / б
очікуваний_результат = 0.0
якщо результат != очікуваний_результат
  впасти "тест 4: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 5: ділення на дріб
