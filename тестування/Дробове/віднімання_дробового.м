;; тест 0: просте віднімання
а = 10.5
б = 3.2
результат = а - б
очікуваний_результат = 7.3
якщо результат != очікуваний_результат
  впасти "тест 0: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 1: віднімання з нулем
а = 42.7
б = 0.0
результат = а - б
очікуваний_результат = 42.7
якщо результат != очікуваний_результат
  впасти "тест 1: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 2: віднімання від'ємних чисел
а = -15.5
б = -25.3
результат = а - б
очікуваний_результат = 9.8
якщо результат != очікуваний_результат
  впасти "тест 2: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 3: віднімання більшого від меншого
а = 5.2
б = 20.8
результат = а - б
очікуваний_результат = -15.600000000000000
якщо результат != очікуваний_результат
  впасти "тест 3: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 4: віднімання від'ємного (подвійний мінус)
а = 50.5
б = -30.3
результат = а - б
очікуваний_результат = 80.8
якщо результат != очікуваний_результат
  впасти "тест 4: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 5: віднімання малих чисел
а = 0.5
б = 0.3
результат = а - б
очікуваний_результат = 0.2
якщо результат != очікуваний_результат
  впасти "тест 5: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 0
а = 2.0
б = 3.0
результат = а - б
очікуваний_результат = -1.0

якщо результат != очікуваний_результат
  впасти "результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

якщо результат не рівно очікуваний_результат
  впасти "результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

якщо результат == очікуваний_результат
інакше
  впасти "результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

якщо результат рівно очікуваний_результат
інакше
  впасти "результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець