якщо результат != очікуваний_результат
  впасти "тест 2: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 3: від'ємна основа, парний степінь
а = -3.0
б = 2.0
результат = а ** б
очікуваний_результат = 9.0
якщо результат != очікуваний_результат
  впасти "тест 3: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 4: від'ємна основа, непарний степінь
а = -2.0
б = 3.0
результат = а ** б
очікуваний_результат = -8.0
якщо результат != очікуваний_результат
  впасти "тест 4: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 5: дробовий степінь (квадратний корінь)
а = 4.0
б = 0.5
результат = а ** б
очікуваний_результат = 2.0
якщо результат != очікуваний_результат
  впасти "тест 5: результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

;; тест 0
а = 15.0
б = 5.0
результат = а ** б
очікуваний_результат = 759375.0

якщо результат != очікуваний_результат
  впасти "результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

якщо результат не рівно очікуваний_результат
  впасти "результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

якщо результат == очікуваний_результат
інакше
  впасти "результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець

якщо результат рівно очікуваний_результат
інакше
  впасти "результат має бути %(очікуваний_результат), а рівно %(результат)"
кінець