значення = [1, "2", так, ні]

ел = значення.забрати()
якщо ел не рівно ні
  впасти
кінець
якщо значення.довжина не рівно 3
  впасти
кінець

ел = значення.забрати()
якщо ел не рівно так
  впасти
кінець
якщо значення.довжина не рівно 2
  впасти
кінець

ел = значення.забрати()
якщо ел не рівно "2"
  впасти
кінець
якщо значення.довжина не рівно 1
  впасти
кінець

ел = значення.забрати()
якщо ел не рівно 1
  впасти
кінець
якщо значення.довжина не рівно 0
  впасти
кінець

ел = значення.забрати()
якщо ел не рівно пусто
  впасти
кінець
якщо значення.довжина не рівно 0
  впасти
кінець