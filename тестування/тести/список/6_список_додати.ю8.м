взяти пак інструменти_тестування

значення = []

значення.додати(1)
якщо значення.довжина не рівно 1
  впасти
кінець
якщо значення[0] не рівно 1
  впасти
кінець
якщо !інструменти_тестування.списки_рівні(значення, [1])
  впасти
кінець

значення.додати("2")
якщо значення.довжина не рівно 2
  впасти
кінець
якщо значення[1] не рівно "2"
  впасти
кінець
якщо !інструменти_тестування.списки_рівні(значення, [1, "2"])
  впасти
кінець