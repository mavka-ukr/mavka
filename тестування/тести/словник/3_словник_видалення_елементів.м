значення = {"а"="б", "в"=10, "г"=так, "ґ"={}, "д"=[]}
початковий_розмір = значення.розмір

якщо видалити значення["в"] == ні
  впасти "видалення елемента вернуло ні"
кінець
якщо значення.чародія_видалити_елемент("а") == ні
  впасти "видалення елемента вернуло ні"
кінець

якщо значення.розмір не рівно початковий_розмір - 2
  впасти "Розмір словника невірний"
кінець
якщо значення["в"] не рівно пусто
  впасти "Видалення елемента невірне"
кінець
якщо значення містить "в"
  впасти
кінець
якщо значення.чародія_отримати_елемент("а") не рівно пусто
  впасти
кінець
якщо значення.чародія_містить("а")
  впасти
кінець