значення = ["а"="б", "в"=10, "г"=так, "ґ"=[=], "д"=[]]

значення["в"] = 20
значення.чародія_змінити_елемент("а", ю"нікод")

якщо значення["в"] не рівно 20
  впасти "Зміна елемента в невірна"
кінець
якщо значення.чародія_отримати_елемент("а") не рівно ю"нікод"
  впасти "Зміна елемента невірна"
кінець