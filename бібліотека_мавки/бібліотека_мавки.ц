взяти визначення мавка;
взяти визначення бібліотека_мавки;

секція мавка {
  секція бібліотека {
    місцева дія обробник_взяття_біб(М: адреса<Машина>, розмір_шляху: натуральне, шлях: памʼять<адреса<Предмет>>) -> ПредметАбоСтанПадіння<Предмет> {
      якщо розмір_шляху == 1 {
        якщо перевірити_чи_предмети_рівні(М, шлях[0], створити_назву(М, "математика") як адреса<Предмет>) {
          вернути взяти_модуль_біб_математика(М);
        }
        якщо перевірити_чи_предмети_рівні(М, шлях[0], створити_назву(М, "читати") як адреса<Предмет>) {
          вернути взяти_модуль_біб_читати(М);
        }
        якщо перевірити_чи_предмети_рівні(М, шлях[0], створити_назву(М, "інтернет") як адреса<Предмет>) {
          вернути взяти_модуль_біб_інтернет(М);
        }
        якщо перевірити_чи_предмети_рівні(М, шлях[0], створити_назву(М, "планувальник") як адреса<Предмет>) {
          вернути взяти_модуль_біб_планувальник(М);
        }
      }
      вернути падіння(М, "Неможливо взяти модуль біб");
    }
  }
}