взяти визначення ./Виділяч;

секція турбо {
  структура Турбіна {
    виділяч: адреса<біб::Виділяч>;
  }

  структура Відкладене;
  структура Періодичне;
  структура ІнтернетОбслуговувач;
  структура ІнтернетЗвʼязок;
  структура Помилка;

  синонім ТипПомилкиВідкликуТурбіниКористувацький = 1;

  структура ПомилкаВідкликуТурбіни {
    тип: позитивне;
    дані: невідома_адреса;
  }

  синонім ВідкликТурбіни = (турбіна: адреса<Турбіна>, аргумент: невідома_адреса, вихід_помилки: адреса<турбо::ПомилкаВідкликуТурбіни>) -> логічне;

  синонім ОбробникПідключенняІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса, успішно: логічне, код_помилки: позитивне) -> ніщо;
  синонім ОбробникВідключенняІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса, з_помилкою: логічне, код_помилки: позитивне) -> ніщо;
  синонім ОбробникДанихІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса, розмір: позитивне, дані: памʼять<п8>) -> ніщо;
  синонім ОбробникЗакриттяІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса) -> ніщо;
  синонім ОбробникЗаписуІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент_інтернет_звʼязку: невідома_адреса, аргумент_запису: невідома_адреса, розмір: позитивне, дані: памʼять<п8>) -> ніщо;
  синонім ОбробникЗапускуІнтернетОбслуговувача = (турбіна: адреса<Турбіна>, інтернет_обслуговувач: адреса<ІнтернетОбслуговувач>, аргумент: невідома_адреса, успішно: логічне, код_помилки: ц32, іа: позитивне, порт: позитивне) -> ніщо;
  синонім ОбробникПідключенняКлієнтаІнтернетОбслуговувача = (турбіна: адреса<Турбіна>, інтернет_обслуговувач: адреса<ІнтернетОбслуговувач>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса) -> ніщо;
  синонім ОбробникЗупинкиІнтернетОбслуговувача = (турбіна: адреса<Турбіна>, інтернет_обслуговувач: адреса<ІнтернетОбслуговувач>, аргумент: невідома_адреса) -> ніщо;

  синонім ВідкликВідкладеного = (турбіна: адреса<Турбіна>, відкладене: адреса<турбо::Відкладене>, аргумент: невідома_адреса, скасовано: логічне, успішно: логічне, код_помилки: ц32) -> ніщо;
  синонім ВідкликПеріодичного = (турбіна: адреса<Турбіна>, періодичне: адреса<турбо::Періодичне>, аргумент: невідома_адреса, скасовано: логічне, успішно: логічне, код_помилки: ц32) -> ніщо;

  синонім ВідкликВводу = (турбіна: адреса<Турбіна>, аргумент: невідома_адреса, скасовано: логічне, розмір_префікса: позитивне, дані_префікса: памʼять<п8>, розмір: позитивне, дані: памʼять<п8>) -> ніщо;
}

зовнішня дія __турбо__створити_турбіну(виділяч: адреса<біб::Виділяч>): адреса<турбо::Турбіна>;
зовнішня дія __турбо__отримати_глобальну_турбіну(): адреса<турбо::Турбіна>;
зовнішня дія __турбо__змінити_глобальну_турбіну(турбіна: адреса<турбо::Турбіна>);
зовнішня дія __турбо__запустити_турбіну(турбіна: адреса<турбо::Турбіна>, відклик_перед: турбо::ВідкликТурбіни, відклик_після: турбо::ВідкликТурбіни, аргумент: невідома_адреса, вихід_помилки: адреса<турбо::ПомилкаВідкликуТурбіни>): логічне;
зовнішня дія __турбо__знищити_турбіну(турбіна: адреса<турбо::Турбіна>);

зовнішня дія __турбо__отримати_інтернет_адресу_з_ю8(турбіна: адреса<турбо::Турбіна>, значення: ю8, вихід: адреса<позитивне>): логічне;

зовнішня дія __турбо__підключити_інтернет_звʼязок(турбіна: адреса<турбо::Турбіна>, іа: позитивне, порт: позитивне, обробник_підключення: турбо::ОбробникПідключенняІнтернетЗвʼязку, обробник_даних: турбо::ОбробникДанихІнтернетЗвʼязку, обробник_закриття: турбо::ОбробникЗакриттяІнтернетЗвʼязку, обробник_відключення: турбо::ОбробникВідключенняІнтернетЗвʼязку, аргумент: невідома_адреса): логічне;
зовнішня дія __турбо__записати_в_інтернет_звʼязок(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>, розмір: позитивне, дані: памʼять<п8>, обробник_запису: турбо::ОбробникЗаписуІнтернетЗвʼязку, аргумент: невідома_адреса);
зовнішня дія __турбо__закінчити_інтернет_звʼязок(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>);
зовнішня дія __турбо__встановити_обробники_інтернет_звʼязку(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>, обробник_даних: турбо::ОбробникДанихІнтернетЗвʼязку, обробник_закриття: турбо::ОбробникЗакриттяІнтернетЗвʼязку, обробник_відключення: турбо::ОбробникВідключенняІнтернетЗвʼязку, аргумент: невідома_адреса);
зовнішня дія __турбо__отримати_ідентифікатор_інтернет_звʼязку(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>): ц32;
зовнішня дія __турбо__отримати_ідентифікатор_інтернет_обслуговувача(турбіна: адреса<турбо::Турбіна>, інтернет_обслуговувач: адреса<турбо::ІнтернетОбслуговувач>): ц32;

зовнішня дія __турбо__запустити_інтернет_обслуговувач(турбіна: адреса<турбо::Турбіна>, іа: позитивне, порт: позитивне, обробник_запуску: турбо::ОбробникЗапускуІнтернетОбслуговувача, обробник_підключення_клієнта: турбо::ОбробникПідключенняКлієнтаІнтернетОбслуговувача, обробник_зупинки: турбо::ОбробникЗупинкиІнтернетОбслуговувача, аргумент: невідома_адреса);

