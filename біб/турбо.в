зовнішня дія __турбо__виконувати_періодично(турбіна: адреса<турбо::Турбіна>, тривалість: позитивне, відклик: турбо::ВідкликПеріодичного, аргумент: невідома_адреса): адреса<турбо::Періодичне>;
зовнішня дія __турбо__скасувати_періодичне(турбіна: адреса<турбо::Турбіна>, періодичне: адреса<турбо::Періодичне>);

зовнішня дія __турбо__прочитати_ввід(турбіна: адреса<турбо::Турбіна>, відклик: турбо::ВідкликВводу, аргумент: невідома_адреса, розмір_префікса: позитивне, дані_префікса: памʼять<п8>);
зовнішня дія __турбо__вивести(турбіна: адреса<турбо::Турбіна>, розмір: позитивне, дані: памʼять<п8>);

секція турбо {
  дія створити_турбіну(виділяч: адреса<біб::Виділяч>): адреса<Турбіна> {
    вернути __турбо__створити_турбіну(виділяч);
  }

  дія отримати_глобальну_турбіну(): адреса<Турбіна> {
    вернути __турбо__отримати_глобальну_турбіну();
  }

  дія змінити_глобальну_турбіну(турбіна: адреса<Турбіна>) {
    __турбо__змінити_глобальну_турбіну(турбіна);
  }

  дія запустити_турбіну(турбіна: адреса<Турбіна>, відклик_перед: турбо::ВідкликТурбіни, відклик_після: турбо::ВідкликТурбіни, аргумент: невідома_адреса, вихід_помилки: адреса<турбо::ПомилкаВідкликуТурбіни>): логічне {
    вернути __турбо__запустити_турбіну(турбіна, відклик_перед, відклик_після, аргумент, вихід_помилки);
  }

  дія знищити_турбіну(турбіна: адреса<Турбіна>) {
    __турбо__знищити_турбіну(турбіна);
  }

  дія отримати_інтернет_адресу_з_ю8(турбіна: адреса<турбо::Турбіна>, значення: ю8, вихід: адреса<позитивне>): логічне {
    вернути __турбо__отримати_інтернет_адресу_з_ю8(турбіна, значення, вихід);
  }

  дія підключити_інтернет_звʼязок(турбіна: адреса<Турбіна>, іа: позитивне, порт: позитивне, обробник_підключення: ОбробникПідключенняІнтернетЗвʼязку, обробник_даних: ОбробникДанихІнтернетЗвʼязку, обробник_закриття: ОбробникЗакриттяІнтернетЗвʼязку, обробник_відключення: ОбробникВідключенняІнтернетЗвʼязку, аргумент: невідома_адреса): логічне {
    вернути __турбо__підключити_інтернет_звʼязок(турбіна, іа, порт, обробник_підключення, обробник_даних, обробник_закриття, обробник_відключення, аргумент);
  }

  дія записати_в_інтернет_звʼязок(турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, розмір: позитивне, дані: памʼять<п8>, обробник_запису: ОбробникЗаписуІнтернетЗвʼязку, аргумент: невідома_адреса) {
    __турбо__записати_в_інтернет_звʼязок(турбіна, інтернет_звʼязок, розмір, дані, обробник_запису, аргумент);
  }

  дія встановити_обробники_інтернет_звʼязку(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>, обробник_даних: турбо::ОбробникДанихІнтернетЗвʼязку, обробник_закриття: турбо::ОбробникЗакриттяІнтернетЗвʼязку, обробник_відключення: турбо::ОбробникВідключенняІнтернетЗвʼязку, аргумент: невідома_адреса) {
    __турбо__встановити_обробники_інтернет_звʼязку(турбіна, інтернет_звʼязок, обробник_даних, обробник_закриття, обробник_відключення, аргумент);
  }

  дія отримати_ідентифікатор_інтернет_звʼязку(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>): ц32 {
    вернути __турбо__отримати_ідентифікатор_інтернет_звʼязку(турбіна, інтернет_звʼязок);
  }

  дія закінчити_інтернет_звʼязок(турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>) {
    __турбо__закінчити_інтернет_звʼязок(турбіна, інтернет_звʼязок);
  }

  дія запустити_інтернет_обслуговувач(турбіна: адреса<Турбіна>, іа: позитивне, порт: позитивне, обробник_запуску: ОбробникЗапускуІнтернетОбслуговувача, обробник_підключення_клієнта: ОбробникПідключенняКлієнтаІнтернетОбслуговувача, обробник_зупинки: ОбробникЗупинкиІнтернетОбслуговувача, аргумент: невідома_адреса) {
    __турбо__запустити_інтернет_обслуговувач(турбіна, іа, порт, обробник_запуску, обробник_підключення_клієнта, обробник_зупинки, аргумент);
  }

  дія отримати_ідентифікатор_інтернет_обслуговувача(турбіна: адреса<турбо::Турбіна>, інтернет_обслуговувач: адреса<турбо::ІнтернетОбслуговувач>): ц32 {
    вернути __турбо__отримати_ідентифікатор_інтернет_обслуговувача(турбіна, інтернет_обслуговувач);
  }

  дія відкласти_виконання(турбіна: адреса<турбо::Турбіна>, тривалість: позитивне, відклик: ВідкликВідкладеного, аргумент: невідома_адреса): адреса<Відкладене> {
    вернути __турбо__відкласти_виконання(турбіна, тривалість, відклик, аргумент);
  }

  дія скасувати_відкладене(турбіна: адреса<Турбіна>, відкладене: адреса<Відкладене>) {
    __турбо__скасувати_відкладене(турбіна, відкладене);
  }

  дія виконувати_періодично(турбіна: адреса<турбо::Турбіна>, тривалість: позитивне, відклик: ВідкликПеріодичного, аргумент: невідома_адреса): адреса<Періодичне> {
    вернути __турбо__виконувати_періодично(турбіна, тривалість, відклик, аргумент);
  }

  дія скасувати_періодичне(турбіна: адреса<Турбіна>, періодичне: адреса<Періодичне>) {
    __турбо__скасувати_періодичне(турбіна, періодичне);
  }

  дія прочитати_ввід_ю8(турбіна: адреса<Турбіна>, відклик: ВідкликВводу, аргумент: невідома_адреса, префікс: ю8) {
    __турбо__прочитати_ввід(турбіна, відклик, аргумент, префікс.розмір, префікс.дані);
  }

  дія вивести_ю8(турбіна: адреса<Турбіна>, значення: ю8) {
    __турбо__вивести(турбіна, значення.розмір, значення.дані);
  }
}