взяти означення ./Кодування;
взяти означення ./Друк;
взяти означення ./Памʼять;

простір біб {
  структура Шлях;
}

зовнішня дія __біб__шлях_з_ю8(значення: адреса<ю8>, вихід: адреса<біб::Шлях>) -> ціле;
зовнішня дія __біб__шлях_з_тексту(значення: адреса<кд>, вихід: адреса<біб::Шлях>) -> ціле;
зовнішня дія __біб__знищити_шлях(шлях: адреса<біб::Шлях>) -> ціле;
зовнішня дія __біб__клонувати_шлях(шлях: адреса<біб::Шлях>, вихід: адреса<біб::Шлях>) -> ціле;
зовнішня дія __біб__вивести_шлях(шлях: адреса<біб::Шлях>) -> ціле;
зовнішня дія __біб__надрукувати_шлях(шлях: адреса<біб::Шлях>) -> ціле;
зовнішня дія __біб__доповнити_шлях_текстом(шлях: адреса<біб::Шлях>, доповнення: адреса<кд>) -> ціле;
зовнішня дія __біб__доповнити_шлях_ю8(шлях: адреса<біб::Шлях>, доповнення: адреса<ю8>) -> ціле;
зовнішня дія __біб__отримати_абсолютний_шлях(шлях: адреса<біб::Шлях>, вихід: адреса<біб::Шлях>) -> ціле;
зовнішня дія __біб__отримати_зовнішній_шлях(шлях: адреса<біб::Шлях>, вихід: адреса<біб::Шлях>) -> ціле;
зовнішня дія __біб__перевірити_чи_шлях_існує(шлях: адреса<біб::Шлях>) -> ціле;

простір біб {
  структура Шлях {
    розмір: природне;
    дані: памʼять<п8>;
    вмісткість: природне;
  }

  дія пустий_шлях() -> Шлях {
    вернути Шлях { 0, пусто, 0 };
  }

  дія шлях_з_тексту(значення: кд, вихід: адреса<Шлях>) -> ціле {
    вернути __біб__шлях_з_тексту(значення::адреса, вихід);
  }

  дія шлях_з_ю8(значення: ю8, вихід: адреса<Шлях>) -> ціле {
    вернути __біб__шлях_з_ю8(значення::адреса, вихід);
  }

  дія знищити_шлях(шлях: Шлях) -> ціле {
    вернути __біб__знищити_шлях(шлях::адреса);
  }

  дія клонувати_шлях(шлях: Шлях, вихід: адреса<Шлях>) -> ціле {
    вернути __біб__клонувати_шлях(шлях::адреса, вихід);
  }

  дія вивести_шлях(шлях: Шлях) -> ціле {
    вернути __біб__вивести_шлях(шлях::адреса);
  }

  дія надрукувати_шлях(шлях: Шлях) -> ціле {
    вернути __біб__надрукувати_шлях(шлях::адреса);
  }

  дія доповнити_шлях_текстом(шлях: адреса<Шлях>, доповнення: кд) -> ціле {
    вернути __біб__доповнити_шлях_текстом(шлях, доповнення::адреса);
  }

  дія доповнити_шлях_ю8(шлях: адреса<Шлях>, доповнення: ю8) -> ціле {
    вернути __біб__доповнити_шлях_ю8(шлях, доповнення::адреса);
  }

  дія абсолютний_шлях(шлях: Шлях, вихід: адреса<Шлях>) -> ціле {
    вернути __біб__отримати_абсолютний_шлях(шлях::адреса, вихід);
  }

  дія зовнішній_шлях(шлях: Шлях, вихід: адреса<Шлях>) -> ціле {
    вернути __біб__отримати_зовнішній_шлях(шлях::адреса, вихід);
  }

  дія перевірити_чи_шлях_існує(шлях: Шлях) -> логічне {
    вернути __біб__перевірити_чи_шлях_існує(шлях::адреса) != 0;
  }
}