синонім ТурбоПодіяДоступноДляЧитання = 1п32;
синонім ТурбоПодіяДоступноДляЗапису = 4п32;

структура ТурбоРушій;
структура ТурбоСлухач;

структура ТурбоПомилка {
  код: ц64;
  дані: невідома_адреса;
}

структура ТурбоВихідІнтернетОбслуговувача {
  ідентифікатор: ц64;
  іа: ю8;
  порт: п64;
}

