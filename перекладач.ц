//     }

//     місцева дія покласти_вказівку_ПокластиКонстанту(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32, позиція_константи: п32) -> п32 {
//       вернути покласти_вказівку_з_аргументом(М, будівельник_вказівок, рядок, ВПокластиКонстанту, позиція_константи);
//     }

//     місцева дія покласти_вказівку_СтворитиДію(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32, кількість_параметрів: п32) -> п32 {
//       вернути покласти_вказівку_з_аргументом(М, будівельник_вказівок, рядок, ВСтворитиДію, кількість_параметрів);
//     }

//     місцева дія покласти_вказівку_СтворитиСтруктуру(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32, кількість_параметрів: п32) -> п32 {
