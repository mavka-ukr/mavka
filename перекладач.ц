// взяти означення ./мавка;

// простір мавка {
//   структура Накопичувач<Т> {
//     розмір: природне;
//     дані: памʼять<Т>;
//     вмісткість: природне;
//   }
  
//   дія покласти_в_накопичувач<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, елемент: Т) {
//     якщо накопичувач.розмір == накопичувач.вмісткість {
//       накопичувач.вмісткість += 1;
//       накопичувач.дані = перевиділити_памʼять<Т>(М, накопичувач.дані, накопичувач.вмісткість);
//     }
  
//     накопичувач.дані[накопичувач.розмір] = елемент;
//     накопичувач.розмір += 1;
//   }
  
//   дія зробити_накопичувач<Т>(М: адреса<Машина>) -> Накопичувач<Т> {
//     вернути Накопичувач<Т> { 0, пусто, 0 };
//   }
  
//   дія звільнити_накопичувач<Т>(М: адреса<Машина>, накопичувач: Накопичувач<Т>) {
//     звільнити_памʼять(М, накопичувач.дані);
//   }

//   структура МісцезнаходженняВказівок {
//     початок: п32;
//     кінець: п32;
//     рядок: п32;
//   }

//   структура БудівельникВказівок {
//     місцезнаходження: Накопичувач<МісцезнаходженняВказівок>;
//     вказівки: Накопичувач<п8>;
//   }

//   простір В1 {
//     дія покласти_рядок_вказівки(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32, позиція_вказівки: п32) {
//       якщо будівельник_вказівок.місцезнаходження.розмір != 0 {
//         змінна м = будівельник_вказівок.місцезнаходження.дані[будівельник_вказівок.місцезнаходження.розмір - 1]::адреса;

//         якщо м.рядок == рядок {
//           якщо м.початок > позиція_вказівки {
//             м.початок = позиція_вказівки;
//           }
//           якщо м.кінець < позиція_вказівки {
//             м.кінець = позиція_вказівки;
//           }
//           вернути;
//         }
//       }

//       покласти_в_накопичувач<МісцезнаходженняВказівок>(
//         М,
//         будівельник_вказівок.місцезнаходження::адреса,
//         МісцезнаходженняВказівок {
//           початок = позиція_вказівки,
//           кінець = позиція_вказівки,
//           рядок = рядок
//         }
//       );
//     }

//     дія покласти_вказівку(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32, вказівка: п8) -> п32 {
//       покласти_рядок_вказівки(М, будівельник_вказівок, рядок, п32(будівельник_вказівок.вказівки.розмір));
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, вказівка);
//       вернути п32(будівельник_вказівок.вказівки.розмір) - 1;
//     }

//     дія покласти_вказівку_з_аргументом(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32, вказівка: п8, аргумент: п32) -> п32 {
//       покласти_рядок_вказівки(М, будівельник_вказівок, рядок, п32(будівельник_вказівок.вказівки.розмір));
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, вказівка);
//       змінна аргумент_як_памʼять_п8 = аргумент::адреса як памʼять<п8>;
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[0]);
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[1]);
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[2]);
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[3]);
//       вернути п32(будівельник_вказівок.вказівки.розмір) - 1;
//     }

//     дія покласти_вказівку_з_двома_аргументами(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32, вказівка: п8, аргумент: п32, другий_аргумент: п32) -> п32 {
//       покласти_рядок_вказівки(М, будівельник_вказівок, рядок, п32(будівельник_вказівок.вказівки.розмір));
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, вказівка);
//       змінна аргумент_як_памʼять_п8 = аргумент::адреса як памʼять<п8>;
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[0]);
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[1]);
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[2]);
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, аргумент_як_памʼять_п8[3]);
//       змінна другий_аргумент_як_памʼять_п8 = другий_аргумент::адреса як памʼять<п8>;
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, другий_аргумент_як_памʼять_п8[0]);
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, другий_аргумент_як_памʼять_п8[1]);
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, другий_аргумент_як_памʼять_п8[2]);
//       покласти_в_накопичувач<п8>(М, будівельник_вказівок.вказівки::адреса, другий_аргумент_як_памʼять_п8[3]);
//       вернути п32(будівельник_вказівок.вказівки.розмір) - 1;
//     }

//     місцева дія перезаписати_аргумент_вказівки(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, позиція: п32, значення: п32) {
//       змінна значення_як_памʼять_п8 = значення::адреса як памʼять<п8>;
//       будівельник_вказівок.вказівки.дані[позиція] = значення_як_памʼять_п8[0];
//       будівельник_вказівок.вказівки.дані[позиція + 1] = значення_як_памʼять_п8[1];
//       будівельник_вказівок.вказівки.дані[позиція + 2] = значення_як_памʼять_п8[2];
//       будівельник_вказівок.вказівки.дані[позиція + 3] = значення_як_памʼять_п8[3];
//     }

//     місцева дія покласти_вказівку_ПокластиПусто(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32) -> п32 {
//       вернути покласти_вказівку(М, будівельник_вказівок, рядок, ВПокластиПусто);
//     }

//     місцева дія покласти_вказівку_ПокластиТак(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32) -> п32 {
//       вернути покласти_вказівку(М, будівельник_вказівок, рядок, ВПокластиТак);
//     }

//     місцева дія покласти_вказівку_ПокластиНі(М: адреса<Машина>, будівельник_вказівок: адреса<БудівельникВказівок>, рядок: п32) -> п32 {
