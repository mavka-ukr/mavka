взяти означення бібліотека_мавки;

простір мавка {
  простір бібліотека {
    місцева дія ініціалізувати_модуль__математика(М: адреса<Машина>, предмет_модуля: адреса<ПредметМодуля>) {
      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._ПІ,
        значення_дробове(ПІ),
        Місцезнаходження { пусто, 0 }
      );
    }
  }
}