      змінна знайдено_задіяне_значення = ні;

      знайдено_задіяне_значення = знайти_задіяне(М, задіяні, 0, М.константи_назв._значення, задіяне_значення::адреса);

      якщо не знайдено_задіяне_значення {
        увімкнути_стан_падіння(
          М,
          значення_предмет(створити_текст(М, "Задіяне \"значення\" має бути цілим або дробовим.")),
          Місцезнаходження { пусто, 0 }
        );
        вернути значення_недійсне();
      }

      якщо задіяне_значення.вид == ЗЦіле {
        вернути значення_дробове(бібліотека_мавки_косинус_д64(д64(задіяне_значення.дані як ц64)));
      } інакше якщо задіяне_значення.вид == ЗДробове {
        вернути значення_дробове(бібліотека_мавки_косинус_д64(задіяне_значення.дані як д64));
      } інакше {
        увімкнути_стан_падіння(
          М,
          значення_предмет(створити_текст(М, "Задіяне \"значення\" має бути цілим або дробовим.")),
          Місцезнаходження { пусто, 0 }
        );
        вернути значення_недійсне();
      }
    }

    дія записати_дію__косинус(
      М: адреса<Машина>,
      предмет_модуля: адреса<ПредметМодуля>
    ) {
