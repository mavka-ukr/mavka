простір мавка {
  простір бібліотека {
    місцева дія ініціалізувати__математика(
      М: адреса<Машина>,
      предмет_модуля: адреса<ПредметМодуля>
    ) {
      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._Е,
        значення_дробове(Е),
        Місцезнаходження { пусто, 0 }
      );
      якщо М.стан_падіння {
        запанікувати(М, "Помилка при ініціалізації модуля бібліотеки \"математика\".");
      }

      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._ПІ,
        значення_дробове(ПІ),
        Місцезнаходження { пусто, 0 }
      );
      якщо М.стан_падіння {
        запанікувати(М, "Помилка при ініціалізації модуля бібліотеки \"математика\".");
      }

      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._ЛОГ2Е,
        значення_дробове(ЛОГ2Е),
        Місцезнаходження { пусто, 0 }
      );
      якщо М.стан_падіння {
        запанікувати(М, "Помилка при ініціалізації модуля бібліотеки \"математика\".");
      }

      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._ЛОГ10Е,
        значення_дробове(ЛОГ10Е),
        Місцезнаходження { пусто, 0 }
      );
      якщо М.стан_падіння {
        запанікувати(М, "Помилка при ініціалізації модуля бібліотеки \"математика\".");
      }

      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._ЛОГ2,
        значення_дробове(ЛОГ2),
        Місцезнаходження { пусто, 0 }
      );
      якщо М.стан_падіння {
        запанікувати(М, "Помилка при ініціалізації модуля бібліотеки \"математика\".");
      }

      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._ЛОГ10,
        значення_дробове(ЛОГ10),
        Місцезнаходження { пусто, 0 }
      );
      якщо М.стан_падіння {
        запанікувати(М, "Помилка при ініціалізації модуля бібліотеки \"математика\".");
      }

      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._КОРІНЬ2,
        значення_дробове(КОРІНЬ2),
        Місцезнаходження { пусто, 0 }
      );
      якщо М.стан_падіння {
        запанікувати(М, "Помилка при ініціалізації модуля бібліотеки \"математика\".");
      }

      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._КОРІНЬ1_2,
        значення_дробове(КОРІНЬ1_2),
        Місцезнаходження { пусто, 0 }
      );
      якщо М.стан_падіння {
        запанікувати(М, "Помилка при ініціалізації модуля бібліотеки \"математика\".");
      }

      записати_дію__синус(М, предмет_модуля);
      записати_дію__косинус(М, предмет_модуля);
      записати_дію__тангенс(М, предмет_модуля);
      записати_дію__арксинус(М, предмет_модуля);
      записати_дію__арккосинус(М, предмет_модуля);
      записати_дію__арктангенс(М, предмет_модуля);
      // записати_дію__арктангенс2(М, предмет_модуля);
      записати_дію__абсолютне(М, предмет_модуля);
      записати_дію__експонента(М, предмет_модуля);
      записати_дію__корінь(М, предмет_модуля);
      записати_дію__стеля(М, предмет_модуля);
      записати_дію__підлога(М, предмет_модуля);
      записати_дію__округлити(М, предмет_модуля);
    }
  }
}