взяти означення мавка/бібліотека;

зовнішня дія бібліотека_мавки_синус_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_косинус_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_тангенс_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_арксинус_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_арккосинус_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_арктангенс_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_арктангенс2_д64(а: д64, б: д64) -> д64;
зовнішня дія бібліотека_мавки_абсолютне_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_експонента_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_корінь_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_стеля_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_підлога_д64(значення: д64) -> д64;
зовнішня дія бібліотека_мавки_округлити_д64(значення: д64) -> д64;

простір мавка {
  простір бібліотека {
    дія рідна_дія__синус(
      М: адреса<Машина>,
      здійснювач: Значення,
      предмет_дії: адреса<ПредметДії>,
      задіяні: Задіяні
    ) -> ЗначенняАбоСтанПадіння {
      змінна задіяне_значення: Значення;
      змінна знайдено_задіяне_значення = ні;

      знайдено_задіяне_значення = знайти_задіяне(М, задіяні, 0, М.константи_назв._значення, задіяне_значення::адреса);

      якщо не знайдено_задіяне_значення {
        увімкнути_стан_падіння(
          М,
          значення_предмет(створити_текст(М, "Задіяне \"значення\" має бути цілим або дробовим.")),
          Місцезнаходження { пусто, 0 }
        );
        вернути значення_недійсне();
      }

      якщо задіяне_значення.вид == ЗЦіле {
        вернути значення_дробове(бібліотека_мавки_синус_д64(д64(задіяне_значення.дані як ц64)));
      } інакше якщо задіяне_значення.вид == ЗДробове {
        вернути значення_дробове(бібліотека_мавки_синус_д64(задіяне_значення.дані як д64));
      } інакше {
        увімкнути_стан_падіння(
          М,
          значення_предмет(створити_текст(М, "Задіяне \"значення\" має бути цілим або дробовим.")),
          Місцезнаходження { пусто, 0 }
        );
        вернути значення_недійсне();
      }
    }

    дія записати_дію__синус(
      М: адреса<Машина>,
      предмет_модуля: адреса<ПредметМодуля>
    ) {
      змінна предмет_дії = створити_рідну_дію(
        М,
        М.константи_назв._синус,
        М.предмет_пустої_послідовності,
        пусто,
        рідна_дія__синус,
        пусто
      );

      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._синус,
        значення_предмет(предмет_дії),
        Місцезнаходження { пусто, 0 }
      );

      якщо М.стан_падіння {
        запанікувати(М, "Помилка при записі дії \"синус\" для модуля бібліотеки \"математика\".");
      }
    }
  }
}

простір мавка {
  простір бібліотека {
    дія рідна_дія__косинус(
      М: адреса<Машина>,
      здійснювач: Значення,
      предмет_дії: адреса<ПредметДії>,
      задіяні: Задіяні
