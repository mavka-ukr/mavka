        М,
        М.константи_назв._косинус,
        М.предмет_пустої_послідовності,
        пусто,
        рідна_дія__косинус,
        пусто
      );

      предмет_модуля_змінити_властивість(
        М,
        предмет_модуля,
        М.константи_назв._косинус,
        значення_предмет(предмет_дії),
        Місцезнаходження { пусто, 0 }
      );

      якщо М.стан_падіння {
        запанікувати(М, "Помилка при записі дії \"косинус\" для модуля бібліотеки \"математика\".");
      }
    }
  }
}

