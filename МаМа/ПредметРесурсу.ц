  зовнішня дія предмет_ресурсу_текст(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_метод_чародія_текст(М, предмет_ресурсу, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_юнікод(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_метод_чародія_юнікод(М, предмет_ресурсу, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_отримати_елемент(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_властивість(М, предмет_ресурсу, назва(М, "чародія_отримати_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_змінити_елемент(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, ключ: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_властивість(М, предмет_ресурсу, назва(М, "чародія_змінити_елемент"), 2, ключ::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_видалити_елемент(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_властивість(М, предмет_ресурсу, назва(М, "чародія_видалити_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_отримати_перебір(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_властивість(М, предмет_ресурсу, назва(М, "чародія_перебір"), 0, пусто, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_отримати_діапазон(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, включно: логічне, до: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    ціль аргументи = виділити_памʼять<адреса<Предмет>>(М, 2);
    якщо включно {
      аргументи[0] = М.предмет_логічного_так як адреса<Предмет>;
    } інакше {
      аргументи[0] = М.предмет_логічного_ні як адреса<Предмет>;
    }
    аргументи[1] = до;
    ціль результат = предмет_ресурсу_виконати_властивість(М, предмет_ресурсу, назва(М, "чародія_діапазон"), 2, аргументи, 0, пусто, місцезнаходження);
    звільнити_памʼять(М, аргументи);
    вернути результат;
  }
}