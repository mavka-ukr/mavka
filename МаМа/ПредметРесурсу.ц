  }

  зовнішня дія предмет_ресурсу_містить(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_властивість(М, предмет_ресурсу, назва(М, "чародія_містить"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_числова_негація(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_властивість(М, предмет_ресурсу, назва(М, "чародія_числова_негація"), 0, пусто, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_отримати_число(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
