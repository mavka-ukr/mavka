взяти визначення МаМа;

секція МаМа {
  дія предмет_ресурсу_виконати_метод_чародія_текст(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_тексту(М, "<ресурс>") як адреса<Предмет>;
  }

  дія предмет_ресурсу_виконати_метод_чародія_юнікод(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_юнікоду_з_ю8(М, ю8"<ресурс>") як адреса<Предмет>;
  }
}

секція МаМа {
  дія рідна_дія_метода_чародія_текст(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ресурс {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_текст\" не для ресурсу") як адреса<Предмет>);
    }
    ціль предмет_ресурсу = предмет_я як адреса<ПредметРесурсу>;
    вернути предмет_ресурсу_виконати_метод_чародія_текст(М, предмет_ресурсу, місцезнаходження);
  }

  дія записати_метод_структури_ресурс_чародія_текст(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_текст = створити_предмет_рідної_дії(М, назва(М, "чародія_текст"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_текст, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ресурс, назва(М, "чародія_текст"), предмет_дії_метода_чародія_текст);
  }

  дія рідна_дія_метода_чародія_юнікод(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ресурс {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_юнікод\" не для ресурсу") як адреса<Предмет>);
    }
    ціль предмет_ресурсу = предмет_я як адреса<ПредметРесурсу>;
    вернути предмет_ресурсу_виконати_метод_чародія_юнікод(М, предмет_ресурсу, місцезнаходження);
  }

  дія записати_метод_структури_ресурс_чародія_юнікод(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_юнікод = створити_предмет_рідної_дії(М, назва(М, "чародія_юнікод"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_юнікод, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ресурс, назва(М, "чародія_юнікод"), предмет_дії_метода_чародія_юнікод);
  }

  зовнішня дія заповнити_предмет_структури_ресурс(М: адреса<Машина>) {
    // методи
    записати_метод_структури_ресурс_чародія_текст(М);
    записати_метод_структури_ресурс_чародія_юнікод(М);
  }

  зовнішня дія створити_предмет_ресурсу(М: адреса<Машина>, значення: невідома_адреса, дія_перед_знищенням: (М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>) -> ніщо): адреса<ПредметРесурсу> {
    ціль предмет_ресурсу = створити_предмет<ПредметРесурсу>(М, М.предмет_структури_ресурс);
    предмет_ресурсу.значення = значення;
    предмет_ресурсу.дія_перед_знищенням = дія_перед_знищенням;
    вернути предмет_ресурсу;
  }

  зовнішня дія предмет_ресурсу_виконати(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_властивість(М, предмет_ресурсу, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_отримати_властивість(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна ціль метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_ресурсу.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_ресурсу_змінити_властивість(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  зовнішня дія предмет_ресурсу_видалити_властивість(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  зовнішня дія предмет_ресурсу_виконати_властивість(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    ціль предмет_властивості = предмет_ресурсу_отримати_властивість(М, предмет_ресурсу, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна ціль накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета ресурсу \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      ціль предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_ресурсу як адреса<Предмет>, кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_додати(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_ресурсу_виконати_властивість(М, предмет_ресурсу, назва(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_ресурсу_відняти(М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
