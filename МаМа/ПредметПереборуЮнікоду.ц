взяти визначення МаМа;

секція МаМа {
  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірЮнікоду {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору юнікоду") як адреса<Предмет>);
    }
    ціль предмет_перебору_юнікоду = предмет_я як адреса<ПредметПереборуЮнікоду>;
    якщо предмет_перебору_юнікоду.поточне_значення == пусто {
      предмет_перебору_юнікоду.позиція = 0;
    } інакше {
      предмет_перебору_юнікоду.позиція += 1;
    }
    якщо предмет_перебору_юнікоду.позиція < предмет_перебору_юнікоду.предмет_юнікоду.значення.розмір {
      предмет_перебору_юнікоду.поточне_значення = створити_предмет_числа(М, д64(предмет_перебору_юнікоду.предмет_юнікоду.значення.дані[предмет_перебору_юнікоду.позиція]));
      вернути М.предмет_логічного_так як адреса<Предмет>;
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  дія записати_метод_структури_ПеребірЮнікоду(М: адреса<Машина>) {
    ціль предмет_дії_метода_далі = створити_предмет_рідної_дії(М, назва(М, "далі"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірЮнікоду, назва(М, "далі"), предмет_дії_метода_далі);
  }

  зовнішня дія заповнити_предмет_структури_ПеребірЮнікоду(М: адреса<Машина>) {
    // методи
    записати_метод_структури_ПеребірЮнікоду(М);
  }

  зовнішня дія створити_предмет_перебору_юнікоду(М: адреса<Машина>, предмет_юнікоду: адреса<ПредметЮнікоду>): адреса<ПредметПереборуЮнікоду> {
    ціль предмет_перебору_юнікоду = створити_предмет<ПредметПереборуЮнікоду>(М, М.предмет_структури_ПеребірЮнікоду);
    предмет_перебору_юнікоду.предмет_юнікоду = предмет_юнікоду;
    предмет_перебору_юнікоду.позиція = 0;
    предмет_перебору_юнікоду.поточне_значення = пусто;
    вернути предмет_перебору_юнікоду;
  }

  зовнішня дія предмет_перебору_юнікоду_виконати(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметЮнікоду>, 0 }, створити_предмет_тексту(М, "Не втілено") як адреса<Предмет>);
  }

  зовнішня дія предмет_перебору_юнікоду_отримати_властивість(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "значення")) {
      вернути предмет_перебору_юнікоду.поточне_значення як адреса<Предмет>;
    }
    змінна ціль метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_юнікоду.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_перебору_юнікоду_змінити_властивість(М: адреса<Машина>, предмет_перебору_юнікоду: адреса<ПредметПереборуЮнікоду>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметЮнікоду>, 0 }, створити_предмет_тексту(М, "Не втілено") як адреса<Предмет>);
