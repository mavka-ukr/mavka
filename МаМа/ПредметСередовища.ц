}

секція МаМа {
  місцева дія заповнити_предмет_структури_Середовище(М: адреса<Машина>) {
    // методи
  }

  місцева дія створити_предмет_середовища(М: адреса<Машина>, зовнішнє: адреса<ПредметСередовища>, предмет_модуля: адреса<ПредметМодуля>, предмет_я: адреса<Предмет>): адреса<ПредметСередовища> {
    змінна предмет_середовища = створити_предмет<ПредметСередовища>(М, М.предмет_структури_Середовище);
    предмет_середовища.зовнішнє = зовнішнє;
    предмет_середовища.підмети = зробити_реєстр_з_даними<адреса<ПредметТексту>, адреса<Предмет>, Машина>(М, М, перевірити_чи_предмети_назви_рівні);
    предмет_середовища.предмет_модуля = предмет_модуля;
    предмет_середовища.предмет_я = предмет_я;
    вернути предмет_середовища;
  }

  місцева дія предмет_середовища_виконати(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_середовища_виконати_властивість(М, предмет_середовища, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  місцева дія предмет_середовища_отримати_властивість(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_середовища.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  місцева дія предмет_середовища_змінити_властивість(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  місцева дія предмет_середовища_виконати_властивість(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна предмет_властивості = предмет_середовища_отримати_властивість(М, предмет_середовища, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета середовища \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_середовища як адреса<Предмет>, кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

