  місцева дія предмет_середовища_отримати_елемент(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_середовища_виконати_властивість(М, предмет_середовища, назва(М, "чародія_отримати_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  місцева дія предмет_середовища_змінити_елемент(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, ключ: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_середовища_виконати_властивість(М, предмет_середовища, назва(М, "чародія_змінити_елемент"), 1, ключ::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  місцева дія предмет_середовища_отримати_перебір(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_середовища_виконати_властивість(М, предмет_середовища, назва(М, "чародія_перебір"), 0, пусто, 0, пусто, місцезнаходження);
  }

  місцева дія предмет_середовища_отримати_діапазон(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, включно: логічне, до: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна аргументи = виділити_памʼять<адреса<Предмет>>(М, 2);
    якщо включно {
      аргументи[0] = М.предмет_логічного_так як адреса<Предмет>;
    } інакше {
      аргументи[0] = М.предмет_логічного_ні як адреса<Предмет>;
    }
    аргументи[1] = до;
    змінна результат = предмет_середовища_виконати_властивість(М, предмет_середовища, назва(М, "чародія_діапазон"), 2, аргументи, 0, пусто, місцезнаходження);
    звільнити_памʼять(М, аргументи);
    вернути результат;
  }
}