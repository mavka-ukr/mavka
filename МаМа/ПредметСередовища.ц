взяти визначення МаМа;

секція МаМа {
  дія предмет_середовища_виконати_метод_чародія_текст(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_тексту(М, "<середовище>") як адреса<Предмет>;
  }

  дія предмет_середовища_виконати_метод_чародія_юнікод(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_юнікоду_з_ю8(М, ю8"<середовище>") як адреса<Предмет>;
  }
}

секція МаМа {
  дія рідна_дія_метода_чародія_текст(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_Середовище {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_текст\" не для середовища") як адреса<Предмет>);
    }
    ціль предмет_середовища = предмет_я як адреса<ПредметСередовища>;
    вернути предмет_середовища_виконати_метод_чародія_текст(М, предмет_середовища, місцезнаходження);
  }

  дія записати_метод_структури_Середовище_чародія_текст(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_текст = створити_предмет_рідної_дії(М, назва(М, "чародія_текст"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_текст, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_Середовище, назва(М, "чародія_текст"), предмет_дії_метода_чародія_текст);
  }

  дія рідна_дія_метода_чародія_юнікод(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_Середовище {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_юнікод\" не для середовища") як адреса<Предмет>);
    }
    ціль предмет_середовища = предмет_я як адреса<ПредметСередовища>;
    вернути предмет_середовища_виконати_метод_чародія_юнікод(М, предмет_середовища, місцезнаходження);
  }

  дія записати_метод_структури_Середовище_чародія_юнікод(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_юнікод = створити_предмет_рідної_дії(М, назва(М, "чародія_юнікод"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_юнікод, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_Середовище, назва(М, "чародія_юнікод"), предмет_дії_метода_чародія_юнікод);
  }

  зовнішня дія заповнити_предмет_структури_Середовище(М: адреса<Машина>) {
    // методи
    записати_метод_структури_Середовище_чародія_текст(М);
    записати_метод_структури_Середовище_чародія_юнікод(М);
  }

  зовнішня дія створити_предмет_середовища(М: адреса<Машина>, зовнішнє: адреса<ПредметСередовища>, предмет_модуля: адреса<ПредметМодуля>, предмет_я: адреса<Предмет>): адреса<ПредметСередовища> {
    ціль предмет_середовища = створити_предмет<ПредметСередовища>(М, М.предмет_структури_Середовище);
    предмет_середовища.зовнішнє = зовнішнє;
    предмет_середовища.підмети = зробити_реєстр_з_даними<адреса<ПредметТексту>, адреса<Предмет>, Машина>(М, М, перевірити_чи_предмети_назви_рівні);
    предмет_середовища.предмет_модуля = предмет_модуля;
    предмет_середовища.предмет_я = предмет_я;
    вернути предмет_середовища;
  }

  зовнішня дія предмет_середовища_виконати(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_середовища_виконати_властивість(М, предмет_середовища, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_середовища_отримати_властивість(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_середовища.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_середовища_змінити_властивість(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  зовнішня дія предмет_середовища_видалити_властивість(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  зовнішня дія предмет_середовища_виконати_властивість(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    ціль предмет_властивості = предмет_середовища_отримати_властивість(М, предмет_середовища, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета середовища \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      ціль предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_середовища як адреса<Предмет>, кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_середовища_додати(М: адреса<Машина>, предмет_середовища: адреса<ПредметСередовища>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_середовища_виконати_властивість(М, предмет_середовища, назва(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

