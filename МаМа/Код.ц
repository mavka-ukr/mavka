  місцева дія надрукувати_код(М: адреса<Машина>, код: адреса<Код>) {
    змінна ціль п: позитивне;
    надрукувати_Ю8(М, ю8сі"Вказівки:");
    п = 0;
    поки п < код.накопичувач_вказівок.розмір {
      вивести_Ю8(М, ю8сі" ");
      ціль вказівка = код.накопичувач_вказівок.дані[п];
      якщо вказівка == ВПокластиПусто {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ПокластиПусто");
      } інакше якщо вказівка == ВПокластиТак {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ПокластиТак");
      } інакше якщо вказівка == ВПокластиНі {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ПокластиНі");
      } інакше якщо вказівка == ВПокластиКонстанту {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ПокластиКонстанту");
        п += 1;
      } інакше якщо вказівка == ВСтворитиДію {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" СтворитиДію");
        п += 1;
      } інакше якщо вказівка == ВСтворитиСтруктуру {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" СтворитиСтруктуру");
      } інакше якщо вказівка == ВСтворитиМодуль {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" СтворитиМодуль");
        п += 1;
      } інакше якщо вказівка == ВСтворитиСписок {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" СтворитиСписок");
        п += 1;
      } інакше якщо вказівка == ВПокластиВСписок {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ПокластиВСписок");
      } інакше якщо вказівка == ВСтворитиСловник {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" СтворитиСловник");
        п += 1;
      } інакше якщо вказівка == ВПокластиВСловник {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ПокластиВСловник");
      } інакше якщо вказівка == ВПокластиСтруктуруОбʼєкт {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ПокластиСтруктуруОбʼєкт");
      } інакше якщо вказівка == ВПокластиЯ {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ПокластиЯ");
      } інакше якщо вказівка == ВЗаписатиМетод {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ЗаписатиМетод");
        п += 1;
      } інакше якщо вказівка == ВВизначити {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" Визначити");
        п += 1;
      } інакше якщо вказівка == ВЗвернутись {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" Звернутись");
        п += 1;
      } інакше якщо вказівка == ВВиконати {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" Виконати");
        п += 1;
      } інакше якщо вказівка == ВВиконатиЗНазвами {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ВиконатиЗНазвами");
        п += 1;
      } інакше якщо вказівка == ВОтриматиВластивість {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ОтриматиВластивість");
        п += 1;
      } інакше якщо вказівка == ВЗмінитиВластивість {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ЗмінитиВластивість");
        п += 1;
      } інакше якщо вказівка == ВВиконатиВластивість {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ВиконатиВластивість");
        п += 1;
        п += 1;
      } інакше якщо вказівка == ВВиконатиВластивістьЗНазвами {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ВиконатиВластивістьЗНазвами");
        п += 1;
        п += 1;
      } інакше якщо вказівка == ВОтриматиЕлемент {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ОтриматиЕлемент");
      } інакше якщо вказівка == ВЗмінитиЕлемент {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ЗмінитиЕлемент");
      } інакше якщо вказівка == ВОтриматиПеребір {
        вивести_п64(М, п64(п));
        надрукувати_Ю8(М, ю8сі" ОтриматиПеребір");
