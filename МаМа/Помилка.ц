взяти визначення МаМа;

секція МаМа {
  місцева дія виділити_місцезнаходження_падіння(М: адреса<Машина>, шлях_до_файлу: ю8, рядок: позитивне): адреса<МісцезнаходженняПадіння> {
    ціль місцезнаходження_падіння = виділити<МісцезнаходженняПадіння>(М);
    якщо виділити_копію_ю8_цілі(М, шлях_до_файлу, місцезнаходження_падіння.шлях_до_файлу::адреса) == ні {
      ПОМЕРТИ(М, ю8"Не вдалося виділити копію Ю8");
    }
    місцезнаходження_падіння.рядок = рядок;
    вернути місцезнаходження_падіння;
  }

  місцева дія виділити_місцезнаходження_падіння_з_вказівки_або_пусто(М: адреса<Машина>, код: адреса<Код>, позиція_вказівки: позитивне): адреса<МісцезнаходженняПадіння> {
    ціль місцезнаходження_вказівки = знайти_місцезнаходження_вказівки(код, позиція_вказівки);
    якщо місцезнаходження_вказівки != пусто {
      вернути виділити_місцезнаходження_падіння(М, код.шлях_до_файлу, місцезнаходження_вказівки.рядок);
    }
    вернути пусто;
  }

  місцева дія звільнити_місцезнаходження_падіння(М: адреса<Машина>, місцезнаходження_падіння: адреса<МісцезнаходженняПадіння>) {
    звільнити(М, місцезнаходження_падіння.шлях_до_файлу.дані як адреса);
    звільнити(М, місцезнаходження_падіння);
  }

  місцева дія виділити_предмет_тексту_помилки_невірний_тип_аргумента(М: адреса<Машина>, назва: ю8, очікується: Накопичувач<адреса<ПредметСтруктури>>, отримано: адреса<ПредметСтруктури>): адреса<Предмет> {
    ціль накопичувач_ю8 = зробити_накопичувач_ю8(М);
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"Невірний тип аргумента \"");
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, назва);
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"\"");
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8": очікується ");
    змінна ціль пп: позитивне = 0;
    поки пп < очікується.розмір {
      ціль тип = очікується.дані[пп];
      якщо тип == пусто {
        покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"\"пусто\"");
      } інакше {
        якщо тип.назва == пусто {
          покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"<структура без назви>");
        } інакше {
          покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"\"");
          ціль назва_ю8 = ю8 { 0, пусто };
          якщо виділити_ю8_з_назви(М, тип.назва, назва_ю8::адреса) == ні {
            ПОМЕРТИ(М, ю8"Не вдалось отримати ю8 з назви");
          }
          покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, назва_ю8);
          звільнити(М, назва_ю8.дані як адреса);
          покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"\"");
        }
      }
      якщо пп < очікується.розмір - 1 {
        покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8" або ");
      }
      пп += 1;
    }
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8", отримано ");
    якщо отримано == пусто {
      покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"\"пусто\"");
    } інакше {
      якщо отримано.назва == пусто {
        покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"<структура без назви>");
      } інакше {
        покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"\"");
          ціль назва_ю8 = ю8 { 0, пусто };
        якщо виділити_ю8_з_назви(М, отримано.назва, назва_ю8::адреса) == ні {
          ПОМЕРТИ(М, ю8"Не вдалось отримати ю8 з назви");
        }
        покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, назва_ю8);
        звільнити(М, назва_ю8.дані як адреса);
        покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"\"");
      }
    }
    ціль значення_ю8 = виділити_ю8_з_накопичувача_ю8(М, накопичувач_ю8::адреса);
    звільнити_значення_накопичувача_ю8(М, накопичувач_ю8);
    ціль предмет_тексу = виділити_предмет_юнікоду_з_ю8(М, значення_ю8);
    звільнити(М, значення_ю8.дані як адреса);
    вернути предмет_тексу як адреса<Предмет>;
  }

  місцева дія виділити_предмет_тексту_помилки_неможливо_виконати_пусто(М: адреса<Машина>): адреса<Предмет> {
