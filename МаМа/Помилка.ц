    ціль назва_чародії_ю8 = ю8 { 0, пусто };
    якщо виділити_ю8_з_назви(М, назва_чародії, назва_чародії_ю8::адреса) == ні {
      ПОМЕРТИ(М, ю8"Не вдалось отримати ю8 з назви");
    }
    ціль накопичувач_ю8 = зробити_накопичувач_ю8(М);
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"Неможливо виконати властивість \"");
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, назва_чародії_ю8);
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"\"");
    ціль значення_ю8 = виділити_ю8_з_накопичувача_ю8(М, накопичувач_ю8::адреса);
    ціль предмет_тексту = виділити_предмет_юнікоду_з_ю8(М, значення_ю8);
    звільнити(М, значення_ю8.дані як адреса);
    звільнити_значення_накопичувача_ю8(М, накопичувач_ю8);
    звільнити(М, назва_чародії_ю8.дані як адреса);
    вернути предмет_тексту як адреса<Предмет>;
  }

  місцева дія виділити_предмет_тексту_помилки_пропущено_аргумент(М: адреса<Машина>, назва: адреса<Назва>): адреса<Предмет> {
    ціль назва_ю8 = ю8 { 0, пусто };
    якщо виділити_ю8_з_назви(М, назва, назва_ю8::адреса) == ні {
      ПОМЕРТИ(М, ю8"Не вдалось отримати ю8 з назви");
    }
    ціль накопичувач_ю8 = зробити_накопичувач_ю8(М);
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"Пропущено аргумент \"");
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, назва_ю8);
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8::адреса, ю8"\"");
    ціль значення_ю8 = виділити_ю8_з_накопичувача_ю8(М, накопичувач_ю8::адреса);
    ціль предмет_тексту = виділити_предмет_юнікоду_з_ю8(М, значення_ю8);
    звільнити(М, значення_ю8.дані як адреса);
    звільнити_значення_накопичувача_ю8(М, накопичувач_ю8);
    звільнити(М, назва_ю8.дані як адреса);
    вернути предмет_тексту як адреса<Предмет>;
  }
}