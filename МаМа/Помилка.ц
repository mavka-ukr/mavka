взяти визначення МаМа;

секція МаМа {
  місцева дія виділити_місцезнаходження_падіння(М: адреса<Машина>, шлях_до_файлу: ю8, рядок: позитивне): адреса<МісцезнаходженняПадіння> {
    ціль місцезнаходження_падіння = виділити<МісцезнаходженняПадіння>(М);
    якщо виділити_копію_ю8(М, шлях_до_файлу, місцезнаходження_падіння.шлях_до_файлу::адреса) == ні {
      ПОМЕРТИ(М, ю8"Не вдалося виділити копію Ю8");
    }
    місцезнаходження_падіння.рядок = рядок;
    вернути місцезнаходження_падіння;
  }

  місцева дія виділити_місцезнаходження_падіння_з_вказівки_або_пусто(М: адреса<Машина>, код: адреса<Код>, позиція_вказівки: позитивне): адреса<МісцезнаходженняПадіння> {
    ціль місцезнаходження_вказівки = знайти_місцезнаходження_вказівки(код, позиція_вказівки);
    якщо місцезнаходження_вказівки != пусто {
      вернути виділити_місцезнаходження_падіння(М, код.шлях_до_файлу, місцезнаходження_вказівки.рядок);
    }
    вернути пусто;
  }

  місцева дія звільнити_місцезнаходження_падіння(М: адреса<Машина>, місцезнаходження_падіння: адреса<МісцезнаходженняПадіння>) {
    звільнити_памʼять(М, місцезнаходження_падіння.шлях_до_файлу.дані);
    звільнити(М, місцезнаходження_падіння);
  }

  місцева дія виділити_предмет_тексту_помилки_невірний_тип_аргумента(М: адреса<Машина>, назва: адреса<Назва>, очікується: Накопичувач<адреса<ПредметСтруктури>>, отримано: адреса<ПредметСтруктури>): адреса<Предмет> {
    змінна ціль накопичувач_т8 = зробити_накопичувач_т8(М);
    покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"Невірний тип аргумента \"");
    покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, назва.значення);
    покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"\"");
    покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8": очікується ");
    змінна ціль пп: позитивне = 0;
    поки пп < очікується.розмір {
      ціль тип = очікується.дані[пп];
      якщо тип == пусто {
        покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"\"пусто\"");
      } інакше {
        якщо тип.назва == пусто {
          покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"<структура без назви>");
        } інакше {
          покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"\"");
          покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, тип.назва.значення);
          покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"\"");
        }
      }
      якщо пп < очікується.розмір - 1 {
        покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8" або ");
      }
      пп += 1;
    }
    покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8", отримано ");
    якщо отримано == пусто {
      покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"\"пусто\"");
    } інакше {
      якщо отримано.назва == пусто {
        покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"<структура без назви>");
      } інакше {
        покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"\"");
        покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, отримано.назва.значення);
        покласти_в_накопичувач_т8(М, накопичувач_т8::адреса, т8"\"");
      }
    }
    змінна ціль значення_т8 = т8 { 0, пусто };
    якщо виділити_т8_з_накопичувача_т8(М, накопичувач_т8::адреса, значення_т8::адреса) == ні {
      // потім: помилка
    }
    звільнити_значення_накопичувача_т8(М, накопичувач_т8);
    ціль предмет_тексу = виділити_предмет_тексту(М, значення_т8);
    звільнити_памʼять(М, значення_т8.дані);
    вернути предмет_тексу як адреса<Предмет>;
  }

  місцева дія виділити_предмет_тексту_помилки_неможливо_виконати_пусто(М: адреса<Машина>): адреса<Предмет> {
