взяти визначення МаМа;

секція МаМа {
  дія предмет_перебору_тексту_виконати_метод_далі(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_перебору_тексту.поточне_значення == пусто {
      предмет_перебору_тексту.позиція = 0;
    } інакше {
      предмет_перебору_тексту.позиція += 1;
    }
    якщо предмет_перебору_тексту.позиція < предмет_перебору_тексту.предмет_тексту.значення.розмір {
      предмет_перебору_тексту.поточне_значення = створити_предмет_числа(М, д64(предмет_перебору_тексту.предмет_тексту.значення.дані[предмет_перебору_тексту.позиція]));
      вернути М.предмет_логічного_так як адреса<Предмет>;
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  дія предмет_перебору_тексту_виконати_метод_чародія_текст(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_тексту(М, "<ПеребірТексту>") як адреса<Предмет>;
  }

  дія предмет_перебору_тексту_виконати_метод_чародія_юнікод(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_юнікоду_з_ю8(М, ю8"<ПеребірТексту>") як адреса<Предмет>;
  }
}

секція МаМа {
  дія записати_параметр_структури_ПеребірТексту_предмет_тексту(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_предмет_тексту = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_предмет_тексту[0] = М.предмет_структури_текст;
    ціль тип_параметра_предмет_тексту = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_предмет_тексту });
    ціль параметр_предмет_тексту = створити_предмет_параметра(М, назва(М, "предмет_тексту"), тип_параметра_предмет_тексту, пусто);
    М.предмет_структури_ПеребірТексту.параметри.дані[0] = параметр_предмет_тексту;
  }

  дія записати_параметр_структури_ПеребірТексту_позиція(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_позиція = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_позиція[0] = М.предмет_структури_число;
    ціль тип_параметра_позиція = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_позиція });
    ціль параметр_позиція = створити_предмет_параметра(М, назва(М, "позиція"), тип_параметра_позиція, пусто);
    М.предмет_структури_ПеребірТексту.параметри.дані[1] = параметр_позиція;
  }

  дія записати_параметр_структури_ПеребірТексту_значення(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_значення = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_значення[0] = М.предмет_структури_число;
    ціль тип_параметра_значення = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_значення });
    ціль параметр_значення = створити_предмет_параметра(М, назва(М, "значення"), тип_параметра_значення, пусто);
    М.предмет_структури_ПеребірТексту.параметри.дані[2] = параметр_значення;
  }

  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірТексту {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору тексту") як адреса<Предмет>);
    }
    ціль предмет_перебору_тексту = предмет_я як адреса<ПредметПереборуТексту>;
    вернути предмет_перебору_тексту_виконати_метод_далі(М, предмет_перебору_тексту, місцезнаходження);
  }

  дія записати_метод_структури_ПеребірТексту_далі(М: адреса<Машина>) {
    ціль предмет_дії_метода_далі = створити_предмет_рідної_дії(М, назва(М, "далі"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірТексту, назва(М, "далі"), предмет_дії_метода_далі);
  }

  дія рідна_дія_метода_чародія_текст(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірТексту {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_текст\" не для перебору тексту") як адреса<Предмет>);
    }
    ціль предмет_перебору_тексту = предмет_я як адреса<ПредметПереборуТексту>;
    вернути предмет_перебору_тексту_виконати_метод_чародія_текст(М, предмет_перебору_тексту, місцезнаходження);
  }

  дія записати_метод_структури_ПеребірТексту_чародія_текст(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_текст = створити_предмет_рідної_дії(М, назва(М, "чародія_текст"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_текст, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірТексту, назва(М, "чародія_текст"), предмет_дії_метода_чародія_текст);
  }

  дія рідна_дія_метода_чародія_юнікод(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірТексту {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_юнікод\" не для перебору тексту") як адреса<Предмет>);
    }
    ціль предмет_перебору_тексту = предмет_я як адреса<ПредметПереборуТексту>;
    вернути предмет_перебору_тексту_виконати_метод_чародія_юнікод(М, предмет_перебору_тексту, місцезнаходження);
  }

  дія записати_метод_структури_ПеребірТексту_чародія_юнікод(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_юнікод = створити_предмет_рідної_дії(М, назва(М, "чародія_юнікод"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_юнікод, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірТексту, назва(М, "чародія_юнікод"), предмет_дії_метода_чародія_юнікод);
  }

  зовнішня дія заповнити_предмет_структури_ПеребірТексту(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ПеребірТексту_предмет_тексту(М);
    записати_параметр_структури_ПеребірТексту_позиція(М);
    записати_параметр_структури_ПеребірТексту_значення(М);

    // методи
    записати_метод_структури_ПеребірТексту_далі(М);
    записати_метод_структури_ПеребірТексту_чародія_текст(М);
    записати_метод_структури_ПеребірТексту_чародія_юнікод(М);
  }

  зовнішня дія створити_предмет_перебору_тексту(М: адреса<Машина>, предмет_тексту: адреса<ПредметТексту>): адреса<ПредметПереборуТексту> {
    ціль предмет_перебору_тексту = створити_предмет<ПредметПереборуТексту>(М, М.предмет_структури_ПеребірТексту);
    предмет_перебору_тексту.предмет_тексту = предмет_тексту;
    предмет_перебору_тексту.позиція = 0;
    предмет_перебору_тексту.поточне_значення = пусто;
    вернути предмет_перебору_тексту;
  }

  зовнішня дія предмет_перебору_тексту_виконати(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_перебору_тексту_отримати_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "предмет_тексту")) {
      вернути предмет_перебору_тексту.предмет_тексту як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "позиція")) {
      вернути створити_предмет_числа(М, д64(предмет_перебору_тексту.позиція)) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "значення")) {
      вернути предмет_перебору_тексту.поточне_значення як адреса<Предмет>;
    }
    змінна ціль метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_тексту.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_перебору_тексту_змінити_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  зовнішня дія предмет_перебору_тексту_видалити_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  зовнішня дія предмет_перебору_тексту_виконати_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    ціль предмет_властивості = предмет_перебору_тексту_отримати_властивість(М, предмет_перебору_тексту, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна ціль накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета перебору тексту \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      ціль предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_перебору_тексту як адреса<Предмет>, кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_перебору_тексту_додати(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, назва(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_тексту_відняти(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_тексту_виконати_властивість(М, предмет_перебору_тексту, назва(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_тексту_помножити(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
