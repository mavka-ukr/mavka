взяти визначення МаМа;

секція МаМа {
  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірТексту {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору тексту") як адреса<Предмет>);
    }
    ціль предмет_перебору_тексту = предмет_я як адреса<ПредметПереборуТексту>;
    якщо предмет_перебору_тексту.поточне_значення == пусто {
      предмет_перебору_тексту.позиція = 0;
    } інакше {
      предмет_перебору_тексту.позиція += 1;
    }
    якщо предмет_перебору_тексту.позиція < предмет_перебору_тексту.предмет_тексту.значення.розмір {
      предмет_перебору_тексту.поточне_значення = створити_предмет_числа(М, д64(предмет_перебору_тексту.предмет_тексту.значення.дані[предмет_перебору_тексту.позиція]));
      вернути М.предмет_логічного_так як адреса<Предмет>;
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  дія записати_метод_структури_ПеребірТексту(М: адреса<Машина>) {
    ціль предмет_дії_метода_далі = створити_предмет_рідної_дії(М, назва(М, "далі"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірТексту, назва(М, "далі"), предмет_дії_метода_далі);
  }

  зовнішня дія заповнити_предмет_структури_ПеребірТексту(М: адреса<Машина>) {
    // методи
    записати_метод_структури_ПеребірТексту(М);
  }

  зовнішня дія створити_предмет_перебору_тексту(М: адреса<Машина>, предмет_тексту: адреса<ПредметТексту>): адреса<ПредметПереборуТексту> {
    ціль предмет_перебору_тексту = створити_предмет<ПредметПереборуТексту>(М, М.предмет_структури_ПеребірТексту);
    предмет_перебору_тексту.предмет_тексту = предмет_тексту;
    предмет_перебору_тексту.позиція = 0;
    предмет_перебору_тексту.поточне_значення = пусто;
    вернути предмет_перебору_тексту;
  }

  зовнішня дія предмет_перебору_тексту_виконати(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметТексту>, 0 }, створити_предмет_тексту(М, "Не втілено") як адреса<Предмет>);
  }

  зовнішня дія предмет_перебору_тексту_отримати_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "значення")) {
      вернути предмет_перебору_тексту.поточне_значення як адреса<Предмет>;
    }
    змінна ціль метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_тексту.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_перебору_тексту_змінити_властивість(М: адреса<Машина>, предмет_перебору_тексту: адреса<ПредметПереборуТексту>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
