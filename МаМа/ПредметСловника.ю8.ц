  дія записати_метод_структури_словник_значення(М: адреса<Машина>) {
    ціль предмет_дії_метода_значення = створити_предмет_рідної_дії(М, назва(М, "значення"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_значення, пусто, пусто, пусто);
