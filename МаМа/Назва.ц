взяти визначення МаМа;

секція МаМа {
  дія додати_назву(М: адреса<Машина>, БН: адреса<БазаНазв>, значення: т8): позитивне;
  дія знайти_або_виділити_й_додати_назву(М: адреса<Машина>, БН: адреса<БазаНазв>, значення: т8): адреса<Назва>;
  дія виділити_назву_текст(М: адреса<Машина>, значення: т8): адреса<Назва>;

  місцева дія виділити_ю8_з_назви(М: адреса<Машина>, назва: адреса<Назва>, вихід: адреса<ю8>): логічне {
    вернути перекодувати_т8_в_ю8(М, назва.значення, вихід, ні);
  }

  місцева дія перевірити_чи_назви_рівні(назва1: адреса<Назва>, назва2: адреса<Назва>): логічне {
    вернути назва1 == назва2;
  }

  місцева дія назва(М: адреса<Машина>, значення: т8): адреса<Назва> {
    вернути знайти_або_виділити_й_додати_назву(М, М.БН::адреса, значення);
  }

  місцева дія позиція_назви(М: адреса<Машина>, значення: т8): позитивне {
    вернути додати_назву(М, М.БН::адреса, значення);
  }

  дія додати_назву(М: адреса<Машина>, БН: адреса<БазаНазв>, значення: т8): позитивне {
    змінна ціль позиція: позитивне = 0;
    поки позиція < БН.дані.розмір {
      ціль назва = БН.дані.дані[позиція];
      якщо перевірити_чи_т8_рівні(назва.значення, значення) {
        вернути позиція;
      }
      позиція = позиція + 1;
    }
    ціль назва = виділити_назву_текст(М, значення);
    покласти_в_накопичувач<адреса<Назва>>(М, БН.дані::адреса, назва);
    вернути БН.дані.розмір - 1;
  }

  дія знайти_або_виділити_й_додати_назву(М: адреса<Машина>, БН: адреса<БазаНазв>, значення: т8): адреса<Назва> {
    ціль позиція_назви = додати_назву(М, БН, значення);
    вернути БН.дані.дані[позиція_назви];
  }

  дія виділити_назву_текст(М: адреса<Машина>, значення: т8): адреса<Назва> {
    ціль назва = виділити<Назва>(М);
    ціль значення_тексту = т8 { 0, пусто };
    якщо виділити_копію_т8(М, значення, значення_тексту::адреса) == ні {
      // потім: помилка
    }
    назва.значення = значення_тексту;
    вернути назва;
  }

  місцева дія зробити_базу_назв(М: адреса<Машина>): БазаНазв {
    ціль БН: БазаНазв;
    БН.дані = зробити_накопичувач<адреса<Назва>>(М);
    БН.С.предок = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"предок");
    БН.С.чародія_додати = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_додати");
    БН.С.чародія_відняти = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_відняти");
    БН.С.чародія_помножити = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_помножити");
    БН.С.чародія_поділити = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_поділити");
    БН.С.чародія_остача = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_остача");
    БН.С.чародія_неповна_частка = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_неповна_частка");
    БН.С.чародія_степінь = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_степінь");
    БН.С.чародія_вліво = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_вліво");
    БН.С.чародія_вправо = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_вправо");
    БН.С.чародія_вправо_зі_заповненням = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_вправо_зі_заповненням");
    БН.С.чародія_двійкове_і = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_двійкове_і");
    БН.С.чародія_двійкове_або = знайти_або_виділити_й_додати_назву(М, БН::адреса, т8"чародія_двійкове_або");
