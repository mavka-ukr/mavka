  синонім ВЗсунутиВправо = 58; // [а, б]() -> [результат]
  синонім ВЗсунутиВправоЗіЗаповненням = 59; // [а, б]() -> [результат]
  синонім ВДвійковеІ = 60; // [а, б]() -> [результат]
  синонім ВДвійковеАБО = 61; // [а, б]() -> [результат]
  синонім ВДвійковеВиключнеАБО = 62; // [а, б]() -> [результат]
  синонім ВДвійковеНЕ = 63; // [а]() -> [результат]
  синонім ВМенше = 70; // [а, б]() -> [результат]
  синонім ВБільше = 71; // [а, б]() -> [результат]
  синонім ВНеБільше = 72; // [а, б]() -> [результат]
  синонім ВНеМенше = 73; // [а, б]() -> [результат]
  синонім ВРівно = 74; // [а, б]() -> [результат]
  синонім ВНеРівно = 75; // [а, б]() -> [результат]
  синонім ВМістить = 76; // [а, б]() -> [результат]
  синонім ВНеМістить = 77; // [а, б]() -> [результат]
  синонім ВЄ = 78; // [а, б]() -> [результат]
  синонім ВНеЄ = 79; // [а, б]() -> [результат]
  синонім ВВідʼємне = 80; // [а]() -> [результат]
  синонім ВЧислове = 81; // [а]() -> [результат]
  синонім ВЗаперечити = 82; // [а]() -> [результат]
  синонім ВЗбільшити = 83; // [а]() -> [результат]
  синонім ВЗбільшитиПопереднє = 84; // [а, б]() -> [результат, б]
  синонім ВЗменшити = 85; // [а]() -> [результат]
  // 86-99
  синонім ВПочатиСпробу = 100; // [назва](позиція_вказівки_зловити) -> []
  синонім ВВпасти = 101; // [предмет_помилки]() -> []
  синонім ВЗакінчитиСпробу = 102; // [](позиція_вказівки_виходу) -> []
  синонім ВВзяти = 103; // [назва, ...](кількість_назв) -> [предмет_модуля]
  синонім ВДати = 104; // [предмет, назва]() -> []
  синонім ВПеребратиДалі = 106; // [предмет_перебору]() -> [предмет_перебору, результат_виконання]
  синонім ВЗбитиТекст = 107; // [...](кількість_предметів) -> [збитий_текст]
  синонім ВЗбитиЮнікод = 108; // [...](кількість_предметів) -> [збитий_юнікод]
  // 109-129
  синонім ВСтворитиПараметр = 133; // [назва, тип, код_значення]() -> [параметр]
  синонім ВСтворитиТип = 134; // [...](кількість_допустимих) -> [тип]
  // 137-199
  синонім ВСтрибнути = 200; // [](позиція_вказівки) -> []
  синонім ВСтрибнутиЯкщоНі = 201; // [предмет](позиція_вказівки) -> []
  синонім ВЗабрати = 202; // [предмет]() -> []
  синонім ВВернути = 203; // [предмет]() -> []
  синонім ВСтрибнутиЯкщоНіЗалишивши = 204; // [предмет](позиція_вказівки) -> [предмет]
  синонім ВСтрибнутиЯкщоТак = 205; // [предмет](позиція_вказівки) -> [предмет]
  синонім ВСтрибнутиЯкщоТакЗалишивши = 206; // [предмет](позиція_вказівки) -> [предмет]
  синонім ВДублювати = 207; // [предмет]() -> [предмет, предмет]
  синонім ВДублюватиПопереднє = 208; // [предмет, предмет2]() -> [предмет, предмет2, предмет]
}

секція МаМа {
  структура Предмет;
  структура ПредметКоду;
  структура ПредметСередовища;
  структура ПредметДії;
  структура ПредметСтруктури;
  структура ПредметЛогічного;
  структура ПредметЧисла;
  структура ПредметТексту;
  структура ПредметЮнікоду;
  структура ПредметСписку;
  структура ПредметСловника;
  структура ПредметБайтів;
  структура ПредметМодуля;
  структура ПредметРесурсу;
  структура ПредметТипу;
  структура ПредметПараметра;
  структура ПредметДіапазонуЧисел;
  структура ПредметПереборуДіапазонуЧисел;
  структура ПредметПереборуСписку;
  структура ПредметПереборуТексту;
  структура ПредметПереборуЮнікоду;
  структура ПредметРозширення;
  структура ПредметПереборуБайтів;
  структура СкладенийПредмет;
  структура Виконувач;
  структура Машина;
}

секція МаМа {
  структура можливо<Т> {
    заповнено: позитивне;
    значення: Т;
  }

  структура Послідовність<Т> {
    розмір: позитивне;
    дані: памʼять<Т>;
  }

  структура Накопичувач<Т> {
    розмір: позитивне;
    дані: памʼять<Т>;
    вмісткість: позитивне;
  }
  дія зробити_накопичувач<Т>(М: адреса<Машина>): Накопичувач<Т>;
  дія зробити_накопичувач_розміром<Т>(М: адреса<Машина>, розмір: позитивне): Накопичувач<Т>;
  дія звільнити_накопичувач<Т>(М: адреса<Машина>, накопичувач: Накопичувач<Т>);
  дія покласти_в_накопичувач<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, значення: Т);
  дія забрати_з_накопичувача<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, вихід: адреса<Т>): логічне;
  дія розширити_накопичувач<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, розмір: позитивне, пусте_значення: Т);
  дія видалити_з_накопичувача<Т>(М: адреса<Машина>, накопичувач: адреса<Накопичувач<Т>>, позиція: позитивне): логічне;

  структура ЕлементРеєстру<К, З> {
    ключ: К;
    значення: З;
  }
  структура Реєстр<К, З> {
    елементи: Накопичувач<ЕлементРеєстру<К, З>>;
    дані_дії_перевірки_ключа: невідома_адреса;
    дія_перевірки_ключа: (дані: невідома_адреса, а: К, б: К) -> логічне;
  }
  дія зробити_реєстр<К, З>(М: адреса<Машина>, дія_перевірки_ключа: (дані: невідома_адреса, а: К, б: К) -> логічне): Реєстр<К, З>;
  дія зробити_реєстр_з_даними<К, З, Д>(М: адреса<Машина>, дані_дії_перевірки_ключа: адреса<Д>, дія_перевірки_ключа: (дані: адреса<Д>, а: К, б: К) -> логічне): Реєстр<К, З>;
  дія звільнити_реєстр<К, З>(М: адреса<Машина>, реєстр: Реєстр<К, З>);
  дія змінити_в_реєстрі<К, З>(М: адреса<Машина>, реєстр: адреса<Реєстр<К, З>>, ключ: К, значення: З);
  дія видалити_з_реєстру<К, З>(М: адреса<Машина>, реєстр: адреса<Реєстр<К, З>>, ключ: К): логічне;
  дія знайти_в_реєстрі<К, З>(М: адреса<Машина>, реєстр: адреса<Реєстр<К, З>>, ключ: К, вихід: адреса<ЕлементРеєстру>): логічне;
}

секція МаМа {
  структура Місцезнаходження {
    шлях: адреса<ПредметТексту> | адреса<ПредметЮнікоду>;
    рядок: позитивне;
  }

  структура ІменованийАргумент {
    назва: адреса<ПредметТексту>;
    предмет: адреса<Предмет>;
  }

  синонім предмет_або_стан_падіння = адреса<Предмет>;
  синонім РіднаДія = (М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження) -> предмет_або_стан_падіння;
  синонім ЗробитиКопіюДанихПредметаДії = (М: адреса<Машина>, предмет_дії: адреса<ПредметДії>) -> невідома_адреса;
  синонім ВідкликПередЗвільненнямПредметаДії = (М: адреса<Машина>, предмет_дії: адреса<ПредметДії>) -> ніщо;

  структура Предмет {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
  }

  структура ПредметСтруктури {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    властивості: Реєстр<адреса<ПредметТексту>, адреса<Предмет>>;
    середовище_створення: адреса<ПредметСередовища>;
    назва: адреса<ПредметТексту>;
    предок: адреса<ПредметСтруктури>;
    параметри: Послідовність<адреса<ПредметПараметра>>;
    методи: Реєстр<адреса<ПредметТексту>, адреса<ПредметДії>>;
    власні_дії: Реєстр<адреса<ПредметТексту>, адреса<ПредметДії>>;
    заборонити_наслідування: позитивне;
  }

  структура ПредметКоду {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    шлях: адреса<ПредметТексту> | адреса<ПредметЮнікоду>;
    константи: Послідовність<адреса<Предмет>>;
    байти: адреса<ПредметБайтів>;
  }

  структура ПредметСередовища {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    зовнішнє: адреса<ПредметСередовища>;
    підмети: Реєстр<адреса<ПредметТексту>, адреса<Предмет>>;
    предмет_модуля: адреса<ПредметМодуля>;
    предмет_я: адреса<Предмет>;
  }

  структура ПредметДії {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    середовище_створення: адреса<ПредметСередовища>;
    предмет_я: адреса<Предмет>;
    назва: адреса<ПредметТексту>;
    параметри: Послідовність<адреса<ПредметПараметра>>;
    тип_результату: адреса<ПредметТипу>;
    код: адреса<ПредметКоду>;
    рідна: РіднаДія;
    дані: невідома_адреса;
    зробити_копію_даних: ЗробитиКопіюДанихПредметаДії;
    відклик_перед_звільненням: ВідкликПередЗвільненнямПредметаДії;
  }

  структура ПредметЛогічного {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    значення: позитивне
  }

  структура ПредметЧисла {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    значення: д64;
  }

  структура ПредметТексту {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    значення: т8;
  }

  структура ПредметЮнікоду {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    значення: ю32;
  }

  структура ПредметСписку {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    елементи: Накопичувач<адреса<Предмет>>;
  }

  структура ПредметСловника {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    елементи: Реєстр<адреса<Предмет>, адреса<Предмет>>;
  }

  структура ПредметБайтів {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    розмір: позитивне;
    дані: памʼять<п8>;
  }

  структура ПредметМодуля {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    назва: адреса<ПредметТексту>;
    властивості: Реєстр<адреса<ПредметТексту>, адреса<Предмет>>;
    назва_паку: адреса<ПредметТексту>;
  }

  структура ПредметРесурсу {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    значення: невідома_адреса;
    дія_перед_знищенням: (М: адреса<Машина>, предмет_ресурсу: адреса<ПредметРесурсу>) -> ніщо;
  }

  структура ПредметТипу {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    може_бути_пусто: позитивне;
    допустимі_структури: Послідовність<адреса<ПредметСтруктури>>;
  }

  структура ПредметПараметра {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    назва: адреса<ПредметТексту>;
    тип_: адреса<ПредметТипу>;
    код_значення: адреса<ПредметКоду>;
  }

  структура ПредметДіапазонуЧисел {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    від: адреса<ПредметЧисла>;
    включно: позитивне;
    до: адреса<ПредметЧисла>;
  }

  структура ПредметПереборуДіапазонуЧисел {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    предмет_діапазону: адреса<ПредметДіапазонуЧисел>;
    поточне_значення: адреса<ПредметЧисла>;
  }

  структура ПредметПереборуСписку {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    предмет_списку: адреса<ПредметСписку>;
    почато: позитивне;
    позиція: позитивне;
    поточне_значення: адреса<Предмет>;
  }

  структура ПредметПереборуТексту {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    предмет_тексту: адреса<ПредметТексту>;
    позиція: позитивне;
    поточне_значення: адреса<ПредметЧисла>;
  }

  структура ПредметПереборуЮнікоду {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    предмет_юнікоду: адреса<ПредметЮнікоду>;
    позиція: позитивне;
    поточне_значення: адреса<ПредметЧисла>;
  }

  структура ПредметРозширення {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    Р: невідома_адреса;
    значення: адреса<Предмет>;
    дія_перед_знищенням: (М: адреса<Машина>, предмет_розширення: адреса<ПредметРозширення>) -> ніщо;
  }

  структура ПредметПереборуБайтів {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    предмет_байтів: адреса<ПредметБайтів>;
    позиція: позитивне;
    поточне_значення: адреса<ПредметЧисла>;
  }

  структура СкладенийПредмет {
    попередній_предмет: адреса<Предмет>;
    позначено: позитивне;
    тип: адреса<ПредметСтруктури>;
    властивості: Реєстр<адреса<ПредметТексту>, адреса<Предмет>>;
  }

  структура ЕлементІсторії {
    місцезнаходження: Місцезнаходження;
    предмет_дії: адреса<ПредметДії>;
    середовище: адреса<ПредметСередовища>;
  }

  структура Машина {
    дані: невідома_адреса;

    виділити_сиру_памʼять: (М: адреса<Машина>, розмір: позитивне) -> памʼять<п8>;
    перевиділити_сиру_памʼять: (М: адреса<Машина>, значення: памʼять<п8>, розмір: позитивне) -> памʼять<п8>;
    звільнити_сиру_памʼять: (М: адреса<Машина>, значення: памʼять<п8>) -> ніщо;
    померти: (М: адреса<Машина>, код: ц32, повідомлення: т8) -> ніщо;

    останній_предмет: адреса<Предмет>;
    кількість_предметів: позитивне;
    кількість_предметів_для_очищення: позитивне;
    потребує_очищення: позитивне;

    взяті_предмети: Реєстр<адреса<Предмет>, адреса<Предмет>>;
    обробник_взяття: (М: адреса<Машина>, назва_паку: адреса<ПредметТексту>, назва_репозиторія: адреса<ПредметТексту>, кількість_назв_шляху: позитивне, назви_шляху: памʼять<адреса<ПредметТексту>>) -> предмет_або_стан_падіння;

    предмет_глобального_середовища: адреса<ПредметСередовища>;
    предмет_структури_предмет: адреса<ПредметСтруктури>;
    предмет_структури_Структура: адреса<ПредметСтруктури>;
    предмет_структури_Код: адреса<ПредметСтруктури>;
    предмет_структури_Середовище: адреса<ПредметСтруктури>;
    предмет_структури_Дія: адреса<ПредметСтруктури>;
    предмет_структури_логічне: адреса<ПредметСтруктури>;
    предмет_структури_число: адреса<ПредметСтруктури>;
    предмет_структури_текст: адреса<ПредметСтруктури>;
    предмет_структури_юнікод: адреса<ПредметСтруктури>;
    предмет_структури_список: адреса<ПредметСтруктури>;
    предмет_структури_словник: адреса<ПредметСтруктури>;
    предмет_структури_байти: адреса<ПредметСтруктури>;
    предмет_структури_Модуль: адреса<ПредметСтруктури>;
    предмет_структури_ресурс: адреса<ПредметСтруктури>;
    предмет_структури_Тип: адреса<ПредметСтруктури>;
    предмет_структури_Параметр: адреса<ПредметСтруктури>;
    предмет_структури_ДіапазонЧисел: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірДіапазонуЧисел: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірСписку: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірТексту: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірЮнікоду: адреса<ПредметСтруктури>;
    предмет_структури_ПеребірБайтів: адреса<ПредметСтруктури>;
    предмет_структури_Розширення: адреса<ПредметСтруктури>;
    предмет_логічного_так: адреса<ПредметЛогічного>;
    предмет_логічного_ні: адреса<ПредметЛогічного>;

    історія: Накопичувач<ЕлементІсторії>;
    накопичувач_предметів: Накопичувач<адреса<Предмет>>;
    притримувачі_предметів: Накопичувач<Накопичувач<адреса<Предмет>>>;
    стан_падіння: позитивне;
    місцезнаходження_падіння: Місцезнаходження;
  }

  структура ЗбереженийСтанРозмірівМашини {
    розмір_історії: позитивне;
    розмір_накопичувача_предметів: позитивне;
    розмір_притримувачів_предметів: позитивне;
  }

  дія виділити<Т>(М: адреса<Машина>): адреса<Т> {
    вернути М.виділити_сиру_памʼять(М, Т.розмір) як адреса<Т>;
  }

  дія перевиділити<Т>(М: адреса<Машина>, значення: адреса<Т>, кількість: позитивне): памʼять<Т> {
    вернути М.перевиділити_сиру_памʼять(М, значення як памʼять<п8>, кількість * Т.розмір) як памʼять<Т>;
  }

  дія звільнити(М: адреса<Машина>, значення: невідома_адреса) {
    М.звільнити_сиру_памʼять(М, значення як памʼять<п8>);
  }

  дія виділити_памʼять<Т>(М: адреса<Машина>, кількість: позитивне): памʼять<Т> {
    вернути М.виділити_сиру_памʼять(М, кількість * Т.розмір) як памʼять<Т>;
  }

  дія перевиділити_памʼять<Т>(М: адреса<Машина>, значення: памʼять<Т>, кількість: позитивне): памʼять<Т> {
    вернути М.перевиділити_сиру_памʼять(М, значення як памʼять<п8>, кількість * Т.розмір) як памʼять<Т>;
  }

  дія звільнити_памʼять(М: адреса<Машина>, значення: невідома_памʼять) {
    М.звільнити_сиру_памʼять(М, значення як памʼять<п8>);
  }

  дія ПОМЕРТИ(М: адреса<Машина>, повідомлення: т8) {
    М.померти(М, 1, повідомлення);
  }

  дія увімкнути_стан_падіння(М: адреса<Машина>, місцезнаходження_падіння: Місцезнаходження) {
    М.стан_падіння = позитивне(так);
    М.місцезнаходження_падіння = місцезнаходження_падіння;
  }

  дія результат_стан_падіння(М: адреса<Машина>, місцезнаходження_падіння: Місцезнаходження, предмет_помилки: адреса<Предмет>): адреса<Предмет> {
    увімкнути_стан_падіння(М, місцезнаходження_падіння);
    вернути предмет_помилки;
  }

  // не забувати очистити історію!!!
  дія вимкнути_стан_падіння(М: адреса<Машина>) {
    М.стан_падіння = позитивне(ні);
    М.місцезнаходження_падіння = Місцезнаходження { пусто як адреса<ПредметТексту>, 0 };
  }
}

// Допоміжне
секція МаМа {
  зовнішня дія перевірити_чи_ю8_рівні(а: ю8, б: ю8): логічне;
  зовнішня дія перевірити_чи_ю32_рівні(а: ю32, б: ю32): логічне;
  зовнішня дія перевірити_чи_т8_рівні(а: т8, б: т8): логічне;
  зовнішня дія перевірити_чи_т32_рівні(а: т32, б: т32): логічне;
  зовнішня дія порахувати_розмір_ю8сі(значення: памʼять<п8>): позитивне;
  зовнішня дія порахувати_розмір_ю32сі(значення: памʼять<п32>): позитивне;
  зовнішня дія порахувати_розмір_т8сі(значення: памʼять<п8>): позитивне;
  зовнішня дія порахувати_розмір_т32сі(значення: памʼять<п32>): позитивне;
  зовнішня дія перевірити_чи_ю8_закінчується_на(значення: ю8, шукане: ю8): логічне;

  зовнішня дія виділити_копію_т8(М: адреса<Машина>, значення: т8, вихід: адреса<т8>): логічне;
  зовнішня дія виділити_копію_ю8(М: адреса<Машина>, значення: ю8, вихід: адреса<ю8>): логічне;
  зовнішня дія виділити_копію_ю32(М: адреса<Машина>, значення: ю32, вихід: адреса<ю32>): логічне;

  зовнішня дія виділити_копію_памʼяті(М: адреса<Машина>, розмір: позитивне, значення: памʼять<п8>, вихід: адреса<памʼять<п8>>): логічне;

  зовнішня дія перекодувати_ю8_в_т8(М: адреса<Машина>, вхід: ю8, вихід: адреса<т8>, з_нульовим_байтом_в_кінці: логічне): логічне;
  зовнішня дія перекодувати_ю8_в_ю32(М: адреса<Машина>, вхід: ю8, вихід: адреса<ю32>, з_нульовим_байтом_в_кінці: логічне): логічне;
  зовнішня дія перекодувати_ю32_в_ю8(М: адреса<Машина>, вхід: ю32, вихід: адреса<ю8>, з_нульовим_байтом_в_кінці: логічне): логічне;
  зовнішня дія перекодувати_т8_в_ю8(М: адреса<Машина>, вхід: т8, вихід: адреса<ю8>, з_нульовим_байтом_в_кінці: логічне): логічне;
}

// Машина
секція МаМа {
  структура Система {
    виділити_сиру_памʼять: (М: адреса<Машина>, розмір: позитивне) -> памʼять<п8>;
    перевиділити_сиру_памʼять: (М: адреса<Машина>, значення: памʼять<п8>, розмір: позитивне) -> памʼять<п8>;
    звільнити_сиру_памʼять: (М: адреса<Машина>, значення: памʼять<п8>) -> ніщо;
    померти: (М: адреса<Машина>, код: ц32, повідомлення: т8) -> ніщо;
  }

  зовнішня дія налаштувати_машину(М: адреса<Машина>, система: Система);
  зовнішня дія розлаштувати_машину(М: адреса<Машина>);
  зовнішня дія виконати_код(М: адреса<Машина>, код: адреса<ПредметКоду>, середовище: адреса<ПредметСередовища>): предмет_або_стан_падіння;
  зовнішня дія виконати_взяти(М: адреса<Машина>, середовище: адреса<ПредметСередовища>, репозиторій: адреса<ПредметТексту>, назви_шляху: Послідовність<адреса<ПредметТексту>>): предмет_або_стан_падіння;
  зовнішня дія отримати_взятий_предмет(М: адреса<Машина>, ключ: адреса<Предмет>, вихід: адреса<адреса<Предмет>>): логічне;
  зовнішня дія покласти_взятий_предмет(М: адреса<Машина>, ключ: адреса<Предмет>, значення: адреса<Предмет>);
}

// Очищувач
секція МаМа {
  зовнішня дія звільнити_предмет(М: адреса<Машина>, предмет: адреса<Предмет>);
  зовнішня дія очистити(М: адреса<Машина>);
}

// Предмет
секція МаМа {
  зовнішня дія перевірити_чи_предмети_назви_рівні(М: адреса<Машина>, а: адреса<ПредметТексту>, б: адреса<ПредметТексту>): логічне;
  зовнішня дія перевірити_чи_предмети_рівні(М: адреса<Машина>, а: адреса<Предмет>, б: адреса<Предмет>): логічне;

  дія створити_предмет<Т>(М: адреса<Машина>, тип: адреса<ПредметСтруктури>): адреса<Т> {
    ціль предмет = М.виділити_сиру_памʼять(М, Т.розмір) як адреса<Т>;
    предмет.попередній_предмет = М.останній_предмет;
    предмет.позначено = позитивне(ні);
    предмет.тип = тип;
    М.останній_предмет = предмет як адреса<Предмет>;
    М.кількість_предметів += 1;
    М.потребує_очищення = позитивне(М.кількість_предметів >= М.кількість_предметів_для_очищення);
    вернути предмет;
  }

  зовнішня дія заповнити_предмет_структури_Структура(М: адреса<Машина>);
  зовнішня дія заповнити_предмет_структури_Код(М: адреса<Машина>);
  зовнішня дія заповнити_предмет_структури_Середовище(М: адреса<Машина>);
  зовнішня дія заповнити_предмет_структури_Дія(М: адреса<Машина>);
