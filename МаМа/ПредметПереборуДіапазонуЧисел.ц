взяти визначення МаМа;

секція МаМа {
  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірДіапазонуЧисел {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору діапазону чисел") як адреса<Предмет>);
    }
    ціль предмет_перебору_діапазону_чисел = предмет_я як адреса<ПредметПереборуДіапазонуЧисел>;
    якщо предмет_перебору_діапазону_чисел.поточне_значення == пусто {
      предмет_перебору_діапазону_чисел.поточне_значення = предмет_перебору_діапазону_чисел.предмет_діапазону.від;
    } інакше {
      предмет_перебору_діапазону_чисел.поточне_значення = створити_предмет_числа(М, предмет_перебору_діапазону_чисел.поточне_значення.значення + 1.0);
    }
    якщо предмет_перебору_діапазону_чисел.предмет_діапазону.включно == позитивне(так) {
      якщо предмет_перебору_діапазону_чисел.поточне_значення.значення <= предмет_перебору_діапазону_чисел.предмет_діапазону.до.значення {
        вернути М.предмет_логічного_так як адреса<Предмет>;
      }
    } інакше {
      якщо предмет_перебору_діапазону_чисел.поточне_значення.значення < предмет_перебору_діапазону_чисел.предмет_діапазону.до.значення {
        вернути М.предмет_логічного_так як адреса<Предмет>;
      }
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  дія записати_метод_структури_ПеребірДіапазонуЧисел_далі(М: адреса<Машина>) {
    ціль предмет_дії_метода_далі = створити_предмет_рідної_дії(М, назва(М, "далі"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірДіапазонуЧисел, назва(М, "далі"), предмет_дії_метода_далі);
  }

  зовнішня дія заповнити_предмет_структури_ПеребірДіапазонуЧисел(М: адреса<Машина>) {
    // методи
    записати_метод_структури_ПеребірДіапазонуЧисел_далі(М);
  }

  зовнішня дія створити_предмет_перебору_діапазону_чисел(М: адреса<Машина>, предмет_діапазону: адреса<ПредметДіапазонуЧисел>): адреса<ПредметПереборуДіапазонуЧисел> {
    ціль предмет_перебору_діапазону_чисел = створити_предмет<ПредметПереборуДіапазонуЧисел>(М, М.предмет_структури_ПеребірДіапазонуЧисел);
    предмет_перебору_діапазону_чисел.предмет_діапазону = предмет_діапазону;
    предмет_перебору_діапазону_чисел.поточне_значення = пусто;
    вернути предмет_перебору_діапазону_чисел;
  }

  зовнішня дія предмет_перебору_діапазону_чисел_виконати(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметТексту>, 0 }, створити_предмет_тексту(М, "Не втілено") як адреса<Предмет>);
  }

  зовнішня дія предмет_перебору_діапазону_чисел_отримати_властивість(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "значення")) {
      вернути предмет_перебору_діапазону_чисел.поточне_значення як адреса<Предмет>;
    }
    змінна ціль метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_діапазону_чисел.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_перебору_діапазону_чисел_змінити_властивість(М: адреса<Машина>, предмет_перебору_діапазону_чисел: адреса<ПредметПереборуДіапазонуЧисел>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметТексту>, 0 }, створити_предмет_тексту(М, "Не втілено") як адреса<Предмет>);
