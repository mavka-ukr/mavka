  місцева дія надрукувати_предмет_структури_в_накопичувач_ю8(предмет_структури: адреса<ПредметСтруктури>, М: адреса<Машина>, накопичувач_ю8: адреса<НакопичувачЮ8>, глибина: позитивне) {
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8, ю8"<структура");
    якщо предмет_структури.назва != пусто {
      покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8, ю8" ");
      ціль назва_ю8 = ю8 { 0, пусто };
      якщо виділити_ю8_з_назви(М, предмет_структури.назва, назва_ю8::адреса) == ні {
        ПОМЕРТИ(М, ю8"Не вдалось отримати ю8 з назви дії");
      }
      покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8, назва_ю8);
      звільнити_памʼять(М, назва_ю8.дані);
    }
    покласти_ю8_в_накопичувач_ю8(М, накопичувач_ю8, ю8">");
  }
