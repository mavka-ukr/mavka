взяти визначення МаМа;

секція МаМа {
  дія предмет_діапазону_чисел_виконати_метод_чародія_перебір(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_перебору_діапазону_чисел(М, предмет_діапазону_чисел) як адреса<Предмет>;
  }

  дія предмет_діапазону_чисел_виконати_метод_чародія_текст(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_тексту(М, "<ДіапазонЧисел>") як адреса<Предмет>;
  }

  дія предмет_діапазону_чисел_виконати_метод_чародія_юнікод(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_юнікоду_з_ю8(М, ю8"<ДіапазонЧисел>") як адреса<Предмет>;
  }
}

секція МаМа {
  дія записати_параметр_структури_ДіапазонЧисел_від(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_від = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_від[0] = М.предмет_структури_число;
    ціль тип_параметра_від = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_від });
    ціль параметр_від = створити_предмет_параметра(М, назва(М, "від"), тип_параметра_від, пусто);
    М.предмет_структури_ДіапазонЧисел.параметри.дані[0] = параметр_від;
  }

  дія записати_параметр_структури_ДіапазонЧисел_включно(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_включно = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_включно[0] = М.предмет_структури_логічне;
    ціль тип_параметра_включно = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_включно });
    ціль параметр_включно = створити_предмет_параметра(М, назва(М, "включно"), тип_параметра_включно, пусто);
    М.предмет_структури_ДіапазонЧисел.параметри.дані[1] = параметр_включно;
  }

  дія записати_параметр_структури_ДіапазонЧисел_до(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_до = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_до[0] = М.предмет_структури_число;
    ціль тип_параметра_до = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_до });
    ціль параметр_до = створити_предмет_параметра(М, назва(М, "до"), тип_параметра_до, пусто);
    М.предмет_структури_ДіапазонЧисел.параметри.дані[2] = параметр_до;
  }

  дія рідна_дія_метода_чародія_перебір(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ДіапазонЧисел {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_перебір\" не для діапазону чисел") як адреса<Предмет>);
    }
    ціль предмет_діапазону_чисел = предмет_я як адреса<ПредметДіапазонуЧисел>;
    вернути предмет_діапазону_чисел_виконати_метод_чародія_перебір(М, предмет_діапазону_чисел, місцезнаходження);
  }

  дія записати_метод_структури_ДіапазонЧисел_чародія_перебір(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_перебір = створити_предмет_рідної_дії(М, назва(М, "чародія_перебір"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_перебір, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ДіапазонЧисел, назва(М, "чародія_перебір"), предмет_дії_метода_чародія_перебір);
  }

  дія рідна_дія_метода_чародія_текст(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ДіапазонЧисел {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_текст\" не для діапазону чисел") як адреса<Предмет>);
    }
    ціль предмет_діапазону_чисел = предмет_я як адреса<ПредметДіапазонуЧисел>;
    вернути предмет_діапазону_чисел_виконати_метод_чародія_текст(М, предмет_діапазону_чисел, місцезнаходження);
  }

  дія записати_метод_структури_ДіапазонЧисел_чародія_текст(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_текст = створити_предмет_рідної_дії(М, назва(М, "чародія_текст"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_текст, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ДіапазонЧисел, назва(М, "чародія_текст"), предмет_дії_метода_чародія_текст);
  }

  дія рідна_дія_метода_чародія_юнікод(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ДіапазонЧисел {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_юнікод\" не для діапазону чисел") як адреса<Предмет>);
    }
    ціль предмет_діапазону_чисел = предмет_я як адреса<ПредметДіапазонуЧисел>;
    вернути предмет_діапазону_чисел_виконати_метод_чародія_юнікод(М, предмет_діапазону_чисел, місцезнаходження);
  }

  дія записати_метод_структури_ДіапазонЧисел_чародія_юнікод(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_юнікод = створити_предмет_рідної_дії(М, назва(М, "чародія_юнікод"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_юнікод, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ДіапазонЧисел, назва(М, "чародія_юнікод"), предмет_дії_метода_чародія_юнікод);
  }

  зовнішня дія заповнити_предмет_структури_ДіапазонЧисел(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ДіапазонЧисел_від(М);
    записати_параметр_структури_ДіапазонЧисел_включно(М);
    записати_параметр_структури_ДіапазонЧисел_до(М);

    // методи
    записати_метод_структури_ДіапазонЧисел_чародія_перебір(М);
    записати_метод_структури_ДіапазонЧисел_чародія_текст(М);
    записати_метод_структури_ДіапазонЧисел_чародія_юнікод(М);
  }

  зовнішня дія створити_предмет_діапазону_чисел(М: адреса<Машина>, від: адреса<ПредметЧисла>, включно: логічне, до: адреса<ПредметЧисла>): адреса<ПредметДіапазонуЧисел> {
    ціль предмет_діапазону_чисел = створити_предмет<ПредметДіапазонуЧисел>(М, М.предмет_структури_ДіапазонЧисел);
    предмет_діапазону_чисел.від = від;
    предмет_діапазону_чисел.включно = позитивне(включно);
    предмет_діапазону_чисел.до = до;
    вернути предмет_діапазону_чисел;
  }

  зовнішня дія предмет_діапазону_чисел_виконати(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_діапазону_чисел_отримати_властивість(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "від")) {
      вернути предмет_діапазону_чисел.від як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "включно")) {
      якщо предмет_діапазону_чисел.включно == позитивне(так) {
        вернути М.предмет_логічного_так як адреса<Предмет>;
      }
      вернути М.предмет_логічного_ні як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "до")) {
      вернути предмет_діапазону_чисел.до як адреса<Предмет>;
    }
    змінна ціль метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_діапазону_чисел.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_діапазону_чисел_змінити_властивість(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  зовнішня дія предмет_діапазону_чисел_видалити_властивість(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  зовнішня дія предмет_діапазону_чисел_виконати_властивість(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    ціль предмет_властивості = предмет_діапазону_чисел_отримати_властивість(М, предмет_діапазону_чисел, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна ціль накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета діапазону чисел \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      ціль предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_діапазону_чисел як адреса<Предмет>, кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_діапазону_чисел_додати(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, назва(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_діапазону_чисел_відняти(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, назва(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_діапазону_чисел_помножити(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, назва(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_діапазону_чисел_поділити(М: адреса<Машина>, предмет_діапазону_чисел: адреса<ПредметДіапазонуЧисел>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_діапазону_чисел_виконати_властивість(М, предмет_діапазону_чисел, назва(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

