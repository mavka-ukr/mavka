    якщо ключ == пусто {
      вернути результат_стан_падіння(М, Місцезнаходження { пусто як адреса<ПредметТексту>, 0 }, створити_предмет_тексту(М, "Ключ отримання елемента зі списку має бути числом") як адреса<Предмет>);
    }
    якщо ключ.тип != М.предмет_структури_число {
