  зовнішня дія предмет_байтів_отримати_елемент(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, ключ: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо ключ == пусто {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Ключ отримання елемента з байтів має бути числом") як адреса<Предмет>);
    }
    якщо ключ.тип != М.предмет_структури_число {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Ключ отримання елемента з байтів має бути числом") як адреса<Предмет>);
    }
    ціль предмет_числа = ключ як адреса<ПредметЧисла>;
    вернути предмет_байтів_виконати_метод_чародія_отримати_елемент(М, предмет_байтів, предмет_числа, місцезнаходження);
  }

  зовнішня дія предмет_байтів_змінити_елемент(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, ключ: адреса<Предмет>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
