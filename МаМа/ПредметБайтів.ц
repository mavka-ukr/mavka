взяти визначення МаМа;

секція МаМа {
  дія предмет_байтів_виконати_метод_чародія_отримати_елемент(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, ключ: адреса<ПредметЧисла>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    ціль позиція = позитивне(ключ.значення);
    якщо предмет_байтів.розмір > позиція {
      вернути створити_предмет_числа(М, д64(предмет_байтів.дані[позиція])) як адреса<Предмет>;
    }
    вернути пусто;
  }

  дія предмет_байтів_виконати_метод_чародія_перебір(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_перебору_байтів(М, предмет_байтів) як адреса<Предмет>;
  }

  дія предмет_байтів_виконати_метод_чародія_текст(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_тексту(М, "<байти>") як адреса<Предмет>;
  }

  дія предмет_байтів_виконати_метод_чародія_юнікод(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_юнікоду_з_ю8(М, ю8"<байти>") як адреса<Предмет>;
  }
}

секція МаМа {
  дія записати_параметр_структури_байти_розмір(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_розмір = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_розмір[0] = М.предмет_структури_число;
    ціль тип_параметра_розмір = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_розмір });
    ціль параметр_розмір = створити_предмет_параметра(М, назва(М, "розмір"), тип_параметра_розмір, пусто);
    М.предмет_структури_байти.параметри.дані[0] = параметр_розмір;
  }

  дія рідна_дія_метода_чародія_отримати_елемент(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_байти {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_отримати_елемент\" не для байтів") як адреса<Предмет>);
    }
    змінна ціль аргумент_ключ: адреса<Предмет> = пусто;
    змінна ціль знайдено_аргумент_ключ = ні;
    якщо кількість_іменованих_аргументів > 0 {
      знайдено_аргумент_ключ = знайти_іменований_аргумент(М, кількість_іменованих_аргументів, іменовані_аргументи, назва(М, "ключ"), аргумент_ключ::адреса);
    }
    якщо кількість_аргументів > 0 {
      якщо знайдено_аргумент_ключ == ні {
        аргумент_ключ = аргументи[0];
        знайдено_аргумент_ключ = так;
      }
    }
    ціль предмет_байтів = предмет_я як адреса<ПредметБайтів>;
    вернути предмет_байтів_виконати_метод_чародія_отримати_елемент(М, предмет_байтів, аргумент_ключ як адреса<ПредметЧисла>, місцезнаходження);
  }

  дія записати_метод_структури_байти_чародія_отримати_елемент(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_ключ = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_ключ[0] = М.предмет_структури_число;
    ціль тип_параметра_ключ = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_ключ });
    ціль параметр_ключ = створити_предмет_параметра(М, назва(М, "ключ"), тип_параметра_ключ, пусто);
    ціль дані_параметрів_метода_чародія_отримати_елемент = виділити_памʼять<адреса<ПредметПараметра>>(М, 1);
    дані_параметрів_метода_чародія_отримати_елемент[0] = параметр_ключ;
    ціль предмет_дії_метода_чародія_отримати_елемент = створити_предмет_рідної_дії(М, назва(М, "чародія_отримати_елемент"), Послідовність<адреса<ПредметПараметра>> { 1, дані_параметрів_метода_чародія_отримати_елемент }, пусто, рідна_дія_метода_чародія_отримати_елемент, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_байти, назва(М, "чародія_отримати_елемент"), предмет_дії_метода_чародія_отримати_елемент);
  }

  дія рідна_дія_метода_чародія_перебір(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_байти {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_перебір\" не для байтів") як адреса<Предмет>);
    }
    ціль предмет_байтів = предмет_я як адреса<ПредметБайтів>;
    вернути предмет_байтів_виконати_метод_чародія_перебір(М, предмет_байтів, місцезнаходження);
  }

  дія записати_метод_структури_байти_чародія_перебір(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_перебір = створити_предмет_рідної_дії(М, назва(М, "чародія_перебір"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_перебір, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_байти, назва(М, "чародія_перебір"), предмет_дії_метода_чародія_перебір);
  }

  дія рідна_дія_метода_чародія_текст(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_байти {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_текст\" не для байтів") як адреса<Предмет>);
    }
    ціль предмет_байтів = предмет_я як адреса<ПредметБайтів>;
    вернути предмет_байтів_виконати_метод_чародія_текст(М, предмет_байтів, місцезнаходження);
  }

  дія записати_метод_структури_байти_чародія_текст(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_текст = створити_предмет_рідної_дії(М, назва(М, "чародія_текст"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_текст, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_байти, назва(М, "чародія_текст"), предмет_дії_метода_чародія_текст);
  }

  дія рідна_дія_метода_чародія_юнікод(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_байти {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_юнікод\" не для байтів") як адреса<Предмет>);
    }
    ціль предмет_байтів = предмет_я як адреса<ПредметБайтів>;
    вернути предмет_байтів_виконати_метод_чародія_юнікод(М, предмет_байтів, місцезнаходження);
  }

  дія записати_метод_структури_байти_чародія_юнікод(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_юнікод = створити_предмет_рідної_дії(М, назва(М, "чародія_юнікод"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_юнікод, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_байти, назва(М, "чародія_юнікод"), предмет_дії_метода_чародія_юнікод);
  }

  зовнішня дія заповнити_предмет_структури_байти(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_байти_розмір(М);

    // методи
    записати_метод_структури_байти_чародія_отримати_елемент(М);
    записати_метод_структури_байти_чародія_перебір(М);
    записати_метод_структури_байти_чародія_текст(М);
    записати_метод_структури_байти_чародія_юнікод(М);
  }

  зовнішня дія виконати_предмет_структури_байти(М: адреса<Машина>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна ціль аргумент_значення: адреса<Предмет> = пусто;
    змінна ціль знайдено_аргумент_значення = ні;
    якщо кількість_іменованих_аргументів > 0 {
      знайдено_аргумент_значення = знайти_іменований_аргумент(М, кількість_іменованих_аргументів, іменовані_аргументи, назва(М, "значення"), аргумент_значення::адреса);
    }
    якщо кількість_аргументів > 0 {
      якщо знайдено_аргумент_значення == ні {
        аргумент_значення = аргументи[0];
        знайдено_аргумент_значення = так;
      }
    }
    якщо знайдено_аргумент_значення == ні {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Пропущено аргумент \"значення\"") як адреса<Предмет>);
    }
    якщо аргумент_значення == пусто {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо отримати байти з пусто") як адреса<Предмет>);
    }
    вернути предмет_виконати_властивість(М, аргумент_значення, назва(М, "чародія_байти"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія створити_предмет_байтів(М: адреса<Машина>, розмір: позитивне, дані: памʼять<п8>): адреса<ПредметБайтів> {
    ціль предмет_байтів = створити_предмет<ПредметБайтів>(М, М.предмет_структури_байти);
    предмет_байтів.розмір = розмір;
    якщо виділити_копію_памʼяті(М, розмір, дані, предмет_байтів.дані::адреса) == ні {
      ПОМЕРТИ(М, "Не вдалось виділити копію памʼяті!");
      вернути пусто;
    }
    вернути предмет_байтів;
  }

  зовнішня дія створити_предмет_байтів_без_копіювання(М: адреса<Машина>, розмір: позитивне, дані: памʼять<п8>): адреса<ПредметБайтів> {
    ціль предмет_байтів = створити_предмет<ПредметБайтів>(М, М.предмет_структури_байти);
    предмет_байтів.розмір = розмір;
    предмет_байтів.дані = дані;
    вернути предмет_байтів;
  }

  зовнішня дія предмет_байтів_виконати(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_байтів_виконати_властивість(М, предмет_байтів, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_байтів_отримати_властивість(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "розмір")) {
      вернути створити_предмет_числа(М, д64(предмет_байтів.розмір)) як адреса<Предмет>;
    }
    змінна ціль метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_байтів.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_байтів_змінити_властивість(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  зовнішня дія предмет_байтів_видалити_властивість(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  зовнішня дія предмет_байтів_виконати_властивість(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    ціль предмет_властивості = предмет_байтів_отримати_властивість(М, предмет_байтів, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна ціль накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета байтів \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      ціль предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_байтів як адреса<Предмет>, кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

