взяти визначення МаМа;

секція МаМа {
  дія предмет_коду_виконати_метод_чародія_текст(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_тексту(М, "<код>") як адреса<Предмет>;
  }

  дія предмет_коду_виконати_метод_чародія_юнікод(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути створити_предмет_юнікоду_з_ю8(М, ю8"<код>") як адреса<Предмет>;
  }
}

секція МаМа {
  дія записати_параметр_структури_Код_версія(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_версія = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_версія[0] = М.предмет_структури_число;
    ціль тип_параметра_версія = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_версія });
    ціль параметр_версія = створити_предмет_параметра(М, назва(М, "версія"), тип_параметра_версія, пусто);
    М.предмет_структури_Код.параметри.дані[0] = параметр_версія;
  }

  дія записати_параметр_структури_Код_шлях(М: адреса<Машина>) {
    ціль параметр_шлях = створити_предмет_параметра(М, назва(М, "шлях"), пусто, пусто);
    М.предмет_структури_Код.параметри.дані[1] = параметр_шлях;
  }

  дія записати_параметр_структури_Код_константи(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_константи = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_константи[0] = М.предмет_структури_список;
    ціль тип_параметра_константи = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_константи });
    ціль параметр_константи = створити_предмет_параметра(М, назва(М, "константи"), тип_параметра_константи, пусто);
    М.предмет_структури_Код.параметри.дані[2] = параметр_константи;
  }

  дія записати_параметр_структури_Код_вказівки(М: адреса<Машина>) {
    ціль допустимі_структури_типу_параметра_вказівки = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_вказівки[0] = М.предмет_структури_байти;
    ціль тип_параметра_вказівки = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_вказівки });
    ціль параметр_вказівки = створити_предмет_параметра(М, назва(М, "вказівки"), тип_параметра_вказівки, пусто);
    М.предмет_структури_Код.параметри.дані[3] = параметр_вказівки;
  }

  дія рідна_дія_метода_чародія_текст(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_Код {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_текст\" не для коду") як адреса<Предмет>);
    }
    ціль предмет_коду = предмет_я як адреса<ПредметКоду>;
    вернути предмет_коду_виконати_метод_чародія_текст(М, предмет_коду, місцезнаходження);
  }

  дія записати_метод_структури_Код_чародія_текст(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_текст = створити_предмет_рідної_дії(М, назва(М, "чародія_текст"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_текст, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_Код, назва(М, "чародія_текст"), предмет_дії_метода_чародія_текст);
  }

  дія рідна_дія_метода_чародія_юнікод(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_Код {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"чародія_юнікод\" не для коду") як адреса<Предмет>);
    }
    ціль предмет_коду = предмет_я як адреса<ПредметКоду>;
    вернути предмет_коду_виконати_метод_чародія_юнікод(М, предмет_коду, місцезнаходження);
  }

  дія записати_метод_структури_Код_чародія_юнікод(М: адреса<Машина>) {
    ціль предмет_дії_метода_чародія_юнікод = створити_предмет_рідної_дії(М, назва(М, "чародія_юнікод"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_чародія_юнікод, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_Код, назва(М, "чародія_юнікод"), предмет_дії_метода_чародія_юнікод);
  }

  зовнішня дія заповнити_предмет_структури_Код(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_Код_версія(М);
    записати_параметр_структури_Код_шлях(М);
    записати_параметр_структури_Код_константи(М);
    записати_параметр_структури_Код_вказівки(М);

    // методи
    записати_метод_структури_Код_чародія_текст(М);
    записати_метод_структури_Код_чародія_юнікод(М);
  }

  зовнішня дія створити_предмет_коду(М: адреса<Машина>, шлях: адреса<ПредметТексту>, константи: Послідовність<адреса<Предмет>>, байти: адреса<ПредметБайтів>): адреса<ПредметКоду> {
    ціль предмет_коду = створити_предмет<ПредметКоду>(М, М.предмет_структури_Код);
    предмет_коду.шлях = шлях;
    предмет_коду.константи = константи;
    предмет_коду.байти = байти;
    вернути предмет_коду;
  }

  зовнішня дія створити_предмет_коду_1(М: адреса<Машина>, шлях: адреса<ПредметЮнікоду>, константи: Послідовність<адреса<Предмет>>, байти: адреса<ПредметБайтів>): адреса<ПредметКоду> {
    ціль предмет_коду = створити_предмет<ПредметКоду>(М, М.предмет_структури_Код);
    предмет_коду.шлях = шлях;
    предмет_коду.константи = константи;
    предмет_коду.байти = байти;
    вернути предмет_коду;
  }

  зовнішня дія предмет_коду_виконати(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_коду_отримати_властивість(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "версія")) {
      вернути створити_предмет_числа(М, 0) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "шлях")) {
      вернути предмет_коду.шлях як адреса<ПредметТексту> як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "константи")) {
      вернути створити_предмет_списку_1(М, предмет_коду.константи) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "вказівки")) {
      вернути предмет_коду.байти як адреса<Предмет>;
    }
    змінна ціль метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_коду.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_коду_змінити_властивість(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  зовнішня дія предмет_коду_видалити_властивість(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }

  зовнішня дія предмет_коду_виконати_властивість(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    ціль предмет_властивості = предмет_коду_отримати_властивість(М, предмет_коду, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна ціль накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета коду \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      ціль предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_коду як адреса<Предмет>, кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_коду_додати(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, назва(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_коду_відняти(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, назва(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_коду_помножити(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, назва(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_коду_поділити(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, назва(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_коду_остача(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, назва(М, "чародія_остача"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_коду_неповна_частка(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, назва(М, "чародія_неповна_частка"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_коду_степінь(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, назва(М, "чародія_степінь"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_коду_зсунути_вліво(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_коду_виконати_властивість(М, предмет_коду, назва(М, "чародія_вліво"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_коду_зсунути_вправо(М: адреса<Машина>, предмет_коду: адреса<ПредметКоду>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
