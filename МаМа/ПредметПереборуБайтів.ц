взяти визначення МаМа;

секція МаМа {
  дія предмет_перебору_байтів_виконати_метод_далі(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_перебору_байтів.поточне_значення == пусто {
      предмет_перебору_байтів.позиція = 0;
    } інакше {
      предмет_перебору_байтів.позиція += 1;
    }
    якщо предмет_перебору_байтів.позиція < предмет_перебору_байтів.предмет_байтів.розмір {
      предмет_перебору_байтів.поточне_значення = створити_предмет_числа(М, д64(предмет_перебору_байтів.предмет_байтів.дані[предмет_перебору_байтів.позиція]));
      вернути М.предмет_логічного_так як адреса<Предмет>;
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }
}

секція МаМа {
  дія записати_параметр_структури_ПеребірБайтів_предмет_байтів(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_предмет_байтів = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_предмет_байтів[0] = М.предмет_структури_байти;
    змінна тип_параметра_предмет_байтів = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_предмет_байтів });
    змінна параметр_предмет_байтів = створити_предмет_параметра(М, назва(М, "предмет_байтів"), тип_параметра_предмет_байтів, пусто);
    М.предмет_структури_ПеребірБайтів.параметри.дані[0] = параметр_предмет_байтів;
  }

  дія записати_параметр_структури_ПеребірБайтів_позиція(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_позиція = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_позиція[0] = М.предмет_структури_число;
    змінна тип_параметра_позиція = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_позиція });
    змінна параметр_позиція = створити_предмет_параметра(М, назва(М, "позиція"), тип_параметра_позиція, пусто);
    М.предмет_структури_ПеребірБайтів.параметри.дані[1] = параметр_позиція;
  }

  дія записати_параметр_структури_ПеребірБайтів_значення(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_значення = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_значення[0] = М.предмет_структури_число;
    змінна тип_параметра_значення = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_значення });
    змінна параметр_значення = створити_предмет_параметра(М, назва(М, "значення"), тип_параметра_значення, пусто);
    М.предмет_структури_ПеребірБайтів.параметри.дані[2] = параметр_значення;
  }

  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірБайтів {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору байтів") як адреса<Предмет>);
    }
    змінна предмет_перебору_байтів = предмет_я як адреса<ПредметПереборуБайтів>;
    вернути предмет_перебору_байтів_виконати_метод_далі(М, предмет_перебору_байтів, місцезнаходження);
  }

  дія записати_метод_структури_ПеребірБайтів_далі(М: адреса<Машина>) {
    змінна предмет_дії_метода_далі = створити_предмет_рідної_дії(М, назва(М, "далі"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірБайтів, назва(М, "далі"), предмет_дії_метода_далі);
  }

  зовнішня дія заповнити_предмет_структури_ПеребірБайтів(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ПеребірБайтів_предмет_байтів(М);
    записати_параметр_структури_ПеребірБайтів_позиція(М);
    записати_параметр_структури_ПеребірБайтів_значення(М);

    // методи
    записати_метод_структури_ПеребірБайтів_далі(М);
  }

  зовнішня дія створити_предмет_перебору_байтів(М: адреса<Машина>, предмет_байтів: адреса<ПредметБайтів>): адреса<ПредметПереборуБайтів> {
    змінна предмет_перебору_байтів = створити_предмет<ПредметПереборуБайтів>(М, М.предмет_структури_ПеребірБайтів);
    предмет_перебору_байтів.предмет_байтів = предмет_байтів;
    предмет_перебору_байтів.позиція = 0;
    предмет_перебору_байтів.поточне_значення = пусто;
    вернути предмет_перебору_байтів;
  }

  зовнішня дія предмет_перебору_байтів_виконати(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_байтів_виконати_властивість(М, предмет_перебору_байтів, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_отримати_властивість(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "предмет_байтів")) {
      вернути предмет_перебору_байтів.предмет_байтів як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "позиція")) {
      вернути створити_предмет_числа(М, д64(предмет_перебору_байтів.позиція)) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "значення")) {
      вернути предмет_перебору_байтів.поточне_значення як адреса<Предмет>;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_байтів.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_перебору_байтів_змінити_властивість(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  зовнішня дія предмет_перебору_байтів_виконати_властивість(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна предмет_властивості = предмет_перебору_байтів_отримати_властивість(М, предмет_перебору_байтів, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета перебору байтів \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_перебору_байтів як адреса<Предмет>, кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_додати(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_байтів_виконати_властивість(М, предмет_перебору_байтів, назва(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_відняти(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_байтів_виконати_властивість(М, предмет_перебору_байтів, назва(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_помножити(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_байтів_виконати_властивість(М, предмет_перебору_байтів, назва(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_поділити(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_байтів_виконати_властивість(М, предмет_перебору_байтів, назва(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_остача(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_байтів_виконати_властивість(М, предмет_перебору_байтів, назва(М, "чародія_остача"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_неповна_частка(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_байтів_виконати_властивість(М, предмет_перебору_байтів, назва(М, "чародія_неповна_частка"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_степінь(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_байтів_виконати_властивість(М, предмет_перебору_байтів, назва(М, "чародія_степінь"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_зсунути_вліво(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_байтів_виконати_властивість(М, предмет_перебору_байтів, назва(М, "чародія_вліво"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_байтів_зсунути_вправо(М: адреса<Машина>, предмет_перебору_байтів: адреса<ПредметПереборуБайтів>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
