взяти визначення МаМа;

секція МаМа {
  дія предмет_перебору_списку_виконати_метод_далі(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_перебору_списку.почато == позитивне(так) {
      предмет_перебору_списку.позиція += 1;
    } інакше {
      предмет_перебору_списку.позиція = 0;
      предмет_перебору_списку.почато = позитивне(так);
    }
    якщо предмет_перебору_списку.позиція < предмет_перебору_списку.предмет_списку.елементи.розмір {
      предмет_перебору_списку.поточне_значення = предмет_перебору_списку.предмет_списку.елементи.дані[предмет_перебору_списку.позиція];
      вернути М.предмет_логічного_так як адреса<Предмет>;
    }
    вернути М.предмет_логічного_ні як адреса<Предмет>;
  }
}

секція МаМа {
  дія записати_параметр_структури_ПеребірСписку_предмет_списку(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_предмет_списку = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_предмет_списку[0] = М.предмет_структури_список;
    змінна тип_параметра_предмет_списку = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_предмет_списку });
    змінна параметр_предмет_списку = створити_предмет_параметра(М, назва(М, "предмет_списку"), тип_параметра_предмет_списку, пусто);
    М.предмет_структури_ПеребірСписку.параметри.дані[0] = параметр_предмет_списку;
  }

  дія записати_параметр_структури_ПеребірСписку_позиція(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_позиція = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_позиція[0] = М.предмет_структури_число;
    змінна тип_параметра_позиція = створити_предмет_типу(М, так, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_позиція });
    змінна параметр_позиція = створити_предмет_параметра(М, назва(М, "позиція"), тип_параметра_позиція, пусто);
    М.предмет_структури_ПеребірСписку.параметри.дані[1] = параметр_позиція;
  }

  дія записати_параметр_структури_ПеребірСписку_значення(М: адреса<Машина>) {
    змінна параметр_значення = створити_предмет_параметра(М, назва(М, "значення"), пусто, пусто);
    М.предмет_структури_ПеребірСписку.параметри.дані[2] = параметр_значення;
  }

  дія записати_параметр_структури_ПеребірСписку_почато(М: адреса<Машина>) {
    змінна допустимі_структури_типу_параметра_почато = виділити_памʼять<адреса<ПредметСтруктури>>(М, 1);
    допустимі_структури_типу_параметра_почато[0] = М.предмет_структури_логічне;
    змінна тип_параметра_почато = створити_предмет_типу(М, ні, Послідовність<адреса<ПредметСтруктури>> { 1, допустимі_структури_типу_параметра_почато });
    змінна параметр_почато = створити_предмет_параметра(М, назва(М, "почато"), тип_параметра_почато, пусто);
    М.предмет_структури_ПеребірСписку.параметри.дані[3] = параметр_почато;
  }

  дія рідна_дія_метода_далі(М: адреса<Машина>, предмет_дії: адреса<ПредметДії>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо предмет_я.тип != М.предмет_структури_ПеребірСписку {
      вернути результат_стан_падіння(М, місцезнаходження, створити_предмет_тексту(М, "Неможливо виконати дію \"далі\" не для перебору списку") як адреса<Предмет>);
    }
    змінна предмет_перебору_списку = предмет_я як адреса<ПредметПереборуСписку>;
    вернути предмет_перебору_списку_виконати_метод_далі(М, предмет_перебору_списку, місцезнаходження);
  }

  дія записати_метод_структури_ПеребірСписку_далі(М: адреса<Машина>) {
    змінна предмет_дії_метода_далі = створити_предмет_рідної_дії(М, назва(М, "далі"), Послідовність<адреса<ПредметПараметра>> { 0, пусто }, пусто, рідна_дія_метода_далі, пусто, пусто, пусто);
    предмет_структури_записати_метод(М, М.предмет_структури_ПеребірСписку, назва(М, "далі"), предмет_дії_метода_далі);
  }

  зовнішня дія заповнити_предмет_структури_ПеребірСписку(М: адреса<Машина>) {
    // параметри
    записати_параметр_структури_ПеребірСписку_предмет_списку(М);
    записати_параметр_структури_ПеребірСписку_позиція(М);
    записати_параметр_структури_ПеребірСписку_значення(М);
    записати_параметр_структури_ПеребірСписку_почато(М);

    // методи
    записати_метод_структури_ПеребірСписку_далі(М);
  }

  зовнішня дія створити_предмет_перебору_списку(М: адреса<Машина>, предмет_списку: адреса<ПредметСписку>): адреса<ПредметПереборуСписку> {
    змінна предмет_перебору_списку = створити_предмет<ПредметПереборуСписку>(М, М.предмет_структури_ПеребірСписку);
    предмет_перебору_списку.предмет_списку = предмет_списку;
    предмет_перебору_списку.почато = позитивне(ні);
    предмет_перебору_списку.позиція = 0;
    предмет_перебору_списку.поточне_значення = пусто;
    вернути предмет_перебору_списку;
  }

  зовнішня дія предмет_перебору_списку_виконати(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, предмет_я: адреса<Предмет>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_виконати"), кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_отримати_властивість(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, назва: адреса<ПредметТексту>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "предмет_списку")) {
      вернути предмет_перебору_списку.предмет_списку як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "почато")) {
      якщо предмет_перебору_списку.почато == позитивне(так) {
        вернути М.предмет_логічного_так як адреса<Предмет>;
      }
      вернути М.предмет_логічного_ні як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "позиція")) {
      вернути створити_предмет_числа(М, д64(предмет_перебору_списку.позиція)) як адреса<Предмет>;
    }
    якщо перевірити_чи_предмети_назви_рівні(М, назва, МаМа::назва(М, "значення")) {
      вернути предмет_перебору_списку.поточне_значення;
    }
    змінна метод: адреса<ПредметДії> = пусто;
    якщо знайти_метод_в_структурі(М, предмет_перебору_списку.тип, назва, метод::адреса) {
      вернути метод як адреса<Предмет>;
    }
    вернути пусто;
  }

  зовнішня дія предмет_перебору_списку_змінити_властивість(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, назва: адреса<ПредметТексту>, значення: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути пусто;
  }

  зовнішня дія предмет_перебору_списку_виконати_властивість(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, назва: адреса<ПредметТексту>, кількість_аргументів: позитивне, аргументи: памʼять<адреса<Предмет>>, кількість_іменованих_аргументів: позитивне, іменовані_аргументи: памʼять<ІменованийАргумент>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    змінна предмет_властивості = предмет_перебору_списку_отримати_властивість(М, предмет_перебору_списку, назва, місцезнаходження);
    якщо М.стан_падіння == позитивне(так) {
      вернути предмет_властивості;
    }
    якщо предмет_властивості == пусто {
      змінна накопичувач_т8_помилки = зробити_накопичувач<п8>(М);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "Неможливо виконати пусту властивість предмета перебору списку \"");
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, назва.значення);
      покласти_в_накопичувач_т8(М, накопичувач_т8_помилки::адреса, "\"");
      змінна предмет_помилки = створити_предмет_тексту(М, т8 { накопичувач_т8_помилки.розмір, накопичувач_т8_помилки.дані });
      звільнити_памʼять(М, накопичувач_т8_помилки.дані);
      вернути результат_стан_падіння(М, місцезнаходження, предмет_помилки як адреса<Предмет>);
    }
    вернути предмет_виконати(М, предмет_властивості, предмет_перебору_списку як адреса<Предмет>, кількість_аргументів, аргументи, кількість_іменованих_аргументів, іменовані_аргументи, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_додати(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_додати"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_відняти(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_відняти"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_помножити(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_помножити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_поділити(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_поділити"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_остача(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_остача"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_неповна_частка(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_неповна_частка"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_степінь(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_степінь"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_зсунути_вліво(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_вліво"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
  }

  зовнішня дія предмет_перебору_списку_зсунути_вправо(М: адреса<Машина>, предмет_перебору_списку: адреса<ПредметПереборуСписку>, аргумент: адреса<Предмет>, місцезнаходження: Місцезнаходження): предмет_або_стан_падіння {
    вернути предмет_перебору_списку_виконати_властивість(М, предмет_перебору_списку, назва(М, "чародія_вправо"), 1, аргумент::адреса як памʼять<адреса<Предмет>>, 0, пусто, місцезнаходження);
