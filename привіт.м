структура Гриб
  ;; опис гриба
кінець

структура Голова
  ;; опис голови
кінець

структура Шия
  ;; опис шиї
кінець

структура Тулуб
  ;; опис тулуба
кінець

структура Кінцівки
  ;; опис кінцівок
кінець

структура Тварина
  голова
  шия
  тулуб
  кінцівки
кінець

структура Людина є Тварина
  імʼя
  свідомість
кінець

адам = Людина(
  Голова(),
  Шия(),
  Тулуб(),
  Кінцівки(),
  "Давид",
  недійсне
)
;; єва = Людина(..., "*", так)

друк(адам є Людина) ;; так
друк(адам є Тварина) ;; так
друк(адам є Гриб) ;; ні