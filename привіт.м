кінець

п = Паспорт(
  номер=1,
  орган_видачі=ОрганВидачі(
    назва="Організація М"
  ),
  дата_видачі="10.02.2001",
  прізвище="Когут",
  імʼя="Давид",
  по_батькові="Богданович"
)

друк(п)