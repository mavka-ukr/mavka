структура Машина
  а
  х
кінець

структура Х
  б
кінець

структура Щось
  ага
кінець

дія ф(а)
  м = Машина(123, а=124)
  
  друк(м.а)
  
  м.а = 200
  
  друк(м.а)
  
  м.а = а
  
  друк(я)
  
  вернути м
кінець

х = Машина(10)
х.ф = ф
х.х = Х(Щось("привіт\р\"\\"))

друк(Машина)
друк(х.ф(321).а == 321)
друк(х.ф(321).а)

друк("привіт\р\"\\")